;=====================================================================
;입調타입：女・大人の汎用구상＋회화用ファイルです
;=====================================================================
@KOJO_TALK_PUB305,ARG,ARG:1

SELECTCASE ARG:1
	CASE 1
		;1:레이디・킬러즈
		PRINTFORMW 「어머……」
		PRINTFORMW 「후후, 무슨 잔재주를 부린 걸까나. 당신이 너무나도 매력적으로 보여요」
		PRINTFORMW 「하지만, 정말로 매력적인 사람이라면, 아무런 잔재주 없이도 사람을 포로로 할 수 있어요」
		PRINTFORMW 「%조사처리(CSTR:ARG:일인칭,"가")% 몸으로, 당신에게 가르쳐 줄・게・요%UNICODE(0x2661)%」
	CASE 2
		; 2:おねだり前の逡巡時(아이さつor問答で목적が決まっている場合)
	PRINTFORML 「당신 나쁜 사람은 아닌 것 같으니, 조금 전의 부탁 들어 주어도 괜찮아요」
	CASE 3
		; 3:交渉の목적を尋ねる
		SELECTCASE CFLAG:ARG:우호도
			CASE IS < 25
				IF RAND:2 == 0
					PRINTFORMW 「인간이 %CSTR:ARG:11%에게 무슨 용건일까?」
				ELSE
					PRINTFORML 「%CSTR:ARG:11%에게 무슨 용건?」
				ENDIF
			CASE IS < 50
				PRINTFORMW 「그래서……%CSTR:ARG:11%에게 무슨 용건인 거야?」
			CASEELSE
				PRINTFORMW 「기분도 좋으니까, %CSTR:ARG:11%에게 용건이 있다면 들어 줄게요」
		ENDSELECT
	CASE 84
		;84:要求前の1文
		PRINT 「그러네……」
	CASE 4
		; 4-6:교섭목적：やらせてくれと言った時の反応
		; 4:取り付く島もなし
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 
				PRINTFORMW 「그…그렇네」
				PRINTFORMW 「조건을 낼게요……」
				PRINTFORML 
				PRINTFORMW 「우선 죽으세요!」
				PRINTFORML 
				PRINTFORMW 격렬하게 거부되었다
			CASEELSE
				IF TALENT:ARG:처녀 == 1
					PRINTFORML 「무, 무슨 소리를 떠는 거야! 이 변태!!」
					PRINTFORMW 격렬하게 거부되었다
				ELSE
					PRINTFORML 「안타깝지만, 너 정도에게 안길 만큼 싼 여자가 아니거든」
					PRINTFORMW 격렬하게 거부되었다
				ENDIF
		ENDSELECT
	CASE 5
		; 5:交渉次第
		IF TALENT:ARG:처녀
			PRINTFORML 
			PRINTFORMW 「헤？　…아」
			PRINTFORMW 「그、그래、%조사처리(CSTR:ARG:일인칭,"와")%…저…H…하고 싶어…?」
			PRINTFORML 
			PRINTFORMW 「무、뭐어 %CSTR:ARG:일인칭%도 경험 풍부하고？　그그、그렇게 되어버리는 것도 어쩔 수 없는걸까」
			PRINTFORMW 「당신의 태도에 따라 생각 줄게 (키릭」
			PRINTFORML 
			PRINTFORMW （말할수없어말할수없어～）
			PRINTFORMW （아직 미경험이라고 말할 수 없어요ー！！）
			PRINTFORML 
		ELSEIF TALENT:ARG:프라이드높음
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 
					PRINTFORMW 「흐～음……」
					PRINTFORMW %조사처리(CALLNAME:ARG,"는")% %조사처리(CALLNAME:PLAYER,"를")% 감정하듯이 바라본다.
					PRINTFORML 
					PRINTFORMW 「…그래서、어느 정도나 공물을 받을 수 있을까?」
					PRINTFORML 
				CASE 1
					PRINTFORML 
					PRINTFORMW 「당신에게 %CSTR:ARG:일인칭%의 신체는 과분하다고 생각을 못하는걸까?」
					PRINTFORMW 「하지만、뭐어…」
					PRINTFORML 
					PRINTFORMW 「나름의 기개를 보여준다면…다시 생각해 줄지도 몰라요、후훗」
					PRINTFORML 
				CASEELSE
					PRINTFORML 
					PRINTFORMW 「이 %CSTR:ARG:일인칭% 를 안고 싶어?」
					PRINTFORMW 「%CSTR:ARG:일인칭%의 미모를 눈 앞에서 보는것으론 부족한 걸까…단」
					PRINTFORML 
					PRINTFORMW 「%조사처리(CSTR:ARG:일인칭,"는")% 싸구려가 아닌거、알고 있겠죠?」
					PRINTFORMW 「자아…얼마나 공물을 받칠 수 있을까?」
					PRINTFORML 
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 
					PRINTFORMW 「%조사처리(CSTR:ARG:11,"를")% 안고 싶어? 후후후 %ハート()%」
					PRINTFORMW 「글쎄、%CSTR:ARG:이인칭%의 태도 나름…、일까나♪」
					PRINTFORML
				CASEELSE
					PRINTFORML
					PRINTFORMW 「글쎄…당신 잘 보면 꽤 귀여우니、조건에 따라 생각해 봐도 괜찮겠네요」
					PRINTFORML
			ENDSELECT
		ENDIF
		PRINTFORMW 조건에 따라 하게 해 주는 것 같다
		PRINTFORML 
	CASE 6
		; 6:一発おｋ
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 
				PRINTFORMW 「어머♪상당히 직정적인 어프로치네」
				PRINTFORMW 「좋아요、다・아・만…」
				PRINTFORML 
				PRINTFORMW 「빠져버리면 안되요？　우후훗%ハート()%」
				PRINTFORML 
			CASEELSE
				PRINTFORML 
				PRINTFORML 「후훗、좋아요%UNICODE(0x2661)% 자아…여기 있답니다아%UNICODE(0x2661)%…」
				PRINTFORML 
		ENDSELECT
		;負けない…
		SIF TALENT:PLAYER:크고아름다움
			PRINTFORML 「당신에게… %조사처리(CSTR:ARG:일인칭,"를")% 채워 줄 수 있을까?」
		PRINTFORMW 쾌히 승낙해 주었다
