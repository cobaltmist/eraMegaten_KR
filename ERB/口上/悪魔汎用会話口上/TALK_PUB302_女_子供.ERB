	IF LOCAL > 30
		PRINTFORMW 「――아앗, 그만, 그만둬요―― 싫어어어어엇!!」
		PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 도망치려는 %조사처리(CALLNAME:ARG,"를")% 억지로 쓰러뜨리고, 몇 번이고 계속해서 범했다……
		PRINTFORMW %CALLNAME:ARG%의 몸 안은 하얀 점액으로 더럽혀졌고, 비부에서는 파과의 피가 섞인 백탁액이 대량으로 흘러내렸다.
		EXP:PLAYER:사정경험      += 5
		EXP:PLAYER:조교경험      += 5
		EXP:ARG:Ｖ경험 += 5
		EXP:ARG:질사경험 += 3
		EXP:ARG:이상경험 += 1
		TALENT:ARG:처녀 = 0
		;처녀관통내역用
		IF PLAYER == MASTER
			SETBIT FLAG:(50000+NO:ARG), 7
			;처녀탈취인수カウント
			FLAG:처녀탈취인수 += 1
		ENDIF
		
		;仲間加入판정＝기교LV*7
		;성공
		IF RAND:100 < (ABL:PLAYER:기교 * 7)
			PRINTFORMW 「더는…… 어떻게 되든 몰라요옷……%UNICODE(0x2661)%」
			EXP:ARG:절정경험 += 1
			RETURN 0
		;실패
		ELSE
			PRINTFORMW 「…귀신―! 외도―! …흐흑,흑흑…」
			RETURN 1
		ENDIF
	;실패
	ELSE
		PRINTFORML 「꺄―! 변태예요!」
		PRINTFORMW 「누가― 구해줘요! 범해져요―!」
		RETURN 1
	ENDIF
;非처녀
ELSE
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% %조사처리(CALLNAME:ARG,"를")% 쓰러트리고, 몇 번이고 범했다……
	PRINTFORMW %CALLNAME:ARG%의 몸 안이 백탁액으로 더럽혀지고, 비부에서 대량의 정액이 방울져 떨어졌다.
	PRINTFORMW 「더는…… 어떻게 되든 모르겠어……%UNICODE(0x2661)%」
	EXP:PLAYER:사정경험      += 5
	EXP:PLAYER:조교경험      += 5
	EXP:ARG:Ｖ경험   += 5
	EXP:ARG:성교경험 += 5
	EXP:ARG:정액경험 += 5
	EXP:ARG:질사경험 += 5
	RETURN 0
ENDIF
