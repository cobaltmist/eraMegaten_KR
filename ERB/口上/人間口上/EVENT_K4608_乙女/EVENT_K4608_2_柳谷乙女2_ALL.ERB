	PRINTFORMW 
	CFLAG:202 = 4
;함락済み
ELSEIF CFLAG:202 < 5 || FLAG:7 == 2
	;연모、친애
	IF TALENT:3 || TALENT:6
		;랜덤で구상が변화する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;음란、창부
	ELSEIF TALENT:4 || TALENT:7
		;랜덤で구상が변화する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;복종、예속
	ELSEIF TALENT:5 || TALENT:8
		;랜덤で구상が변화する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	CFLAG:202 = 5
ENDIF
RETURN 0

