ENDIF

;-------------------------------------------------
;조교開始時（2回눈以降、X1をキャラ번호に변경する）
;-------------------------------------------------
@EVENTTRAIN_SECOND_K4630
;반발각인Lv3
IF MARK:3 == 3 && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 4
;굴복각인Lv0
ELSEIF MARK:2 == 0 && (CFLAG:202 < 1 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 1
;굴복각인Lv1
ELSEIF MARK:2 == 1 && (CFLAG:202 < 2 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 2
;굴복각인Lv2
ELSEIF MARK:2 == 2 && (CFLAG:202 < 3 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 3
;굴복각인Lv3＋함락소질無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0) && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 4
;함락済み
ELSEIF CFLAG:202 < 5 || FLAG:7 == 2
	;연모、친애
	IF TALENT:3 || TALENT:6
		;랜덤で구상が변화する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;음란、창부
	ELSEIF TALENT:4 || TALENT:7
		;랜덤で구상が변화する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;복종、예속
	ELSEIF TALENT:5 || TALENT:8
		;랜덤で구상が변화する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	CFLAG:202 = 5
ENDIF
RETURN 0

