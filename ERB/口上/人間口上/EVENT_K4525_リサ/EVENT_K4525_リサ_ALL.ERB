				CSTR:이인칭 = %CALLNAME:MASTER%
			ELSE
				CSTR:이인칭 = %CALLNAME:MASTER%♡
			ENDIF
		ENDIF
	;음란
	ELSEIF TALENT:4
		PRINTFORMW 
	;복종
	ELSEIF TALENT:5
			PRINTFORMW 
	ENDIF
	CFLAG:201 = 5
	RETURN 0
ENDIF

