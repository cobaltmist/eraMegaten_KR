;-------------------------------------------------
;@KOJO_MESSAGE_PALAMCNG_A（4518をキャラ번호に置換）
;조교자の사정をトリガーにした구상
;-------------------------------------------------
@KOJO_MESSAGE_PALAMCNG_A_K4518_LEWD
;-------------------------------------------------
;조교자の사정時のテキスト
;-------------------------------------------------
;-------------------------------------------------
;사정개소변경時のテキスト　有効時はここで関数종료
;-------------------------------------------------
IF CFLAG:106
	CALL PALAM_MESSAGE_A
	;머리카락
	IF CFLAG:106 == 1
	;顔
	ELSEIF CFLAG:106 == 2
	;가슴
	ELSEIF CFLAG:106 == 3
	;腹
	ELSEIF CFLAG:106 == 4
	;腋
	ELSEIF CFLAG:106 == 5
	;太腿
	ELSEIF CFLAG:106 == 6
	;割れ눈
	ELSEIF CFLAG:106 == 7
	;尻
	ELSEIF CFLAG:106 == 8
	;ゴム
	ELSEIF CFLAG:106 == 9
		;섹스로사정かつ、애널섹스以外を実行時
		IF GROUPMATCH(SELECTCOM , 60 , 61 , 66 , 68 , 69 , 72)
		ELSE
		ENDIF
	ENDIF
	RETURN 0
ENDIF

;-------------------------------------------------
;손、입以外で사정
;対応コマンドは키스、스마타、발コキ、애널強制봉사、더블스마타
;-------------------------------------------------
IF TFLAG:9 == 1
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;조수를범한다로사정
;-------------------------------------------------
IF TFLAG:7 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;입으로사정
;-------------------------------------------------
IF TFLAG:0 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;손으로사정
;-------------------------------------------------
IF TFLAG:1 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;가슴으로사정
;-------------------------------------------------
IF TFLAG:34 > 0
	CALL PALAM_MESSAGE_A
ENDIF

;-------------------------------------------------
;섹스로사정
;-------------------------------------------------
IF TFLAG:2 > 0
	CALL PALAM_MESSAGE_A
	;애널섹스系
	IF SELECTCOM == 62 || SELECTCOM == 63 || SELECTCOM == 67 || SELECTCOM == 70 || SELECTCOM == 71 || SELECTCOM == 73 || SELECTCOM == 78 || SELECTCOM == 79 || SELECTCOM == 512 || SELECTCOM == 515
	;섹스系
	ELSE
	ENDIF
ENDIF

;-------------------------------------------------
;역강간으로사정させた
;-------------------------------------------------
IF SELECTCOM == 64 && TFLAG:10 > 0
	CALL PALAM_MESSAGE_A
	;함락済み
ENDIF

