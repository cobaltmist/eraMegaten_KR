		;-------------------------------------------------
		;오니＆蔦
		;-------------------------------------------------
	CASE 1
			K_CALL:K = MESSAGE_LOSE_COLOSSEUM
			;연모、친애
			IF TALENT:3 || TALENT:6
				WSTR:(K++) = 
			;음란、창부
			ELSEIF TALENT:4 || TALENT:7
				WSTR:(K++) = 
			;복종、예속
			ELSEIF TALENT:5 || TALENT:8
				WSTR:(K++) = 
			ENDIF
			;그외
			IF !구상유무확인(0)
				WSTR:(K++) = 
			ENDIF
ENDSELECT
CALL 구상변수표시
;구상がなければ別の구상を呼び出す
IF RESULT == 0
	TRYCCALLFORM KOJO_MESSAGE_LOSE_COLOSSEUM_ZZZ
	CATCH
		CALL MESSAGE_LOSE_COLOSSEUM
	ENDCATCH
ENDIF


