;	RETURN 0
;SIF NO:TARGET != 4501
;	RETURN 0
;--------------------------------------------------
;特定キャラが조수、主人時に特殊キャラ구상を呼び出す(Y1を特定キャラ、4501を奴隷のキャラ번호に置換）
;--------------------------------------------------
;IF NO:MASTER == Y1
;	CALL EVENTEND_PY1_K4501_1
;	RETURN 0
;ENDIF
;IF ASSI > 0 && ASSIPLAY
;	SIF NO:ASSI == Y1
;	CALL EVENTEND_PY1_K4501_1
;	RETURN 0
;ENDIF
;--------------------------------------------------
;조교종료시のセリフ
;--------------------------------------------------
DRAWLINE
;반발각인Lv3
IF MARK:3 == 3 && (CFLAG:217 < 1 || FLAG:7 == 2)
	PRINTFORML 「이제 차라리 죽여 주세요…」
	PRINTFORMW 베스는 모든 것을 단념한 표정으로 혼잣말로 중얼거렸다。
	CFLAG:217 = 1
;굴복각인Lv1以下
ELSEIF MARK:2 <= 1 && (CFLAG:219 < 1 || FLAG:7 == 2)
	PRINTFORML 「…신이시여…, 당신께선 복음을 전해 주지 않으시는 겁니까…？」
	PRINTFORMW 베스는 울면서, 누구에게 말하는지 알 수 없는 소리를 무심결에 말했다。
	CFLAG:219 = 1
;굴복각인Lv2
ELSEIF MARK:2 == 2
	PRINTFORML 「…기분 좋지 않습니다…」
	PRINTFORMW 베스는 무뚝뚝하게 말하며 %CALLNAME:MASTER%에게서 시선을 돌렸다。
	CFLAG:219 = 2
;굴복각인Lv3＋함락소질無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0)
	PRINTFORML 「%CSTR:일인칭%…이상해진 건가…」
	PRINTFORMW 베스는 중얼거리며, 멍하니 천장을 올려다 보고 있었다。
	CFLAG:219 = 3
;함락済み＋체력500미만
ELSEIF BASE:0 < 500 && (CFLAG:218 < 1 || FLAG:7 == 2)
	;연모、친애
	IF TALENT:3 || TALENT:6
		PRINTFORML 베스는 %CALLNAME:MASTER%로부터의 격렬한 조교에 견디지 못하고 쓰러졌다。
		PRINTFORML %조사처리(CALLNAME:MASTER,"가")% 베스를 쉬게 하려고, 방을 나서려 하자
		PRINTFORML 베스는 힘을 다 소모해 버린 몸를 놀려, %CALLNAME:MASTER%의 발치를 붙잡고 간청해왔다。
		PRINTFORML 「싫어…%CALLNAME:MASTER%…가지 마…。좀 더…갖고 싶은데…」
		PRINTFORML %조사처리(CALLNAME:MASTER,"가")% 이 이상은 생명과 연관된다는 취지의 말로 설득하려 했지만 베스는 계속 말했다。
		PRINTFORML 「…상관없어요…, %CALLNAME:MASTER%에게 라면…죽어도 상관없어…」
		PRINTFORML 함부로 말하지 말라고 %조사처리(CALLNAME:MASTER,"는")% 베스를 가볍게 꾸짖었으나
		PRINTFORML 그러나, %CALLNAME:MASTER%의 몸에 달라 붙어 떨어지지 않던 베스는 더욱 바짝 달라 붙었다。
		PRINTFORMW 「고마워요…。정말 좋아해요…%CALLNAME:MASTER%…♡」
	;음란、창부
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;복종、예속
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:218 = 1
;함락済み
ELSEIF CFLAG:219 < 4 || FLAG:7 == 2
	;연모、친애
	IF TALENT:3 || TALENT:6
		PRINTFORML 「벌써…끝인거야…？%CSTR:일인칭%, 아직 더 할 수 있는데…？」
		PRINTFORML 베스는 %CALLNAME:MASTER%에게 안기며 달라 붙었다。
		PRINTFORML 「좀 더, 좀 더, 좀 더, 해요…。%CSTR:일인칭%들, 연인 같은 것이니까…♡」
		PRINTFORMW 베스는 %CALLNAME:MASTER%의 입술에 뜨거운 입술을 갖다대며, 더욱 몸을 겹쳐왔다…。
	;음란、창부
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;복종、예속
	ELSEIF TALENT:5 || TALENT:8
		IF RAND:2 == 0
			PRINTFORMW 「또 와주십시오, %CSTR:이인칭%」
		ELSE
			PRINTFORMW 「%조사처리(CSTR:일인칭,"를")% 버리지 말아 주세요…부탁합니다」
		ENDIF
	ENDIF
	CFLAG:219 = 4
ENDIF
RETURN 0

;--------------------------------------------------
;@KOJO_MESSAGE_COM関係（4501をキャラ번호に置換）
;コマンド実行時に출력されます
;--------------------------------------------------
@KOJO_MESSAGE_COM_K4501_1
;조수が조교した時に구상をスキップする（好みに応じて使う、行머리の;を消すとスキップするようになる）
;SIF ASSI > 0 && ASSIPLAY
;	RETURN 0
;볼재갈着用時には地の文を표시して구상をスキップする
IF TEQUIP:20 &&  SELECTCOM != 107
	CALL TRAIN_MESSAGE
	RETURN 0
ENDIF
;--------------------------------------------------
;特定キャラが조수、主人時に特殊キャラ구상を呼び出す(Y1を特定キャラ、4501を奴隷のキャラ번호に置換）
;--------------------------------------------------
;IF NO:MASTER == Y1
;	CALL KOJO_MESSAGE_COM_PY1_K4501_1
;	RETURN 0
;ENDIF
;IF ASSI > 0 && ASSIPLAY
;	IF NO:ASSI == Y1
;		CALL KOJO_MESSAGE_COM_PY1_K4501_1
;		RETURN 0
;	ENDIF
;ENDIF
;-------------------------------------------------
;コマンド実行時のセリフ CFLAG 300～500
;-------------------------------------------------
;=================================================
;애무계コマンド
;=================================================
;-------------------------------------------------
;0,애무
;-------------------------------------------------
SELECTCASE SELECTCOM
	CASE 0
		CALL TRAIN_MESSAGE
		;初めて
		IF CFLAG:300 == 0
			PRINTFORML 베스의 아름다운 몸을 상냥하게 어루만져 주었다。
			PRINTFORMW 「…！…그만두세요…！」
			CFLAG:300 = 1
		;二回눈以降
		ELSE
			;함락済み
			IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:300 < 4 || FLAG:7 == 2)
				;연모、친애
				IF TALENT:3 || TALENT:6
					PRINTFORML 베스의 온 몸을 상냥하게 쓰다듬어 주었다。
					PRINTFORML 「…응…！…아응！」
					PRINTFORML 「우흐…그만두세요…기분 좋아…！」
					PRINTFORMW 베스는 %CALLNAME:MASTER%의 손이 비부에 닿을 때마다 허덕였다。
				;음란、창부
				ELSEIF TALENT:4 || TALENT:7
					PRINTFORMW 
				;10/14
				;복종、예속
				ELSEIF TALENT:5 || TALENT:8
					PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% 강하게 베스의
					IF RAND:3 == 0
						PRINTFORM 몸을
					ELSEIF RAND:2 == 0
						PRINTFORM 온 몸을
					ELSE
						PRINTFORM 민감한 장소를
					ENDIF
					IF RAND:2 == 0
						PRINTFORML 쓰다듬었다。
					ELSE
						PRINTFORML 어루만졌다。
					ENDIF
					IF RAND:2 == 0
						PRINTFORML 「……으」
					ELSE
						PRINTFORML 「응……」
					ENDIF
					PRINTFORM 베스는%CALLNAME:PLAYER%의 손가락의
					IF RAND:2 == 0
						PRINTFORM 움직임
					ELSE
						PRINTFORM 자극
					ENDIF
					PRINTFORMW 에 간신히 소리를 흘렸지만, 그 눈동자는 슬픔과 절망으로 어둡고 탁해져 있었다。
				ENDIF
				CFLAG:300 = 4
			;굴복각인Lv3
			ELSEIF MARK:2 == 3 && (CFLAG:300 < 3 || FLAG:7 == 2)
				PRINTFORML 「응…！아…！…싫어…그만둬…이상하게 되 버려…！」
				PRINTFORMW 베스는 쾌락의 소리를 다 얼버무리진 못하는 것 같다。
				CFLAG:300 = 3
			;それ以外
			ELSEIF CFLAG:300 < 2 || FLAG:7 == 2
				PRINTFORML 베스의 부드러운 몸을 마음껏 문지르며 애무했다。
				PRINTFORMW 베스는 무슨 짓을 당하든 반응하지 않겠다는 태도를 보이고 있다。
				;C감각3이상
				IF ABL:3 >= 3
					PRINTFORML 「히으…！…으응…！」
					PRINTFORMW 그러나 민감한 곳에 닿으면 인내도 효과가 없는 것 같다。
				ENDIF
				CFLAG:300 = 2
			ENDIF
		ENDIF


	;-------------------------------------------------
	;1,커닐링구스
	;-------------------------------------------------
	CASE 1
		CALL TRAIN_MESSAGE
		;初めて
		IF CFLAG:301 == 0
			PRINTFORML 「…싫어…그만둬…！」
			PRINTFORMW 베스가 울면서 간절히 호소해 왔지만, 신경 쓰지 않고 베스의 비부에 혀를 대고 빨았다…。
			CFLAG:301 = 1
		;二回눈以降
		ELSE
			;함락済み
			IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:301 < 4 || FLAG:7 == 2)
				;연모、친애
				IF TALENT:3 || TALENT:6
					PRINTFORML 「우아…！카흑…！…후욱！」
					PRINTFORML 베스의 비밀스러운 곳에 혀를 넣고 빨고 돌릴 때마다, 환희에 찬 헐떡거리는 소리가 새어 나왔다。
					PRINTFORML 「맛있다는 듯이 빨고 있어…♡」
					PRINTFORML 「%CSTR:일인칭%의 보지…맛있어？…%CALLNAME:MASTER%…」
					PRINTFORMW 베스는 %CALLNAME:MASTER%의 머리를 양손으로 단단히 붙잡고, 결코 떼어 놓으려고 하지 않았다。
				;음란、창부
				ELSEIF TALENT:4 || TALENT:7
					PRINTFORMW 
				;복종、예속
				ELSEIF TALENT:5 || TALENT:8
					PRINTFORML 「%조사처리(CSTR:일인칭,"는")%……%CSTR:이인칭%의 성노예 입니다」
					PRINTFORML 「부디, 마음껏 맛 봐 주세요」
					PRINTFORMW 베스의 비부에 혀가 닿자 %CALLNAME:TARGET%는 슬픔과 서러움으로 가득 찬 어조로 말했다。
				ENDIF
				CFLAG:301 = 4
			;굴복각인Lv3
			ELSEIF MARK:2 == 3 && (CFLAG:301 < 3 || FLAG:7 == 2)
				PRINTFORML 「응…！…으흐응！」
				PRINTFORML 베스의 비부에서는 끊임없이 물보라가 뿜어져 나왔다。
				PRINTFORMW 몸이 서서히 노예다워지고 있는 것이다。
				CFLAG:301 = 3
			;それ以外
			ELSEIF CFLAG:301 < 2 || FLAG:7 == 2
				PRINTFORML 「몇 번을 겪었는데도…으윽…！」
				PRINTFORMW 베스는 이를 악물고 소리를 내는 것을 참고 있다。
				CFLAG:301 = 2
			ENDIF
		ENDIF


	;-------------------------------------------------
	;2,펠라한다
	;-------------------------------------------------
	CASE 2
		CALL TRAIN_MESSAGE
		;初めて
		IF CFLAG:302 == 0
			PRINTFORMW 
			CFLAG:302 = 1
		;二回눈以降
		ELSE
			;함락済み
			IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:302 < 4 || FLAG:7 == 2)
				;연모、친애
				IF TALENT:3 || TALENT:6
					PRINTFORMW 
				;음란、창부
				ELSEIF TALENT:4 || TALENT:7
					PRINTFORMW 
				;복종、예속
				ELSEIF TALENT:5 || TALENT:8
					PRINTFORMW 
				ENDIF
				CFLAG:302 = 4
			;굴복각인Lv3
			ELSEIF MARK:2 == 3 && (CFLAG:302 < 3 || FLAG:7 == 2)
				PRINTFORMW 
				CFLAG:302 = 3
			;それ以外
			ELSEIF CFLAG:302 < 2 || FLAG:7 == 2
				PRINTFORMW 
				CFLAG:302 = 2
			ENDIF
		ENDIF


	;-------------------------------------------------
	;3,손가락삽입
	;-------------------------------------------------
	CASE 3
		CALL TRAIN_MESSAGE
		;初めて
		IF CFLAG:303 == 0
			PRINTFORML 「시…싫어…！그만두세요！」
			PRINTFORMW 베스는 격렬하게 머리를 흔들며 거절했지만, 상관하지 않고 비부에 손가락을 꽂아 넣었다…。
			CFLAG:303 = 1
		;二回눈以降
		ELSE
			;함락済み
			IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:303 < 4 || FLAG:7 == 2)
				;연모、친애
				IF TALENT:3 || TALENT:6
					PRINTFORML 「아아…♡」
					PRINTFORML 베스는질 안에서 날뛰는 %CALLNAME:MASTER%의 손가락 감촉에 푹 빠져 들었다。
					PRINTFORML 「…하아, 하아…。저기…이거는 넣지 않는 거야…？」
					PRINTFORMW 베스는 기대를 품은 시선으로 %CALLNAME:MASTER%의 페니스를 바라보고 있다。
				;음란、창부
				ELSEIF TALENT:4 || TALENT:7
					PRINTFORMW 
				;복종、예속
				ELSEIF TALENT:5 || TALENT:8
					PRINTFORMW 
				ENDIF
				CFLAG:303 = 4
			;굴복각인Lv3
			ELSEIF MARK:2 == 3 && (CFLAG:303 < 3 || FLAG:7 == 2)
			PRINTFORML 「……응…！………크응！」
			PRINTFORML 베스는 %CALLNAME:MASTER%의 손가락 놀림에 참지 못하고 헐떡이는 소리를 내고 있다。
			PRINTFORMW …%조사처리(CALLNAME:MASTER,"를")% 바라보는 시선이 음란하게 느껴지는 것은 기분탓이 아닌 것 같다 。
				CFLAG:303 = 3
			;それ以外
			ELSEIF CFLAG:303 < 2 || FLAG:7 == 2
			PRINTFORML 「%CSTR:이인칭%…후우…！…너무합니다……으극！」
			PRINTFORML 베스는 비부를 희롱하는 ぶ%조사처리(CALLNAME:MASTER,"를")% 슬픈 눈으로 바라봤지만
			PRINTFORMW 민감해졌는지, 손가락 놀림에 대해 솔직한 반응을 보였다。
				CFLAG:303 = 2
			ENDIF
		ENDIF


	;-------------------------------------------------
	;4,애널핥기
	;-------------------------------------------------
	CASE 4
		CALL TRAIN_MESSAGE
		;初めて
		IF CFLAG:304 == 0
			PRINTFORML 「그, 그런 곳 빠는 것은…그, 그만둬！」
			PRINTFORML %조사처리(CALLNAME:MASTER,"는")% 신경 쓰지 않고 베스의 국화꽃에 혀를 대자
			PRINTFORMW 베스는 거세게 몸부림 쳤다…。
			CFLAG:304 = 1
		;二回눈以降
		ELSE
			;함락済み
			IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:304 < 4 || FLAG:7 == 2)
				;연모、친애
				IF TALENT:3 || TALENT:6
					PRINTFORML 엉덩이 구멍에 입을 대고 핥을 때마다, 베스는 허덕였다。
					PRINTFORML 「히익…！아앙！」
					PRINTFORMW 「…변태에요…%CALLNAME:MASTER%…。…뭐, %CSTR:일인칭%도 인가…」
				;음란、창부
				ELSEIF TALENT:4 || TALENT:7
					PRINTFORMW 
				;복종、예속
				ELSEIF TALENT:5 || TALENT:8
					PRINTFORMW 
				ENDIF
				CFLAG:304 = 4
			;굴복각인Lv3
			ELSEIF MARK:2 == 3 && (CFLAG:304 < 3 || FLAG:7 == 2)
				PRINTFORML 「힉…！히잉…！아하아…！」
				PRINTFORMW 엉덩이 구멍에서 오는 자극에, 베스는 더 이상 참을 수 없게 되어갔다。
				CFLAG:304 = 3
			;それ以外
			ELSEIF CFLAG:304 < 2 || FLAG:7 == 2
				PRINTFORML 「그만둬…괴롭！く아응！…습니다…！」
				PRINTFORMW 엉덩이 구멍이 민감해 진 것 같다。베스는 국화꽃에 혀가 닿을 때마다 정신이 혼미해져 갔다。
				CFLAG:304 = 2
			ENDIF
		ENDIF


	;-------------------------------------------------
	;5,애널애무
	;-------------------------------------------------
	CASE 5
		CALL TRAIN_MESSAGE
		;初めて
		IF CFLAG:305 == 0
			PRINTFORML 「라니, 다시 생각해 주세요！%CSTR:이인칭%…！그렇게 상스러운 건…」
			PRINTFORML 하고 싶지 않아,  라고 말하기 전에 베스의 엉덩이 구멍을 손가락으로 쑤셨다。
			PRINTFORML 「히야앙！？」
			PRINTFORMW 베스는 몸을 크게 구부리며 엉덩이 구멍으로부터 오는 자극에 허리를 벌벌 떨었다…。
			CFLAG:305 = 1
		;二回눈以降
		ELSE
			;함락済み
			IF (TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8) && (CFLAG:305 < 4 || FLAG:7 == 2)
				;연모、친애
				IF TALENT:3 || TALENT:6
					PRINTFORML 「좀 더…으응！좀 더…격렬하게…아앙！…휘저어 줘…♡」
					PRINTFORML 손가락의 움직임 만으로는 만족하지 못했는지, 스스로 허리를 움직여 오며
					PRINTFORMW 베스는 엉덩이 구멍에서 전해지는 쾌락을 누렸다。
				;음란、창부
				ELSEIF TALENT:4 || TALENT:7
					PRINTFORMW 
				;복종、예속
				ELSEIF TALENT:5 || TALENT:8
					PRINTFORMW 
				ENDIF
				CFLAG:305 = 4
			;굴복각인Lv3
			ELSEIF MARK:2 == 3 && (CFLAG:305 < 3 || FLAG:7 == 2)
