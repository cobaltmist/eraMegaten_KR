ELSEIF MARK:2 == 2
	PRINTFORMW 
	CFLAG:219 = 2
;굴복각인Lv3＋함락素質無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0)
	PRINTFORMW 
	CFLAG:219 = 3
;함락済み＋체력500未満
ELSEIF BASE:0 < 500 && (CFLAG:218 < 1 || FLAG:7 == 2)
	;연모、친애
	IF TALENT:3 || TALENT:6
		PRINTFORMW 「…하아……저기、%CSTR:이인칭%」
		PRINTFORMW 숨을 허덕이며 엎드려 눕는 %조사처리(CALLNAME,"를")% 보자、%조사처리(CALLNAME,"는")% 기쁜듯이 미소지었다
		PRINTFORMW 
		PRINTFORMW 「%CSTR:일인칭%…정말로 행복해%ハート()%」
		PRINTFORMW 「또 함께 하자、약속%ハート()%」
	;음란、창부
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;복종、예속
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:218 = 1
;함락済み
ELSEIF CFLAG:219 < 4 || FLAG:7 == 2
	;연모、친애
	IF TALENT:3 || TALENT:6
		PRINTFORMW 「저기、있잖아、…응…」
		PRINTFORMW 눈을 감고 키스를 기다리는 %CALLNAME%에게 상냥하게 입맞춤을 했다
	;음란、창부
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;복종、예속
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:219 = 4
ENDIF
RETURN 0

