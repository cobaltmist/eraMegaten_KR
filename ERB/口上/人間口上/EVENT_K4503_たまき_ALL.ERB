@KOJO_MESSAGE_PALAMCNG_D_K4503
;조수が조교した時に구상をスキップする（好みに応じて使う、行머리の;を消すとスキップするようになる）
;SIF ASSI > 0 && ASSIPLAY
;	RETURN 0 
;볼재갈着用時には地の文を표시して구상をスキップする
IF TEQUIP:20 &&  SELECTCOM != 107
	CALL PALAM_MESSAGE_D
	RETURN 0
ENDIF
;--------------------------------------------------
;特定キャラが조수、主人時に特殊キャラ구상を呼び出す(Y1を特定キャラ、X1を奴隷のキャラ番号に置換）
;--------------------------------------------------
;IF NO:MASTER == Y1
;	CALL KOJO_MESSAGE_PALAMCNG_D_PY1_K4503
;	RETURN 0
;ENDIF
;IF ASSI > 0 && ASSIPLAY
;	IF NO:ASSI == Y1
;		CALL KOJO_MESSAGE_PALAMCNG_D_PY1_K4503
;		RETURN 0
;	ENDIF
;ENDIF
;-------------------------------------------------
;パラメータ変化時のテキスト
;-------------------------------------------------
;-------------------------------------------------
;初めて윤활がLV3超えた
;-------------------------------------------------
P = PALAM:4 + CUP:4
IF P > PALAMLV:4 && CFLAG:250 == 0
	CALL PALAM_MESSAGE_D
	;로션を使った場合
	IF SELECTCOM == 180
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	CFLAG:250 = 1
ENDIF

;-------------------------------------------------
;初めて욕정がLV2超えた
;-------------------------------------------------
P = PALAM:6 + CUP:6
IF P > PALAMLV:3 && CFLAG:251 == 0
	CALL PALAM_MESSAGE_D
	;미약を使った場合
	IF SELECTCOM == 181
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	CFLAG:251 = 1
ENDIF

;-------------------------------------------------
;初めて굴복がLV3超えた
;-------------------------------------------------
P = PALAM:7 + CUP:7
IF P > PALAMLV:3 && CFLAG:252 == 0
	CALL PALAM_MESSAGE_D
	PRINTFORMW 
	CFLAG:252 = 1
ENDIF

;-------------------------------------------------
;初めて복종がLV3超えた
;-------------------------------------------------
P = PALAM:5 + CUP:5
IF P > PALAMLV:3 && CFLAG:253 == 0
	CALL PALAM_MESSAGE_D
	;함락済み
	IF TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	CFLAG:253 = 1
ENDIF

;-------------------------------------------------
;初めて공포がLV3超えた
;-------------------------------------------------
P = PALAM:11 + CUP:11
IF P > PALAMLV:3 && CFLAG:254 == 0
	CALL PALAM_MESSAGE_D
	;함락済み
	IF TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	CFLAG:254 = 1
ENDIF


