;-------------------------------------------------
;조교開始時（2回目以降、X1をキャラ番号に변경する）
;-------------------------------------------------
@EVENTTRAIN_SECOND_K4503
;반발각인Lv3
IF MARK:3 == 3 && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 4
;굴복각인Lv0
ELSEIF MARK:2 == 0 && (CFLAG:202 < 1 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 1
;굴복각인Lv1
ELSEIF MARK:2 == 1 && (CFLAG:202 < 2 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 2
;굴복각인Lv2
ELSEIF MARK:2 == 2 && (CFLAG:202 < 3 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 3
;굴복각인Lv3＋함락素質無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0) && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:202 = 4
;함락済み
ELSEIF CFLAG:202 < 5 || FLAG:7 == 2
	;연모、친애
	IF TALENT:3 || TALENT:6
		;랜덤で구상が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 「아、네～에、지금열께」
			PRINTFORMW 「응、츗……에헤헤헷%ハート()%」
		ELSEIF K:1 == 1
			PRINTFORMW 「어서 오세요%ハート()%　……응」
			PRINTFORMW 「…읏……응～～～」
			PRINTFORMW 눈을 감고 키스를 조르는 %CALLNAME%의 턱에 손가락을 걸어、살그머니 입술을 거듭했다
			PRINTFORML 
			PRINTFORMW 「……응후후～♪읏%ハート()%　핫%ハート()%」
			PRINTFORMW 뺨을 붉히며、한층 더 키스를 조르는 %조사처리(CALLNAME,"를")% 안아 올려、
			PRINTFORMW 입술을 차지하면서 침대로 이동했다
		ELSE
			PRINTFORMW 「어서 오세요、다・앙・신%ハート()%」
			PRINTFORMW 「나로 할래？나로 할래？그렇지 않으면、나・로・해%ハート()%」
			PRINTFORMW 선택사항이 없는 것에 쓴웃음 지으며 %조사처리(CALLNAME,"를")% 껴안아、
			PRINTFORMW 거듭 입술을 맞추어 회답을 전했다
		ENDIF
	;음란、창부
	ELSEIF TALENT:4 || TALENT:7
		;랜덤で구상が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;복종、예속
	ELSEIF TALENT:5 || TALENT:8
		;랜덤で구상が変化する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	CFLAG:202 = 5
ENDIF
RETURN 0

