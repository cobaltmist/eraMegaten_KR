;	RETURN 0
;ENDIF
;IF ASSI > 0 && ASSIPLAY
;	SIF NO:ASSI == Y1
;	CALL EVENTEND_PY1_K4631
;	RETURN 0
;ENDIF
;--------------------------------------------------
;조교종료시のセリフ
;--------------------------------------------------
DRAWLINE
;반발각인Lv3
IF MARK:3 == 3 && (CFLAG:217 < 1 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:217 = 1
;굴복각인Lv1以下
ELSEIF MARK:2 <= 1 && (CFLAG:219 < 1 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:219 = 1
;굴복각인Lv2
ELSEIF MARK:2 == 2
	PRINTFORMW 
	CFLAG:219 = 2
;굴복각인Lv3＋함락소질無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0)
	PRINTFORMW 
	CFLAG:219 = 3
;함락済み＋체력500미만
ELSEIF BASE:0 < 500 && (CFLAG:218 < 1 || FLAG:7 == 2)
	;연모、친애
	IF TALENT:3 || TALENT:6
		PRINTFORMW 
	;음란、창부
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;복종、예속
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:218 = 1
;함락済み
ELSEIF CFLAG:219 < 4 || FLAG:7 == 2
	;연모、친애
	IF TALENT:3 || TALENT:6
		PRINTFORMW 
	;음란、창부
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;복종、예속
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:219 = 4
ENDIF
RETURN 0

