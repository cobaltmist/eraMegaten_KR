;	CALL EVENTTRAIN_PY1_K4631
;	RETURN 0
;ENDIF
;IF ASSI > 0 && ASSIPLAY
;	IF NO:ASSI == Y1
;		CALL EVENTTRAIN_PY1_K4631
;		RETURN 0
;	ENDIF
;ENDIF
;--------------------------------------------------
;簡易조수구상の発動時に初조교時구상をスキップ（Y1を조수のキャラ번호にする）
;複数人いる際には、SIF NO:ASSI == Y1 || NO:ASSI == Z1と추가していく
;--------------------------------------------------
;IF CFLAG:201 == 0 && ASSI > 0
;	SIF NO:ASSI == Y1
;		CFLAG:201 =1
;ENDIF
;-------------------------------------------------
;조교開始時（一回のみ） CFLAG201
;-------------------------------------------------
DRAWLINE
;初조교時
IF CFLAG:201 == 0 && TALENT:연모 == 0
	CFLAG:201 = 1
	RETURN 0
;굴복각인Lv1
ELSEIF CFLAG:201 < 2 && MARK:2 == 1
	PRINTFORMW 
	CFLAG:201 = 2
	RETURN 0
;굴복각인Lv2
ELSEIF CFLAG:201 < 3 && MARK:2 == 2
	PRINTFORMW 
	CFLAG:201 = 3
	RETURN 0
;굴복각인Lv3
ELSEIF CFLAG:201 < 4 && MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0)
	PRINTFORMW 
	CFLAG:201 = 4
	RETURN 0
;함락
ELSEIF CFLAG:201 < 5 && (TALENT:3 || TALENT:4 || TALENT:5)
	;연모
	IF TALENT:3
		IF CFLAG:미조교함락 == 1
			;조교せずに연모をつけた場合
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;음란
	ELSEIF TALENT:4
		PRINTFORMW 
	;복종
	ELSEIF TALENT:5
		PRINTFORMW 
	ENDIF
	CFLAG:201 = 5
	RETURN 0
ENDIF

;-------------------------------------------------
;簡易조수구상（Y1を조수のキャラ번호に置換、X1をキャラ번호に변경する) CFLAG203
;掛け合いをするキャラを増やす場合は、構文をコピー＆ペーストをして、CFLAGの数を204、205と増やす
;-------------------------------------------------
;조수の有無を체크（いない場合は二回目以降へ飛ぶ）
IF ASSI < 0 
	CALL EVENTTRAIN_SECOND_K4631
;조수
;ELSEIF NO:ASSI == Y1
