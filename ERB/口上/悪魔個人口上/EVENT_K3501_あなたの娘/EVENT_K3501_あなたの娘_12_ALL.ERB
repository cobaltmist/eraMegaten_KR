SIF !((CFLAG:딸성격 == 2 || CFLAG:딸성격 == 0 || CFLAG:딸성격 == 9) && !TALENT:남자)
	RETURN -1
SIF ARG == 1
	RETURN 0
;구상名
PRINTFORML 말괄량이 딸 대사
;구상설명文
PRINTFORML 


