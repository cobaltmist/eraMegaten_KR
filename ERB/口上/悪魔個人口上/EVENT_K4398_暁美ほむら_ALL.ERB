		;던전内성욕처리：仲魔と섹스する
		;やられちゃう側
		;PLAYERに実行者の번호が入っています
		;-------------------------------------------------
	CASE 79
		CALL EVENT_MESSAGE
		IF (CFLAG:602 < 1 || FLAG:7 == 2)
			PRINTFORMW
			CFLAG:602 = 1
		ENDIF
		;-------------------------------------------------
		;던전内성욕처리：仲魔に봉사させる
		;봉사する側
		;PLAYERに実行者の번호が入っています
		;-------------------------------------------------
	CASE 80
		CALL EVENT_MESSAGE
		IF (CFLAG:603 < 1 || FLAG:7 == 2)
			PRINTFORMW
			CFLAG:603 = 1
		ENDIF
		;-------------------------------------------------
		;던전内성욕처리：その場で자위
		;봉사する側
		;-------------------------------------------------
	CASE 81
		CALL EVENT_MESSAGE
		IF (CFLAG:604 < 1 || FLAG:7 == 2)
			PRINTFORMW
			CFLAG:604 = 1
		ENDIF
		;-------------------------------------------------
		;던전内성욕처리：物陰で자위
		;봉사する側
		;-------------------------------------------------
	CASE 82
		CALL EVENT_MESSAGE
		IF (CFLAG:605 < 1 || FLAG:7 == 2)
			PRINTFORMW
			CFLAG:605 = 1
		ENDIF
		;-------------------------------------------------
		;던전内성욕처리：仲魔と섹스する
		;やる側
		;PLAYERに相手の번호が入っています
		;-------------------------------------------------
	CASE 83
		CALL EVENT_MESSAGE
		IF (CFLAG:606 < 1 || FLAG:7 == 2)
			PRINTFORMW
			CFLAG:606 = 1
		ENDIF
		;-------------------------------------------------
		;던전内성욕처리：仲魔に봉사させる
		;봉사される側
		;PLAYERに봉사する側の번호が入っています
		;-------------------------------------------------
	CASE 84
		CALL EVENT_MESSAGE
		IF (CFLAG:607 < 1 || FLAG:7 == 2)
			PRINTFORMW
			CFLAG:607 = 1
		ENDIF
		;-------------------------------------------------
		;던전内성욕처리：윤간이벤트
		;봉사する側
		;PLAYERに実行者=윤간する악마の번호が入っています
		;-------------------------------------------------
	CASE 85
		CALL EVENT_MESSAGE
		IF (CFLAG:608 < 1 || FLAG:7 == 2)
			PRINTFORMW
			CFLAG:608 = 1
		ENDIF
	CASEELSE
		SIF FLAG:6 > 0
			CALL EVENT_MESSAGE
ENDSELECT



