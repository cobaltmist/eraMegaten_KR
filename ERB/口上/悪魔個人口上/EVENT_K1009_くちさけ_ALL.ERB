;	ENDIF
;ENDIF
;-------------------------------------------------
;조교後のテ키스ト
;-------------------------------------------------
;-------------------------------------------------
;처녀상실(처녀のみ)
;-------------------------------------------------
IF TFLAG:3 > 0
	CALL PALAM_MESSAGE_C
	;연모か친애かつ主人の섹스による처녀상실
	IF TFLAG:20
		PRINTFORMW 
	;それ以外の時に처녀상실
	ELSE
		;함락済み
		IF TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8
			PRINTFORMW 
		;굴복각인Lv3
		ELSEIF MARK:2 == 3
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF


