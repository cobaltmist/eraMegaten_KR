		CFLAG:802 = 0
	ENDIF







;-------------------------------------------------
;性転換関数
;HALF：性転換（＋乳関連）
;FULL：체격など全て更新
;-------------------------------------------------
@K_1006_TRANS_SEXUAL_HALF
	IF TALENT:MASTER:남자 == 1
		TALENT:남자 = 0
		TALENT:처녀 = CFLAG:MASTER:원처녀
		TALENT:폭유 = 0
		TALENT:거유 = 0
		TALENT:빈유 = 0
		TALENT:절벽 = 0
		IF RAND:5 == 0
			TALENT:폭유 = 1
		ELSEIF RAND:4 == 0
			TALENT:거유 = 1
		ELSEIF RAND:3 == 0
			;普乳
		ELSEIF RAND:2 == 0
			TALENT:빈유 = 1
		ELSE
			TALENT:절벽 = 1
		ENDIF
	ELSE
		TALENT:남자 = 1
		TALENT:폭유 = 0
		TALENT:거유 = 0
		TALENT:빈유 = 0
		TALENT:절벽 = 0
	ENDIF
	;性転換경험＋１
	EXP:80 += 1




@K_1006_TRANS_SEXUAL_FULL

	;人外要素の삭제
	TALENT:짐승 = 0
	TALENT:새 = 0
	TALENT:파충류 = 0
	TALENT:물고기 = 0
	TALENT:부정형 = 0
	TALENT:이형 = 0
	TALENT:갈퀴발 = 0
	TALENT:갈퀴 = 0

	;初めての性転換時
	IF CFLAG:220 == 3
		;女になる場合
		IF TALENT:MASTER:남자
			;もともと女ということにする
			CFLAG:원성별 =  2
			TALENT:처녀 = CFLAG:MASTER:원처녀
			CFLAG:현가슴사이즈 = CFLAG:MASTER:원가슴사이즈
			;女時の가슴のサイズが설정されて無いなら랜덤
			IF CFLAG:MASTER:원가슴사이즈 == 0
				SELECTCASE RAND:100
					CASE IS < 40
						CFLAG:현가슴사이즈 = 3
					CASE IS < 60
						CFLAG:현가슴사이즈 = 2
					CASE IS < 80
						CFLAG:현가슴사이즈 = 4
					CASE IS < 90
						CFLAG:현가슴사이즈 = 5
					CASE IS < 100
						CFLAG:현가슴사이즈 = 1
				ENDSELECT
			ENDIF
			SELECTCASE CFLAG:현가슴사이즈
				CASE 1
					TALENT:절벽 = 1
				CASE 2
					TALENT:빈유 = 1
				CASE 3
				CASE 4
					TALENT:거유 = 1
				CASE 5
					TALENT:폭유 = 1
			ENDSELECT
			SIF CFLAG:MASTER:원Ｖ민감
				TALENT:Ｖ민감 = 1
			SIF CFLAG:MASTER:원Ｖ둔감
				TALENT:Ｖ둔감 = 1
			SIF TALENT:오토코노코 == 1
				CFLAG:원오토코노코 = 1
			TALENT:남자 = 0
			TALENT:오토코노코 = 0
		;男になる場合
		ELSE
			;もともと남자ということにする
			CFLAG:원성별 =  1
			CFLAG:원처녀 = TALENT:MASTER:처녀
			;マスターの가슴サイズを保存する
			TALENT:절벽 = 0
			TALENT:빈유 = 0
			TALENT:거유 = 0
			TALENT:폭유 = 0
			CFLAG:원가슴사이즈 = CFLAG:MASTER:현가슴사이즈
			SIF TALENT:MASTER:Ｖ민감
				CFLAG:원Ｖ민감 = 1
			SIF TALENT:MASTER:Ｖ둔감
				CFLAG:원Ｖ둔감 = 1
			SIF CFLAG:MASTER:원오토코노코
				TALENT:오토코노코 = 1
			TALENT:남자 = 1
		ENDIF
		CFLAG:220 = 4
	;2回눈以降
	;更新、なので、솔직함に自分のフラグから持ってこない
	ELSE
		;女になる場合
		IF TALENT:MASTER:남자
			TALENT:처녀 = CFLAG:MASTER:원처녀
			;女時の가슴のサイズが설정されて無いなら랜덤
			IF CFLAG:MASTER:원가슴사이즈 == 0
				SELECTCASE RAND:100
					CASE IS < 40
						CFLAG:현가슴사이즈 = 3
					CASE IS < 60
						CFLAG:현가슴사이즈 = 2
					CASE IS < 80
						CFLAG:현가슴사이즈 = 4
