	SIF CFLAG:816 >= 100
		CFLAG:805 += 1
	;레벨100を超えていたら＋１
	SIF BASE:LV >= 100
		CFLAG:805 += 1
	;애정경험300이상
	SIF EXP:23 >= 300
		CFLAG:805 += 1
	;호감도が1万이상또는マイナス1万以下
	SIF CFLAG:의존도 >= 10000 || CFLAG:의존도 <= -10000
		CFLAG:805 += 1

