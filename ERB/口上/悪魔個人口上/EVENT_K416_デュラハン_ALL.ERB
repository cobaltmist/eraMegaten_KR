				;愛ＮＴＲ
				PRINTFORMW 
			ELSEIF TALENT:ＮＴＲ == 3 || TALENT:ＮＴＲ == 6
				;隷ＮＴＲ
				PRINTFORMW 
			ENDIF
			CFLAG:604 = 1
		ENDIF
		;-------------------------------------------------
		;;ＮＴＲ요바이・야외조교
		;;隷ＮＴＲ상태(TALENT:ＮＴＲが 3 か 6)でCFLAG:현ＮＴＲ진행도が6이상의 場合１日の종료시に発生。
		;-------------------------------------------------
	CASE 62
		CALL EVENT_MESSAGE
		IF (CFLAG:605 < 1 || FLAG:7 == 2)
			IF TALENT:ＮＴＲ == 1 || TALENT:ＮＴＲ == 4
				;愛ＮＴＲ
				PRINTFORMW 
			ELSEIF TALENT:ＮＴＲ == 3 || TALENT:ＮＴＲ == 6
				;隷ＮＴＲ
				PRINTFORMW 
			ENDIF
			CFLAG:605 = 1
		ENDIF
	CASEELSE
		SIF FLAG:6 > 0
			CALL EVENT_MESSAGE
ENDSELECT
