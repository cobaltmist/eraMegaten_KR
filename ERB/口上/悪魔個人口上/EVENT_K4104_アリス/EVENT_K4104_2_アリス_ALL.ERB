IF TEQUIP:20 &&  SELECTCOM != 107
	CALL PALAM_MESSAGE_D
	RETURN 0
ENDIF
;--------------------------------------------------
;特定キャラが조수、主人時に特殊キャラ구상を呼び出す(Y1を特定キャラ、4104_2を奴隷のキャラ번호に置換）
;--------------------------------------------------
;IF NO:MASTER == Y1
;	CALL KOJO_MESSAGE_PALAMCNG_D_PY1_K4104_2
;	RETURN 0
;ENDIF
;IF ASSI > 0 && ASSIPLAY
;	IF NO:ASSI == Y1
;		CALL KOJO_MESSAGE_PALAMCNG_D_PY1_K4104_2
;		RETURN 0
;	ENDIF
;ENDIF
;-------------------------------------------------
;パラメータ변화時のテ키스ト
;-------------------------------------------------
;-------------------------------------------------
;初めて윤활がLV3超えた
;-------------------------------------------------
P = PALAM:4 + UP:4
IF P > PALAMLV:4 && CFLAG:250 == 0
	CALL PALAM_MESSAGE_D
	;로션を使った場合
	IF SELECTCOM == 180
		PRINTFORMW 「야아응！ 엄청 끈적끈적해……」
	;それ以外
	ELSEIF TEQUIP:속옷（하） == 0
		PRINTFORMW 「응, 앙…%CSTR:一人称%의 가랑이가 질척질척해…」
	ELSE
		PRINTFORMW 「응、아응…팬티 안이 질척질척…」
	ENDIF
	CFLAG:250 = 1
ENDIF

;-------------------------------------------------
;初めて욕정がLV2超えた
;-------------------------------------------------
P = PALAM:6 + UP:6
IF P > PALAMLV:3 && CFLAG:251 == 0
	CALL PALAM_MESSAGE_D
	;미약を使った場合
	IF SELECTCOM == 181
		PRINTFORMW 「뭐, 뭐야 이거…몸이 뜨거워어…!」
	;それ以外
	ELSEIF TALENT:3 || TALENT:6
		PRINTFORMW 「가슴이 꾸ー욱 해서、기분 좋아아…」
	ELSE
		PRINTFORMW 「뭔가 두근두근거려…왜 그러지?」
	ENDIF
	CFLAG:251 = 1
ENDIF

;-------------------------------------------------
;初めて굴복がLV3超えた
;-------------------------------------------------
P = PALAM:7 + UP:7
IF P > PALAMLV:3 && CFLAG:252 == 0
	CALL PALAM_MESSAGE_D
	PRINTFORMW 
	CFLAG:252 = 1
ENDIF

;-------------------------------------------------
;初めて복종がLV3超えた
;-------------------------------------------------
P = PALAM:5 + UP:5
IF P > PALAMLV:3 && CFLAG:253 == 0
	CALL PALAM_MESSAGE_D
	;함락済み
	IF TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8
		IF TALENT:3 || TALENT:6
			PRINTFORMW 「에헤헷♪ %CSTR:二人称% 정말 좋아%UNICODE(0x2661)%」
		ELSE
			PRINTFORMW 
		ENDIF
	;それ以外
	ELSE
		PRINTFORMW 「요즘 %조사처리(CSTR:二人称,"과")% 있는 거 엄청 재밌어♪」
	ENDIF
	CFLAG:253 = 1
ENDIF

;-------------------------------------------------
;初めて공포がLV3超えた
;-------------------------------------------------
P = PALAM:11 + UP:11
IF P > PALAMLV:3 && CFLAG:254 == 0
	CALL PALAM_MESSAGE_D
	;함락済み
	IF TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	CFLAG:254 = 1
ENDIF


