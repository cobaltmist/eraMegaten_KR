;-------------------------------------------------
;조교시 악마호무라 전투패턴
;-------------------------------------------------
@SET_ACTION_AkumaHomura,ARG
#LOCALSIZE 2
#LOCALSSIZE 2

CFLAG:ARG:입력행동 = -1
CFLAG:ARG:타겟 = -1

IF CFLAG:ARG:행동한횟수 == 0
	CFLAG:ARG:입력행동 = [[스킬:ＡＴＴＡＣＫ]]
ELSE
	SELECTCASE RAND:100
		CASE 0 TO 50
			CFLAG:ARG:입력행동 = [[스킬:사랑을품은검은날개]]
		CASE 51 TO 70
			CFLAG:ARG:입력행동 = [[스킬:자애의기도]]
		CASE 71 TO 99
			CFLAG:ARG:입력행동 = [[스킬:구제의손]]
	ENDSELECT
ENDIF

;アクション実行不能
CALL CHECK_ACTIONABLE, ARG, CFLAG:ARG:입력행동

IF RESULT == 0
	CFLAG:ARG:입력행동 = [[스킬:ＡＴＴＡＣＫ]]
ENDIF

SIF CFLAG:ARG:타겟 < 1
	CALL RANDOM_TARGET, ARG, CFLAG:ARG:입력행동

