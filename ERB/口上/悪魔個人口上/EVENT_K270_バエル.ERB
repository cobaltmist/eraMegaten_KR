;-------------------------------------------------
;調教開始時（2回目以降、X1をキャラ番号に変更する）
;-------------------------------------------------
@EVENTTRAIN_SECOND_K270

;反発刻印Lv3
IF MARK:반발각인 == 3 && FLAG:7 == 2
	PRINTFORML 『避けるんや～！』
	PRINTFORMW %CALLNAME%は蛙を掴むとこちらに投げつけてきた
	PRINTFORML 
	PRINTFORML 
	PRINTFORML 『……』
	PRINTFORML 『人様を勝手に投げたらアカンやろ』
	PRINTFORMW 少女は蛙の前に正座して説教を受けている
	PRINTFORMW 『まぁ、%CSTR:二人称%が嫌いなのはわかるんやが』
	PRINTFORML 『これも契約や。腹くくりや』
	PRINTFORMW 少女は不機嫌そうにベッドに突っ伏した
	CFLAG:202 = 4
;屈服刻印Lv2以下
ELSEIF MARK:굴복각인 <= 2 && (CFLAG:202 < 1 || FLAG:7 == 2)
	IF RAND:2 == 0
		PRINTFORML 『ほれ、調教の時間やで』
		PRINTFORMW 何か言いたげな%CALLNAME%はベッドに横になった
	ELSE
		PRINTFORML 『来おったで』
		PRINTFORML 『ほら、準備せんかい』
		PRINTFORMW %CALLNAME%はベッドの下から淫具箱を取り出すとベッドの上に並べ始めた
	ENDIF
	CFLAG:202 = 1
;屈服刻印Lv3＋陥落素質無し
ELSEIF MARK:굴복각인 == 3 && (TALENT:연모 == 0 && TALENT:음란 == 0 && TALENT:복종 == 0 && TALENT:친애 == 0 && TALENT:창부 == 0 && TALENT:예속 == 0) && (CFLAG:202 < 4 || FLAG:7 == 2)
	IF RAND:2 == 0
		PRINTFORMW 『あと少しや』
	ELSE
		PRINTFORMW 『%CSTR:二人称%の調教の腕も確かやな』
	ENDIF
	PRINTFORMW 『あと少しでオトせるで』
	CFLAG:202 = 4
;陥落済み
ELSEIF CFLAG:202 < 5 || FLAG:7 == 2
	;연모、친애
	IF TALENT:연모 || TALENT:친애
		IF RAND:2 == 0
			PRINTFORMW %CALLNAME%が来ると、少女は嬉しそうな顔をして蛙を掴み上げた
			PRINTFORML 
			PRINTFORML 『ちょっ、ちょっと待ちや！』
			PRINTFORMW 部屋からつまみ出されそうになると蛙は慌てて少女を止めようとする
			IF TALENT:발정가능
				PRINTFORMW 『発情した痴態を見られとうないんは判るんやが』
			ELSE
				PRINTFORMW 『%CSTR:二人称%との睦言を覗かれるのが恥ずかしいんは判るんやが』
			ENDIF
			PRINTFORMW 『どうやってコミュニケーションをとるんや？』
			PRINTFORML 
			PRINTFORMW 少女は渋々と蛙を部屋の隅に置いた
			PRINTFORMW 『大丈夫や、口外せんから楽しんどき』
		ELSE
			PRINTFORMW 『遅いで』
			PRINTFORML 『この子も待ちくたびれとるやんか』
			IF TALENT:발정가능 == 1
				PRINTFORMW いつもの可愛らしいはずの少女はそこにはおらず、発情した雌がいた
			ELSE
				PRINTFORMW 少女は肯いている
			ENDIF
			PRINTFORML 
			PRINTFORMW 『ほな、後は若いもん同士楽しんどきや』
		ENDIF
	;음란、창부
	ELSEIF TALENT:음란 || TALENT:창부
		IF RAND:2 == 0
			PRINTFORMW 『遅いで』
			PRINTFORML 『あと１分遅かったら、オナニー始めとったで』
			IF TALENT:발정가능 == 1
				PRINTFORMW 少女が座るベッドには色々な淫具が転がっていた
			ELSE
				PRINTFORMW 少女は肯いている
			ENDIF
			PRINTFORMW 『ほな、可愛がってあげな』
		ELSE
			PRINTFORML 『ほら、%CSTR:二人称%がきおったで』
			PRINTFORMW 『よぉ我慢したな』
			PRINTFORMW 少女が座るベッドには色々な淫具が転がっていた
			PRINTFORMW 『我慢した分、一杯可愛がってもらいや』
		ENDIF
	;복종、예속
	ELSEIF TALENT:복종 || TALENT:예속
		PRINTFORML 『ほら、%CSTR:一人称%がきおったで』
		PRINTFORML 『きちんと挨拶せんかい』
		PRINTFORMW %CALLNAME%は慌てて床に土下座をした
		PRINTFORMW 『ほな、いつも通り好きに使いや』
	ENDIF
	CFLAG:202 = 5
ELSE
	PRINTFORML 『ほら、%CSTR:一人称%がきおったで』
	IF RAND:2 == 0
		PRINTFORMW %CALLNAME%は猫の様にベッドで丸くなっていたが、蛙の声でもそもそと動き始めた
	ELSE
		PRINTFORMW %CALLNAME%は椅子に座ってミルクを飲んでいたが、もの惜しげにカップをテーブルに置くと調教の準備を始めた
	ENDIF
ENDIF
RETURN 0

;-------------------------------------------------
;EVENTEND関係（270をキャラ番号に置換）
;調教終了時のセリフ
;-------------------------------------------------
@EVENTEND_K270

;--------------------------------------------------
;調教終了時のセリフ
;--------------------------------------------------
DRAWLINE
;反発刻印Lv3
IF MARK:반발각인 == 3 && (CFLAG:217 < 1 || FLAG:7 == 2)
		PRINTFORMW 『あかんで、%CSTR:二人称%』
		PRINTFORML 『次はからは嫌われんようにせんと』
		PRINTFORMW 少女はこちらを睨んでいる
	CFLAG:217 = 1
;屈服刻印Lv3
ELSEIF MARK:굴복각인 == 3
	;体力500未満
	IF BASE:0 < 500 && (CFLAG:218 < 1 || FLAG:7 == 2)
		;음란、창부、발정중
		IF TALENT:음란 || TALENT:창부 || TALENT:발정가능
			PRINTFORMW 『盛りの付いた猫は途中でストップせんと死ぬまで止まらんで』
			PRINTFORMW 少女は先程までの調教を思い出して微笑んでいる
		;연모、친애
		ELSEIF TALENT:연모 || TALENT:친애
			PRINTFORMW 『いくら可愛いからって、ヤりすぎやな』
			PRINTFORMW 少女は肩で息をしながらこちらに顔を向けて微笑んでいる
		;복종、예속
		ELSEIF TALENT:복종 || TALENT:예속
			PRINTFORMW 『随分可愛がったな』
			PRINTFORML 『ほら、お前さんも満足やろ？』
			PRINTFORMW 少女は肩で息をしながら肯いている
		ELSE
			PRINTFORMW 『ちょっ～とヤリすぎやな、%CSTR:二人称%』
			PRINTFORMW 『次はもうちっと優しくせんといかんで』
		ENDIF
		CFLAG:218 = 1
	ELSE
		;연모、친애
		IF TALENT:연모 || TALENT:친애
			PRINTFORML 『ほら、今日はこれで終いや』
			PRINTFORMW 少女は頬を膨らませて不満そうな表情をしている
			IF RAND:2 == 0
				PRINTFORMW 『また次に%CSTR:二人称%に可愛がってもらいや』
			ELSE
				PRINTFORMW 『%CSTR:二人称%も忙しいんやから、我慢しときや』
			ENDIF
		;음란、창부
		ELSEIF TALENT:음란 || TALENT:창부
			PRINTFORML 『今日はたっぷり可愛がってもらえたか？』
			IF PALAM:욕정 >= 8
				PRINTFORML 少女は肯いている
				PRINTFORMW 『そうかぁ、良かったなぁ』
			ELSE
				PRINTFORMW 少女は頬を膨らませて不満そうな表情をしている
				PRINTFORML 『そうかぁ』
				IF RAND:2 == 0
					PRINTFORMW 『次に期待しときや』
				ELSE
					PRINTFORML 『オナニーでもしとくか？』
					PRINTFORMW 少女は肯いている
				ENDIF
			ENDIF
		;복종、예속
		ELSEIF TALENT:복종 || TALENT:예속
			IF (PALAM:치정 + PALAM:고통) >= 12
				PRINTFORML 『良かったなぁ』
				PRINTFORMW 『次も楽しみにしときや』
			ELSE
				PRINTFORML 『そうかぁ』
				PRINTFORMW 『続きは次回のお楽しみやな』
			ENDIF
		ELSE
			PRINTFORML 『今日もお疲れや』
			PRINTFORMW 少女は何か不満そうだ
			IF RAND:2 == 0
				PRINTFORML 『なんや、もっと可愛がって欲しいんか？』
				PRINTFORMW 少女は肯いている
			ELSE
				PRINTFORMW 『なんや、調教終わったのに嬉しくないんか？』
				PRINTFORML 『それとも、身体が疼いてしょうがないんか？』
				PRINTFORMW %CALLNAME%は赤面しながらポカポカと蛙を叩いた
			ENDIF
		ENDIF
		CFLAG:219 = 4
	ENDIF
;それ以外
ELSEIF  (CFLAG:219 < 1 || FLAG:7 == 2)
	IF RAND:2 == 0
		PRINTFORMW 『お疲れや、%CSTR:二人称%』
	ELSE
		PRINTFORMW 『よぉ頑張ったの、%CSTR:二人称%』
	ENDIF
	IF RAND:2 == 0
		PRINTFORML 『そしたら、飯でも食いに行くか』
		PRINTFORMW 少女は肯いた
	ELSE
		PRINTFORML 『ほれ、最後に部屋の片づけや』
		PRINTFORMW 少女は不満そうだ
	ENDIF
	CFLAG:219 = 1
ENDIF
RETURN 0

