	;로션を使った場合
	IF SELECTCOM == 180
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	CFLAG:250 = 1
ENDIF

;-------------------------------------------------
;初めて욕정がLV2超えた
;-------------------------------------------------
P = PALAM:6 + CUP:6
IF P > PALAMLV:3 && CFLAG:251 == 0
	CALL PALAM_MESSAGE_D
	;미약を使った場合
	IF SELECTCOM == 181
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	CFLAG:251 = 1
ENDIF

;-------------------------------------------------
;初めて굴복がLV3超えた
;-------------------------------------------------
P = PALAM:7 + CUP:7
