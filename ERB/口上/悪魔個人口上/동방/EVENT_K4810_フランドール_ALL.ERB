	;個別といいつつ他キャラによっても分岐
	CASE "개별END"
		;関係者陥落状況
		LOCAL:1 = 0
		LOCAL:2 = 0
		LOCAL:3 = 0
		LOCAL:4 = 0
		LOCAL:5 = 0
		REPEAT CHARANUM
			LOCAL = 0
			;関係者番号設定
			;4809_レミリア
			IF NO:COUNT == 4809
				LOCAL = 1
				LOCAL:11 = COUNT
			;4805_紅美鈴
			ELSEIF NO:COUNT == 4805
				LOCAL = 2
				LOCAL:12 = COUNT
			;4806_小悪魔
			ELSEIF NO:COUNT == 4806
				LOCAL = 3
				LOCAL:13 = COUNT
			;4807_パチュリー
			ELSEIF NO:COUNT == 4807
				LOCAL = 4
				LOCAL:14 = COUNT
			;4808_咲夜
			ELSEIF NO:COUNT == 4808
				LOCAL = 5
				LOCAL:15 = COUNT
			ENDIF
			IF LOCAL > 0
				IF TALENT:COUNT:친애
					LOCAL:LOCAL = 6
				ELSEIF TALENT:COUNT:창부
					LOCAL:LOCAL = 7
				ELSEIF TALENT:COUNT:예속
					LOCAL:LOCAL = 8
				ENDIF
			ENDIF
		REND

		;恋慕エンド
		IF (TALENT:아내 || TALENT:남편) && CFLAG:21 == 1
			DRAWLINE
			WSTR:(K++) = …………
			WSTR:(K++) = ………
			WSTR:(K++) = ……
