@EVENTTRAIN_SECOND_K4302_1
;반발각인Lv3
IF MARK:3 == 3 && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 「만약 또 다시 이런 행위를 위한 호출이 있다면……」
	PRINTFORMW 나지막히 읊조리는 %CALLNAME%의 눈동자엔 증오와 혐오가 가득하다.
	CFLAG:202 = 4
;굴복각인Lv0
ELSEIF MARK:2 == 0 && (CFLAG:202 < 1 || FLAG:7 == 2)
	IF RAND:2 == 0
		PRINTFORMW 「%CSTR:11%는……」
	ELSE
		PRINTFORMW 「%CSTR:12%…」
	ENDIF
	IF RAND:2 == 0
		PRINTFORMW %CALLNAME%는 괴로운 듯 눈을 질끈 감는다.
	ELSE
		PRINTFORMW 그리고 무언가를 참는 듯이 입술을 꽉 깨물었다.
	ENDIF
	CFLAG:202 = 1
;굴복각인Lv1
ELSEIF MARK:2 == 1 && (CFLAG:202 < 2 || FLAG:7 == 2)
	IF RAND:2 == 0
		PRINTFORMW 「또 여기에…」
	ELSE
		PRINTFORMW 「%CSTR:12%가 무슨 생각을 하는건지, 도저히 알 수가 없네요.」
	ENDIF
	CFLAG:202 = 2
;굴복각인Lv2
ELSEIF MARK:2 == 2 && (CFLAG:202 < 3 || FLAG:7 == 2)
	IF RAND:2 == 0
		PRINTFORMW 「…………」
	ELSE
		PRINTFORMW 「언제까지 이런 행위를 계속할 생각입니까？」
	ENDIF
	CFLAG:202 = 3
;굴복각인Lv3＋함락소질無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0) && (CFLAG:202 < 4 || FLAG:7 == 2)
	IF RAND:2 == 0
		PRINTFORMW 「%CSTR:12%의 명령이라면……」
	ELSE
		PRINTFORMW 「어떠한 것이라도 따르는 게 저의 본분입니다.」
	ENDIF
	CFLAG:202 = 4
;함락済み
ELSEIF CFLAG:202 < 5 || FLAG:7 == 2
	;연모、친애
	IF TALENT:3 || TALENT:6
		IF RAND:2 == 0
			PRINTFORMW 「불러 주셨군요, %CSTR:12%」
			PRINTFORMW 「당신과 함께 보낼 이 시간을, %CSTR:11%는 결코 잊지 않겠습니다.」
			PRINTFORMW %CALLNAME%는 상냥한 미소와 함께, 살그머니 %CALLNAME:MASTER%의 머리를 쓰다듬는다.
		ELSE
			PRINTFORMW %CALLNAME%은 기쁜 듯한 얼굴로 %CALLNAME:MASTER%를 꼭 껴안아, 아무 말도 하지 않은채 그저 입맞춤을 주고 받는다.
			PRINTFORMW 그들에겐 이미, 말이 필요없었다.
		ENDIF
	;음란, 창부
	ELSEIF TALENT:4 || TALENT:7
		IF RAND:2 == 0
			PRINTFORMW 「자…시작할까요.」
		ELSE
			PRINTFORMW 「마음껏, 써주세요.」
		ENDIF
		PRINTFORM %CALLNAME:MASTER%의 눈앞에서, 벌거벗은 채 양팔을 활짝 펼쳐 보인다.
		IF RAND:2 == 0
			PRINTFORMW 무척이나 아름답고, 매력적인 육체가 눈에 들어왔다.
		ELSE
			PRINTFORMW %CALLNAME%는 자기도 모르게 침을 꿀꺽 삼켰다.
		ENDIF
	;복종、예속
	ELSEIF TALENT:5 || TALENT:8
		;랜덤で구상が변화する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	CFLAG:202 = 5
ENDIF
RETURN 0

