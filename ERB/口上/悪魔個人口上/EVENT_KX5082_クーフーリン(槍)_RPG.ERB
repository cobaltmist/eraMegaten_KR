;-------------------------------------------------
;調教開始時（一回のみ） CFLAG201
;-------------------------------------------------
DRAWLINE
;初調教時
IF CFLAG:201 == 0 && TALENT:연모 == 0
	CFLAG:201 = 1
	RETURN 0
;屈服刻印Lv1
ELSEIF CFLAG:201 < 2 && MARK:2 == 1
	PRINTFORMW 
	CFLAG:201 = 2
	RETURN 0
;屈服刻印Lv2
ELSEIF CFLAG:201 < 3 && MARK:2 == 2
	PRINTFORMW 
	CFLAG:201 = 3
	RETURN 0
;屈服刻印Lv3
ELSEIF CFLAG:201 < 4 && MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0)
	PRINTFORMW 
	CFLAG:201 = 4
	RETURN 0
;함락
ELSEIF CFLAG:201 < 5 && (TALENT:3 || TALENT:4 || TALENT:5)
	;연모
	IF TALENT:3
		IF CFLAG:미조교함락 == 1
			;調教せずに恋慕をつけた場合
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;음란
	ELSEIF TALENT:4
		PRINTFORMW 
	;복종
	ELSEIF TALENT:5
		PRINTFORMW 
	ENDIF
	CFLAG:201 = 5
	RETURN 0
ENDIF

