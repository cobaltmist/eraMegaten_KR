		ELSE
			BREAK
		ENDIF
	NEXT
	PRINTFORML ┗%"━" * (文字数/2)%┛
ELSE
	LOCAL:3 = 1
	PRINT_IMG @"A844_NAVI_1"
	PRINTFORML ┏┓%ARGS + "┏" + "━" * (((文字数 - 4) - STRLENS(ARGS)+1) / 2)%┓
	FOR LOCAL, 1, 3
