	PRINTFORML 　
	LOCAL:3++
	PRINT_IMG @"A844_NAVI_%TOSTR(LOCAL:3)%"
	PRINTFORML ┏┓%ARGS + "┏" + "━" * (((文字数 - 4) - STRLENS(ARGS)+1) / 2)%┓
	FOR LOCAL, 1, 3
