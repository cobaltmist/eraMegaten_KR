#DIM 文字数
SIF ARGS:1 == ""
	RETURN 0
SIF ARGS != "MESSAGE"
	ARGS = ＠%ARGS%
SIF STRLENS(ARGS) % 2 == 1
	ARGS += " "
文字数 = 64
FOR LOCAL, 0, 3
	SIF STRLENS(ARGS:LOCAL) == 0
		BREAK
	LOCAL:1 = STRLENS(ARGS:LOCAL)
	SIF LOCAL == 0
		LOCAL:1 += 4
	SIF 文字数 < LOCAL:1
		文字数 = LOCAL:1
	SIF 文字数 % 2 == 1
		文字数 ++
NEXT

LOCAL:2 = 2
