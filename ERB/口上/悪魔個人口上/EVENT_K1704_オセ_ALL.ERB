;--------------------------------------------------------------------------------------------------
;★오세の타입変動に関係した関数
;--------------------------------------------------------------------------------------------------
;-------------------------------------------------
;†조교자の사정による오세の타입変動　CFLAG:801
;-------------------------------------------------
@OSE_TYPE_CHANGE_1_K1704
;오세の입に사정
IF TFLAG:0 > 0
	CFLAG:801 += 1
;오세に中出し
ELSEIF TFLAG:2 > 0
	;애널に中出し
	IF SELECTCOM == 62 || SELECTCOM == 63 || SELECTCOM == 67 || SELECTCOM == 70 || SELECTCOM == 71 || SELECTCOM == 73 || SELECTCOM == 78 || SELECTCOM == 79 || SELECTCOM == 512 || SELECTCOM == 515 || SELECTCOM == 514
		;당신が中出し
		IF ASSIPLAY == 0
			;함락済み
			IF TALENT:3 || TALENT:4 || TALENT:5 || TALENT:6 || TALENT:7 || TALENT:8
				CFLAG:801 += 2
				;친애なら上昇
				SIF TALENT:친애
					CFLAG:801 += 1
				;오세が남편なら下降
				SIF TALENT:남편
					CFLAG:801 -= 1
			ELSE
				CFLAG:801 += 1
			ENDIF
		;당신以外が中出し
		ELSE
			CFLAG:801 += 1
		ENDIF
	;애널광なら上昇
	SIF TALENT:애널광
		CFLAG:801 += 3
	ENDIF
;역강간で당신が사정
ELSEIF TFLAG:12 > 0
	CFLAG:801 -= 1
	;오세が친애or남편なら下降
	SIF TALENT:친애
		CFLAG:801 -= 2
	SIF TALENT:남편
		CFLAG:801 -= 3
ENDIF

;입内or中出しで정액중독Lv4이상なら上昇
SIF ABL:정액중독 >= 4 && (TFLAG:0 > 0 || TFLAG:2 > 0 )
	CFLAG:801 += 2

;-------------------------------------------------
;†촉수による오세の타입変動（촉수コマンドは一括）　CFLAG:801
;-------------------------------------------------
@OSE_TYPE_CHANGE_2_K1704
;애널촉수、촉수유두자극、촉수긴박、촉수관장、촉수구욕、촉수페니스자극を実行した場合
IF GROUPMATCH(SELECTCOM , 162 , 164 , 166 , 167 , 168 , 169 )
	CFLAG:801 += 1
	;촉수가사정したら上昇
	SIF TFLAG:15 >0
		CFLAG:801 += 1
	;촉수가사정して촉수중독Lv4이상なら上昇
	SIF TFLAG:15 >0 && ABL:촉수중독 >= 4
		CFLAG:801 += 3
	;애널コマンドで애널광なら上昇
	SIF TALENT:애널광 && (SELECTCOM == 162 || SELECTCOM == 167)
		CFLAG:801 += 3
	;입内or中出しで정액중독Lv4이상なら上昇
	SIF ABL:정액중독 >= 4 && (TFLAG:0 > 0 || TFLAG:2 > 0 )
	CFLAG:801 += 2
ENDIF

;-------------------------------------------------
;†오세の절정or사정による오세の타입変動　CFLAG:801
;-------------------------------------------------
@OSE_TYPE_CHANGE_3_K1704
;절정の판정
;Ａ절정した場合
IF NOWEX:2 > 0 
	;強절정
	IF TFLAG:29 > 1
		CFLAG:801 += 2
	;절정
	ELSEIF TFLAG:29 > 0
		CFLAG:801 += 1
	ENDIF
	;애널광なら上昇
	SIF TALENT:애널광
		CFLAG:801 += 2
ENDIF

;Ｂ절정した場合
IF NOWEX:3 > 0 
	;強절정
	IF TFLAG:29 > 1
		CFLAG:801 += 2
	;절정
	ELSEIF TFLAG:29 > 0
		CFLAG:801 += 1
	ENDIF
ENDIF

;오세が사정した場合
;Ａ系조교時사정で上昇
IF TFLAG:10 > 0
	IF GROUPMATCH(SELECTCOM , 4 , 5 , 45 , 46 , 62 , 63 , 67 , 70 , 71 , 73 , 500 , 512 , 514 )
		CFLAG:801 += 2
		;애널광なら上昇
		SIF TALENT:애널광
			CFLAG:801 += 2
	;조교자に中出しで下降
	ELSEIF GROUPMATCH(SELECTCOM , 64 , 92 , 93 , 94 , 95 , 96 , 97 )
		CFLAG:801 -= 2
		;大量사정
		SIF TFLAG:10 == 2
			CFLAG:801 -= 2
		;오세に주도권
		SIF TEQUIP:주도권
			CFLAG:801 -= 3
		;相手が당신で친애なら下降
		SIF TALENT:친애 && ASSIPLAY == 0
			CFLAG:801 -= 1
		;相手が당신で오세が남편なら下降
		SIF TALENT:남편 && ASSIPLAY == 0
			CFLAG:801 -= 2
		;애널광なら上昇
		SIF TALENT:애널광
			CFLAG:801 += 2
	;펠라한다、파이즈리한다で사정
	ELSEIF GROUPMATCH(SELECTCOM , 2 , 90 , 91 )
		CFLAG:801 -= 1
		;大量사정
		SIF TFLAG:10 == 2
			CFLAG:801 -= 2
			;오세に주도권
		SIF TEQUIP:주도권
			CFLAG:801 -= 3
	ENDIF
;입内、中出しで조교자の정액중독Lv4이상なら下降
SIF ABL:PLAYER:정액중독 >= 4 && ( SELECTCOM == 2 || SELECTCOM == 64 || SELECTCOM == 92 || SELECTCOM == 93 || SELECTCOM == 94 || SELECTCOM == 95 || SELECTCOM == 96 || SELECTCOM == 97)
	CFLAG:801 -= 2
ENDIF

