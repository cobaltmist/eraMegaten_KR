;122,조수를범하게한다
;-------------------------------------------------
	CASE 122
		SELECTCASE ARG
			;気持ちよくて感じている
			CASE 1
				PRINTFORMW %조수는% 기분 좋아 보인다
			;こっちに来てと言うセリフ
			CASE 2
				PRINTFORMW %조수는% %조사처리(CALLNAME, "을")% 유혹하고 있다
			;大丈夫というセリフ
			CASE 3
				PRINTFORMW %조수는% %CALLNAME%에게 범해져도 평온한 듯 하다
			;범해지며 강한 척을 하는 대사
			CASE 4
				PRINTFORMW %CALLNAME%에게 범해지며 %조수는% 강한 척을 하고 있다
			;능숙해졌네 대사
			CASE 5
				PRINTFORMW %조수는% %조사처리(CALLNAME, "이")% 능숙해지는 것을 느끼고 있는 것 같다
		ENDSELECT
		
