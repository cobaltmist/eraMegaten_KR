@KOJO_MESSAGE_ASSIPAIR_COM_PUB303,ARG

SIF ASSIPLAY == 1
	RETURN 0
SELECTCASE SELECTCOM

;-------------------------------------------------
;122,조수를범하게한다
;-------------------------------------------------
	CASE 122
		SELECTCASE ARG
			;기분 좋아서 느끼고 있다
			CASE 1
				PRINTFORMW 『앙、아응…너무 기분 좋아%UNICODE(0x2661)%』
				RETURN 1
			;이쪽으로 오라고 하는 대사
			CASE 2
				PRINTFORMW 『자아、이 쪽으로 와！』
				PRINTFORMW 『당신의 것、받아들여・줄・게%UNICODE(0x2661)%』
				RETURN 1
			;괜찮아라는 대사
			CASE 3
				IF 0 == 1
					PRINTFORMW 『사양할거 없어！』
				ELSE	
					PRINTFORMW 『괜찮아！』
					PRINTFORMW 『이ー언니가 리드해줄게！』
				ENDIF	
				RETURN 1
			;허세 부리는 대사
			;CASE 4
			;	PRINTFORMW 『』
			;	RETURN 1
			;능숙함을 칭찬하는 대사
			;CASE 5
			;	PRINTFORMW 『』
			;	RETURN 1
		ENDSELECT
		
;-------------------------------------------------
;124,더블펠라
;-------------------------------------------------
	CASE 124
			SELECTCASE ARG
				;같이 기분 좋게 하자는 대사
				CASE 1
					PRINTFORMW 『자아、같이 기분 좋게 하자！』
					RETURN 1
				;내가 더 기분 좋게라는 대사
				CASE 2
					PRINTFORMW 『%CSTR:ASSI:일인칭% 쪽이 더 잘하는걸！』
					PRINTFORMW 『그렇지、%CSTR:ASSI:이인칭%%UNICODE(0x2661)%』
					RETURN 1
				;충격 받은 나머지 하는 대사
				CASE 3
					PRINTFORMW 『그렇지 않아！%CSTR:ASSI:일인칭%도 지지 않는걸！』
					RETURN 1
				;평소보다 느낀다는 대사
				CASE 4
					PRINTFORMW 『%CSTR:ASSI:이인칭%의 자지、평소보다 움찔거리네』
					RETURN 1
				;레즈 플레이 같은 대사
				CASE 5
					IF ABL:ASSI:레즈끼 >= 3
						PRINTFORMW 『에헤헤、%조사처리(CALLNAME,"와")% 같이 하는게 기쁠지도』
						RETURN 1
					ENDIF
					PRINTFORMW 『%CSTR:ASSI:일인칭%랑 하는게 그렇게 좋아？』
					RETURN 1
				;두 명의 페라는 기분이 좋지?대사
				CASE 6
					PRINTFORMW 『둘이서 하는 펠라는 기분 좋지？%CSTR:ASSI:이인칭%』
					RETURN 1
			ENDSELECT

;-------------------------------------------------
;125,더블파이즈리%조사처리(CALLNAME,"가")% %조사처리(CSTR:일인칭,"와")%
;-------------------------------------------------
	CASE 125
		SELECTCASE ARG
			;같이 기분 좋게 하자는 대사
			CASE 1
				PRINTFORMW 『자아、둘이서 함께 파이즈리하자！』
				RETURN 1
			;내가 더 기분 좋게 라는 대사
			CASE 2
				PRINTFORMW 『%CALLNAME%보다 %CSTR:ASSI:일인칭%의 가슴이 더 기분 좋은거네。%CSTR:ASSI:이인칭%%UNICODE(0x2661)%』
				RETURN 1
			;충격 받은 나머지 하는 대사
			CASE 3
				PRINTFORMW 『말했겠다～！저～얼대로 지지 않을테니까！』
				RETURN 1
			;평소보다 느낀다는 대사
			;CASE 4
				PRINTFORMW 『%CSTR:ASSI:이인칭%、평소보다 느끼고 있네』
				RETURN 1
			;서로의 유두가 닿아서 기분 좋다는 대사
			CASE 5
				PRINTFORMW 『응%UNICODE(0x2661)%유두 스쳐서 기분 좋아…』
				RETURN 1
			;두 사람의 파이즈리로 기분 좋아진 대사
			CASE 6
				PRINTFORMW 『%CSTR:ASSI:일인칭%들의 파이즈리로 기분 좋아지라고！』
				RETURN 1
			;파이즈리로 느끼고 있어？대사
			CASE 7
				PRINTFORMW 『파이즈리로 느끼고 있어？』
				RETURN 1
		ENDSELECT
;-------------------------------------------------
;185,조수를범한다,
;-------------------------------------------------
	CASE 185
		SELECTCASE ARG
			;범해서 기분 좋다는 대사
			CASE 1
				PRINTFORMW 『앙、앙、기분 좋아아%UNICODE(0x2661)%』
				RETURN 1
			;우리들、정말 사이가 좋죠？대사
			CASE 2
				PRINTFORMW 『%CSTR:ASSI:일인칭%랑 %CSTR:ASSI:이인칭%랑 정말 사이가 좋아%UNICODE(0x2661)%』
				PRINTFORMW 『그렇지、%CSTR:ASSI:이인칭%%UNICODE(0x2661)%』
				RETURN 1
			;부럽죠？대사
			CASE 3
				PRINTFORMW 『에헤헤～、정말 부럽지～』
				RETURN 1
			;이쪽이 신경 쓰이나 보네 대사
			CASE 4
				PRINTFORMW 『있잖아、%조사처리(CALLNAME,"는")% 이쪽이 신경 쓰이는 것 같네』
				RETURN 1
		ENDSELECT

;-------------------------------------------------
;193,콘돔정음（조수）
;-------------------------------------------------
	CASE 193
		SELECTCASE ARG
			;정액이라도 맛있어 대사
			CASE 1
				PRINTFORMW 『츕、츄르릅…하아%UNICODE(0x2661)% 정액맛있어♪』
				RETURN 1
			;이것은 내 것이야！대사
			CASE 2
				PRINTFORMW 『이건 %CSTR:ASSI:일인칭% 꺼니까！』
				PRINTFORMW 『%CALLNAME%한테 주지 않을거니까！츄읍…츄릅』
				RETURN 1
			;부럽죠？대사
			CASE 3
				PRINTFORMW 『부럽지？』
				RETURN 1
			;먼저 받을게요 대사（사이가 좋음）
			CASE 4
				PRINTFORMW 『%CSTR:ASSI:일인칭% 먼저 정액 받을게』
				RETURN 1
		ENDSELECT
;-------------------------------------------------
;500,３Ｐ
;-------------------------------------------------
	CASE 500
		SELECTCASE ARG
			;안쪽이 너무 기분 좋다 대사
			CASE 1
				PRINTFORMW 『아앙…！%CALLNAME%의 안、정말 기분 좋아아…』
				RETURN 1
			;계속 하는거야！대사
			CASE 2
				PRINTFORMW 『그거야！%CSTR:ASSI:일인칭%만의 허리 움직임으로 계속 하는거야！』
				RETURN 1
			;너무 기분 좋은것 같아 대사
			CASE 3
				PRINTFORMW 『%CALLNAME% 엄청 기분 좋은것 처럼 하고 있네。』
				RETURN 1
			;여기에 넣어 대사
			CASE 4
				PRINTFORMW 『자아%CSTR:ASSI:이인칭%、여기에 넣어줘ー』
				RETURN 1
		ENDSELECT
;-------------------------------------------------
;510,더블스마타
;-------------------------------------------------
	CASE 510
		SELECTCASE ARG
			;같이 기분 좋게 하자는 대사
			CASE 1
				PRINTFORMW 『자아、같이 자지 기분 좋게 하자！』
				RETURN 1
			;내가 더 기분좋게 한다는 대사
			CASE 2
				PRINTFORMW 『%CSTR:ASSI:일인칭%의 보지 쪽이 기분 좋은걸！』
				PRINTFORMW 『그렇지！%CSTR:ASSI:이인칭%！』
				RETURN 1
			;충격 받은 나머지 하는 대사
			CASE 3
				PRINTFORMW 『믓ー！그런 말까지 들으면 절대로 질 수 없으니까！』
				RETURN 1
			;평소보다 느낀다는 대사
			CASE 4
				PRINTFORMW 『%CSTR:ASSI:이인칭%、엄청 기분 좋은것 같네』
				RETURN 1
			;서로 기분이 좋네 대사
			;CASE 5
			;	PRINTFORMW 『』
			;	RETURN 1
			;기분 좋아 대사
			CASE 6
				PRINTFORMW 『보지 스쳐서 기분 좋아아%UNICODE(0x2661)%』
				RETURN 1
		ENDSELECT
