	IF SELECTCOM == 180
		PRINTFORMW 「こんなヘンなの塗って……ヌルヌルだわ……」
	;それ以外
	ELSE
		IF RAND:2 == 0
			PRINTFORMW 「ぬ、濡れてなんかなわ！」
			PRINTFORMW 「これは汗……汗だわ！　触られるから汗かいたわ！」
		ELSE
			PRINTFORML （どうしよう……気持ちよくて濡れてきちゃってる……）
			PRINTFORMW %CALLNAME%は顔を上気させながら自分の秘部を見つめている。
		ENDIF
	ENDIF
	CFLAG:250 = 1
ENDIF

;-------------------------------------------------
;初めて欲情がLV2超えた
;-------------------------------------------------
P = PALAM:6 + CUP:6
IF P > PALAMLV:3 && CFLAG:251 == 0
	K_CALL:K = PALAM_MESSAGE_D
	;媚薬を使った場合
	IF SELECTCOM == 181
		;屈服刻印Lv3以下　か　反発刻印Lv3　の場合
