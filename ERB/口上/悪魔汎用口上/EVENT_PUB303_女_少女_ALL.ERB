IF CFLAG:ARG:입력행동 == 2022 && NO:ARG == 407
;초메기도사용時
	;CALLFORM SKILL_NAME_{CFLAG:ARG:입력행동},ARG
	PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
	PRINTFORML ┃%"" + 조사처리(CSTR:일인칭,"가") + " 가게 해・줄・게" + UNICODE(0x2661),72,LEFT%┃
	PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛

ELSEIF ARG:1 > 0
	;충성도が上昇する行動をとった場合に표시する구상

	CALLFORM SKILL_NAME_{CFLAG:ARG:입력행동},ARG
	;사용した스킬の이름をLOCALSに代入
	LOCALS = %RESULTS%

	SELECTCASE ARG:1
		;물리데미지を与える行動で、영맹持ちの충성도が上昇
		CASE GETNUM(TALENT,"영맹")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ 가라앗！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;마법데미지を与える行動で、교활持ちの충성도が上昇
		CASE GETNUM(TALENT,"교활")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ 해버렷！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;高위력の行動で、잔학持ちの충성도が上昇
		CASE GETNUM(TALENT,"잔학")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ 화려하게 날아가버렷♪",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;相手の약점を付いた공격or行動不能な상태이상の적に対する공격で、현명持ちの충성도が上昇
		CASE GETNUM(TALENT,"현명")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ 역시" + CSTR:일인칭 + "♪",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;파마相性의 行動で、천사持ちの충성도が上昇
		CASE GETNUM(TALENT,"천사")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ " + "천벌로 정하는 거야！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;주살相性의 行動で、타천사持ちの충성도が上昇
		CASE GETNUM(TALENT,"타천사")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "라는 탐미한 느낌으로 오싹오싹해버려" + UNICODE(0x2661),72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;회복・상태회복行動では우애持ちの충성도が上昇
		CASE GETNUM(TALENT,"우애")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ 사랑을 느껴！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;상태변화では교만持ちの충성도が上昇
		CASE GETNUM(TALENT,"교만")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%"이런녀석、" + LOCALS + "로 충분하지！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;特殊では냉정持ちの충성도が上昇
		CASE GETNUM(TALENT,"냉정")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "네？ 응、맡겨줘！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;우둔持ちは主人とのLv差だけ충성도が上昇
		CASE GETNUM(TALENT,"우둔")
			PRINTFORML ┏＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "？ " + CSTR:이인칭 + "이 하는 말이라면 틀림 없네",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;허심持ちは최저２だけ충성도が上昇
		CASE GETNUM(TALENT,"허심")
			PRINTFORML ┏＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "지、알고 있어？",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
	ENDSELECT
;ELSE
;	;システムによらず충성도を上げたいとか他があったらここに記載
ENDIF
