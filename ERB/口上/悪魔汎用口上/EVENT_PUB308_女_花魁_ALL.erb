	;大데미지、HPの4割이상
	ELSEIF 데미지비율(ARG, ARG:2) > 40
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
		IF RAND:2 == 0
			PRINTFORML ┃%"머리가 아파요～！",72,LEFT%┃
		ELSE
			PRINTFORML ┃%"도와주지 않으면 화낼 것이와요",72,LEFT%┃
		ENDIF
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
	;흡수
	ELSEIF ARG:2 < 0
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%"어머나, 빨아들이겠사와요",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
	ENDIF
;회피された
ELSEIF TFLAG:전투이벤트 == 17
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%"어머머… 봐드릴 수 없겠사와요",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛

ENDIF


@BATTLE_MESSAGE_PUB308,ARG,ARG:1 = 0

;選択したコマンドにより分岐させたい場合、IF文の조건にコマンドNoを記入すること
;IF CFLAG:ARG:입력행동 == 
;	PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
;	PRINTFORML ┃%"",72,LEFT%┃
;	PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛

;ELSEIF ARG:1 > 0
IF ARG:1 > 0
	;충성도が上昇する行動をとった場合に표시する구상

	CALLFORM SKILL_NAME_{CFLAG:ARG:입력행동},ARG
	;사용した스킬の이름をLOCALSに代入
	LOCALS = %RESULTS%

	SELECTCASE ARG:1
;		;물리데미지を与える行動で、영맹持ちの충성도が上昇
		CASE GETNUM(TALENT,"영맹")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "를 받아보시와요！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;마법데미지を与える行動で、교활持ちの충성도が上昇
		CASE GETNUM(TALENT,"교활")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "를 드셔보시와요！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;高위력の行動で、잔학持ちの충성도が上昇
		CASE GETNUM(TALENT,"잔학")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "로 화려하게 가주겠사와요！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;相手の약점を付いた공격or行動不能な상태이상の적に対する공격で、현명持ちの충성도が上昇
		CASE GETNUM(TALENT,"현명")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！ 과연 " + CSTR:일인칭 + "이시와요♪",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;파마相性의 行動で、천사持ちの충성도が上昇
		CASE GETNUM(TALENT,"천사")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "로 끝내주겠사와요！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;주살相性의 行動で、타천사持ちの충성도が上昇
		CASE GETNUM(TALENT,"타천사")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%"후후… " + LOCALS + "로 가겠사와요",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;회복・상태회복行動では우애持ちの충성도が上昇
		CASE GETNUM(TALENT,"우애")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "! 사랑을 느낄 수 있사와요？",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;상태변화では교만持ちの충성도が上昇
		CASE GETNUM(TALENT,"교만")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "로 충분하네요",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;特殊では냉정持ちの충성도が上昇
		CASE GETNUM(TALENT,"냉정")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "이른바 농간이라는 것이와요",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;우둔持ちは主人とのLv差だけ충성도が上昇
		CASE GETNUM(TALENT,"우둔")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "로 좋사와요？",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;허심持ちは최저２だけ충성도が上昇
		CASE GETNUM(TALENT,"허심")
			PRINTFORML ┏>MESSAGE━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "로 되었사와요…",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
	ENDSELECT
;ELSE
;	;システムによらず충성도を上げたいとか他があったらここに記載
ENDIF

