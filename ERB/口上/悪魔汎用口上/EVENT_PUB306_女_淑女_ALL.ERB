@KOJO_MESSAGE_ENDING_PUB306
;--------------------------------------------------
;特定主人時に特殊キャラ구상を呼び出す(Y1を特定キャラ、X1を奴隷のキャラ번호に置換）
;--------------------------------------------------
;IF NO:MASTER == Y1
;	CALL KOJO_MESSAGE_ENDING_PY1_PUB306
;	RETURN 0
;ENDIF
;--------------------------------------------------
SELECTCASE TSTR:END종별
	CASE "개별END"
		;연모1명エンド
		IF TALENT:아내 || TALENT:남편 && CFLAG:21 == 1
			DRAWLINE
			IF TALENT:비관적
				PRINTFORMW 「%조사처리(CSTR:일인칭,"를")% 파트너로……입니까？」
				PRINTFORMW 「그렇지만、벌써 계약을 하고 있잖아요」
				PRINTFORMW %조사처리(CALLNAME:MASTER,"는")% %CALLNAME%에게 파트너에 대해 설명해줬다
				PRINTFORMW 「에……%조사처리(CSTR:일인칭,"과")% 함께………입니까？」
				PRINTFORMW 「그、그렇지만 %조사처리(CSTR:일인칭,"는")% 악마예요　그런 %CSTR:일인칭%인데도 %조사처리(CSTR:이인칭,"와")% 파트너가 될 수 있다니……」
				PRINTFORMW %조사처리(CALLNAME:MASTER,"는")% 그래도 상관 없다고 말했다
				PRINTFORMW 「정말……정말로 좋습니까？………이런%조사처리(CSTR:일인칭,"라")%도………」
				PRINTFORMW 「아아……안 돼……기뻐서………눈물이………」
				PRINTFORMW %조사처리(CALLNAME,"는")% 잠시 눈물을 흘렸다
				PRINTFORMW ……………………
				PRINTFORMW 「미안해요、무심코 울어 버려서　너무 기뻐서」
				PRINTFORMW 「그렇지만、%조사처리(CSTR:일인칭,"는")% 악마니까、단단히 각오해 주세요」
				PRINTFORMW 「사랑합니다　%CSTR:이인칭%%UNICODE(0x2661)%」
			ELSE
				PRINTFORMW 「%조사처리(CSTR:일인칭,"를")% 파트너로 선택하시겠다고요」
				PRINTFORMW 「후후、놀라워할 줄 생각하셨나 봐요？　이제 새삼 그런 것으로 놀라진 않는답니다」
				PRINTFORMW 「그래요、%조사처리(CSTR:이인칭,"는")% 악마이든 인간이든 상관없이 조교를 해오지 않았습니까」
				PRINTFORMW 「그러니、인간인 %CSTR:일인칭%든、악마인 %조사처리(CSTR:일인칭,"를")% 사랑해 주셨겠지요……」
				PRINTFORMW 「%조사처리(CSTR:일인칭,"가")% 파트너가 된다는 것은、앞으로도 오래도록 %조사처리(CSTR:이인칭,"와")% 함께 있을 수 있다는 것」
				PRINTFORMW 「이제는 파트너로서 앞으로도、잘 부탁드려요　%CSTR:이인칭%%UNICODE(0x2661)%」
			ENDIF
			PRINTFORMW …………
			PRINTFORMW ………
			PRINTFORMW ……
			;요정の場合
			;IF (NO > 500 && NO <= 600)
				PRINTFORMW ――――어느 날 밤――――
				PRINTFORMW 「어서오세요　%CSTR:이인칭%%UNICODE(0x2661)%」
				PRINTFORMW 직장에서 집으로 돌아오자 에이프런 차림의 %조사처리(CALLNAME,"가")% 있었다
				PRINTFORMW 이것은 한 번쯤 해보고 싶었다는 %CALLNAME%의 요망 때문이다
				PRINTFORMW 「오늘은 %조사처리(CSTR:이인칭,"를")% 위해서 맛있는 음식을 준비했습니다%UNICODE(0x2661)%」
				SIF  ABL:요리기능 < 1
					PRINTFORMW %조사처리(CALLNAME,"는")% 요리를 제대로 만들 수 있었을까……
					PRINTFORMW 그러고 보니、자신을 위해 요리 공부를 하고 있었다는 것을 기억해냈다
				PRINTFORMW 「자아 드셔주세요%UNICODE(0x2661)%」
				PRINTFORMW %조사처리(CALLNAME:MASTER,"는")% %CALLNAME%의 요리를 입으로 가져갔다
				PRINTFORMW 맛은 \@ABL:요리기능 > 4 ? 아주 일품이다  # 아주 맛있다 \@
				PRINTFORMW ………？
				PRINTFORMW 잠시 먹고 있자니 몸에 묘한 위화감이 있었다
				PRINTFORMW 「후후、슬슬 효과가 나오고 있나 보네요」
				PRINTFORMW 「요리에 미약을 슬쩍 넣었거든요」
				PRINTFORMW 「신주쿠에서 산 특별 미약이에요」
				PRINTFORMW 「오늘은 절대 재우지 않을 거랍니다%UNICODE(0x2661)%　그럼 요리…」
				PRINTFORMW %조사처리(CALLNAME:MASTER,"는")% 더 이상 참을 수 없게 되어、그대로 덮치고는  %CALLNAME%의 에이프런을 풀어헤치기 시작했다 
				PRINTFORMW 「저기、요리는 아직 남았는데…………」
				PRINTFORMW ……………………%UNICODE(0x2661)%%UNICODE(0x2661)%%UNICODE(0x2661)%%UNICODE(0x2661)%
				PRINTFORMW 「……아앙%UNICODE(0x2661)%　……최고에요%UNICODE(0x2661)%　……사랑합니다、%CSTR:이인칭%%UNICODE(0x2661)%」
			;ELSEIF TALENT:천사
				;PRINTFORMW 
			;ELSE
				;PRINTFORMW 
			;ENDIF
			PRINTFORMW 　　　　　　　　―― Extra Ending　（여성・연모1명エンド）
		;음란1명エンド
		;ELSEIF TALENT:음마 && CFLAG:21 == 1
			;DRAWLINE
			;PRINTFORMW …………
			;PRINTFORMW ………
			;PRINTFORMW ……
			;PRINTFORMW 

			;PRINTFORMW 
			;PRINTFORMW 　　　　　　　　―― Extra Ending　（◯◯エンド）
		;복종1명エンド
		;ELSEIF TALENT:노리개 && CFLAG:21 == 1
			;DRAWLINE
			;PRINTFORMW …………
			;PRINTFORMW ………
			;PRINTFORMW ……
			;PRINTFORMW 

			;PRINTFORMW 
			;PRINTFORMW 　　　　　　　　―― Extra Ending　（◯◯エンド）
		ENDIF
	;CASE "조교師END"
	;CASE "희대의조교사END"
	;CASE "식당경영END"
	;CASE "창관경영END"
	;CASE "아이돌END"
ENDSELECT
;個別ENDの場合ここでRETURN 1すると、その後ゲームは続かず次周に行くかゲーム종료かの選択になります
RETURN 0


