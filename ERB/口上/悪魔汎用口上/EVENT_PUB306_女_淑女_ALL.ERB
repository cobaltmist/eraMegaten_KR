@BATTLE_MESSAGE_PUB306,ARG,ARG:1 = 0
IF CFLAG:ARG:입력행동 == 2022 && NO:ARG == 3
;초메기도사용時
	;CALLFORM SKILL_NAME_{CFLAG:ARG:입력행동},ARG
	PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
	PRINTFORML ┃%"당신이 이룬 것、재로 만들어 드리겠어요！",72,LEFT%┃
	PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛

ELSEIF ARG:1 > 0
	;충성도が上昇する行動をとった場合に표시する구상

	CALLFORM SKILL_NAME_{CFLAG:ARG:입력행동},ARG
	;사용した스킬の이름をLOCALSに代入
	LOCALS = %RESULTS%

	SELECTCASE ARG:1
		;물리데미지を与える行動で、영맹持ちの충성도が上昇
		CASE GETNUM(TALENT,"영맹")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%"돌격합니다！" + LOCALS + "！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;마법데미지を与える行動で、교활持ちの충성도が上昇
		CASE GETNUM(TALENT,"교활")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%"우후후후♪" + LOCALS + "！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;高위력の行動で、잔학持ちの충성도が上昇
		CASE GETNUM(TALENT,"잔학")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%CSTR:일인칭 +"、 잔혹한가요？",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;相手の약점を付いた공격or行動不能な상태이상の적に対する공격で、현명持ちの충성도が上昇
		CASE GETNUM(TALENT,"현명")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "계산 중이에요！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;파마相性의 行動で、천사持ちの충성도が上昇
		CASE GETNUM(TALENT,"천사")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%"천주를 먹어보아라！" + LOCALS + "！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;주살相性의 行動で、타천사持ちの충성도が上昇
		CASE GETNUM(TALENT,"타천사")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "！자、괴로워해라！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;회복・상태회복行動では우애持ちの충성도が上昇
		CASE GETNUM(TALENT,"우애")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "사랑을 느낍니다！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;상태변화では교만持ちの충성도が上昇
		CASE GETNUM(TALENT,"교만")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%"당신들에게는 이 정도로 충분합니다 。" + LOCALS + "！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
		;特殊では냉정持ちの충성도が上昇
		CASE GETNUM(TALENT,"냉정")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%"맡겨 주세요。" + LOCALS + "！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;우둔持ちは主人とのLv差だけ충성도が上昇
		CASE GETNUM(TALENT,"우둔")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%LOCALS + "라고요？ " + CSTR:이인칭 + "의 말이라면 틀림 없겠네요",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
;		;허심持ちは최저２だけ충성도が上昇
		CASE GETNUM(TALENT,"허심")
			PRINTFORML ┏┓＠%CALLNAME:ARG,20,LEFT%　┏━━━━━━━━━━━━━━━━━━━━━━┓
			PRINTFORML ┃%"네。" + LOCALS +  "！",72,LEFT%┃
			PRINTFORMW ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
	ENDSELECT
;ELSE
;	;システムによらず충성도を上げたいとか他があったらここに記載
ENDIF


