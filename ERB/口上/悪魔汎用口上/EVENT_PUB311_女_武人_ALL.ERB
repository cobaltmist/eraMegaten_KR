;--------------------------------------------------
;구상셀렉트기능：구상설명
;--------------------------------------------------
;この関数のコメントアウトを外すと、구상の選択時に選択肢として候補にあがります
;既に구상のあるキャラや회화타입の別版の구상を作成する時に이용してください
;その際、この関数を含めたこのファイル内の関数名すべてに_任意の1-49(汎用구상なら1-9)の数字を加えてください
;例： EVENTTRAIN_PUB311 → EVENTTRAIN_PUB311_12
;--------------------------------------------------
@셀렉트구상설명_PUB311,ARG = 0
SIF ARG == 1
	RETURN 0
;;구상名
PRINTFORML 범용녀・무인구상
;;구상설명文
PRINTFORML 

;--------------------------------------------------
;EVENTTRAIN関係（X1をキャラ번호に置換）
;조교開始時のセリフ CFLAG 201～219を사용
;--------------------------------------------------
@EVENTTRAIN_PUB311
;--------------------------------------------------
;엑스트라모드構文　CFLAG:220を사용
;はいを選択した時に初조교時に구상설정用の소질に변경できます
;下記に처녀化する場合の構文を샘플として載せておきます
;--------------------------------------------------
;IF CFLAG:220 == 0
;	PRINTL 처녀モードに移行しますか？
;	PRINTL [0] はい
;	PRINTL [1] いいえ
;	$INPUT_LOOP
;	INPUT
;	IF RESULT == 0
;		PRINTFORMW ―처녀モードに소질、능력を변경します―
;		TALENT:TARGET:0 = 1
;		ABL:TARGET:4 = 0
;		EXP:TARGET:0 = 0
;	ELSEIF RESULT == 1
;		PRINTFORMW ―통상モードで조교を開始します―
;	ELSE
;		GOTO INPUT_LOOP
;	ENDIF
;	CFLAG:220 = 1
;ENDIF
;--------------------------------------------------
;特定キャラが조수、主人時に特殊キャラ구상を呼び出す(Y1を特定キャラ、X1を奴隷のキャラ번호に置換）
;--------------------------------------------------
;IF NO:MASTER == Y1
;	CALL EVENTTRAIN_PY1_PUB311
;	RETURN 0
;ENDIF
;IF ASSI > 0 && ASSIPLAY
;	IF NO:ASSI == Y1
;		CALL EVENTTRAIN_PY1_PUB311
;		RETURN 0
;	ENDIF
;ENDIF
;--------------------------------------------------
;簡易조수구상の発動時に初조교時구상をスキップ（Y1を조수のキャラ번호にする）
;複数人いる際には、SIF NO:ASSI == Y1 || NO:ASSI == Z1と추가していく
;--------------------------------------------------
;IF CFLAG:201 == 0 && ASSI > 0
;	SIF NO:ASSI == Y1
;		CFLAG:201 =1
;ENDIF
;-------------------------------------------------
;조교開始時（一回のみ） CFLAG201
;-------------------------------------------------
DRAWLINE
;初조교時
IF CFLAG:201 == 0 && TALENT:연모 == 0
	PRINTFORMW 「%조사처리(CSTR:일인칭,"를")% 조교하겠다라」
	PRINTFORMW 「…재미있군、해봐도 좋아｣
	CFLAG:201 = 1
	RETURN 0
;굴복각인Lv1
ELSEIF CFLAG:201 < 2 && MARK:2 == 1
	PRINTFORMW 「이 정도로 %조사처리(CSTR:일인칭,"를")% 굴복 시킬 수 있을 것 같아？」
	PRINTFORMW 「아직 %조사처리(CSTR:일인칭,"는")% 아무렇지도 않아｣
	CFLAG:201 = 2
	RETURN 0
;굴복각인Lv2
ELSEIF CFLAG:201 < 3 && MARK:2 == 2
	PRINTFORMW 「이제 슬슬 포기하는게 어때｣
	PRINTFORMW 「더 이상은 시간 낭비라고｣
	CFLAG:201 = 3
	RETURN 0
;굴복각인Lv3
ELSEIF CFLAG:201 < 4 && MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0)
	PRINTFORMW 「또、온건가？｣
	PRINTFORMW 「별로、기다리고 있던 것은 아니야｣
	CFLAG:201 = 4
	RETURN 0
;함락
ELSEIF CFLAG:201 < 5 && (TALENT:3 || TALENT:4 || TALENT:5)
	;연모
	IF TALENT:3
		IF CFLAG:미조교함락 == 1
			;조교せずに연모をつけた場合
			PRINTFORMW 「%조사처리(CSTR:일인칭,"를")%…조교？」
			PRINTFORMW 「저기… 상냥하게 해 줄 수 있을까」
		ELSE
			PRINTFORMW 「또、와줬네｣
			PRINTFORMW 「당신이 원한다면 앞르도도 소중히 대해줬으면 좋겠어」
		ENDIF
	;음란
	ELSEIF TALENT:4
		PRINTFORMW 「기다렸다고｣
		PRINTFORMW 「자아、시작하자｣
	;복종
	ELSEIF TALENT:5
		PRINTFORMW 
	ENDIF
	CFLAG:201 = 5
	RETURN 0
ENDIF

;-------------------------------------------------
;簡易조수구상（Y1を조수のキャラ번호に置換、X1をキャラ번호に변경する) CFLAG203
;掛け合いをするキャラを増やす場合は、構文をコピー＆ペーストをして、CFLAGの数を204、205と増やす
;-------------------------------------------------
;조수の有無を체크（いない場合は二回目以降へ飛ぶ）
IF ASSI < 0 
	CALL EVENTTRAIN_SECOND_PUB311
;조수
;ELSEIF NO:ASSI == Y1
;구상のある조수が居ない場合は、통상の二回目以降の구상へ飛ぶ
ELSE
	IF CFLAG:ASSI:딸의친부모고유번호딸 == CFLAG:TARGET:캐릭터고유번호 || CFLAG:ASSI:딸의부친고유번호딸 == CFLAG:TARGET:캐릭터고유번호
		IF CFLAG:203 & 1
			IF FAVORITE_1(TARGET) == ASSI
				PRINTFORMW 「%CALLNAME:ASSI%…」
				IF TALENT:ＮＴＲ == 1 || TALENT:ＮＴＲ == 4
					PRINTFORMW 「기다리고 있었어」
					PRINTFORMW 「자아、시작해 볼까」
				ELSEIF TALENT:ＮＴＲ == 2 || TALENT:ＮＴＲ == 5 || ((TALENT:4 || TALENT:7) && TALENT:ＮＴＲ == 0)
					PRINTFORMW 「더 이상 참을 수가 없는데 말이야」
					PRINTFORMW 「몸이 쑤셔 죽겠어」
				ELSEIF TALENT:ＮＴＲ == 3 || TALENT:ＮＴＲ == 6
					PRINTFORMW 
				ELSE
					PRINTFORMW 「아직 %CSTR:일인칭%에게 달콤함이 부족한가」
					PRINTFORMW 「어쩔 수 없는 아이구나」
				ENDIF
			ELSE
				PRINTFORMW 「또、%CALLNAME:ASSI%의 상대…」
				PRINTFORMW 「이 아이에게 무엇을 시키고 싶은건데 %조사처리(CSTR:이인칭,"는")%」
			ENDIF
		ELSE
		;親子初めて
			PRINTFORMW 「어째서 %조사처리(CALLNAME:ASSI,"가")% 여기에 있느냐」
			PRINTFORMW 「설마、이 아이마저 말려 들게 하는건가」
			CFLAG:203 |= 1
		ENDIF
	ELSEIF CFLAG:ASSI:딸의양부모고유번호딸 == CFLAG:TARGET:캐릭터고유번호
		IF CFLAG:203& 2
			IF FAVORITE_1(TARGET) == ASSI
				IF TALENT:ＮＴＲ == 1 || TALENT:ＮＴＲ == 4
					PRINTFORMW 「%CALLNAME:ASSI%、더 가까이 와줘」
					PRINTFORMW 「지금의 %조사처리(CSTR:일인칭,"는")% %CALLNAME:ASSI%전용이야」
				ELSEIF TALENT:ＮＴＲ == 2 || TALENT:ＮＴＲ == 5 || ((TALENT:4 || TALENT:7) && TALENT:ＮＴＲ == 0)
					PRINTFORMW 「%CALLNAME:ASSI%의 손으로%조사처리(CSTR:일인칭,"를")% 엉망진창으로 해줘」
					PRINTFORMW 「%조사처리(CSTR:일인칭,"는")% 이렇게 음란한 여자야」
				ELSEIF TALENT:ＮＴＲ == 3 || TALENT:ＮＴＲ == 6
					PRINTFORMW 
				ELSE
					PRINTFORMW 「이번 상대는 %CALLNAME:ASSI%인가」
					PRINTFORMW 「봐요、그렇게 긴장하지 말아요」
				ENDIF
			ELSE
				PRINTFORMW 「%CALLNAME:ASSI%인가」
				PRINTFORMW 「괜찮으니까 %CSTR:일인칭%에게 맡겨줘」
			ENDIF
		ELSE
			;義理親子初めて
			PRINTFORMW 「그 아이는 %CALLNAME:ASSI%」
			PRINTFORMW 「%CSTR:일인칭%에게 성교육을 시키고 싶은건가」
			CFLAG:203 |= 2
		ENDIF
	ELSE
		CALL EVENTTRAIN_SECOND_PUB311
	ENDIF
ENDIF

;-------------------------------------------------
;조교開始時（2回目以降、X1をキャラ번호に변경する）
;-------------------------------------------------
@EVENTTRAIN_SECOND_PUB311
;반발각인Lv3
IF MARK:3 == 3 && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 「무엇을 하려는거지」
	CFLAG:202 = 4
;굴복각인Lv0
ELSEIF MARK:2 == 0 && (CFLAG:202 < 1 || FLAG:7 == 2)
	PRINTFORMW 「또 온건가｣
	PRINTFORMW 「%CSTR:이인칭%도 %CSTR:일인칭%의 상대를 하고 있을 겨를이 없을텐데｣
	CFLAG:202 = 1
;굴복각인Lv1
ELSEIF MARK:2 == 1 && (CFLAG:202 < 2 || FLAG:7 == 2)
	PRINTFORMW 「아직 오는 건가｣
	PRINTFORMW 「이제 슬슬 포기하면 좋을 것을｣
	CFLAG:202 = 2
;굴복각인Lv2
ELSEIF MARK:2 == 2 && (CFLAG:202 < 3 || FLAG:7 == 2)
	PRINTFORMW 「언제까지 이러고 있을거냐」
	PRINTFORMW 「%CSTR:이인칭%에게 이런 시간은 무의미하다」
	CFLAG:202 = 3
;굴복각인Lv3＋함락소질無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0) && (CFLAG:202 < 4 || FLAG:7 == 2)
	PRINTFORMW 「와주었군」
	PRINTFORMW 「그렇게나 내 몸이 좋은건가？」
	CFLAG:202 = 4
;함락済み
ELSEIF CFLAG:202 < 5 || FLAG:7 == 2
	;연모、친애
	IF TALENT:3 || TALENT:6
		;랜덤で구상が변화する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 「후후っ♪、와 주었는지」
			PRINTFORMW 「잠시 %조사처리(CSTR:일인칭,"가")% %조사처리(CSTR:이인칭,"를")% 독점할 수 있겠구나」
		ELSEIF K:1 == 1
			PRINTFORMW 「후후っ♪、와 주었는지」
			PRINTFORMW 「잠시 %조사처리(CSTR:일인칭,"가")% %조사처리(CSTR:이인칭,"를")% 독점할 수 있겠구나」
		ELSE
			PRINTFORMW 「후후っ♪、와 주었는지」
			PRINTFORMW 「잠시 %조사처리(CSTR:일인칭,"가")% %조사처리(CSTR:이인칭,"를")% 독점할 수 있겠구나」
		ENDIF
	;음란、창부
	ELSEIF TALENT:4 || TALENT:7
		;랜덤で구상が변화する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 「어서 와줘」
			PRINTFORMW 「이제 이쪽의 준비는 다 되었다고」
		ELSEIF K:1 == 1
			PRINTFORMW 「어서 와줘」
			PRINTFORMW 「이제 이쪽의 준비는 다 되었다고」
		ELSE
			PRINTFORMW 「어서 와줘」
			PRINTFORMW 「이제 이쪽의 준비는 다 되었다고」
		ENDIF
	;복종、예속
	ELSEIF TALENT:5 || TALENT:8
		;랜덤で구상が변화する（使わない場合はすべて同じにすればよい）
		K:1 = RAND:3
		IF K:1 == 0
			PRINTFORMW 
		ELSEIF K:1 == 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	CFLAG:202 = 5
ENDIF
RETURN 0

