@ARRANGE_CHARALIFE, ARG
#DIM 合体後충성도 , 1
#DIM 合体後레벨 , 1
LOCAL = FLAG:SHOP커맨드

IF EQUALCHECK(LOCAL,102,103,104,106,122)
	LOCAL = 1
ELSEIF EQUALCHECK(LOCAL,101,120) && (FLAG:合体予定악마1 <= -1 || (FLAG:合体予定악마2 <= -1 && FLAG:合体人数 == 3))
	LOCAL = 1
ELSEIF EQUALCHECK(LOCAL,101,120) 
	IF FLAG:合体人数 == 3
		CALL RESULT_FUSION,FLAG:合体予定악마1,FLAG:合体予定악마2,ARG
		合体後충성도 = (BASE:(FLAG:合体予定악마1):충성도 + BASE:(FLAG:合体予定악마2):충성도 + BASE:ARG:충성도)/3
	ELSE
		CALL RESULT_FUSION,FLAG:合体予定악마1,ARG
		合体後충성도 = (BASE:(FLAG:合体予定악마1):충성도 + BASE:ARG:충성도)/2
	ENDIF
	LOCAL = RESULT > 0 ? 2 # 3
ELSE
	LOCAL = 4
ENDIF

SELECTCASE LOCAL
	CASE 1
		IF FLAG:커스텀게임화면
			PRINTFORM LV:{BASE:ARG:LV,3} 
			PRINTFORM ({CSVBASE(NO:ARG,GETNUM(BASE,"LV"),0),3})  
			PRINTFORM HP[{BASE:ARG:ＨＰ, 5}/{MAXBASE:ARG:ＨＰ, 5}] 
			PRINTFORM MP[{BASE:ARG:ＭＰ, 5}/{MAXBASE:ARG:ＭＰ, 5}]
		ELSE
			PRINTFORM LV:{BASE:ARG:LV,3} 
			PRINTFORM ({CSVBASE(NO:ARG,GETNUM(BASE,"LV"),0),3})  
			PRINTFORM ＨＰ({BASE:ARG:ＨＰ, 4}/{MAXBASE:ARG:ＨＰ, 4}) 
			PRINTFORM ＭＰ({BASE:ARG:ＭＰ, 4}/{MAXBASE:ARG:ＭＰ, 4})
		ENDIF
	CASE 2
		PRINTFORM LV:{BASE:ARG:LV,3} 
		合体後레벨 = CSVBASE(RESULT,GETNUM(BASE,"LV"),0)
		;特殊合体の色変えが来た場合は水色、레벨オーバーで黒色、両方は종족명が水色であとは青色となる
		SIF 合体後레벨 > BASE:MASTER:LV + FLAG:경험ED수 && 合体後충성도 < POWER(合体後레벨 -(BASE:MASTER:LV + FLAG:경험ED수) , 2)*500 && EQUIP:MASTER:스티븐 == 0
			SETCOLOR COLOR("gray")
		SIF RESULT:1
			SETCOLOR COLOR("aqua")
;EDIT RASIEL-ADD-ON START ------------------------------------------------
		;ラジエルアドオン処理が適用される대상の場合、水色
		SIF EXTRACHECK_DEVIL_CHILDREN(RESULT) && EQUIP:MASTER:라지엘의서
			SETCOLOR COLOR("aqua")
;EDIT RASIEL-ADD-ON END   ------------------------------------------------
;정령合体랭크変動表記用改変ここから
		LOCAL:1 = GETCOLOR()
		IF RESULT:2 == 1
			SETCOLOR 0X66FFFF
			PRINT  ↑
			SETCOLOR LOCAL:1
		ELSEIF RESULT:2 == -1
			SETCOLOR 0X990000
			PRINT  ↓
			SETCOLOR LOCAL:1
		ELSE
			PRINT  ：
		ENDIF
		RESULT:2 = 0
;정령合体랭크変動表記用改変ここまで
		PRINTFORM %CSV종족명(RESULT,0),6,LEFT% 
		SIF 合体後레벨 > BASE:MASTER:LV + FLAG:경험ED수 && 合体後충성도 < POWER(合体後레벨 -(BASE:MASTER:LV + FLAG:경험ED수) , 2)*500 && EQUIP:MASTER:스티븐 == 0
			SETCOLOR COLOR("gray")
		PRINTFORM LV{CSVBASE(RESULT,GETNUM(BASE,"LV"),0),2} 
		LOCAL:1 = GETCOLOR()
		SELECTCASE CSVABL(RESULT, GETNUM(ABL, "속성LD"), 0)
			CASE 1
				SETCOLOR 0x66FFFF
				PRINT L
			CASE 2
				SETCOLOR 0xC0C0C0
				PRINT N
			CASE 3
				SETCOLOR 0x990000
				PRINT D
		ENDSELECT
		PRINTD /
		SELECTCASE CSVABL(RESULT, GETNUM(ABL, "속성LC"), 0)
			CASE 1
				SETCOLOR 0x66FFFF
				PRINT L
			CASE 2
				SETCOLOR 0xC0C0C0
				PRINT N
			CASE 3
				SETCOLOR 0x990000
				PRINT C
		ENDSELECT
		PRINT 　
		SETCOLOR LOCAL:1
		;PRINTFORM %CSVNAME(RESULT,0),18,LEFT%
		;インス토르ソフト：コラプトダイアリーを사용している場合
		;全함락制覇している場合はハートが付く
		IF EQUIP:MASTER:크래프트다이어리 && FLAG:(50000+RESULT) == 255
			PRINTFORM %CSVNAME(RESULT,0),17,LEFT%
			SETCOLOR COLOR("PINK")
			PRINTFORM %ハート()%
			RESETCOLOR
		ELSE
			PRINTFORM %CSVNAME(RESULT,0),18,LEFT%
		ENDIF
		
		;解析率
		IF FLAG:(20000+RESULT) <= 0
			SETCOLOR 0xFFFFFF
		ENDIF
		IF FLAG:(20000+RESULT) > 999
			SETCOLOR 0x404040
			PRINT 100％ 
		ELSE
			PRINTFORM {FLAG:(20000+RESULT)/10,3}％ 
		ENDIF
		RESETCOLOR
		
		;インス토르ソフト：コラプトダイアリーを사용している場合
		;誕生予定の악마の함락履歴を표시する
		IF EQUIP:MASTER:크래프트다이어리
				LOCAL:1 = 0
				LOCAL:2 = RESULT
			FOR LOCAL:1, 1, 9
				SETCOLOR GETBIT(FLAG:(50000+LOCAL:2), LOCAL:1) ? COLOR("aqua") # COLOR("light-gray")
				PRINTFORM %AUTO_SPLIT("/恋/親/淫/娼/옷/隷/ 처녀", "/", LOCAL:1)%
				RESETCOLOR
			NEXT
		ENDIF


	CASE 3
		PRINT 　　　 
		PRINTFORM  %"：×" , 43,LEFT%
	CASE 4
		LOCAL:1 = MAX(3,STRLENS(TOSTR(MAXCARRAY(BASE:ARG:LV))))
		PRINTFORM LV:{BASE:ARG:LV,LOCAL:1} 
		LOCAL:1 = MAX(4,STRLENS(TOSTR(MAXCARRAY(BASE:ARG:0))))
		LOCAL:2 = MAX(4,STRLENS(TOSTR(MAXCARRAY(BASE:ARG:1))))
		IF FLAG:커스텀게임화면
			PRINTFORM 체력({BASE:ARG:0, LOCAL:1}/{MAXBASE:ARG:0, LOCAL:1}) 
			PRINTFORM 기력({BASE:ARG:1, LOCAL:2}/{MAXBASE:ARG:1, LOCAL:2}) 
		ELSE
			PRINTFORM 체력({BASE:ARG:0, LOCAL:1}/{MAXBASE:ARG:0, LOCAL:1}) 
			PRINTFORM 기력({BASE:ARG:1, LOCAL:2}/{MAXBASE:ARG:1, LOCAL:2}) 
		ENDIF
		PRINTFORM 스트레스:{CFLAG:ARG:스트레스치, 3}
ENDSELECT
