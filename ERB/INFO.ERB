					ELSE
						PRINTFORM %" " * 6%
					ENDIF
					;解析度表示追加　ここまで
				CASE [[SHOP:셋업]]
					IF ABL:(LOCAL):종족 == 0
						PRINTFORM [　－　]
					ELSE
						PRINTFORM [\@ CFLAG:(LOCAL):所属ＣＯＭＰ == -1 ? 자택서버 # ＣＯＭＰ \@]
					ENDIF
				CASE 1201
					SIF 文字色변경 != 0
						PRINT 　전서와 차이 있음
				CASE [[SHOP:요바이필터]]
					PRINTFORM 　요바이허가：\@ CFLAG:LOCAL:요바이불허가 ? [×] # [○] \@
				CASE 665
					PRINTFORM 　일시적함락소질변경：\@ CFLAG:LOCAL:일시적함락소질변경 ? [○] # [×] \@
				CASEELSE
					SELECTCASE CFLAG:LOCAL:노역플래그
						CASE 1	;식당で働く
							SETCOLOR COLOR("PASTEL-BLUE")
						CASE 2	;体を売る
							SETCOLOR COLOR("PASTEL-RED")
						CASE 3	;奴隷の렌탈
							SETCOLOR COLOR("PASTEL-PURPLE")
						CASE 4	;音楽活動
							SETCOLOR COLOR("PASTEL-GREEN")
						CASE 5	;乱交라이브
							SETCOLOR COLOR("PASTEL-YELLOW")
						CASE 6	;道場
							SETCOLOR COLOR("WHITE")
						CASE 7	;？？？？
							SETCOLOR COLOR("GRAY")
						CASE 8	;？？？？
							SETCOLOR COLOR("GRAY")
						CASE 9	;？？？？
							SETCOLOR COLOR("GRAY")
						CASE 10 TO 19	;경영점포
							SELECTCASE [[점포:종류]]:(CFLAG:LOCAL:노역플래그-10)
								CASE 10	;공중변소
									SETCOLOR COLOR("PASTEL-BLUEGREEN")
								CASE 11	;ストリップ劇場
									SETCOLOR COLOR("PINK")
								CASE 12	;見せ物小屋
									SETCOLOR COLOR("DARK-PINK")
								CASE 13	;メシア教会
									SETCOLOR COLOR("AQUA")
								CASE 14	;ガイア教会
									SETCOLOR COLOR("RED")
								CASE 15	;教会
									SETCOLOR COLOR("LIGHT-GRAY")
								CASEELSE
									SETCOLOR COLOR("WHITE")
							ENDSELECT
						CASEELSE
							SETCOLOR COLOR("WHITE")
					ENDSELECT
					PRINTFORM %GET_JOB_OMIT(CFLAG:LOCAL:노역플래그 > 9 ? [[점포:종류]]:(CFLAG:LOCAL:노역플래그-10) # CFLAG:LOCAL:노역플래그)%
					RESETCOLOR
					;[TR]추가部分(アラインメント표시)
					PRINTS " "
					CALL PRINT_ALIGNMENT(LOCAL,1,0)
					CALL 구상呼び出し , "存在" , LOCAL
					IF RESULT == 1
						PRINT 　전
					ELSEIF RESULT == 2
						PRINT 　범
					ELSE
						PRINT 　―
					ENDIF
			ENDSELECT
			;SIF FLAG:SHOP커맨드 == 122
			;	PRINTFORM 　MAG:{BASE:LOCAL:ＭＡＧ, 8}/{MAXBASE:LOCAL:ＭＡＧ, 8}
			IF FLAG:SHOP커맨드 == 122 || FLAG:SHOP커맨드 == 172
				IF BASE:LOCAL:ＭＡＧ == MAXBASE:LOCAL:ＭＡＧ
					SETCOLOR COLOR("pink")
				ELSEIF BASE:LOCAL:ＭＡＧ >= MAXBASE:LOCAL:ＭＡＧ / 4 * 3
					SETCOLOR 0xff66ff
				ELSEIF BASE:LOCAL:ＭＡＧ >= MAXBASE:LOCAL:ＭＡＧ / 2
					SETCOLOR 0xff99ff
				ELSEIF BASE:LOCAL:ＭＡＧ >= MAXBASE:LOCAL:ＭＡＧ / 4
					SETCOLOR 0xffccff
				ENDIF
				IF FLAG:커스텀게임화면
					PRINTFORM 　MAG:{BASE:LOCAL:ＭＡＧ,9}/{MAXBASE:LOCAL:ＭＡＧ,9}
				ELSE
					PRINTFORM 　MAG:{BASE:LOCAL:ＭＡＧ,6}/{MAXBASE:LOCAL:ＭＡＧ,6}
				ENDIF
				RESETCOLOR
			ENDIF
			CALL ARRANGE_위험일, LOCAL

			PRINTL 
			RESETCOLOR
	ENDSELECT
