@SHOP_COM_188
#LOCALSIZE 5
CLEARLINE LINECOUNT
FOR LOCAL , 0 , 100
	TRYCCALLFORM GET_DUNGEON_NAME_{LOCAL}
		IF RESULTS == "타르타로스"
			;PRINTFORML #DEFINE 던전%TOSTR(LOCAL , "00")% %RESULTS%
			;PRINTFORML #DIM 던전%TOSTR(LOCAL , "00")%MAP数 22
			PRINTFORML #DIM SAVEDATA 던전 %TOSTR(LOCAL , "00")% , 22 , 30 , 30
			CONTINUE
		ENDIF
		;PRINTFORML #DEFINE 던전%TOSTR(LOCAL , "00")% %RESULTS%
		LOCAL:2 = 0 , 0 , 0
		FOR FLAG:현M , 0 , 100
			FLAG:최대X = 0
			FLAG:최대Y = 0
			CALLFORM MAKE_FLOOR_{LOCAL}
			LOCAL:2 = MAX(FLAG:최대X , LOCAL:2)
			LOCAL:3 = MAX(FLAG:최대Y , LOCAL:3)
			SIF (FLAG:최대X + FLAG:최대Y) > 2
				LOCAL:4 = FLAG:현M
		NEXT
		PRINTFORML #DIM SAVEDATA 던전 %TOSTR(LOCAL , "00")% , {LOCAL:4+1} , {LOCAL:2} , {LOCAL:3}
	CATCH
		PRINTFORML #DIM SAVEDATA 던전 %TOSTR(LOCAL , "00")% , 1 , 1 , 1
	ENDCATCH
;	PRINTFORML CASE "던전%TOSTR(LOCAL , "00")%"
;	PRINTFORML 던전%TOSTR(LOCAL , "00")%:(ARG:3):(ARG:1):(ARG:2) = LOCAL
NEXT
