@SET_ACTION_41996666,ARG
#LOCALSIZE 1
#LOCALSSIZE 1
LOCAL=ARG
IF (CFLAG:LOCAL:601 % 31 ) == 30
	;およそ３０回行動で리캄도라
	CFLAG:LOCAL:입력행동 = [[스킬:리캄도라]]
ELSE
	CFLAG:LOCAL:601 += 1
	;特に何もなければ랜덤행동。
	CFLAG:LOCAL:입력행동 = 0
	CALL 랜덤행동,LOCAL
ENDIF
SIF !(CFLAG:LOCAL:입력행동)
	RETURN 0
CALL CHECK_ACTIONABLE,LOCAL,CFLAG:LOCAL:입력행동
IF RESULT == 1
	CALL RANDOM_TARGET,LOCAL,CFLAG:LOCAL:입력행동
ELSE
	;なんかエラーあれば통상공격に
	CFLAG:ARG:입력행동 = 0
	CALL RANDOM_TARGET,ARG,CFLAG:LOCAL:입력행동
ENDIF
RETURN 0

