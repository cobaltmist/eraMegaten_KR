		MAXBASE:ARG:속도 = 500
		CFLAG:ARG:604 = BASE:ARG:운
		BASE:ARG:운 = 500
		MAXBASE:ARG:운 = 500
		CALL SYNC_STATUS,ARG
	ENDIF
	CFLAG:ARG:크리티컬강화 = 8
	CFLAG:ARG:기합플래그 = 1
	CFLAG:ARG:입력행동 = [[스킬:지모의만찬]]
ELSEIF LOCAL == 99
	;寝てる奴いれば영면의유혹
	IF CFLAG:ARG:603 > 0 && CFLAG:ARG:604 > 0
		BASE:ARG:힘 -= 500
		MAXBASE:ARG:힘 -= 500
		CFLAG:ARG:ＨＰ보정 /= 100
		BASE:ARG:ＨＰ /= 100
		BASE:ARG:속도 = CFLAG:ARG:603
		MAXBASE:ARG:속도 = CFLAG:ARG:603
		CFLAG:ARG:603 = 0
		BASE:ARG:운 = CFLAG:ARG:604
		MAXBASE:ARG:운 = CFLAG:ARG:604
		CFLAG:ARG:604 = 0
		CALL SYNC_STATUS,ARG
	ENDIF
	CFLAG:ARG:입력행동 = [[스킬:영면의유혹]]
