;=======================================================================
;持ってないマガタマサーチ( ARG = (0,初級)　(1,中級)　(2,上級) )
;持ってないのがあるなら１を返す、全部取ってるなら０
;=======================================================================
@FIND_MAGATAMA_MOTTENAI,ARG
#FUNCTION
#LOCALSIZE 2
IF ARG == 0
	FOR LOCAL,8200,8208
		SIF ITEM:LOCAL < 1
			RETURNF 1
	NEXT
ELSEIF ARG == 1
	FOR LOCAL,8208,8215
		SIF ITEM:LOCAL < 1
			RETURNF 1
	NEXT
ELSEIF ARG == 2
	;上級、マサカド様は除く
	FOR LOCAL,8215,8224
		SIF ITEM:LOCAL < 1
			RETURNF 1
	NEXT
ENDIF
RETURNF 0
;=======================================================================
;マガタマチーム決定( ARG = (0,初級)　(1,中級)　(2,上級) )
;=======================================================================
@COLOSSEUM_MAGATAMA_BATTLE,ARG
IF ARG == 0
	LOCAL=RAND:8+8200
	$SYOKYU_CLSM90
	FOR LOCAL,LOCAL,8208
		IF ITEM:(LOCAL) < 1
			CALL COLOSSEUM_MAGATAMA_TEAMDA(LOCAL)
			;PRINTFORML ￥{RESULT}、{RESULT:1}＠
			RETURN RESULT,RESULT:1
		ENDIF
	NEXT
	LOCAL-=8
	GOTO SYOKYU_CLSM90
ELSEIF ARG == 1
	LOCAL=RAND:7+8208
	$CHUKYU_CLSM90
	FOR LOCAL,LOCAL,8215
		IF ITEM:(LOCAL) < 1
			CALL COLOSSEUM_MAGATAMA_TEAMDA(LOCAL)
			RETURN RESULT,RESULT:1
		ENDIF
	NEXT
	LOCAL-=7
	GOTO CHUKYU_CLSM90
ELSEIF ARG == 2
	LOCAL=RAND:9+8215
	$JOKYU_CLSM90
	FOR LOCAL,LOCAL,8224
		IF ITEM:(LOCAL) < 1
			CALL COLOSSEUM_MAGATAMA_TEAMDA(LOCAL)
			RETURN RESULT,RESULT:1
		ENDIF
	NEXT
	LOCAL-=9
	GOTO JOKYU_CLSM90
ELSEIF ARG == 3
	IF ITEM:8224 < 1
		CALL COLOSSEUM_MAGATAMA_TEAMDA(8224)
		RETURN 100,8224
	ELSE
		CALL COLOSSEUM_MAGATAMA_TEAMDA(8225)
		RETURN 100,9999
	ENDIF
ENDIF
RETURN 0,0
