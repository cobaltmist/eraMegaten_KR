#LOCALSSIZE 1
LOCAL=0
LOCAL:2=0
FOR LOCAL:1,1,7
	LOCALS = 포지션{LOCAL:1}
	SIF FLAG:LOCALS == -1
		CONTINUE
	SIF CFLAG:(FLAG:LOCALS):ステート == GET_STATE_NUM("DYING")
		CONTINUE
	FOR LOCAL:2,1,7
		IF CFLAG:(FLAG:LOCALS):물리반사플래그 || MAXBASE:(FLAG:LOCALS):검격 == 999 || MAXBASE:(FLAG:LOCALS):검격 <= 0 || MAXBASE:(FLAG:LOCALS):타격 == 999 || MAXBASE:(FLAG:LOCALS):타격 <= 0
			IF CFLAG:(FLAG:LOCALS):타격가드킬 > 0
				LOCAL = 1
			ELSE
				LOCAL = 101
			ENDIF
			BREAK
		ELSEIF CFLAG:(FLAG:LOCALS):ステート == GET_STATE_NUM("SLEEP")
			LOCAL = 99
			SIF BASE:(FLAG:LOCALS):DYING > 10
				LOCAL = 1
			BREAK
		ENDIF
	NEXT
NEXT
IF (CFLAG:ARG:602 == 0) && (((BASE:ARG:ＨＰ)*100)/(MAXBASE:ARG:ＨＰ) < 51)
	;HP가 반감하면 한번만 메・디아라한
	IF CFLAG:ARG:603 > 0 && CFLAG:ARG:604 > 0
		BASE:ARG:힘 -= 500
		MAXBASE:ARG:힘 -= 500
		CFLAG:ARG:ＨＰ보정 *= 100
		BASE:ARG:ＨＰ *= 100
		BASE:ARG:속도 = CFLAG:ARG:603
		MAXBASE:ARG:속도 = CFLAG:ARG:603
		CFLAG:ARG:603 = 0
		BASE:ARG:운 = CFLAG:ARG:604
		MAXBASE:ARG:운 = CFLAG:ARG:604
		CFLAG:ARG:604 = 0
		CALL SYNC_STATUS,ARG
	ENDIF
	CFLAG:ARG:입력행동 = [[스킬:메・디아라한]]
	CFLAG:ARG:602 = 1
ELSEIF LOCAL == 101
	;검격・타격に내성あって가드킬まだなら가드브레이크る
	IF CFLAG:ARG:603 == 0 || CFLAG:ARG:604 == 0
		BASE:ARG:힘 += 500
		MAXBASE:ARG:힘 += 500
		CFLAG:ARG:ＨＰ보정 *= 100
		BASE:ARG:ＨＰ *= 100
		CFLAG:ARG:603 = BASE:ARG:속도
		BASE:ARG:속도 = 500
		MAXBASE:ARG:속도 = 500
		CFLAG:ARG:604 = BASE:ARG:운
		BASE:ARG:운 = 500
		MAXBASE:ARG:운 = 500
		CALL SYNC_STATUS,ARG
	ENDIF
	CFLAG:ARG:입력행동 = [[스킬:타격가드브레이크]]
ELSEIF LOCAL == 1
	;검격・타격に내성あるなら지모의만찬で突然の死
	IF CFLAG:ARG:603 == 0 || CFLAG:ARG:604 == 0
		BASE:ARG:힘 += 500
		MAXBASE:ARG:힘 += 500
		CFLAG:ARG:ＨＰ보정 *= 100
		BASE:ARG:ＨＰ *= 100
		CFLAG:ARG:603 = BASE:ARG:속도
