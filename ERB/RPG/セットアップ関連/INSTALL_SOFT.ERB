			SIF (LOCAL == [[아이템:저주의레드카펫]])
				CONTINUE
			EQUIP:MASTER:(ITEMNAME:(LOCAL)) = 0
		NEXT
		;読込モードならセット内容を先머리から順に読み込み、セットがONかつ容量が발りるものをONにする
		;デビルチェンバーから処理する
		IF ITEM:(572) > 0 && GETBIT(FLAG:(LOCAL:7 - 215),(8))
			IF NUM_NAKAMA_HEADCOUNT() < 7
				EQUIP:MASTER:(ITEMNAME:572) = 1
			ELSE
				PRINTL ※경고※
				PRINTW 소지악마가 6체를 넘었기 때문에 데빌챔버를 기동할 수 없습니다
			ENDIF
		ENDIF
		FOR LOCAL,500,600
			SIF ITEM:(LOCAL) < 1
				CONTINUE
			;저주의레드카펫は一括処理대상外
			SIF (LOCAL == [[아이템:저주의레드카펫]])
				CONTINUE
			;デビルチェンバーは先に処理したので一括処理대상外
			SIF (LOCAL == [[아이템:데빌챔버]])
				CONTINUE
			;소프트세트x(500〜563)か소프트세트x_2(564〜599)かの分岐
			IF LOCAL <= 563
				GETBIT FLAG:(LOCAL:7 - 220),(LOCAL-500)
				SIF RESULT && FLAG:ＣＯＭＰ용량-NUM_NAKAMA()-소프트용량() >= SOFT_SIZE(LOCAL)
					EQUIP:MASTER:(ITEMNAME:(LOCAL)) = 1
			ELSE
				GETBIT FLAG:(LOCAL:7 - 215),(LOCAL-564)
				SIF RESULT && FLAG:ＣＯＭＰ용량-NUM_NAKAMA()-소프트용량() >= SOFT_SIZE(LOCAL)
					EQUIP:MASTER:(ITEMNAME:(LOCAL)) = 1
			ENDIF
		NEXT
	ENDIF
	GOTO PRINT_LIST
ELSEIF RESULT == 950
	;登録モードの切り替え
	LOCAL:8 = !LOCAL:8
	GOTO PRINT_LIST
;---- EDIT 017 ADD END -------------------------

ELSEIF RESULT < 500 || RESULT >= 600 || MATCH(I,RESULT) == 0
	GOTO INPUT_LOOP_1
ENDIF
LOCAL = RESULT
IF SOFT_SIZE(LOCAL) > -1
	IF EQUIP:MASTER:(ITEMNAME:(LOCAL))
;---- EDIT 014 MOD START -------------------------
		IF (LOCAL == [[아이템:저주의레드카펫]])
			PRINTL 끌 수가 없다！
			GOTO PRINT_LIST
		ENDIF
		;---- EDIT 018 ADD START -------------------------
		IF LOCAL == [[아이템:데빌챔버]] && NUM_NAKAMA_HEADCOUNT() < 7 && FLAG:ＣＯＭＰ용량 - 소프트용량() + 1 < NUM_NAKAMA_HEADCOUNT()
			PRINTW 끄면 용량 오버해버립니다！
			GOTO PRINT_LIST
		ENDIF
		;---- EDIT 018 ADD END ---------------------------
		EQUIP:MASTER:(ITEMNAME:(LOCAL)) = 0
	ELSEIF FLAG:ＣＯＭＰ용량-NUM_NAKAMA()-소프트용량() >= SOFT_SIZE(LOCAL) && ITEM:(LOCAL)
		IF (LOCAL == [[아이템:저주의레드카펫]])
			IF BASE:MASTER:LV > 5
				PRINTL 기동할 수 없는 모양이다……
				GOTO PRINT_LIST
			ENDIF
			PRINTL 정말로 소프트를 기동합니까？(끌 수 없게 됩니다)
			CALL INPUT_YN
			SIF RESULT == 1
				GOTO PRINT_LIST
			MONEY:1 += 1
		ENDIF
;---- EDIT 014 MOD END -------------------------
		;---- EDIT 024 ADD START -------------------------
		;ハーモナイザーと魔装術の排他処理
		;魔装術の場合
		IF (LOCAL == [[아이템:마장술]])
			;ハーモナイザーを起動しているか見る
			IF EQUIP:MASTER:(ITEMNAME:574) == 1
				PRINTL ＞ＡＴＴＥＮＴＩＯＮ...『어플：하모나이저』와 간섭、작동 불능
				PRINTW ＞마장술의 기동을 중단합니다
				GOTO PRINT_LIST
			ENDIF
		;ハーモナイザーの場合
		ELSEIF (LOCAL == [[아이템:하모나이저]])
			;魔装術を起動しているか見る
			IF EQUIP:MASTER:(ITEMNAME:563) == 1
				PRINTL ＞ＡＴＴＥＮＴＩＯＮ...『어플：마장술』과 간섭、작동 불능
				PRINTW ＞하모나이저의 기동을 중단합니다
				GOTO PRINT_LIST
			ENDIF
		ENDIF
		;---- EDIT 024 END START -------------------------
		EQUIP:MASTER:(ITEMNAME:(LOCAL)) = 1
	;---- EDIT 018 ADD START -------------------------
	;容量いっぱいでも、圧縮した結果容量内に収まるなら起動できる
	ELSEIF LOCAL == [[아이템:데빌챔버]] && NUM_NAKAMA_HEADCOUNT() > 0 && NUM_NAKAMA_HEADCOUNT() < 7
		EQUIP:MASTER:(ITEMNAME:(LOCAL)) = 1
	;---- EDIT 018 ADD END ---------------------------
	ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_1
	ENDIF
ELSE
		CLEARLINE 1
		GOTO INPUT_LOOP_1
ENDIF
GOTO PRINT_LIST

