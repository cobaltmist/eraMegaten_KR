;-------------------------------------------------------------------------------
; [メモ]
; 타격版の회전베기みたいな포지션。돌려차기と메가톤프레스の中間。
; 전체中데미지。
;-------------------------------------------------------------------------------


;스킬名
@SKILL_NAME_237,ARG
RESULTS = 베어넘기기

;　명중
@SKILL_HITRATE_237, ARG
RETURN 95

;　위력
@SKILL_POWER_237, ARG
RETURN 180

;　최저공격횟수
@SKILL_MINATTACKNUMBER_237,ARG
RETURN 1

;　최대공격횟수
@SKILL_MAXATTACKNUMBER_237,ARG
RETURN 1

;　ＨＰ2を支払うかＭＰ3を支払うか
@SKILL_COSTTYPE_237, ARG
RETURN 3
;소비なし＝1

;　コストの量
@SKILL_COST_237,ARG
RETURN 12

;　相性
@SKILL_TYPE_237,ARG
RETURN 2
;타격

;　1EXTRAか2MAGICか
@SKILL_DECIDE_TYPE_237, ARG
RETURN 1


;　1물리데미지で計算するか2마법데미지で計算するか
@SKILL_DAMAGETYPE_237, ARG
RETURN 1


;　대상が1적か2아군か3場전체か4特殊か
@SKILL_TARGET_237
RETURN 1

;　사거리
@SKILL_RANGE_237,ARG
RETURN 3

;　범위　1 1체　2 1열　3 전체　4 적아군전체
@SKILL_SPHERE_237,ARG
RETURN 3

;　 効果の타입　1공격、2회복、3상태변화、4特殊、5상태회복
@SKILL_EFECT_237
RETURN 1

;　 特殊処理の有無、及び処理内容
@SKILL_SPECIALEFECT_237,ARG
;ARG = 1だった場合は処理の有無を返します
SIF ARG == 1
	RETURN 0
;以下に特殊な処理を記述します

;계승타입
@SKILL_SUCCESSION_TYPE_237
RETURN 2
;타격

;속도보정
@SKILL_SPEED_237, ARG = -1
RETURN 0

;　구상の표시とその타입
@SKILL_MESSAGE_237,ARG


;전투時以外に사용できるか
@SKILL_ACTIONABLE_FIELD_237
RETURN 0

;스킬변화？用랭크
@SKILL_RANK_237
RETURN 7


;テン플레이트用、추가효과シリーズ
;추가효과스테이터스
@SKILL_ADDTIONAL_STATE_237,ARG
RETURN 0

;추가효과상성
@SKILL_ADDTIONAL_TYPE_237,ARG
RETURN 0

;추가효과기본부여확률
@SKILL_ADDTIONAL_BASE_RATE_237,ARG
RETURN 0

;추가효과부여확률上限
@SKILL_ADDTIONAL_MAX_RATE_237,ARG
RETURN 0

;====================================================
;스킬及び行動の実行
;====================================================
@ACTION_237,ARG,ARG:1
CALL ATTACK_FIELD,ARG,ARG:1,237






;====================================================
;実行内容ここまで
;====================================================

;　 설명文の표시
@SKILL_EXPLAIN_237
PRINTL [후열] 적 전체에 타격속성 중 물리데미지.

;継承に필요한部位
@継承部位_237,ARG
RETURN 1



;사용조건
@SKILL_SPECIAL_ACTIONABLE_237,ARG
RETURN 1
;とくにない

