;-------------------------------------------------------------------------------
; [メモ]
; 打撃版の回転斬りみたいなポジション。回し蹴りとメガトンプレスの中間。
; 全体중 데미지。
;-------------------------------------------------------------------------------


;스킬명
@SKILL_NAME_237,ARG
RESULTS = 베어넘기기

;　명중
@SKILL_HITRATE_237, ARG
RETURN 95

;　위력
@SKILL_POWER_237, ARG
RETURN 180

;　최저공격횟수
@SKILL_MINATTACKNUMBER_237,ARG
RETURN 1

;　최대공격횟수
@SKILL_MAXATTACKNUMBER_237,ARG
RETURN 1

;　2 ＨＰ지불　3 ＭＰ지불
@SKILL_COSTTYPE_237, ARG
RETURN 3
;소비없음＝1

;　코스트량
@SKILL_COST_237,ARG
RETURN 12

;　상성
@SKILL_TYPE_237,ARG
RETURN 2
;타격

;　1 EXTRA　2 MAGIC
@SKILL_DECIDE_TYPE_237, ARG
RETURN 1


;　1 물리데미지로 계산　2 마법데미지로 계산
@SKILL_DAMAGETYPE_237, ARG
RETURN 1


;　대상　1 적　2 아군　3 양쪽전체　4 특수
@SKILL_TARGET_237
RETURN 1

;　사거리
@SKILL_RANGE_237,ARG
RETURN 3

;　범위　1 1체　2 1열　3 전체　4 적아군전체
@SKILL_SPHERE_237,ARG
RETURN 3

;　 효과타입　1 공격、2 회복、3 상태변화、4 특수、5 상태회복
@SKILL_EFECT_237
RETURN 1

;　 특수처리 여부 및 처리내용
@SKILL_SPECIALEFECT_237,ARG
;ARG = 1인 경우는 처리 여부를 반환합니다
SIF ARG == 1
	RETURN 0
;아래쪽에 특수처리를 기술합니다

;계승타입
@SKILL_SUCCESSION_TYPE_237
RETURN 2
;타격

;속도보정
@SKILL_SPEED_237, ARG = -1
RETURN 0

;　구상의 표시와 그 타입
@SKILL_MESSAGE_237,ARG


;전투시 이외에 사용 가능한가
@SKILL_ACTIONABLE_FIELD_237
RETURN 0

;스킬변화？용 랭크
@SKILL_RANK_237
RETURN 7


;템플릿용、추가효과 시리즈
;추가효과 스테이터스
@SKILL_ADDTIONAL_STATE_237,ARG
RETURN 0

;추가효과 상성
@SKILL_ADDTIONAL_TYPE_237,ARG
RETURN 0

;추가효과 기본부여확률
@SKILL_ADDTIONAL_BASE_RATE_237,ARG
RETURN 0

;추가효과 부여확률상한
@SKILL_ADDTIONAL_MAX_RATE_237,ARG
RETURN 0

;====================================================
;스킬 및 행동의 실행
;====================================================
@ACTION_237,ARG,ARG:1
CALL ATTACK_FIELD,ARG,ARG:1,237






;====================================================
;실행내용 여기까지
;====================================================

;　 설명문 표시
@SKILL_EXPLAIN_237
PRINTL 적 전체에 중위력의 타격 상성 공격을 행한다。

;계승에 필요한 부위
@계승부위_237,ARG
RETURN 1



;사용조건
@SKILL_SPECIAL_ACTIONABLE_237,ARG
RETURN 1
