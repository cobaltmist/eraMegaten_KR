;====================================================
;스킬及び行動の実行
;====================================================
@ACTION_249,ARG,ARG:1
LOCAL = POS(ARG:1)
A = 0
B = 0
C = 0
D = 0

CALL 상성소질체크, ARG, GET_TYPE_NUM("타격"), 1, 1
IF RESULT == 1 && CFLAG:ARG:집중플래그 > 0
	IF (MAXBASE:LOCAL:타격 < 100 || MAXBASE:LOCAL:타격 == 999) && CFLAG:LOCAL:타격가드킬 < 1
		CFLAG:LOCAL:타격가드킬 = 1
		A = 1
	ENDIF
	IF CFLAG:LOCAL:물리반사플래그 == 1
		B = CFLAG:LOCAL:물리반사플래그
		CFLAG:LOCAL:물리반사플래그 = 0
	ENDIF
	IF CFLAG:LOCAL:무효플래그 > 0
		C = CFLAG:LOCAL:무효플래그
		CFLAG:LOCAL:무효플래그 = 0
	ENDIF
	IF CFLAG:LOCAL:타격무효화횟수 > 0
		D = CFLAG:LOCAL:타격무효화횟수
		CFLAG:LOCAL:타격무효화횟수 = 0
	ENDIF
	IF A + B + C + D > 0
		PRINTFORML 　　TARGET:[{ARG:1,2}] %CALLNAME:LOCAL,20,LEFT%　>>>>>>　내성을 관통했다!
		CFLAG:ARG:집중플래그 = 0
	ENDIF
ENDIF
CALL ATTACK_SINGLE,ARG,LOCAL,249

SIF A == 1
	CFLAG:LOCAL:타격가드킬 = 0
SIF B > 0
	CFLAG:LOCAL:물리반사플래그 = B
SIF C > 0
	CFLAG:LOCAL:무효플래그 = C
SIF D > 0
	CFLAG:LOCAL:타격무효화횟수 = D

