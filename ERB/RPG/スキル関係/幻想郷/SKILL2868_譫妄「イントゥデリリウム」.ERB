FOR LOCAL, 1, 17
	SIF POS(LOCAL) < 0
		CONTINUE
	SIF GET_STATE(CFLAG:POS(LOCAL):상태이상) == "DYING"
		CONTINUE
	SIF GET_STATE(CFLAG:POS(LOCAL):상태이상) == "GOOD"
		CONTINUE
	CALL ATTACK_SINGLE,ARG,POS(LOCAL),2868
NEXT
