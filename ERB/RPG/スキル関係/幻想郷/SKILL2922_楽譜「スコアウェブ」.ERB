;====================================================
;스킬及び行動の実行
;====================================================
@ACTION_2922,ARG,ARG:1
#LOCALSIZE 3
IF CFLAG:ARG:포지션 < 7
	LOCAL:1 = 7
	LOCAL:2 = 12 
ELSE
	LOCAL:1 = 1
	LOCAL:2 = 4
ENDIF
FOR LOCAL, LOCAL:1, LOCAL:2
	SIF POS(LOCAL) < 0
		CONTINUE
	SIF GET_STATE(CFLAG:POS(LOCAL):ステート) == "DYING"
		CONTINUE
	FLAG:확산플래그 = POS(LOCAL)
	CALL ATTACK_SINGLE, ARG, POS(LOCAL), 2922
NEXT
FLAG:확산플래그 = -1
