	IF EQUIP:ARG:검 == 2370
		RESULTS = 볼텍스
	ELSEIF EQUIP:ARG:검 == 2371
		RESULTS = 인페르노 
	ELSE
		RESULTS = 虚刃連斬
	ENDIF
ENDIF

