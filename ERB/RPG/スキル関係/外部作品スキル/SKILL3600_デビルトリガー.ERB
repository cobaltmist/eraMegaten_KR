;스킬及び行動の実行
;====================================================

@ACTION_3600,ARG,ARG:1
#LOCALSIZE 4
IF SKILL_CHANGE(ARG, [[キャラ:단테]]) == 1 && SKILLGAGE_D_GET(ARG, 3600)
	CALL ATTACK_SINGLE,ARG,POS(ARG:1),3600
ELSEIF SKILL_CHANGE(ARG, [[キャラ:트리쉬(DMC)]]) == 1 && SKILLGAGE_D_GET(ARG, 3600)
	CALL ATTACK_SINGLE,ARG,POS(ARG:1),3600
ELSEIF FINDCHARA_M(ARG,0,4998,4999) == 1 && SKILLGAGE_D_GET(ARG, 3600) && EQUIP:ARG:검 == 2370
	CALL ATTACK_SINGLE,ARG,POS(ARG:1),3600
ELSEIF FINDCHARA_M(ARG,0,4998,4999) == 1 && SKILLGAGE_D_GET(ARG, 3600) && EQUIP:ARG:검 == 2371
	CALL ATTACK_FIELD,ARG,ARG:1,3600
ELSEIF FINDCHARA_M(ARG,0,4998,4999) == 1 && SKILLGAGE_D_GET(ARG, 3600) && EQUIP:ARG:검 != 2370 && EQUIP:ARG:검 != 2371
	CALL ATTACK_SINGLE,ARG,POS(ARG:1),3600
ELSEIF SKILL_CHANGE(ARG, [[キャラ:버질]]) == 1 && SKILLGAGE_D_GET(ARG, 3600)
	LOCAL:3 = CFLAG:ARG:타겟
	IF ARG:1 == 17
		LOCAL:1 = 1
		LOCAL:2 = 4
	ELSEIF ARG:1 == 18
		LOCAL:1 = 4
		LOCAL:2 = 7
	ELSEIF ARG:1 == 19
		LOCAL:1 = 1
		LOCAL:2 = 7
	ELSEIF ARG:1 == 20
		LOCAL:1 = 7
		LOCAL:2 = 12
	ELSEIF ARG:1 == 21
		LOCAL:1 = 12
		LOCAL:2 = 17
	ELSEIF ARG:1 == 22
		LOCAL:1 = 7
		LOCAL:2 = 17
	ELSEIF ARG:1 == 23
		LOCAL:1 = 1
		LOCAL:2 = 17
	ENDIF
	FOR LOCAL, LOCAL:1, LOCAL:2
		SIF POS(LOCAL) < 0
			CONTINUE
		SIF GET_STATE(CFLAG:POS(LOCAL):ステート) == "DYING"
			CONTINUE
		CFLAG:ARG:타겟 = LOCAL
		CALL ATTACK_SINGLE, ARG, POS(LOCAL),3600,1
	NEXT
	CFLAG:ARG:타겟 = LOCAL:3
	WAIT
ELSE
	CALL SKILLGAGE_F_SET,ARG,3600,0
	CALL SKILLGAGE_D_SET,ARG,3600,5
	PRINTFORMW %조사처리(CALLNAME:ARG,"는")% 데빌트리거를 사역마	인간화했다！
ENDIF
