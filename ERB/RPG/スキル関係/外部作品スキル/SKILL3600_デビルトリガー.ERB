	;데미지を受けた적の数に応じて스킬게이지アップ
	FOR LOCAL,0,CHARANUM
		SIF CFLAG:LOCAL:PT플래그 == CFLAG:ARG:PT플래그
			CONTINUE
		IF CFLAG:LOCAL:現데미지 > 0
			CALL SKILLGAGE_F_ADD,ARG,3600,3
		ELSE
			CONTINUE
		ENDIF
		;会心、약점を出すとさらにアップ
		SIF GETBIT(CFLAG:LOCAL:受けフラグ,21)
			CALL SKILLGAGE_F_ADD,ARG,3600,2
		SIF GETBIT(CFLAG:LOCAL:受けフラグ,22)
			CALL SKILLGAGE_F_ADD,ARG,3600,2
	NEXT
	;도발をしても上がる
	SIF CFLAG:ARG:입력행동 == 515
		CALL SKILLGAGE_F_ADD,ARG,3600,7
	;無限の挑発しても上がる
	SIF CFLAG:ARG:입력행동 == [[스킬:끝없는도발]]
		CALL SKILLGAGE_F_ADD,ARG,3600,7
ENDIF
;데미지を受けた又はカウンター、トリガー이외の공격を受けたら
;怒りゲージが増える
IF CFLAG:ARG:現데미지 > 0 || GETBIT(CFLAG:ARG:受けフラグ,18) || GETBIT(CFLAG:ARG:受けフラグ,19)
	LOCAL = 60 * CFLAG:ARG:現데미지 / MAXBASE:ARG:ＨＰ
	LOCAL = LOCAL < 2 ? 2 # LOCAL
	CALL SKILLGAGE_F_ADD,ARG,3600,LOCAL
ENDIF
;스킬게이지が100이상だと100にする
SIF SKILLGAGE_F_GET(ARG,3600) > 100
	CALL SKILLGAGE_F_SET,ARG,3600,100
RETURN 0

