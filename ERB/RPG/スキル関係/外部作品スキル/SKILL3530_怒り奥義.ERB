@INPUT_COM_13530,ARG
#LOCALSSIZE 1
#LOCALSIZE 3
#DIM STOCK
#DIM LCOUNT
#DIM LINE
LINE = LINECOUNT
VARSET LOCAL
STOCK = SKILLGAGE_H_GET(ARG,3530)
FOR LCOUNT,0,3
	CALL SKILLGAGE_H_SET,ARG,3530,LCOUNT
	CALL CHECK_ACTIONABLE,ARG,3530
	IF RESULT && (LCOUNT < 2 || (LCOUNT == 2 && (SKILLGAGE_H_GET(ARG,[[스킬:분노폭발]]) == 200 || 현HP비율(ARG) < 26)))
		LOCAL:LCOUNT = 1
		CALL SKILL_NAME_3530,ARG
		LOCALS = %RESULTS%
		CALL TRUE_SKILL_COST,ARG,3530
		PRINTFORML [{LCOUNT+1}] %LOCALS, 38, LEFT%{RESULT, 3}ＭＰ
	ENDIF
NEXT
DRAWLINE
PRINTL [0] ＣＡＮＣＥＬ
CALL INPUTINT(0,1,2,3)
IF RESULT > 0 && LOCAL:(RESULT - 1) > 0
	CALL SKILLGAGE_H_SET,ARG,3530,RESULT - 1
	CALL SELECT_SKILL_INPUT,ARG,3530
	SIF RESULT > -1
		RETURN 1
ELSEIF RESULT == 0
	CALL SKILLGAGE_H_SET,ARG,3530,STOCK
	RETURN -1
ENDIF
CLEARLINE LINECOUNT - LINE
RESTART

