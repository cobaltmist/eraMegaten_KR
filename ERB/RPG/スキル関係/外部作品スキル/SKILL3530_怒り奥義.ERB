	CALL SKILLGAGE_H_SET,ARG,3530,0

;====================================================
;스킬 및 행동의 실행
;====================================================

@ACTION_3530,ARG,ARG:1
#LOCALSIZE 5
#DIM STOCK,6

IF SKILL_CHANGE(ARG,[[キャラ:나코루루]])
	STOCK:0 = MAXBASE:ARG:공격
	STOCK:1 = CFLAG:ARG:공격강화
	MAXBASE:ARG:공격 = MAXBASE:ARG:마법위력
	CFLAG:ARG:공격강화 = CFLAG:ARG:마법위력강화
	;修羅
	IF SKILLGAGE_F_GET(ARG,[[스킬:분노폭발]]) == 0
		IF SKILLGAGE_H_GET(ARG,3530) == 0
			CALL ATTACK_SPREAD,ARG,ARG:1,3530
		ELSEIF SKILLGAGE_H_GET(ARG,3530) > 0
			CALL ATTACK_FIELD,ARG,ARG:1,3530
			;PT判定
			IF CFLAG:(ARG):PT플래그 == 0
				LOCAL:1 = 1
				LOCAL:2 = 7
				LOCAL:3 = 7
				LOCAL:4 = 17
			ELSE
				LOCAL:1 = 7
				LOCAL:2 = 17
				LOCAL:3 = 1
				LOCAL:4 = 7
			ENDIF
			;敵
			FOR LOCAL,LOCAL:1,LOCAL:2
				SIF POS(LOCAL) < 0
					CONTINUE
				CFLAG:POS(LOCAL):공격강화 = MAX(-16, CFLAG:POS(LOCAL):공격강화 - 4)
				CFLAG:POS(LOCAL):마법위력강화 = MAX(-16, CFLAG:POS(LOCAL):마법위력강화 - 4)
			NEXT
			PRINTFORML \@ LOCAL:1 == 7 ? 적 # 아군 \@의 공격력、마법위력을 낮췄다！
			PRINTL 
			;味方
			FOR LOCAL,LOCAL:3,LOCAL:4
				SIF POS(LOCAL) < 0
					CONTINUE
				CFLAG:POS(LOCAL):방어강화 = MIN(32, CFLAG:POS(LOCAL):방어강화 + 4)
				CFLAG:POS(LOCAL):마법효과강화 = MIN(32, CFLAG:POS(LOCAL):마법효과강화 + 4)
				SIF SKILLGAGE_H_GET(ARG,3530) == 2
					CALL ALL_HEAL, ARG, POS(LOCAL), 0
			NEXT
			PRINTFORMW \@ LOCAL:3 == 1 ? 아군 # 적 \@의 방어력、마법효과가 높아졌다！
			CALL SKILLGAGE_D_SET,ARG,[[스킬:분노폭발]],0
		ENDIF
	;나찰
	ELSE
		CALL ATTACK_SINGLE,ARG,POS(ARG:1),3530
		IF SKILLGAGE_H_GET(ARG,3530) == 0
			CFLAG:ARG:마법위력강화 = MIN(32, CFLAG:ARG:마법위력강화 + 4)
			PRINTFORMW %CALLNAME:ARG%의 마법위력이 높아졌다！
		ELSE
			CALL SKILLGAGE_D_SET,ARG,[[스킬:분노폭발]],0
		ENDIF
	ENDIF
	MAXBASE:ARG:공격 = STOCK:0
	CFLAG:ARG:공격강화 = STOCK:1
ELSEIF SKILL_CHANGE(ARG,[[キャラ:요시토라]])
	FLAG:확산플래그 = 0
	IF SKILLGAGE_H_GET(ARG,3530) == 0
		CALL ATTACK_RAND,ARG,ARG:1,3530
		SIF SKILLGAGE_F_GET(ARG,[[스킬:분노폭발]]) == 1
			CALL SIX_TATI_CHANGE(ARG)
	ELSEIF SKILLGAGE_H_GET(ARG,3530) == 1
		LOCAL = POS(ARG:1)
		LOCAL:1 = 0
		LOCAL:2 = 0
		
		IF (MAXBASE:LOCAL:검격 < 100 || MAXBASE:LOCAL:검격 == 999) && CFLAG:LOCAL:검격가드킬 < 1
			CFLAG:LOCAL:검격가드킬 = 1
			LOCAL:1 = 0
		ENDIF
		IF CFLAG:LOCAL:검격무효화횟수 > 0
			LOCAL:2 = CFLAG:LOCAL:검격무효화횟수
			CFLAG:LOCAL:검격무효화횟수 = 0
		ENDIF
		CALL ATTACK_SINGLE,ARG,LOCAL,3530,1
		SIF LOCAL:1
			CFLAG:LOCAL:검격가드킬 = 0
		SIF LOCAL:2
			CFLAG:LOCAL:검격무효화횟수 = LOCAL:2
		IF FLAG:확산플래그
			CFLAG:LOCAL:검격무효화횟수 -= 6
			IF CFLAG:LOCAL:검격무효화횟수 < 0
				CFLAG:LOCAL:검격가드킬 = -CFLAG:LOCAL:검격무효화횟수
				PRINTFORMW [{ARG:1}]%CALLNAME:LOCAL,20% >>>>> {-CFLAG:LOCAL:검격무효화횟수}턴、검격내성을 무효화！
				CFLAG:LOCAL:검격무효화횟수 = 0
			ELSE
				PRINTFORMW [{ARG:1}]%CALLNAME:LOCAL,20% >>>>> 검격무효화횟수가 감소！
			ENDIF
		ENDIF
	ELSEIF SKILLGAGE_H_GET(ARG,3530) == 2
		LOCAL = POS(ARG:1)
		LOCAL:1 = 0
		IF (MAXBASE:LOCAL:검격 < 100 || MAXBASE:LOCAL:검격 == 999) && CFLAG:LOCAL:검격가드킬 < 1
			CFLAG:LOCAL:검격가드킬 = 1
			LOCAL:1 = 0
		ENDIF
		CALL ATTACK_SINGLE,ARG,LOCAL,3530
		SIF LOCAL:1
			CFLAG:LOCAL:검격가드킬 = 0
	ENDIF
	SIF FLAG:확산플래그
		CALL SKILLGAGE_D_SET,ARG,[[스킬:분노폭발]],0
	FLAG:확산플래그 = 0
