	IF SKILLGAGE_H_GET(ARG,3530) == 0
		CALL ATTACK_FIELD,ARG,ARG:1,3530
	;修羅
	ELSEIF SKILLGAGE_F_GET(ARG,[[스킬:분노폭발]]) == 0
		SELECTCASE ARG:1
		CASE 1 TO 6,17,18,19
			LOCAL = 22
		CASE 7 TO 16,20,21,22
			LOCAL = 19
		CASEELSE
			IF INRANGE(CPOS(ARG), 1, 6)
				LOCAL = 19
			ELSE
				LOCAL = 22
			ENDIF
		ENDSELECT
		
		CALL ATTACK_FIELD,ARG,ARG:1,3530
		SIF SKILLGAGE_H_GET(ARG,3530) == 2
			CALL HEAL_FIELD,ARG,LOCAL,3530
		CALL SKILLGAGE_D_SET,ARG,[[스킬:분노폭발]],0
	;羅刹
	ELSE
		IF SKILLGAGE_H_GET(ARG,3530) == 1
			CALL ATTACK_RAND,ARG,ARG:1,3530
			CALL SKILLGAGE_D_SET,ARG,[[스킬:분노폭발]],0
		ELSEIF SKILLGAGE_H_GET(ARG,3530) == 2
			
			LOCAL = POS(ARG:1)
