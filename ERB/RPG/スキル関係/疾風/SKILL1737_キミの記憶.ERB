;テン플레이트用、추가효과シリーズ
;추가효과스테이터스
@SKILL_ADDTIONAL_STATE_1737,ARG
A = RAND:11
IF A == 0
	RETURN GET_STATE_NUM("HAPPY")
ELSEIF A == 1
	RETURN GET_STATE_NUM("CHARM")
ELSEIF A == 2
	RETURN GET_STATE_NUM("PANIC")
ELSEIF A == 3
	RETURN GET_STATE_NUM("SLEEP")
ELSEIF A == 4
	RETURN GET_STATE_NUM("BIND")
ELSEIF A == 5
	RETURN GET_STATE_NUM("CLOSE")
ELSEIF A == 6
	RETURN GET_STATE_NUM("POISON")
ELSEIF A == 7
	RETURN GET_STATE_NUM("BOMB")
ELSEIF A == 8
	RETURN GET_STATE_NUM("PALYZE")
ELSEIF A == 9
	RETURN GET_STATE_NUM("SHOCK")
ELSE
	RETURN GET_STATE_NUM("FREEZE")
ENDIF
RETURN RAND:10 + 1

;추가효과상성
@SKILL_ADDTIONAL_TYPE_1737,ARG
IF A < 4
	RETURN GET_TYPE_NUM("정신")
ELSEIF A < 9
	RETURN GET_TYPE_NUM("신경")
ELSEIF A == 9
	RETURN GET_TYPE_NUM("전격")
ELSE
	RETURN GET_TYPE_NUM("빙결")
ENDIF
RETURN GET_TYPE_NUM("만능")

