;====================================================
;（予定）と付けられている方は仮置きの物。実際に採用されるかどうかはわからない。
@AUTO_PU_SKILL_災厄の巨焔,ARG,ARGS
#FUNCTION
#DIM SKILL_RANK
#DIM SKILL_NUM
#DIM FC_FLAG,2
#DIM DYNAMIC ALIVE_FLAG
#DIM DYNAMIC L_E_COUNT,2
;ARG = 使用者
;ARGS （데미지↑、피데미지↑、명중↑、회피↑、魔威↑、魔効↑、etc…）といった感じの奴
;文字列を参考にする都合上、どの文字列を使うか事前に定めておいた方がいいかも
;CSTR:ARG:100 = EXTRA勢出典作品、『アークナイツ』とでも書き込む（予定）
;CSTR:ARG:101 = EXTRA勢出典作品でどのキャラなのか、とりあえず『スルト（アークナイツ）』とでも書き込む（予定）
;CSTR:ARG:102 = 専用スキルその1のスキルランク変数として使用、この場合は『レーヴァテイン（ＡＮ）』が対象（予定）
;CSTR:ARG:103 = 専用スキルその2のスキルランク変数として使用、この場合は『災厄の巨焔』が対象（予定）
;CSTR:ARG:104 = 専用スキルその3のスキルランク変数として使用、この場合は『ラグナロク（ＡＮ）』が対象（予定）
;FC_FLAG = 기합、集中フラグ確保変数
;L_E_COUNT = 対象数
;ALIVE_FLAG = 非DYINGキャラカウント変数
SKILL_RANK = TOINT(CSTR:ARG:103)
SKILL_NUM = PU_SKILLNUM_GET(ARG,"災厄の巨焔")
SELECTCASE ARGS
	CASE "気合フラグ確保"
		;気合フラグを一時的に確保
		SIF FC_FLAG:0 == 1
			RETURNF 1
		IF CFLAG:ARG:기합플래그 > 0
			FC_FLAG:0 = 1
		ENDIF
	CASE "集中フラグ確保"
		;コンセフラグを一時的に確保
		SIF FC_FLAG:1 == 1
			RETURNF 1
		IF CFLAG:ARG:집중플래그 > 0
			FC_FLAG:1 = 1
		ENDIF
	CASE "気合確保変数リセット"
			FC_FLAG:0 = 0
	CASE "集中確保変数リセット"
			FC_FLAG:1 = 0
ENDSELECT
IF SKILLGAGE_D_GET(ARG,SKILL_NUM) > 0
	SELECTCASE ARGS
		CASE "気合集中フラグ確保"
			;気合、コンセフラグを一時的に確保
			SIF FC_FLAG:0 == 1 && FC_FLAG:1 == 1
				RETURNF 1
			IF CFLAG:ARG:기합플래그 > 0
				FC_FLAG:0 = 1
			ENDIF
			IF CFLAG:ARG:집중플래그 > 0
				FC_FLAG:1 = 1
			ENDIF
		CASE "ダメージ↑"
			;기합、コンセフラグを一時的に確保
			IF CFLAG:ARG:기합플래그 > 0
				FC_FLAG:0 = 1
			ENDIF
			IF CFLAG:ARG:집중플래그 > 0
				FC_FLAG:1 = 1
			ENDIF
			;여데미지とかに挟む場合の処理
			SELECTCASE SKILL_RANK
				CASE 1
					RETURNF 25
				CASE 2
					RETURNF 33
				CASE 3
					RETURNF 40
				CASE 4
					RETURNF 60
			ENDSELECT
		CASE "ダメージ条件↑"
			;特定条件時に데미지増加
			;この場合、対象が一体だった場合にのみ、여데미지上昇
			SELECTCASE CFLAG:ARG:타겟
				CASE 17
					L_E_COUNT:0 = 1
					L_E_COUNT:1 = 4
				CASE 18 
					L_E_COUNT:0 = 3
					L_E_COUNT:1 = 7
				CASE 19
					L_E_COUNT:0 = 1
					L_E_COUNT:1 = 7
				CASE 20
					L_E_COUNT:0 = 7
					L_E_COUNT:1 = 12
				CASE 21
					L_E_COUNT:0 = 12
					L_E_COUNT:1 = 17
				CASE 22
					L_E_COUNT:0 = 7
					L_E_COUNT:1 = 17
			ENDSELECT
			FOR COUNT,L_E_COUNT:0,L_E_COUNT:1
				SIF POS(COUNT) > -1 && GET_STATE(CFLAG:POS(COUNT):상태이상) != "DYING"
					ALIVE_FLAG += 1
				SIF ALIVE_FLAG > 1
					BREAK
			NEXT
			IF ALIVE_FLAG < 2
				SELECTCASE SKILL_RANK
					CASE 1
						RETURNF 45
					CASE 2
						RETURNF 37
					CASE 3
						RETURNF 35
					CASE 4
						RETURNF 20
				ENDSELECT
			ENDIF
		CASE "気合集中フラグ返却"
			IF FC_FLAG:0 == 1
				FC_FLAG:0 = 0
				CFLAG:ARG:기합플래그 = 1
			ENDIF
			IF FC_FLAG:1 == 1
				FC_FLAG:1 = 0
				CFLAG:ARG:집중플래그 = 1
			ENDIF
		;CASE "命中判定↑"
