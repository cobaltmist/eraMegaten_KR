		;		CASEELSE
		;			RETURNF 4
		;	ENDSELECT
		;CASEELSE
		;	RETURNF 0
	ENDSELECT
ENDIF
IF ARGS == "Hゲージ最大値"
	;スキルHゲージの最大値を決める処理
	SELECTCASE SKILL_RANK
		CASE 1
			RETURNF 28
		CASE 2
			RETURNF 25
		CASE 3
			RETURNF 22
		CASE 4
			RETURNF 18
	ENDSELECT
ELSEIF ARGS == "Hゲージ初期値"
	;スキルHゲージの初期値を決める処理
	SELECTCASE SKILL_RANK
		CASE 1
			RETURNF 7
		CASE 2
			RETURNF 8
		CASE 3
			RETURNF 9
		CASE 4
			RETURNF 12
	ENDSELECT
ELSEIF ARGS == "Dゲージ最大値"
	;スキルDゲージの最大値を決める処理
	SELECTCASE SKILL_RANK
		CASE 1
			RETURNF 3
		CASE 2 TO 4
			RETURNF 4
	ENDSELECT
ENDIF
