;====================================================
;トリガー発動条件
;====================================================
@SKILL_TRIGGER_ACTIONABLE_災厄の巨焔,ARG,ARG:1
#DIM SKILL_NUM
#DIM ALIVE_FLAG
#DIM L_E_COUNT,2
#DIM L_TARGET,11
SIF ARG != ARG:1
	RETURN 0
SKILL_NUM = PU_SKILLNUM_GET(ARG,"災厄の巨焔")
IF (AUTO_PU_SKILL_災厄の巨焔(ARG,"気合フラグ確保") + AUTO_PU_SKILL_災厄の巨焔(ARG,"集中フラグ確保")) > 0
	IF CFLAG:ARG:입력행동 != -1
		CALLFORM SKILL_EFECT_{CFLAG:ARG:입력행동}
		IF RESULT == 1 && SKILLGAGE_D_GET(ARG,SKILL_NUM) < 1
			TRYCALLFORM SKILL_DAMAGETYPE_{CFLAG:ARG:입력행동}
			IF RESULT == 1
				CALLF AUTO_PU_SKILL_災厄の巨焔(ARG,"気合確保変数リセット")
			ELSEIF RESULT == 2
				CALLF AUTO_PU_SKILL_災厄の巨焔(ARG,"集中確保変数リセット")
			ENDIF
		ENDIF
	ENDIF
ENDIF
SIF SKILLGAGE_D_GET(ARG,SKILL_NUM) < 1
	RETURN 0
CALLFORM SKILL_EFECT_{CFLAG:ARG:입력행동}
SIF RESULT != 1 || CFLAG:ARG:타겟 < 1
	RETURN 0
SIF CFLAG:ARG:입력행동 == SKILL_NUM
	RETURN 0
SELECTCASE CFLAG:ARG:타겟
	CASE 1 TO 3
		L_E_COUNT:0 = 1
		L_E_COUNT:1 = 4
	CASE 4 TO 6
		L_E_COUNT:0 = 4
		L_E_COUNT:1 = 7
	CASE 17,18
		L_E_COUNT:0 = 1
		L_E_COUNT:1 = 7
	CASE 7 TO 11
		L_E_COUNT:0 = 7
		L_E_COUNT:1 = 12
	CASE 12 TO 16
		L_E_COUNT:0 = 12
		L_E_COUNT:1 = 17
	CASE 20,21
		L_E_COUNT:0 = 7
		L_E_COUNT:1 = 17
	CASEELSE
		;全体攻撃は発動しないものとする！
		RETURN 0
ENDSELECT
ALIVE_FLAG = 0
VARSET L_TARGET
FOR COUNT,L_E_COUNT:0,L_E_COUNT:1
	IF POS(COUNT) > -1 && GET_STATE(CFLAG:POS(COUNT):상태이상) != "DYING" && CFLAG:ARG:타겟 != COUNT
		L_TARGET:ALIVE_FLAG = POS(COUNT)
		ALIVE_FLAG += 1
	ENDIF
NEXT
SIF ALIVE_FLAG < 1
	RETURN 0
RESULT:1 = CPOS(L_TARGET:(RAND:ALIVE_FLAG))
RETURN 1

