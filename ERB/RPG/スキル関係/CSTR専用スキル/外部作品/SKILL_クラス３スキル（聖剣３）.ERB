;====================================================
;스킬 및 행동의 실행
;====================================================
@ACTION_クラス３スキル（聖剣３）,ARG,ARG:1,SKILL_NUM
#DIM SKILL_NUM
#LOCALSIZE 4
#DIM クラス技変数
クラス技変数 = PU_SKILLNUM_GET(ARG,"クラス２スキル（聖剣３）")
IF SKILL_CHANGE(ARG, [[キャラ:리스]])
	;光時、攻撃前に強化
	IF CFLAG:ARG:900 == 1
		IF CFLAG:(ARG):PT플래그 == 0
			LOCAL:1 = 7
			LOCAL:2 = 17
		ELSE
			LOCAL:1 = 1
			LOCAL:2 = 7
		ENDIF

		FOR LOCAL,LOCAL:1,LOCAL:2
			SIF POS(LOCAL) < 0
				CONTINUE
			CFLAG:POS(LOCAL):마법위력강화 = MIN(32, CFLAG:POS(LOCAL):마법위력강화 + 8)
			CFLAG:POS(LOCAL):방어강화 = MIN(32, CFLAG:POS(LOCAL):방어강화 + 8)
		NEXT
		PRINTFORMW \@ CFLAG:ARG:PT플래그 == 0 ? 적 # 아군 \@의 마법위력・방어력이 대폭 UP！
	ENDIF
	CALL ATTACK_FIELD,ARG,ARG:1,SKILL_NUM;			範囲攻撃(列、全体)
ELSEIF SKILL_CHANGE(ARG, [[キャラ:샤를로트]])
	;光の場合自動ディア発動
	IF CFLAG:ARG:900 == 1
		IF CFLAG:(ARG):PT플래그
			LOCAL:1 = 1
			LOCAL:2 = 7
		ELSE
			LOCAL:1 = 7
			LOCAL:2 = 17
		ENDIF
		FOR LOCAL,LOCAL:1,LOCAL:2
			SIF POS(LOCAL) < 0
				CONTINUE
			SIF GET_STATE(CFLAG:POS(LOCAL):상태이상) == "DYING"
				CONTINUE
			CALL HEAL_SINGLE,ARG,POS(LOCAL),401
		NEXT
	ENDIF
	CALL ATTACK_FIELD,ARG,ARG:1,SKILL_NUM;			範囲攻撃(列、全体)
ELSEIF SKILL_CHANGE(ARG, [[キャラ:안젤라]])
	;光の場合無条件魔力UP
	SIF CFLAG:ARG:900 == 1
		CFLAG:ARG:마법위력강화 = MIN(32, CFLAG:ARG:마법위력강화 + 4)
	CALL ATTACK_SPREAD,ARG,ARG:1,SKILL_NUM;		拡散攻撃
ENDIF
	;リキャストセット、数値は５＝４ターン
CALL SKILLGAGE_D_SET,ARG,クラス技変数,5

