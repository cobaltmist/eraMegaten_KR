;====================================================
;스킬 및 행동의 실행
;====================================================
@ACTION_リラックス,ARG,ARG:1,SKILL_NUM
#DIM SKILL_NUM

;死亡以外の状態異常も回復する
CALL ALL_CURE_STATE, ARG, ARG, 0

;MP回復
IF CFLAG:ARG:998 == 2
	PRINTFORML 　　TARGET:[{CPOS(ARG),2}] %CALLNAME:ARG,20,LEFT%　>>>>>>　{MAXBASE:ARG:ＭＰ / 3}회복
	CALL VAR_MP, ARG, MAXBASE:ARG:ＭＰ / 3, 3
ELSE
	PRINTFORML 　　TARGET:[{CPOS(ARG),2}] %CALLNAME:ARG,20,LEFT%　>>>>>>　{MAXBASE:ARG:ＭＰ / (5 - (함락(ARG) > 0))}회복
	CALL VAR_MP, ARG, MAXBASE:ARG:ＭＰ / (5 - (함락(ARG) > 0)), 3
ENDIF

;自身の補助効果を全て消す
FOR LOCAL:1,0,6
	CFLAG:ARG:(GET_BATTLESTATUS(LOCAL:1) + "강화") = MAX(0, CFLAG:ARG:(GET_BATTLESTATUS(LOCAL:1) + "강화"))
	CFLAG:ARG:(GET_BATTLESTATUS(LOCAL:1) + "강화") = MIN(0, CFLAG:ARG:(GET_BATTLESTATUS(LOCAL:1) + "강화"))
NEXT
PRINTFORMW %CALLNAME:ARG%の補助効果が全て消えた！

CALL SKILLGAGE_F_ADD,ARG,SKILL_NUM,1

;====================================================
;실행내용 여기까지
;====================================================

;====================================================
;전투중 이외에서의 실행
;====================================================
@ACTION_FIELD_リラックス,ARG,ARG:1,SKILL_NUM
#DIM SKILL_NUM
CLEARLINE 1

SETCOLOR 0x66FFFF
IF SKILLGAGE_F_GET(ARG,SKILL_NUM) == 0
	IF GETCONFIGS("描画インターフェース") != "WINAPI"
		SIF NO:ARG == [[キャラ:마르푸샤]]
			CALL ドリンク_マルフーシャ
		IF NO:ARG == [[キャラ:라이카(Marufusha)]]
			IF RAND:2
				CALL ドリンク_ライカ
			ELSEIF RAND:2
				CALL アクション_ライカ
			ELSE
				CALL アクション_ライカ_2
			ENDIF
		ENDIF
		IF NO:ARG == [[キャラ:벨카(Marufusha)]]
			IF RAND:2
				CALL ドリンク_ベルカ
			ELSE
				CALL アクション_ベルカ
			ENDIF
		ENDIF
		IF NO:ARG == [[キャラ:스트렐카(Marufusha)]]
			IF RAND:2
				CALL ドリンク_ストレルカ
			ELSE
				CALL アクション_ストレルカ
			ENDIF
		ENDIF
		IF NO:ARG == [[キャラ:펠리세트]]
			IF RAND:2
				CALL ドリンク_フェリセット
			ELSE
				CALL アクション_フェリセット_2
			ENDIF
		ENDIF
		IF NO:ARG == [[キャラ:알리비나]]
			IF RAND:2
				CALL ドリンク_アリビナ
			ELSEIF RAND:2
				CALL アクション_アリビナ
			ELSEIF RAND:2
				CALL アクション_アリビナ_3
			ELSE
				CALL アクション_アリビナ_2
			ENDIF
		ENDIF
		IF NO:ARG == [[キャラ:비온]]
			IF RAND:2
				CALL ドリンク_ビオン
			ELSEIF RAND:2
				CALL アクション_ビオン
			ELSE
				CALL アクション_ビオン_2
			ENDIF
		ENDIF
		IF NO:ARG == [[キャラ:에노스]]
			IF RAND:2
				CALL ドリンク_エノス
			ELSEIF RAND:2
				CALL アクション_エノス
			ELSE
				CALL アクション_エノス_2
			ENDIF
		ENDIF
		PRINTL 
	ENDIF
	PRINTL 残りキット数：★★☆
ELSEIF SKILLGAGE_F_GET(ARG,SKILL_NUM) == 1
	IF GETCONFIGS("描画インターフェース") != "WINAPI"
		SIF NO:ARG == [[キャラ:마르푸샤]]
