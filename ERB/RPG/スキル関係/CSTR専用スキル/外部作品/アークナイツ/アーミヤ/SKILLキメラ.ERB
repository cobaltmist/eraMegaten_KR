;ターン開始時に行う処理
@SKILL_TURNSTART_キメラ,ARG
#DIM SKILL_NUM
#DIM ABSOURPTION
SIF !ACTIONABLE_CHARA_F(ARG)
	RETURN 0
SKILL_NUM = PU_SKILLNUM_GET(ARG,"キメラ")
ABSOURPTION = PU_SKILLNUM_GET(ARG,"アブソープション")
;スキル起動中(キメラのFゲージが-1以下)でなければ終了
SIF SKILLGAGE_F_GET(ARG,SKILL_NUM) > -1
	RETURN 0
TRYCALLFORM SKILL_EFECT_{CFLAG:ARG:입력행동}
IF RESULT == 1
	TRYCALLFORM SKILL_DAMAGETYPE_{CFLAG:ARG:입력행동},ARG
	IF RESULT == 2
		;CALLFORM SKILL_NAME_{CFLAG:ARG:입력행동},ARG
		;RESETCOLOR
		;PRINTFORMW %CALLNAME:ARG%が唱えた%RESULTS%が強く変質し始めた………！
		CALL SKILLGAGE_F_SET,ARG,ABSOURPTION,CFLAG:ARG:입력행동
		CALL SKILLGAGE_H_SET,ARG,ABSOURPTION,CFLAG:ARG:타겟
		CFLAG:ARG:입력행동 = SKILL_NUM
		CFLAG:ARG:타겟 = CFLAG:ARG:PT플래그 == 0 ? 19 # 22
		;SETCOLOR CFLAG:ARG:PT플래그 == 0 ? 0xff0033 # 0x33ffcc
	ENDIF
ENDIF
RETURN 0

;ターン終了時に行う処理
@SKILL_TURNEND_キメラ,ARG
#DIM SKILL_NUM
#DIM ABSOURPTION
#DIM L_GAGE
SIF ARG < 0 || CSTR:ARG:104 == ""
	RETURN 0
SKILL_NUM = PU_SKILLNUM_GET(ARG,"キメラ")
ABSOURPTION = PU_SKILLNUM_GET(ARG,"アブソープション")
;アブソープションのFゲージがクールタイムゲージとして使われてなければキメラのFゲージを、それ以外ならアブソープションのFゲージを対象にする
L_GAGE = SKILLGAGE_F_GET(ARG,ABSOURPTION) > -1 ? SKILLGAGE_F_GET(ARG,SKILL_NUM) # SKILLGAGE_F_GET(ARG,ABSOURPTION)
IF GET_STATE(CFLAG:ARG:상태이상) == "DYING"
	;スキル起動中ならゲージ初期化&クールタイムゲージセット
	IF SKILLGAGE_F_GET(ARG,SKILL_NUM) < 0
		CALL SKILLGAGE_F_SET(ARG,SKILL_NUM,0)
		CFLAG:ARG:입력행동 = SKILLGAGE_F_GET(ARG,ABSOURPTION)
		CFLAG:ARG:타겟 = SKILLGAGE_H_GET(ARG,ABSOURPTION)
		CALL SKILLGAGE_F_SET(ARG,ABSOURPTION,-70)
		CALL SKILLGAGE_H_SET(ARG,ABSOURPTION,0)
	ENDIF
	RETURN 0
ENDIF
;Fゲージが0or正の値（起動していない状態）か、負の値（効果発動中 or クールタイムゲージ）で処理を変更
L_GAGE = L_GAGE > -1 ? MIN(L_GAGE + 5,120) # MIN(L_GAGE + 5 ,0)
;効果時間が終了した際にメッセージ表示
IF L_GAGE == 0 && SKILLGAGE_F_GET(ARG,SKILL_NUM) < 0
	PRINTL
	PRINTL ……
	PRINTFORMW 능력을 과도하게 사용한 %ARG는% 쓰러져버렸다…
	CALL DEAD_CHARA(ARG)
	CALL SKILLGAGE_F_SET(ARG,SKILL_NUM,0)
	CFLAG:ARG:입력행동 = SKILLGAGE_F_GET(ARG,ABSOURPTION)
	CFLAG:ARG:타겟 = SKILLGAGE_H_GET(ARG,ABSOURPTION)
	CALL SKILLGAGE_F_SET(ARG,ABSOURPTION,-70)
	CALL SKILLGAGE_H_SET(ARG,ABSOURPTION,0)
	RETURN 0
ENDIF
;クールタイムの有無に応じてFゲージをセットするスキルを決定
CALL SKILLGAGE_F_SET(ARG,SKILLGAGE_F_GET(ARG,ABSOURPTION) > -1 ? SKILL_NUM # ABSOURPTION,L_GAGE)
RETURN 0

