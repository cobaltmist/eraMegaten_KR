L_GAGE = SKILLGAGE_F_GET(ARG,SKILL_NUM)
IF GET_STATE(CFLAG:ARG:상태이상) == "DYING"
	;死んだ場合はゲージを初期値にリセットする
	L_MAX_F = TOINT(AUTO_SPLIT("7_8_9_12","_",TOINT(CSTR:ARG:103)-1))
	SIF L_GAGE != L_MAX_F
		CALL SKILLGAGE_F_SET(ARG,SKILL_NUM,L_MAX_F)
	RETURN 0
ENDIF
