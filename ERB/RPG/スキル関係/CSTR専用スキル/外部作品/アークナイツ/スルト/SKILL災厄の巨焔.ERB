IF SKILLGAGE_F_GET(ARG,SKILL_NUM) < 0
	SELECTCASE ARGS
		CASE "気合集中フラグ確保"
			;気合、コンセフラグを一時的に確保
			SIF FC_FLAG:0 == 1 && FC_FLAG:1 == 1
				RETURNF 1
			IF CFLAG:ARG:기합플래그 > 0
				FC_FLAG:0 = 1
			ENDIF
			IF CFLAG:ARG:집중플래그 > 0
				FC_FLAG:1 = 1
			ENDIF
		CASE "ダメージ↑"
			;気合、コンセフラグを一時的に確保
			IF CFLAG:ARG:기합플래그 > 0
				FC_FLAG:0 = 1
			ENDIF
			IF CFLAG:ARG:집중플래그 > 0
				FC_FLAG:1 = 1
			ENDIF
			;与ダメージとかに挟む場合の処理
			SELECTCASE SKILL_RANK
				CASE 1
					RETURNF 25
				CASE 2
					RETURNF 33
				CASE 3
					RETURNF 40
				CASE 4
					RETURNF 60
			ENDSELECT
		CASE "ダメージ条件↑"
			;特定条件時にダメージ増加
			;この場合、対象が一体だった場合にのみ、与ダメージ上昇
			SELECTCASE CFLAG:ARG:타겟
				CASE 17
					L_E_COUNT:0 = 1
					L_E_COUNT:1 = 4
				CASE 18 
					L_E_COUNT:0 = 3
					L_E_COUNT:1 = 7
				CASE 19
					L_E_COUNT:0 = 1
					L_E_COUNT:1 = 7
				CASE 20
					L_E_COUNT:0 = 7
					L_E_COUNT:1 = 12
				CASE 21
					L_E_COUNT:0 = 12
					L_E_COUNT:1 = 17
				CASE 22
					L_E_COUNT:0 = 7
					L_E_COUNT:1 = 17
			ENDSELECT
			FOR COUNT,L_E_COUNT:0,L_E_COUNT:1
				SIF POS(COUNT) > -1 && GET_STATE(CFLAG:POS(COUNT):상태이상) != "DYING"
					ALIVE_FLAG += 1
				SIF ALIVE_FLAG > 1
					BREAK
			NEXT
			IF ALIVE_FLAG < 2
				SELECTCASE SKILL_RANK
					CASE 1
						RETURNF 45
					CASE 2
						RETURNF 37
					CASE 3
						RETURNF 35
					CASE 4
						RETURNF 20
				ENDSELECT
			ENDIF
		CASE "気合集中フラグ返却"
			IF FC_FLAG:0 == 1
				FC_FLAG:0 = 0
				CFLAG:ARG:기합플래그 = 1
			ENDIF
			IF FC_FLAG:1 == 1
				FC_FLAG:1 = 0
				CFLAG:ARG:집중플래그 = 1
			ENDIF
		;CASE "命中判定↑"
		;	;命中判定に挟む場合の処理
