@참가조건표시_4603,ARG
SELECTCASE ARG
	CASE 1
		PRINTFORM 아쿠아다인
	CASE 2
		PRINTFORM 마그다인
ENDSELECT

@発動者조건_4603 , ARG
;마그다인を使えること
SIF HAVE_SKILL(ARG, [[스킬:마그다인]]) == 0
	RETURN 0
CALL CHECK_ACTIONABLE , ARG , [[스킬:마그다인]]
SIF RESULT
	RETURN 1
RETURN 0


@참가者조건_4603, ARG , ARG:1
SIF CPOS(ARG) < 1 || CPOS(ARG) > 6
	RETURN 0
CALL CHECK_COST_LINCAGE, ARG, 4603, ARG:1
SIF RESULT == 0
	RETURN 0
SELECTCASE ARG:1
	CASE 1
		;아쿠아다인を使えること
		IF HAVE_SKILL(ARG, [[스킬:아쿠아다인]])
			CALL CHECK_ACTIONABLE , ARG , [[스킬:아쿠아다인]]
			SIF RESULT
				RETURN 1
		ENDIF
		RETURN 0
ENDSELECT

