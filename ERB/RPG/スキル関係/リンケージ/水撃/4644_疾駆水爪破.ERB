@참가조건표시_4644,ARG
SELECTCASE ARG
	CASE 1
		PRINTFORM 수격계 마법
	CASE 2
		PRINTFORM 질풍계 마법
	CASE 3
		PRINTFORM 마하・아쿠아
ENDSELECT

@발동자조건_4644 , ARG
;마하・아쿠아を使えること
SIF HAVE_SKILL(ARG, [[스킬:마하・아쿠아]]) == 0
	RETURN 0
CALL CHECK_ACTIONABLE , ARG , [[스킬:마하・아쿠아]]
SIF RESULT
	RETURN 1
RETURN 0


@참가자조건_4644, ARG , ARG:1
SIF CPOS(ARG) < 1 || CPOS(ARG) > 6
	RETURN 0
CALL CHECK_COST_LINCAGE, ARG, 4644, ARG:1
SIF RESULT == 0
	RETURN 0
SELECTCASE ARG:1
	CASE 1
		;수격相性의 공격마법스킬を사용가능なこと
		CALL HAVE_SKILL_SEARCH(ARG, "수격", , "공격", "마법", 4644)
		RETURN RESULT
	CASE 2
		;질풍相性의 공격마법스킬を사용가능なこと
		CALL HAVE_SKILL_SEARCH(ARG, "질풍", , "공격", "마법", 4644)
		RETURN RESULT
ENDSELECT

