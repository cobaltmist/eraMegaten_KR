@SKILL_HIT_EFFECT_59,ARG,ARG:1
SIF FLAG:확산플래그 == 1
	PRINTFORML 　　TARGET:[{CPOS(ARG:1),2}] %CALLNAME:(ARG:1),20,LEFT%　>>>>>>　상성을 관통했다！

;====================================================
;스킬及び行動の実行
;====================================================
@ACTION_59,ARG,ARG:1
LOCAL = POS(ARG:1)
A = 0
B = 0
C = 0
D = 0
FLAG:확산플래그 = 0

CALL 상성소질체크, ARG, GET_TYPE_NUM("검격"), 1, 1
IF RESULT == 1 && CFLAG:ARG:집중플래그 > 0
	IF (MAXBASE:LOCAL:검격 < 100 || MAXBASE:LOCAL:검격 == 999) && CFLAG:LOCAL:검격가드킬 < 1
		CFLAG:LOCAL:검격가드킬 = 1
		A = 1
	ENDIF
	IF CFLAG:LOCAL:물리반사플래그 == 1
		B = CFLAG:LOCAL:물리반사플래그
		CFLAG:LOCAL:물리반사플래그 = 0
	ENDIF
	IF CFLAG:LOCAL:무효플래그 > 0
		C = CFLAG:LOCAL:무효플래그
		CFLAG:LOCAL:무효플래그 = 0
	ENDIF
	IF CFLAG:LOCAL:검격무효화횟수 > 0
		D = CFLAG:LOCAL:검격무효화횟수
		CFLAG:LOCAL:검격무효화횟수 = 0
	ENDIF
	IF A + B + C + D > 0
		FLAG:확산플래그 = 1
		CFLAG:ARG:집중플래그 = 0
	ENDIF
ENDIF
CALL ATTACK_SINGLE,ARG,LOCAL,59

SIF A == 1
	CFLAG:LOCAL:검격가드킬 = 0
SIF B > 0
	CFLAG:LOCAL:물리반사플래그 = B
SIF C > 0
	CFLAG:LOCAL:무효플래그 = C
SIF D > 0
	CFLAG:LOCAL:검격무효화횟수 = D
FLAG:확산플래그 = 0

