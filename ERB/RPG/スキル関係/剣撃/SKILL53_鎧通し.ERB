;====================================================
;스킬 및 행동의 실행
;====================================================
@ACTION_53,ARG,ARG:1
LOCAL = POS(ARG:1)
A = 0

IF CFLAG:LOCAL:검격무효화횟수 < 1 && CFLAG:LOCAL:물리반사플래그 < 1 && CFLAG:LOCAL:무효플래그 < 1
	IF BASE:LOCAL:검격 < 100 && BASE:LOCAL:검격 >= 0 && CFLAG:LOCAL:검격가드킬 < 1
		CFLAG:LOCAL:검격가드킬 = 1
		A = 1
	ENDIF
	SIF A > 0
		PRINTFORML [{LOCAL}]%CALLNAME:LOCAL,20% >>>>> 상성을 관통했다！
ENDIF
CALL ATTACK_SINGLE,ARG,LOCAL,53


SIF A == 1
	CFLAG:LOCAL:검격가드킬 = 0

