;스킬及び行動の実行
;====================================================
@ACTION_2502,ARG,ARG:1
#LOCALSIZE 1
#DIM CHARA
FOR LOCAL,1,7
	CHARA = POS(LOCAL)
	SIF CHARA < 0 || GROUPMATCH(GET_STATE(CFLAG:CHARA:ステート),"DYING","STONE")
		CONTINUE
	IF EQUIP:CHARA:악세사리 == 5499
		PRINTFORMW [{LOCAL,2}] %CALLNAME:CHARA,20,LEFT%  >>>>>>  DEBUG-BLOCK
		RETURN
	ENDIF
	PRINTFORM [{LOCAL,2}] %CALLNAME:CHARA,20,LEFT%  >>>>>>  
	CALL BTL_CALC_DAMAGE_EXE(ARG:0, CHARA, 99999, GET_TYPE_NUM("만능"), 2, 0)
NEXT




