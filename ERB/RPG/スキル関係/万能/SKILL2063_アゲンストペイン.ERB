;세트専用
;非常に高いクリティカル率を誇る만능물리속성공격
;명중弱体の影響を倍受けるので、弱体しとこうという感じに

;스킬名
@SKILL_NAME_2063,ARG
RESULTS = 어게인스트페인

;　명중
@SKILL_HITRATE_2063, ARG
SIF ARG == -1
	RETURN 100
;통상のンダに加えて판정を受ける
RETURN 100 * MAX(32, 32 + CFLAG:ARG:명중강화) / 32
;　위력
@SKILL_POWER_2063, ARG
RETURN 500

;　최저공격횟수
@SKILL_MINATTACKNUMBER_2063,ARG
RETURN 1

;　최대공격횟수
@SKILL_MAXATTACKNUMBER_2063,ARG
RETURN 1

@SKILL_CRITICAL_RATE_2063, ARG
RETURN 60
;　ＨＰ2を支払うかＭＰ3を支払うか
@SKILL_COSTTYPE_2063, ARG
RETURN 3
;소비なし＝1

;　コストの量
@SKILL_COST_2063,ARG
RETURN 0

;　相性
@SKILL_TYPE_2063,ARG
RETURN 17
;만능

;　1EXTRAか2MAGICか
@SKILL_DECIDE_TYPE_2063, ARG
RETURN 1
;EX

;　1물리데미지で計算するか2마법데미지で計算するか
@SKILL_DAMAGETYPE_2063, ARG
RETURN 1


;　대상が1적か2아군か3場전체か4特殊か
@SKILL_TARGET_2063
RETURN 1

;　사거리
@SKILL_RANGE_2063,ARG
RETURN 1

;　범위　1 1체　2 1열　3 전체　4 적아군전체
@SKILL_SPHERE_2063,ARG
RETURN 1

;　 効果の타입　1공격、2회복、3상태변화、4特殊、5상태회복
@SKILL_EFECT_2063
RETURN 1

;　 特殊処理の有無、及び処理内容
@SKILL_SPECIALEFECT_2063,ARG
;ARG = 1だった場合は処理の有無を返します
SIF ARG == 1
	RETURN 0
;以下に特殊な処理を記述します

;계승타입 継承不能
@SKILL_SUCCESSION_TYPE_2063
RETURN 20

;속도보정
@SKILL_SPEED_2063, ARG = -1
RETURN 0

;　구상の표시とその타입
@SKILL_MESSAGE_2063,ARG

;전투時以外に사용できるか
@SKILL_ACTIONABLE_FIELD_2063
RETURN 0

;스킬변화？用랭크
@SKILL_RANK_2063
RETURN 0

;テン플레이트用、추가효과シリーズ
;추가효과스테이터스
@SKILL_ADDTIONAL_STATE_2063,ARG
RETURN 0


;추가효과상성
@SKILL_ADDTIONAL_TYPE_2063,ARG
RETURN 0

;추가효과기본부여확률
@SKILL_ADDTIONAL_BASE_RATE_2063,ARG
RETURN 0

;추가효과부여확률上限
@SKILL_ADDTIONAL_MAX_RATE_2063,ARG
RETURN 0

@적専用_2063
RETURN 1
;====================================================
;스킬及び行動の実行
;====================================================
@ACTION_2063,ARG,ARG:1
CALL ATTACK_SINGLE, ARG, POS(ARG:1), 2063






;====================================================
;実行内容ここまで
;====================================================

;　 설명文の표시
@SKILL_EXPLAIN_2063
PRINTL [전열] 전열의 적 1체에 만능속성 초특대 물리데미지. 크리티컬이 발생하기 쉽다.

;継承に필요한部位
@継承部位_2063,ARG
RETURN 1

