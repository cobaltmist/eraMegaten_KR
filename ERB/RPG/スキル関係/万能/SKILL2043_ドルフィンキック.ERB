;====================================================
;스킬及び行動の実行
;====================================================
@ACTION_2043,ARG,ARG:1
IF ARG:1 == 22
	LOCAL:1 = 7
	LOCAL:2 = 17
ELSE
	LOCAL:1 = 1
	LOCAL:2 = 7
ENDIF

FOR LOCAL,LOCAL:1,LOCAL:2
	LOCALS = 포지션{LOCAL}
	SIF FLAG:LOCALS < 0 || FLAG:LOCALS == ARG
		CONTINUE
	SIF GET_STATE(CFLAG:(FLAG:LOCALS):상태이상) == "DYING"
		CONTINUE
	PRINTFORM 　　TARGET:[{CPOS(FLAG:LOCALS),2}] %CALLNAME:(FLAG:LOCALS),20,LEFT%　>>>>>>　
	CALLFORM JUDG_HIT_1,ARG,FLAG:LOCALS,50
	IF RESULT == 1 && CFLAG:(FLAG:LOCALS):보스플래그 == 0
		;명중した
		SELECTCASE LOCAL
			CASE 1 TO 3,7 TO 11
				PRINTFORMW {BASE:(FLAG:LOCALS):ＨＰ/2} 데미지
				CALL VAR_HP,FLAG:LOCALS,-(BASE:(FLAG:LOCALS):ＨＰ/2),0
			CASEELSE
				PRINTFORMW {BASE:(FLAG:LOCALS):ＨＰ/4} 데미지
				CALL VAR_HP,FLAG:LOCALS,-(BASE:(FLAG:LOCALS):ＨＰ/4),0
		ENDSELECT
	ELSE
		;外した
		PRINTW MISS
	ENDIF
NEXT





