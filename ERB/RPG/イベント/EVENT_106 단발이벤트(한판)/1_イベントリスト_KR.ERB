
;이벤트플래그:19:0 を参照する部分
;32個(CASE 31)まで작성가능
@EVENT_106_LIST_0(ARG,ARG:1)
;ARG:1 0 = 타이틀
;　　　1 = 출현체크
;　　　2 = 이벤트
;
SELECTCASE ARG
CASE 0 ;세라픽게이트
	IF ARG:1 == 0
		IF 세라픽게이트진행도 == 0
			RESULTS = 【EX:발키리 프로파일】차원 게이트 출현

		ELSEIF 세라픽게이트진행도 == 1
			RESULTS = 【EX:발키리 프로파일】세라픽 게이트 녹색 에리어 최심부

		ELSEIF 세라픽게이트진행도 == 2
			RESULTS = 【EX:발키리 프로파일】세라픽 게이트 백색 에리어 최심부
		ENDIF
		
	ELSEIF ARG:1 == 1
		SIF GETBIT(FLAG:EXTRA이벤트설정, 0) == 0
			RETURN 0

		IF 세라픽게이트진행도 == 0 && FLAG:경험ED수 > 4 && BASE:MASTER:LV >= 60
			RETURN 1
		ELSEIF 세라픽게이트진행도 == 1
			RETURN 1
		ELSEIF 세라픽게이트진행도 == 2
			RETURN 1
		ELSE
			RETURN 0
		ENDIF

		RETURN 0

	ELSEIF ARG:1 == 2
		CALL 세라픽게이트
	ENDIF

CASE 1
	IF ARG:1 == 0
		RESULTS = 【EX:원신】단죄의 황녀

	ELSEIF ARG:1 == 1
		SIF GETBIT(FLAG:EXTRA이벤트설정, 0) == 0
			RETURN 0

		SIF BASE:MASTER:LV >= 20
			RETURN 1
		
		RETURN 0
	
	ELSEIF ARG:1 == 2
		CALL 단죄의황녀
		RETURN 1
	ENDIF

CASEELSE
	RETURN 0
ENDSELECT
RETURN 0