;====================
;조건
;====================
@PLAYABLE_EVENT_74
#LOCALSIZE 2
#LOCALSSIZE 2
;시설 타격후. 어쌔신이 나타났을 때
IF 의뢰플래그:62:0 == 3
	RESULTS = XCOM2 - 앞으로의 계획에 관하여
	RETURN 1

;섄의 마지막 선물 작전 이후 새로운 DLC 임무
ELSEIF 의뢰플래그:62:0 == 22
	RESULTS = XCOM2 - 발렌 박사의 실험체
	RETURN 1

;외계인 사냥꾼 DLC 완료.
ELSEIF 의뢰플래그:62:0 == 29
	RESULTS = XCOM2 - 외계인 사냥꾼
	RETURN 1

ENDIF
RETURN 0

;====================
;내용
;====================
@SHOPCOM_EVENT_74
#LOCALSIZE 2
#LOCALSSIZE 2
;시설 타격후. 어쌔신이 나타났을 때
IF 의뢰플래그:62:0 == 3
	CALL EVENT_74_1
	RETURN 0
ENDIF

;섄의 마지막 선물 작전 이후 새로운 DLC 임무
IF 의뢰플래그:62:0 == 22
	CALL EVENT_74_2
	RETURN 0
ENDIF

;외계인 사냥꾼 DLC 완료.
IF 의뢰플래그:62:0 == 29
	CALL EVENT_74_3
	RETURN 0
ENDIF

@EVENT_74_FLAG_RESET
#LOCALSIZE 2
#LOCALSSIZE 2
SIF GETBIT(FLAG:이벤트출현2,74 - 64)
	CLEARBIT FLAG:이벤트출현2, 74 - 64