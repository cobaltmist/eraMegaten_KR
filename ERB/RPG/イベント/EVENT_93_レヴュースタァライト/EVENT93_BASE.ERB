;   依頼登録フラグをリセットする
;-------------------------------------------------
@EVENT_93_FLAG_RESET
;- フラグがONの場合、クリアしてOFFに変更
SIF GETBIT(FLAG:이벤트출현2,93 - 64)
	CLEARBIT FLAG:이벤트출현2, 93 - 64

;====================
;조건
;====================

@PLAYABLE_EVENT_93
#DIM DYNAMIC 先生の数
	;イベント蹴ったら出ない
	SIF 이벤트플래그:93:0 <0
		RETURN 0
		
	LOADGLOBAL
	
	;仕様変更に伴うセーブデータの互換を保つ処理
	LOCAL = GETCHARA([[キャラ:카렌（무대소녀）]])
	;顔グラ解禁されてない場合
	IF LOCAL > -1 && CFLAG:LOCAL:제한얼굴이미지개방 < 1
		;愛城華恋トップスタァイベントを通過しているのなら
		IF 다이바나나의기억:3:1
			;顔グラ解禁処理を入れて、
			CFLAG:LOCAL:제한얼굴이미지개방 = 1
			;旧アドレスを新アドレスに変更する
			SIF CFLAG:LOCAL:악마변신얼굴이미지 == 3
				CFLAG:LOCAL:악마변신얼굴이미지 = 64
		;トップスタァイベントを通過していない
		ELSE
			;顔グラを変身体にしていた場合、元々のデフォグラに変更
			SIF CFLAG:LOCAL:얼굴이미지 == 3
				CFLAG:LOCAL:얼굴이미지 = 0
			SIF CFLAG:LOCAL:조교얼굴이미지 == 3
				CFLAG:LOCAL:조교얼굴이미지 = 0
			SIF CFLAG:LOCAL:SHOP얼굴이미지 == 3
				CFLAG:LOCAL:SHOP얼굴이미지 = 0
		ENDIF
	ENDIF
	;以下同じ（For使えという意見は甘んじて受け入れる）
	LOCAL = GETCHARA([[キャラ:쥰나]])
	IF LOCAL > -1 && CFLAG:LOCAL:제한얼굴이미지개방 < 1
		IF 다이바나나의기억:3:3
			CFLAG:LOCAL:제한얼굴이미지개방 = 1
			SIF CFLAG:LOCAL:악마변신얼굴이미지 == 3
				CFLAG:LOCAL:악마변신얼굴이미지 = 64
		ELSE
			SIF CFLAG:LOCAL:얼굴이미지 == 3
				CFLAG:LOCAL:얼굴이미지 = 0
			SIF CFLAG:LOCAL:조교얼굴이미지 == 3
				CFLAG:LOCAL:조교얼굴이미지 = 0
			SIF CFLAG:LOCAL:SHOP얼굴이미지 == 3
				CFLAG:LOCAL:SHOP얼굴이미지 = 0
		ENDIF
	ENDIF
	LOCAL = GETCHARA([[キャラ:후타바（무대소녀）]])
	IF LOCAL > -1 && CFLAG:LOCAL:제한얼굴이미지개방 < 1
		IF 다이바나나의기억:3:5
			CFLAG:LOCAL:제한얼굴이미지개방 = 1
			SIF CFLAG:LOCAL:악마변신얼굴이미지 == 3
				CFLAG:LOCAL:악마변신얼굴이미지 = 64
		ELSE
			SIF CFLAG:LOCAL:얼굴이미지 == 3
				CFLAG:LOCAL:얼굴이미지 = 0
			SIF CFLAG:LOCAL:조교얼굴이미지 == 3
				CFLAG:LOCAL:조교얼굴이미지 = 0
			SIF CFLAG:LOCAL:SHOP얼굴이미지 == 3
				CFLAG:LOCAL:SHOP얼굴이미지 = 0
		ENDIF
	ENDIF
	LOCAL = GETCHARA([[キャラ:마야（무대소녀）]])
	IF LOCAL > -1 && CFLAG:LOCAL:제한얼굴이미지개방 < 1
		IF 다이바나나의기억:3:8
			CFLAG:LOCAL:제한얼굴이미지개방 = 1
			SIF CFLAG:LOCAL:악마변신얼굴이미지 == 3
				CFLAG:LOCAL:악마변신얼굴이미지 = 64
		ELSE
			SIF CFLAG:LOCAL:얼굴이미지 == 3
				CFLAG:LOCAL:얼굴이미지 = 0
			SIF CFLAG:LOCAL:조교얼굴이미지 == 3
				CFLAG:LOCAL:조교얼굴이미지 = 0
			SIF CFLAG:LOCAL:SHOP얼굴이미지 == 3
				CFLAG:LOCAL:SHOP얼굴이미지 = 0
		ENDIF
	ENDIF
	LOCAL = GETCHARA([[キャラ:클로딘]])
	IF LOCAL > -1 && CFLAG:LOCAL:제한얼굴이미지개방 < 1
		IF 다이바나나의기억:3:9
			CFLAG:LOCAL:제한얼굴이미지개방 = 1
			SIF CFLAG:LOCAL:악마변신얼굴이미지 == 3
				CFLAG:LOCAL:악마변신얼굴이미지 = 64
		ELSE
			SIF CFLAG:LOCAL:얼굴이미지 == 3
				CFLAG:LOCAL:얼굴이미지 = 0
			SIF CFLAG:LOCAL:조교얼굴이미지 == 3
				CFLAG:LOCAL:조교얼굴이미지 = 0
			SIF CFLAG:LOCAL:SHOP얼굴이미지 == 3
				CFLAG:LOCAL:SHOP얼굴이미지 = 0
		ENDIF
	ENDIF
	
	;メガテン系イベント以外不許可の場合、離脱
	;ただし大場がこっちを認識しているのなら逃れられない
	SIF 다이바나나의기억:0:1 < 2 && (GETBIT(FLAG:EXTRA이벤트설정, 0) == 0 || GETBIT(FLAG:EXTRA이벤트필터, 13) == 1)
		RETURN 0
	
	;組織貢献度10000で解放
	SIF FLAG:조직공헌도 < 10000
		RETURN 0
		
	;이벤트플래그:93:0 が 9 以上のときだけ、特定の悪魔がいるかどうか判定
	IF 이벤트플래그:93:0 > 8
		先生の数 = 0
		IF 보컬리스트・세이렌진행 > 1
			FOR LOCAL, 0, CHARANUM
				;劇場憑きのセイレーンはここで判定
				IF NO:LOCAL == [[キャラ:세이렌]] && CFLAG:LOCAL:998 == 1
					先生の数 ++
					BREAK
				ENDIF
			NEXT
		ENDIF
		;シナリオライター・ジークフリート（大劇場）
		IF 시나리오라이터・지크프리트진행 > 1
			FOR LOCAL, 0, CHARANUM
				;劇場憑きのジークフリートはここで判定
				IF NO:LOCAL == [[キャラ:지크프리트]] && CFLAG:LOCAL:998 == 1
					先生の数 ++
					BREAK
				ENDIF
			NEXT
		ENDIF
		;PRINTV 先生の数
		;PRINTL ﾆｬｰﾝ
		;WAIT
	ENDIF

	;(舞台少女戦制覇後は省略)
	IF 이벤트플래그:93:0 == 0 && GLOBAL:기린의기억 < 1
		RESULTS = （EX:RSL）착신・ki-ringtone
		RETURN 1
	;(舞台少女戦制覇後は省略)
	ELSEIF 이벤트플래그:93:0 == 1 && GLOBAL:기린의기억 < 1
		RESULTS = （EX:RSL）정열의 레뷰 【-Re_PLAY-】
		RETURN 1
	;舞台少女戦＆個別イベント3終了後かつ、マヨ公加入可能フラグ後、上司を名乗る何者かの来訪　舞台少女全員揃い
	ELSEIF 이벤트플래그:93:0 == 8 && EVENT93_個別イベ3終わった判定() > 8 && (이벤트플래그:1:0 > 1 && BASE:MASTER:LV > 40) && (EVENT93_人数確認() > 8)
		RESULTS = （EX:RSL）……상사가 와 있다
		RETURN 1
	;上司襲来後、劇場憑き悪魔のうち、セイレーンとジークフリートがいる
	ELSEIF 이벤트플래그:93:0 == 10 && 先生の数 > 1 && (EVENT93_人数確認() > 8)
		RESULTS = （EX:RSL）무대소녀와 극장의 악마
		RETURN 1
	;先生ズとの顔合わせ後、劇場憑き悪魔のうち、セイレーンとジークフリートがいる
	;2022/04/30　全員実装
	ELSEIF 이벤트플래그:93:0 == 11 && 先生の数 > 1 && (이벤트플래그:93:10 == [[キャラ:카렌（무대소녀）]] || 이벤트플래그:93:10 == [[キャラ:히카리]] || 이벤트플래그:93:10 == [[キャラ:마히루]] || 이벤트플래그:93:10 == [[キャラ:쥰나]] || 이벤트플래그:93:10 == [[キャラ:후타바（무대소녀）]] || 이벤트플래그:93:10 == [[キャラ:카오루코]] || 이벤트플래그:93:10 == [[キャラ:나나]] || 이벤트플래그:93:10 == [[キャラ:마야（무대소녀）]] || 이벤트플래그:93:10 == [[キャラ:클로딘]]) && (EVENT93_人数確認() > 8)
		LOCAL = EVENT93_推しの位置取得()
		;推しのレベルが低すぎる場合、怒られる
		IF BASE:LOCAL:LV < 40
			RESULTS = （EX:RSL）세이렌 선생님이 화났다……
		ELSE
			RESULTS = （EX:RSL）【경고】무대소녀를 합숙에 보낸다【경고】
		ENDIF
		RETURN 1
	;合宿に送ったあと
	ELSEIF (이벤트플래그:93:0 == 12 || 이벤트플래그:93:0 == 13) && 先生の数 > 1 && 이벤트플래그:93:10 > 0
		SELECTCASE 이벤트플래그:93:10
			CASE [[キャラ:나나]]
				IF 다이바나나의기억_L:0:1 < 3
					RESULTS = （EX:RSL）빛나는 무지개와도 같은 행복한 나날을, 영원히
				ELSE
					RESULTS = （EX:RSL）『다이바 나나』
				ENDIF
			CASEELSE
				RESULTS = （EX:RSL）별을 목표로 한 소녀들에게
		ENDSELECT
		RETURN 1
	;기린のありがたいコメント
	ELSEIF 이벤트플래그:93:0 == 9
		RESULTS = （EX:RSL）※지하극장에는 아무도 없다
		RETURN 1
	;舞台少女戦は終わったけど個別イベントの消化がまだ
	ELSEIF 이벤트플래그:93:0 == 8 && EVENT93_個別イベ3終わった判定() < 9
		RESULTS = （EX:RSL）무대소녀와 마주한다
		RETURN 1
	;舞台少女戦が終わってない
	ELSEIF 이벤트플래그:93:0 < 8
		RESULTS = （EX:RSL）기린의 지하극장에서 레뷰에 도전한다
		RETURN 1
	ENDIF
RETURN 0


