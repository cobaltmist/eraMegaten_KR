;=================================================
;   의뢰:フラグリセット
;-------------------------------------------------
;   MENU画面で「依頼出現」が出る様、
;   의뢰登録フラグをリセットする
;-------------------------------------------------
@EVENT_98_FLAG_RESET

;- フラグがONの場合、クリアしてOFFに변경
SIF GETBIT(FLAG:이벤트출현2,98 - 64)
	CLEARBIT FLAG:이벤트출현2, 98 - 64

;====================
;조건
;====================
@PLAYABLE_EVENT_98
;メガテン系이벤트以外불허가の場合
SIF GETBIT(FLAG:EXTRA이벤트설정, 0) == 0 || GETBIT(FLAG:EXTRA이벤트필터, 4) == 1
	RETURN 0
SIF BASE:MASTER:LV < 11
	RETURN 0

IF 이벤트플래그:98:0 == 0
	RESULTS = （Fate ＭＯＤ-1）게임 소프트 「HolyChalice」
	RETURN 1
ELSEIF 이벤트플래그:98:0 == 2
	CALL CHECK_Fate
	IF RESULT == 1
		RESULTS = （Fate ＭＯＤ-2）성배전쟁
		RETURN 1
	ENDIF
ELSEIF 이벤트플래그:98:0 == 3
	CALL CHECK_Fate
	IF RESULT == 1
		RESULTS = （Fate ＭＯＤ-3）개전
		RETURN 1
	ENDIF
ELSEIF 이벤트플래그:98:0 == 4
	CALL CHECK_Fate
	IF RESULT == 1
		RESULTS = （Fate ＭＯＤ-4）만행 蛮行
		RETURN 1
	ENDIF
ELSEIF 이벤트플래그:98:0 == 5
	CALL CHECK_Fate
	IF RESULT == 1
		IF (이벤트플래그:98:2 % 10 == 0 && 이벤트플래그:98:2 / 10 == 2) || (이벤트플래그:98:2 % 10 == 0 && 이벤트플래그:98:2 / 10 == 3)
			RESULTS = （Fate ＭＯＤ-5）음모
		ELSE
			RESULTS = （Fate ＭＯＤ-5）사냥
		ENDIF
		RETURN 1
	ENDIF
ELSEIF 이벤트플래그:98:0 == 6
	CALL CHECK_Fate
	IF RESULT == 1
		RESULTS = （Fate ＭＯＤ-6）교회
		RETURN 1
	ENDIF
	RETURN 1
ELSEIF 이벤트플래그:98:0 == 7
	CALL CHECK_Fate
	IF RESULT == 1
		RESULTS = （EX:Fate ＭＯＤ-7）蟲蔵
		RETURN 1
	ENDIF
	RETURN 1
ELSEIF 이벤트플래그:98:0 == 8
	CALL CHECK_Fate
	IF RESULT == 1
		RESULTS = （EX:Fate ＭＯＤ-8）狂戦士
		RETURN 1
	ENDIF
	RETURN 1

ENDIF

RETURN 0

