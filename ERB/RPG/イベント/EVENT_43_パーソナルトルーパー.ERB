;====================
;条件
;ゲーム開始時点から選択可能
;====================
@PLAYABLE_EVENT_43
#LOCALSIZE 1
#LOCALSSIZE 1
	;PTのEXTRAがOFF
	IF GETBIT(FLAG:EXTRA이벤트설정, 0) == 0 || GETBIT(FLAG:EXTRA이벤트필터, 1) == 1
		;염라대왕タイカのEXTRAもOFF
		;→参照するEXフィルタを19（アマネEX=実質AC5とのクロスオーバーシナリオ）に変更
		IF GETBIT(FLAG:EXTRA이벤트설정, 0) == 0 || GETBIT(FLAG:EXTRA이벤트필터, 19) == 1
			RETURN 0
		ELSEIF !이벤트플래그:43:0
			RESULTS = （EX:eraten X ARMORED CORE）데빌헌터 격파
			;デビルハンター→デビルバスターに変更。「デビルハンター」はDMC側の用語、SMT4のは「人外ハンター」なので。
			이벤트플래그:43:1 = 2
			이벤트플래그:43:9 = -1
			RETURN 1
		ELSE
			RETURN 0
		ENDIF
	ELSEIF 이벤트플래그:43:0 == 0 && BASE:MASTER:LV > 9
		RESULTS =  （EX:PT）환영의 유령
		이벤트플래그:43:9 = 1
		RETURN 1
	;Mr.K작성ルート
	ELSEIF 이벤트플래그:43:1 == 0
		IF 이벤트플래그:43:0 == 1 && (FLAG:조직공헌도 >= 100000 && BASE:MASTER:LV > 24)
			IF GET_CHARAPARAM([[キャラ:팬텀]], "LV") > 25
				RESULTS = （EX:PT）야밤의 고철과 석양의 백기사
				이벤트플래그:43:9 = 2
				RETURN 1
			ENDIF
		ELSEIF 이벤트플래그:43:0 == 2 && BASE:MASTER:LV > 34
			RESULTS = （EX:PT）크로스게이트에서 나타나는 자
			이벤트플래그:43:9 = 3
			RETURN 1
		ELSEIF 이벤트플래그:43:0 == 3 && BASE:MASTER:LV > 34 && GET_CHARAPARAM([[キャラ:팬텀]], "LV", 1) > 34 && GET_CHARAPARAM([[キャラ:나하트]], "LV", 1) > 34 && GET_CHARAPARAM([[キャラ:아벤트]], "LV", 1) > 34
				RESULTS = （EX:PT）「무한의 프론티어 (그들이 태어난 세상)」
					이벤트플래그:43:9 = 4
				RETURN 1
		ENDIF
	;別人추가ルート
	ELSEIF 이벤트플래그:43:1 == 1
		IF 이벤트플래그:43:0 == 1 && (FLAG:조직공헌도 >= 100000 && BASE:MASTER:LV > 24)
			IF GET_CHARAPARAM([[キャラ:팬텀]], "LV") > 25
				RESULTS = （EX:PT）시험운용데이터의 입수
				이벤트플래그:43:9 = 12
				RETURN 1
			ENDIF
		ELSEIF 이벤트플래그:43:0 == 2
			RESULTS = （EX:eraten X 機動戦士ガンダム）「데빌수트」
			이벤트플래그:43:9 = 13
			RETURN 1
		ENDIF
	ENDIF

	;スパロボ好き加筆
	IF INRANGE(이벤트플래그:43:0, 1, 98)
		SIF FINDCHARA_B([[キャラ:팬텀]], 0) > -1
			CFLAG:(RESULT:1):이벤트가입 = 1
		IF !GETBIT(이벤트플래그:43:3, 0) && GET_CHARAPARAM([[キャラ:팬텀]], "LV", 1) > 29
			RESULTS = （EX:PT）유령의 환영은 그 몸을 얻는다
			이벤트플래그:43:9 = 21
			RETURN 1
		ENDIF
	ENDIF

