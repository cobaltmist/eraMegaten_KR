;昔のスキルから変更する処理
;スキル順変更に対応
@EVENT_43_SKILLCHANGE(ARG)
#LOCALSIZE 1
#LOCALSSIZE 1
SIF !INRANGE(ARG, 0, CHARANUM-1)
	RETURN
SELECTCASE NO:ARG
	CASE [[キャラ:팬텀]]
		LOCAL = HAVE_SKILL(ARG, [[스킬:메기도라]], 1)
		IF LOCAL
			LOCALS = 스킬{LOCAL}
			ABL:ARG:LOCALS = [[스킬:플래시봄]]
		ENDIF
	CASE [[キャラ:아벤트]]
	CASE [[キャラ:나하트]]
ENDSELECT

;初期悪魔のスキルを所持しているロボットに合わせる処理
@EVENT_43_SKILL_SYNC(PILOT, ROBOT)
#DIM PILOT
#DIM ROBOT
SIF !INRANGE(PILOT, 0, CHARANUM) || !INRANGE(ROBOT, 0, CHARANUM)
	RETURN

FOR LOCAL, 1, FLAG:스킬수+1
	SIF ABL:ROBOT:("스킬" + TOSTR(LOCAL))
		ABL:PILOT:("초기변신악마스킬" + TOSTR(LOCAL)) = ABL:ROBOT:("스킬" + TOSTR(LOCAL))
NEXT

FOR LOCAL, 1, 21
	LOCAL:1 = ABL:ROBOT:("습득스킬" + TOSTR(LOCAL))
	IF LOCAL:1
		ABL:PILOT:("초기변신악마습득스킬" + TOSTR(LOCAL)) = LOCAL:1
		ABL:PILOT:("초기변신악마습득LV" + TOSTR(LOCAL)) = ABL:ROBOT:("습득LV" + TOSTR(LOCAL))
	ENDIF
NEXT




