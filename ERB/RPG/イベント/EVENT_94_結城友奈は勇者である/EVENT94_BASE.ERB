@PLAYABLE_EVENT_94
;メガテン系이벤트이외불허가の場合
SIF GETBIT(FLAG:EXTRA이벤트설정, 0) == 0 || GETBIT(FLAG:EXTRA이벤트필터, 2) == 1
	RETURN 0
	
;초기개방
IF 이벤트플래그:94:0 == 0
	RESULTS = (EX:유우키 유우나는 용사다 MOD) 프롤로그
	RETURN 1
ENDIF
IF 이벤트플래그:94:0 == 1
	RESULTS = (EX:유우키 유우나는 용사다 MOD) 가르쳐 줘！오오쿠니누시 선생님！
	RETURN 1
ENDIF
IF 이벤트플래그:94:0 == 2
	RESULTS = (EX:유우키 유우나는 용사다 MOD) 어서 와！용사부에！
	RETURN 1
ENDIF
IF 이벤트플래그:94:0 == 3 && 이벤트플래그:94:10 == 0
	RESULTS = (EX:유우키 유우나는 용사다 MOD) 보너스
	RETURN 1
ENDIF
RETURN 0


;RETURN 1 で이벤트출현
;====================
;内容
;====================
@SHOPCOM_EVENT_94
;プロローグ
IF 이벤트플래그:94:0 == 0
	CALL EVENT_94_0
	RETURN 0
ELSE
ENDIF
;教えて！오오쿠니누시先生！
IF 이벤트플래그:94:0 == 1
	CALL EVENT_94_1
	RETURN 0
ELSE
ENDIF
;ようこそ勇者部へ！
IF 이벤트플래그:94:0 == 2
	CALL EVENT_94_2
	RETURN 0
ELSE
ENDIF
;おまけとか
IF 이벤트플래그:94:0 == 3 && 이벤트플래그:94:10 == 0
	CALL EVENT_94_3
	RETURN 0
ELSE
ENDIF


