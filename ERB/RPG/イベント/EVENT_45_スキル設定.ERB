	스킬数 = 12
ENDIF

이벤트플래그:45:19 = 0
FOR LOCAL, 1, (스킬数+1)
	IF 이벤트플래그:45:LOCAL > 0 && 이벤트플래그:45:LOCAL != (3900 + 이벤트플래그:45:14)
		TRYCALLFORM SKILL_COST_{이벤트플래그:45:LOCAL}
			이벤트플래그:45:16 += RESULT
			이벤트플래그:45:19 += 1
	ENDIF
NEXT
이벤트플래그:45:16 *= 100+(50*(이벤트플래그:45:19-1))
이벤트플래그:45:16 /=  100
이벤트플래그:45:19 = 1
;-----------------------------
;속도보정
IF 이벤트플래그:45:18 > 0
	이벤트플래그:45:16 += 이벤트플래그:45:18
ELSEIF 이벤트플래그:45:18 < 0
	;減っても10まで
	이벤트플래그:45:16 -= MIN(10, 이벤트플래그:45:18 / 2)
ENDIF

;만능속성보정　
IF 이벤트플래그:45:14 == 0
	;万能属性補正
	IF 이벤트플래그:45:8 == 17
		이벤트플래그:45:16 += 10
	;신경～呪殺逆補正
	ELSEIF INRANGE(이벤트플래그:45:14, 9, 12)
		이벤트플래그:45:16 -= 10
	ENDIF
ENDIF
;異常属性
IF 이벤트플래그:45:14 == 2
	;万能属性補正
	IF 이벤트플래그:45:12 == 17
		이벤트플래그:45:16 += 5
	;신경～呪殺逆補正
	ELSEIF INRANGE(이벤트플래그:45:14, 9, 12)
		이벤트플래그:45:16 -= 5
	ENDIF
ELSEIF 이벤트플래그:45:14 == 0
	;万能属性補正
	IF 이벤트플래그:45:9 == 17
		이벤트플래그:45:16 += 5
	;신경～呪殺逆補正
	ELSEIF INRANGE(이벤트플래그:45:9, 9, 12)
		이벤트플래그:45:16 -= 5
	ENDIF
ENDIF

;HPを支払う場合は二倍
;正直コスト2倍は軽すぎる気がしないでもけど保留
IF 이벤트플래그:45:17 == 2
	이벤트플래그:45:16 *= 2
ENDIF

