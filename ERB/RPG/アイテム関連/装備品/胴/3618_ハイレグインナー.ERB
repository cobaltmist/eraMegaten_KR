@전투능력수정_3618,ARG,ARG:1 = -1

SELECTCASE ARG
	;방어60
	CASE 2
		RETURN 60
	;회피20
	CASE 3
		IF ARG:1 > -1
			SELECTCASE ABL:(ARG:1):노출증
				CASE 0
					RETURN -10
				CASE 1
					RETURN 10
				CASE IS < 4
					RETURN 20
				CASEELSE
					RETURN 25
			ENDSELECT
		ELSE
			RETURN 20
		ENDIF
	CASE 5
		RETURN 6
ENDSELECT
RETURN 0
