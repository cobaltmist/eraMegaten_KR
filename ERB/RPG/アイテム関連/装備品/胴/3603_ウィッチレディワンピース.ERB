@기본능력수정_3603,ARG,ARG:1 = -1
SELECTCASE ARG
ENDSELECT
RETURN 0

@전투능력수정_3603,ARG,ARG:1 = -1
SELECTCASE ARG
	CASE 2
		SIF ARG:1 == -1
			RETURN 5
		IF EQUIP:(ARG:1):머리 == 3203 && EQUIP:(ARG:1):발 == 4601
			LOCAL = BASE:(ARG:1):LV / 3
			LOCAL += ABL:(ARG:1):Ｃ감각 + ABL:(ARG:1):Ｖ감각 + ABL:(ARG:1):Ａ감각 + ABL:(ARG:1):Ｂ감각
			SIF LOCAL < 5
				LOCAL = 5
			RETURN LOCAL
		ELSE
			RETURN 5
		ENDIF
	CASE 3
		SIF ARG:1 == -1
			RETURN 5
		IF EQUIP:(ARG:1):머리 == 3203 && EQUIP:(ARG:1):발 == 4601
			LOCAL = BASE:(ARG:1):LV / 3
			RETURN LOCAL
		ELSE
			RETURN 5
		ENDIF
	CASE 4
		SIF ARG:1 == -1
			RETURN 3
		IF EQUIP:(ARG:1):머리 == 3203 && EQUIP:(ARG:1):발 == 4601
			LOCAL = BASE:(ARG:1):LV / 4
			LOCAL += ABL:(ARG:1):Ｃ감각 + ABL:(ARG:1):Ｖ감각 + ABL:(ARG:1):Ａ감각 + ABL:(ARG:1):Ｂ감각
			SIF LOCAL < 3
				LOCAL = 3
			RETURN LOCAL
		ELSE
			RETURN 3
		ENDIF
	CASE 5
		SIF ARG:1 == -1
			RETURN 2
		IF EQUIP:(ARG:1):머리 == 3203 && EQUIP:(ARG:1):발 == 4601
			LOCAL = BASE:(ARG:1):LV / 4 + 2
			RETURN LOCAL
		ELSE
			RETURN 3
		ENDIF
ENDSELECT
RETURN 0

