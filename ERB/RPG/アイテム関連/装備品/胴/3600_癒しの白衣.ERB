@기본능력수정_3600,ARG,ARG:1 = -1

SELECTCASE ARG
ENDSELECT
RETURN 0

@전투능력수정_3600,ARG,ARG:1 = -1

SELECTCASE ARG
	CASE 2
		SIF ARG:1 == -1
			RETURN 5
		IF EQUIP:(ARG:1):머리 == 3200 && EQUIP:(ARG:1):발 == 4600
			LOCAL = BASE:(ARG:1):LV / 2 * 3
			SIF TALENT:(ARG:1):치료
				LOCAL = LOCAL * 3 / 2
			RETURN LOCAL
		ELSE
			RETURN 5
		ENDIF
	CASE 3
		SIF ARG:1 == -1
			RETURN 5
		IF EQUIP:(ARG:1):머리 == 3200 && EQUIP:(ARG:1):발 == 4600
			LOCAL = BASE:(ARG:1):LV / 2
			SIF TALENT:(ARG:1):치료
				LOCAL += LOCAL * 3 / 5
			RETURN LOCAL
		ELSE
			RETURN 5
		ENDIF
	CASE 5
		SIF ARG:1 == -1
			RETURN 5
		SIF TALENT:(ARG:1):치료
			RETURN 10
		RETURN 5
ENDSELECT
RETURN 0

