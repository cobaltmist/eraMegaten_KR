@전투능력수정_4528,ARG,ARG:1 = -1

SELECTCASE ARG
	;防御2
	CASE 2
		RETURN 7
	;回避10
	CASE 3
		IF ARG:1 > -1
			SELECTCASE ABL:(ARG:1):노출증
				CASE 0
					RETURN -5
				CASE 1
					RETURN 5
				CASE IS < 4
					IF EQUIP:(ARG:1):머리 == 3004 && EQUIP:(ARG:1):몸통 == 3545
						RETURN 17
					ELSE
						RETURN 12
					ENDIF
				CASEELSE
					IF EQUIP:(ARG:1):머리 == 3004 && EQUIP:(ARG:1):몸통 == 3545
						RETURN 22
					ELSE
						RETURN 17
					ENDIF
			ENDSELECT
		ELSE
			RETURN 12
		ENDIF
	CASE 5
		RETURN 3
ENDSELECT
RETURN 0

