@PLAYABLE_REQUEST_12
SELECTCASE 의뢰플래그:12:0
	CASE 0
		IF 던전フラグ:61:0 == 5
			RESULTS = 반란군으로부터의 사자
			RETURN 1
		ENDIF
	CASE 1
		IF 던전フラグ:63:0 == 8
			RESULTS = 대마왕의 딸
			RETURN 1
		ENDIF
	CASE 2
		IF 이벤트플래그:25:0 == 1
			RESULTS = 천계로부터의 침략
			RETURN 1
		ENDIF
ENDSELECT
RETURN 0

