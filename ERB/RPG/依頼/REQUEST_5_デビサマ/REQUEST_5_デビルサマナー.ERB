@PLAYABLE_REQUEST_5
SELECTCASE 의뢰フラグ:5:0
	CASE 0
		IF (던전フラグ:1:1 == 1 || 던전フラグ:34:8 == 1 || 던전フラグ:61:0 == 5)
			RESULTS = 교수댁 조사 의뢰
			RETURN 1
		ENDIF
	CASE 1
		IF 던전フラグ:49:0 == 1
			RESULTS = 교수의 은신처
			RETURN 1
		ENDIF
	CASE 2
		;IF 던전フラグ:81:0 == 1
		IF 던전フラグ:79:0 == 1
			RESULTS = 불온분자 배제
			RETURN 1
		ENDIF
	CASE 3
		;古墳迷宮クリア：エンディング
		IF 던전フラグ:85:0 == 1
			RESULTS = 데빌서머너
			RETURN 1
		ENDIF

ENDSELECT
RETURN 0

@EXPLAIN_REQUEST_5
IF 의뢰フラグ:5:0 == 0
	CALL EXPLAIN_REQUEST_5_1
ELSEIF 의뢰フラグ:5:0 == 1 && 던전フラグ:49:0 == 1
	CALL EXPLAIN_REQUEST_5_2
ELSEIF 의뢰フラグ:5:0 == 2 && 던전フラグ:79:0 == 1;&& 던전フラグ:81:0 == 1
	CALL EXPLAIN_REQUEST_5_3
ELSEIF 의뢰フラグ:5:0 == 3 && 던전フラグ:85:0 == 1
	CALL EXPLAIN_REQUEST_5_4
ENDIF

@REQUEST_5
IF 의뢰フラグ:5:0 == 0
	CALL REQUEST_5_1
ELSEIF 의뢰フラグ:5:0 == 1 && 던전フラグ:49:0 == 1
	CALL REQUEST_5_2
ELSEIF 의뢰フラグ:5:0 == 2 && 던전フラグ:79:0 == 1;&& 던전フラグ:81:0 == 1
	CALL REQUEST_5_3
ELSEIF 의뢰フラグ:5:0 == 3 && 던전フラグ:85:0 == 1
	CALL REQUEST_5_4
	;ここには来ないはずだが念のため
	의뢰フラグ:5:0 = 4
ENDIF
CALL FLAG_RESET,5,3