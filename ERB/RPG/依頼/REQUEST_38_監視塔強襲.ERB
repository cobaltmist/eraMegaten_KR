IF 의뢰플래그:38:3 > 2
	LOCALS = 노랫소리가 몸을 좀먹는다……
	CALL MESSAGE_B(1, LOCALS)
	FOR LOCAL,1,7
		IF POS(LOCAL) > -1
			LOCAL:1 = RAND:100
			;상태이상にかかっていたら処理しない
			IF CFLAG:(POS(LOCAL)):상태이상 > 0
				CONTINUE
			;ターン開始時に記録した방어플래그を参照し、1ならガード성공。TFLAGをリセット
			ELSEIF TFLAG:(POS(LOCAL)) == 1
				PRINTFORMW TARGET:[{LOCAL,2}] %CALLNAME:POS(LOCAL),20,LEFT%　>>>>>>　GUARD　%CALLNAME:(POS(LOCAL))%
				TFLAG:(POS(LOCAL)) = 0
			;とりあえず확률2割
			ELSEIF LOCAL:1 < 20
				PRINTFORMW TARGET:[{LOCAL,2}] %CALLNAME:POS(LOCAL),20,LEFT%　>>>>>>　HAPPY
				;●HAPPYになる処理をここに入れる
				CFLAG:(POS(LOCAL)):상태이상 = GET_STATE_NUM("HAPPY")
			ELSE
				PRINTFORMW TARGET:[{LOCAL,2}] %CALLNAME:POS(LOCAL),20,LEFT%　>>>>>>　MISS
			ENDIF
		ELSE
		ENDIF
		
	NEXT
ELSE
ENDIF
