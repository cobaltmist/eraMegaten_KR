;아바타 행동패턴
@SET_ACTION_5740, ARG
#LOCALSIZE 2
#LOCALSSIZE 2
;원 모어. 약점을 잡았을 시 사이오닉 리피터 사격.
IF CFLAG:ARG:１moreフラグ > 0
	CFLAG:ARG:입력行動 = [[스킬:ＧＵＮ]]
	CALL RANDOM_TARGET,ARG,[[스킬:ＧＵＮ]]

;행동횟수가 0번. 반드시 아바타 순간이동
ELSEIF CFLAG:ARG:行動した횟수 == 0
	CFLAG:ARG:입력行動 = 3701;아바타 순간이동
	CALL RANDOM_TARGET,ARG,3701;아바타 순간이동

;아바타 순간이동 다음 행동. 1-1, 1-2, 1-3으로 나눔.
ELSEIF CFLAG:ARG:입력行動 == 3701;아바타 순간이동
	SELECTCASE RAND:100
	CASE 0 TO 24;25%
		CFLAG:ARG:입력行動 = [[스킬:ＧＵＮ]]
		CALL RANDOM_TARGET,ARG,[[스킬:ＧＵＮ]]
	CASE 26 TO 49 ;25%
		CFLAG:ARG:입력行動 = 1273;아바타용 정신지배
		CALL RANDOM_TARGET,ARG,1273;아바타용 정신지배
	CASE 50 TO 64 ;15%
		CFLAG:ARG:입력行動 = 3723;공허의창
		CALL RANDOM_TARGET,ARG,3723;공허의창
	CASE 65 TO 99 ;35%
		CFLAG:ARG:입력行動 = 3763;차원균열
		CALL RANDOM_TARGET,ARG,3763;차원균열
	ENDSELECT

;사이오닉 리피터 루트. 순간이동-리피터-사이오니스트 스킬 3개중 1개
ELSEIF CFLAG:ARG:입력行動 == [[스킬:ＧＵＮ]] && CFLAG:ARG:行動した횟수 == 2
	SELECTCASE RAND:3
	CASE 0
		CFLAG:ARG:입력行動 = 3723;공허의창
		CALL RANDOM_TARGET,ARG,3723;공허의창
	CASE 1
		CFLAG:ARG:입력行動 = 3722;실성·분립
		CALL RANDOM_TARGET,ARG,3722;실성·분립
	CASE 2
		CFLAG:ARG:입력行動 = 3721;영혼불꽃·착취
		CALL RANDOM_TARGET,ARG,3721;영혼불꽃·착취
	ENDSELECT

;아바타용 정신지배 루트. 정신 지배가 약하다고요? 차원균열 갑니다!
ELSEIF CFLAG:ARG:입력行動 == 1273 && CFLAG:ARG:行動した횟수 == 2
	CFLAG:ARG:입력行動 = 3763;차원균열
	CALL RANDOM_TARGET,ARG,3763;차원균열

;공허의 창 루트. 1방 때리고 파열시킨다!
ELSEIF CFLAG:ARG:입력行動 == 3723 && CFLAG:ARG:行動した횟수 == 2
	CFLAG:ARG:입력行動 = 3722;실성·분립
	CALL RANDOM_TARGET,ARG,3722;실성·분립

;차원균열 루트. 차원균열로 한방 거하게 때린뒤 자기 힐링.
ELSEIF CFLAG:ARG:입력行動 == 3763 && CFLAG:ARG:行動した횟수 == 2
	CFLAG:ARG:입력行動 = 3721;영혼불꽃·착취
	CALL RANDOM_TARGET,ARG,3721;영혼불꽃·착취

ENDIF

