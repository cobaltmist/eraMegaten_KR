FLAG:도주불가플래그 = 0
FLAG:회화불능플래그 = 0
FLAG:선제기습취소 = 0

;=================================================
;   sub EVENT_BATTLE_TURN_END_REQUEST30_6
;=================================================
;   의뢰:전투中処理(ターン종료시)
;-------------------------------------------------
;   6日눈、7日눈用の전투中処理(ターン종료시)
;-------------------------------------------------
;   FLAG:승리플래그に1をセットすると、전투を強制종료出来る
;   FLAG:패배플래그に1をセットすると、ゲームオーバーに出来る
;-------------------------------------------------
@EVENT_BATTLE_TURNEND_REQUEST30_6
#DIM L_POSITION
#DIM L_NO
#DIM L_CNT
L_CNT = 0

;-----------------------------
;- 処理開始
;-----------------------------
SELECTCASE FLAG:デビサバ２進行度
		
	;-----------------------------
	;- 2025:전투中(対HIGHヤマト)
	;-----------------------------
	CASE 2025 , 3025
		
		FOR LOCAL, 7 , 17
			L_POSITION = POS(LOCAL)
			
			;該当位置に誰もいない
			IF L_POSITION < 0
				CONTINUE
			;該当位置がヤマト
			ELSEIF NO:L_POSITION == [[キャラ:야마토]]
				L_NO = L_POSITION
			;該当する位置のキャラが死んでない
			ELSEIF CFLAG:L_POSITION:ステート != GET_STATE_NUM("DYING")
				L_CNT ++
			ENDIF
		NEXT
		;ヤマトが死んでたら
		SIF CFLAG:L_NO:ステート == GET_STATE_NUM("DYING")
			RETURN
		
		;ヤマトの행동횟수調整
		IF (L_CNT == 0 || 現HP割合(L_NO) < 30) && CFLAG:L_NO:행동횟수 <= 4
			CUSTOMDRAWLINE =
			SETCOLOR 0xDD0000
			CALL MESSAGE_BL(1,"야마토: 후하하핫…！흐럅, 계속 간다！네놈들은 따라잡을수 없다！")
			RESETCOLOR
			CUSTOMDRAWLINE =
			CALL MESSAGE_B(3,,"야마토의 움직임이 빨라졌다！",)
			
			CFLAG:L_NO:행동횟수 = 5
			CFLAG:L_NO:속도보정 = 25
			CALL SYNC_STATUS,L_NO
		ELSEIF (L_CNT <= 4 || 現HP割合(L_NO) < 60) && CFLAG:L_NO:행동횟수 == 3
			CUSTOMDRAWLINE =
			SETCOLOR 0xDD0000
			CALL MESSAGE_BL(1,"야마토：아직이다…아직 더있다！자 따라잡아 봐라…！")
			RESETCOLOR
			CUSTOMDRAWLINE =
			CALL MESSAGE_B(3,,"야마토의 움직임이 빨라졌다！",)
			
			CFLAG:L_NO:행동횟수 = 4
			CFLAG:L_NO:속도보정 = 15
			CALL SYNC_STATUS,L_NO
		ENDIF
	;-----------------------------
	;- 1045 , 2045:전투中(알코르)
	;-----------------------------
	CASE 1045 , 2045 , 3115 , 3125
		LOCAL = FINDCHARA_ENEMY(3008)
		L_NO = RESULT:1
		SIF L_NO < 0 || CPOS(L_NO) <= 0 || LOCAL == 0 || LOCAL == 3
			RETURN
		
		IF FLAG:경과턴수 % 6 == 0
			
			;물리무효それ以外내성
			BASE:L_NO:검격 = 0
			BASE:L_NO:타격 = 0
			BASE:L_NO:전술 = 0
			BASE:L_NO:화염 = 50
			BASE:L_NO:빙결 = 50
			BASE:L_NO:전격 = 50
			BASE:L_NO:충격 = 50
			BASE:L_NO:지변 = 50
			BASE:L_NO:수격 = 50
			BASE:L_NO:질풍 = 50
			BASE:L_NO:중력 = 50
			BASE:L_NO:만능 = 100
			CALL SYNC_STATUS,(L_NO)

			ABL:L_NO:스킬1 = [[스킬:혼탁의장기]]
			ABL:L_NO:스킬2 = [[스킬:중재의뇌화]]
			ABL:L_NO:스킬3 = [[스킬:마하・잔다인]]
			ABL:L_NO:스킬4 = [[스킬:치유촉진(대)]]
			
			CALL MESSAGE_B(3,"알코르의 모습이 큐브가 되었다！","알코르의 내성이 변했다！","알코르의 스킬이 변했다！")
		ELSEIF FLAG:경과턴수 % 3 == 0
			LOCAL = FINDCHARA_ENEMY(3008)
			L_NO = RESULT:1
			SIF L_NO < 0 || CPOS(L_NO) <= 0 || LOCAL == 0 || LOCAL == 3
				RETURN
			
			;물리충격질풍약점만능내성それ以外반사
			BASE:L_NO:검격 = 150
			BASE:L_NO:타격 = 150
			BASE:L_NO:전술 = 150
			BASE:L_NO:화염 = 999
			BASE:L_NO:빙결 = 999
			BASE:L_NO:전격 = 999
			BASE:L_NO:충격 = 150
			BASE:L_NO:지변 = 999
			BASE:L_NO:수격 = 999
			BASE:L_NO:질풍 = 150
			BASE:L_NO:중력 = 999
			BASE:L_NO:만능 = 50
			CALL SYNC_STATUS,(L_NO)
			
			;混沌の波紋(하이퍼카운터で代用)、데스바운드(메테오피스트、연속사격)、만마의난무
			ABL:L_NO:스킬1 = [[스킬:하이퍼카운터]]
			ABL:L_NO:스킬2 = [[스킬:메테오피스트]]
			ABL:L_NO:스킬3 = [[스킬:연속사격]]
			ABL:L_NO:스킬4 = [[스킬:만마의난무]]
			
			CALL MESSAGE_B(3,"알코르의 모습이 가시모양이 되었다！","알코르의 내성이 변했다！","알코르의 스킬이 변했다！")
		ENDIF
	;-----------------------------
	;- 4057:전투中(真ポラリス戦　摂理)
	;-----------------------------
	CASE 4057 , 3137 , 3147
		L_CNT = 1
		
		FOR LOCAL, 7 , 17
			L_POSITION = POS(LOCAL)
			
			;ポラリスじゃない、あるいは誰もいない
			SIF L_POSITION < 0 || NO:L_POSITION != 3311
				CONTINUE
			
			;보스플래그ついた本体だったら
			SIF CFLAG:L_POSITION:보스플래그 == 1
				L_NO = L_POSITION
				
			;該当するキャラが死んでない
			IF CFLAG:L_POSITION:ステート != GET_STATE_NUM("DYING")
				L_CNT ++
			;取り巻きが死んで３ターン立った
			ELSEIF !CFLAG:L_POSITION:보스플래그 && CFLAG:L_POSITION:ステート経過ターン > 2
				CALL MESSAGE_BL(3,,CALLNAME:L_POSITION+"가 재생했다",)
				CFLAG:L_POSITION:ステート = 0
				CFLAG:L_POSITION:ステート経過ターン = 0
				CALL HEALTH_CHARA,(L_POSITION)
				
				L_CNT ++
			ENDIF
		NEXT
		
		
		;本体が死んでたら
		IF CFLAG:L_NO:ステート == GET_STATE_NUM("DYING")
			FLAG:승리플래그 = 1
		;당신が死んでたら
		ELSEIF CFLAG:MASTER:ステート == GET_STATE_NUM("DYING")
			FLAG:패배플래그 = 1
		;ポラリスの행동횟수調整
		ELSEIF L_CNT < CFLAG:L_NO:행동횟수
			CFLAG:L_NO:행동횟수 = L_CNT
			CALL MESSAGE_B(3,,"폴라리스의 행동횟수가 감소했다！",)
		ELSEIF L_CNT > CFLAG:L_NO:행동횟수
			CFLAG:L_NO:행동횟수 = L_CNT
			CALL MESSAGE_B(3,,"폴라리스의 행동횟수가 늘어났다！",)
		ENDIF
ENDSELECT


;HIGHヤマトの사고패턴
@SET_ACTION_HIGHヤマト,ARG
;ＨＰ15％以下
IF 現HP割合(ARG) < 15 && RAND:2 == 0
	CALL CHECK_ACTIONABLE,ARG,[[스킬:메・디아라한]]
	SIF RESULT == 0
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력행동 = [[스킬:메・디아라한]]
	CALL RANDOM_TARGET,ARG,[[스킬:메・디아라한]]
	RETURN 1
;ＨＰ15％이상
ELSEIF 現HP割合(ARG) >= 15
	IF CFLAG:ARG:명중강화 < 0 || CFLAG:ARG:마법위력강화 < 0
		CALL CHECK_ACTIONABLE,ARG,[[스킬:데・쿤다]]
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = [[스킬:데・쿤다]]
		CALL RANDOM_TARGET,ARG,[[스킬:데・쿤다]]
		RETURN 1
	ELSE
		SELECTCASE RAND:100
			;80％
			CASE IS < 80
				CALL CHECK_ACTIONABLE,ARG,[[스킬:메기도라]]
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력행동 = [[스킬:메기도라]]
				CALL RANDOM_TARGET,ARG,[[스킬:메기도라]]
				RETURN 1
			;20％
			CASE IS < 100
				CALL CHECK_ACTIONABLE,ARG,[[스킬:중재의뇌화]]
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력행동 = [[스킬:중재의뇌화]]
				CALL RANDOM_TARGET,ARG,[[스킬:중재의뇌화]]
				RETURN 1
		ENDSELECT
	ENDIF
ELSE
	$DECIDE_ACTION
	CALL SEARCH_ACT,ARG
	IF RESULT == 0
		CFLAG:ARG:입력행동 = -1
		CFLAG:ARG:방어플래그 = 1
		RETURN 0
	ENDIF
	LOCAL = RAND:RESULT
	;스킬번호決定
	LOCAL:1 = S:(10+LOCAL)
	$TARGET_LOOP
	
	CALL RANDOM_TARGET,ARG,LOCAL:1
	CALLFORM SKILL_TARGET_{LOCAL:1}
	;ここで스킬번호を行動内容に代入
	SIF CFLAG:ARG:타겟 == -1
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력행동 = LOCAL:1

ENDIF

;前座ヤマトの사고패턴
@SET_ACTION_前座ヤマト,ARG
;1more
IF CFLAG:ARG:１more플래그 > 0
	;메기도라온
	CALL CHECK_ACTIONABLE,ARG,[[스킬:메기도라온]]
	SIF RESULT == 0
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력행동 = [[스킬:메기도라온]]
	CALL RANDOM_TARGET,ARG,[[스킬:메기도라온]]
;ＨＰ10％以下
ELSEIF 現HP割合(ARG) < 10
	IF CFLAG:ARG:집중플래그 == 1
		CALL CHECK_ACTIONABLE,ARG,[[스킬:메기도라온]]
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = [[스킬:메기도라온]]
		CALL RANDOM_TARGET,ARG,[[스킬:메기도라온]]
		RETURN 1
	ELSE
		CALL CHECK_ACTIONABLE,ARG,[[스킬:디아라한]]
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = [[스킬:디아라한]]
		CALL RANDOM_TARGET,ARG,[[스킬:디아라한]]
		RETURN 1
	ENDIF
;ＨＰ10％이상
ELSEIF 現HP割合(ARG) >= 10
	IF CFLAG:ARG:집중플래그 == 1
		CALL CHECK_ACTIONABLE,ARG,[[스킬:메기도라온]]
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = [[스킬:메기도라온]]
		CALL RANDOM_TARGET,ARG,[[스킬:메기도라온]]
		RETURN 1
	ELSEIF CFLAG:ARG:명중강화 < -2 || CFLAG:ARG:마법위력강화 < -2
		CALL CHECK_ACTIONABLE,ARG,[[스킬:데・쿤다]]
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = [[스킬:데・쿤다]]
		CALL RANDOM_TARGET,ARG,[[스킬:데・쿤다]]
		RETURN 1
	ELSEIF CFLAG:ARG:행동한횟수 == 0
		SELECTCASE RAND:100
			;40％
			CASE IS < 40
				CALL CHECK_ACTIONABLE,ARG,[[스킬:컨센트레이트]]
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력행동 = [[스킬:컨센트레이트]]
				CALL RANDOM_TARGET,ARG,[[스킬:컨센트레이트]]
				RETURN 1
			;40％
			CASE IS < 80
				CALL CHECK_ACTIONABLE,ARG,[[스킬:마카・카쟈]]
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력행동 = [[스킬:마카・카쟈]]
				CALL RANDOM_TARGET,ARG,[[스킬:마카・카쟈]]
				RETURN 1
			;20％
			CASE IS < 100
				CALL CHECK_ACTIONABLE,ARG,[[스킬:메기도라온]]
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력행동 = [[스킬:메기도라온]]
				CALL RANDOM_TARGET,ARG,[[스킬:메기도라온]]
				RETURN 1
		ENDSELECT
	ELSE
		SELECTCASE RAND:100
			;60％
			CASE IS < 60
				CALL CHECK_ACTIONABLE,ARG,[[스킬:메기도라온]]
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력행동 = [[스킬:메기도라온]]
				CALL RANDOM_TARGET,ARG,[[스킬:메기도라온]]
				RETURN 1
			;30％
			CASE IS < 90
				CALL CHECK_ACTIONABLE,ARG,[[스킬:마카・카쟈]]
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력행동 = [[스킬:마카・카쟈]]
				CALL RANDOM_TARGET,ARG,[[스킬:마카・카쟈]]
				RETURN 1
			;10％
			CASE IS < 100
				CALL CHECK_ACTIONABLE,ARG,[[스킬:컨센트레이트]]
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력행동 = [[스킬:컨센트레이트]]
				CALL RANDOM_TARGET,ARG,[[스킬:컨센트레이트]]
				RETURN 1
		ENDSELECT
	ENDIF
ELSE
	$DECIDE_ACTION
	CALL SEARCH_ACT,ARG
	IF RESULT == 0
		CFLAG:ARG:입력행동 = -1
		CFLAG:ARG:방어플래그 = 1
		RETURN 0
	ENDIF
	LOCAL = RAND:RESULT
	;스킬번호決定
	LOCAL:1 = S:(10+LOCAL)
	$TARGET_LOOP
	
	CALL RANDOM_TARGET,ARG,LOCAL:1
	CALLFORM SKILL_TARGET_{LOCAL:1}
	;ここで스킬번호を行動内容に代入
	SIF CFLAG:ARG:타겟 == -1
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력행동 = LOCAL:1

ENDIF
