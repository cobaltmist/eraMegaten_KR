;-------------------------------------------------------------------------------
; 【当ファイルについて】
; 当ファイルは데빌서바이버２の이벤트をCALLするだけの、メインファイルである。
; 詳細な処理は各サ부흐ァイルに記載する。
; （サ부흐ァイルは日付ごとに１ファイル用意する）
;
;
; 【フラグについて】
; 데빌서바이버２の이벤트では、"FLAG:デビサバ２進行度"を사용している。
; ３桁の数値が以下の仕様で설정される。
;
; ・３桁눈…日付区分。０～７
; ・２桁눈…이벤트区分。０～９
; ・１桁눈…이벤트詳細区分。０～９
;
; ◆例
; 例えば一日눈の場合は、000～099を사용出来る。
; この内이벤트ごとに、0～9の詳細区分を사용가능。
; その為、例えば[두베戦]と[이오加入]の二つの이벤트を一日눈に入れたい場合、
;
;	・[두베戦] … 000～009
;	・[이오加入] … 010～019
;
; といった形でフラグを사용出来る。
; 具体的なフラグの割り振り方は、
; 各日付ごとのサ부흐ァイルを参照すれば詳細が확인出来る。
;
;-------------------------------------------------------------------------------

;===============================================================================
;  ◇記載関数一覧(記載順)
;
;		Module Name						Explanation
;
;	・ PLAYABLE_REQUEST_xxx				의뢰:조건
;	・ EXPLAIN_REQUEST_xxx				의뢰:설명
;	・ REQUEST_xxx						의뢰:メイン処理
;	・ EVENT_BATTLE_TURNTOP_REQUESTxxx	의뢰:전투中処理(ターン開始時)
;	・ EVENT_BATTLE_TURNEND_REQUESTxxx	의뢰:전투中処理(ターン종료시)
;
; *- 独自関数
;
;	・ REQUEST_30_FLAG_RESET			의뢰:フラグ리셋
;	・ REQUEST_30_DAY_TITLE				의뢰:一日のタイトル
;	・ REQUEST_30_DAY_END				의뢰:一日の종료
;	・ REQUEST_30_TURN_END				의뢰:ターンの종료
;	・ REQUEST_30_BASE_LV				의뢰:기초레벨
;
;===============================================================================

;====================
;조건
;====================
@PLAYABLE_REQUEST_30
#DIM DYNAMIC L_FLAG_REQ
#DIM DYNAMIC L_FLAG_EVENT_DIGIT

;-----------------------------
;- 共通체크
;-----------------------------

;- デバッグ
IF FLAG:DEBUG
	;OK

;- 主人레벨30이상
ELSEIF BASE:MASTER:LV >= 30
	;OK

;- 50日제한
ELSEIF DAY < 51
	;OK

ELSE
	;NG
	RETURN 0
ENDIF

;-----------------------------
;- 処理開始
;-----------------------------

;- フラグをセット
L_FLAG_REQ = FLAG:デビサバ２進行度

;- フラグをセット(이벤트フラグ:２桁눈)
L_FLAG_EVENT_DIGIT = L_FLAG_REQ % 100


SELECTCASE L_FLAG_REQ
	;-----------------------------
	;- [1ST DAY]
	;-----------------------------
	CASE 000 TO 099

		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [00-09] 전투:두베
			CASE 00 TO 09
				RESULTS = 일본 정부조직의 비밀임무
				RETURN 1
			; [10-19] 이오加入
			CASE 10 TO 19
				RESULTS = 집스로 인재 회수（1ST DAY）
				RETURN 1
		ENDSELECT

	;-----------------------------
	;- [2ND DAY]
	;-----------------------------
	CASE 100 TO 199
		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [00-09] 전투:ハッカー
			CASE 00 TO 09
				RESULTS = 집스의 의뢰（2ND DAY）
				RETURN 1
			; [10-19] 전투:메라크
			CASE 10 TO 19
				RESULTS = 집스의 의뢰（2ND DAY - 오사카）
				RETURN 1
		ENDSELECT


	;-----------------------------
	;- [3RD DAY]
	;-----------------------------
	CASE 200 TO 299
		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [00-09] 전투:名古屋捜索
			CASE 00 TO 09
				RESULTS = 집스의 의뢰（3RD DAY）
				RETURN 1
			; [10-19] 전투:名古屋支局
			CASE 10 TO 19
				RESULTS = 집스의 의뢰（3RD DAY - 나고야）
				RETURN 1
		ENDSELECT

	;-----------------------------
	;- [4TH DAY]
	;-----------------------------
	CASE 300 TO 399
		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [00-09] 메그레즈
			CASE 00 TO 09
				RESULTS = 집스의 의뢰（4TH DAY）
				RETURN 1
			; [10-19] 메그레즈対策会議
			CASE 10 TO 19
				RESULTS = 집스의 의뢰（4TH DAY - 메그레즈 대책회의）
				RETURN 1
			; [20-29] ニカイアの主
			CASE 20 TO 29
				RESULTS = 집스의 의뢰（4TH DAY - 도쿄）
				RETURN 1
			; [30-39] デルタ浮上戦
			CASE 30 TO 39
				RESULTS = 집스의 의뢰（4TH DAY - 델타 부상전）
				RETURN 1
		ENDSELECT
	;-----------------------------
	;- [5TH DAY]
	;-----------------------------
	CASE 400 TO 499
		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [00-09] 黒い傷
			CASE 00 TO 09
				RESULTS = 로나우도의 호출（5TH DAY - 무의 세계）
				RETURN 1
			; [10-19] 撃墜のために
			CASE 10 TO 19
				RESULTS = 집스의 의뢰（5TH DAY - 격추를 위해）
				RETURN 1
			; [20-29] ビブロンス戦
			CASE 20 TO 29
				RESULTS = 예상치 못한 적대（5TH DAY）
				RETURN 1
			; [30-39] 巨大方陣
			CASE 30 TO 39
				RESULTS = 로나우도의 호출（5TH DAY - 거대방진）
				RETURN 1
			; [40-49] 오토메加入
			CASE 40 TO 49
				RESULTS = 인재 파견（5TH DAY）
				RETURN 1
			; [50-59] 미자르&알리오츠戦
			CASE 50 TO 59
				RESULTS = 집스의 의뢰5TH DAY - 결전 이대거두）
				RETURN 1
			; [60-69] 아이리の加入
			CASE 60 TO 69
				RESULTS = 집스에서 인재 회수（5TH DAY）
				RETURN 1
			; [70-79] 種の意思
			CASE 70 TO 79
				RESULTS = 종의 의사（5TH DAY）
				RETURN 1
		ENDSELECT

	;-----------------------------
	;- [6TH DAY ヤマトルート]
	;-----------------------------
	CASE 1000 TO 1099
		
		;大阪勢の面々が전투참가불가능になる또는매각とかされていなくなる
		SIF L_FLAG_EVENT_DIGIT >= 10 && (GETCHARA([[キャラ:야마토]]) < 0 || GETCHARA([[キャラ:후미]]) < 0 || GETCHARA([[キャラ:케이타]]) < 0)
			RETURN 0
		SIF L_FLAG_EVENT_DIGIT >= 10 && (GETCHARA([[キャラ:마코토]]) < 0 || CFLAG:GETCHARA([[キャラ:마코토]]):전투참가불가능 > 0)
			RETURN 0
		
		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [00-09] 다이치戦
			CASE 00 TO 09
				RESULTS = 야마토의 호출（6TH DAY）
				RETURN 1
			; [10-29] ロナウド戦
			CASE 10 TO 29
				RESULTS = 희언의 결말（6TH DAY）
				RETURN 1
			; [30-39] ベネちゃん
			CASE 30 TO 39
				RESULTS = 격파 수 7（6TH DAY）
				RETURN 1
			; [50-59] ラストバトル
			CASE 40 TO 49
				RESULTS = 올바른 내일로（7TH DAY）
				RETURN 1
		ENDSELECT
	;-----------------------------
	;- [6TH DAY ロナウドルート]
	;-----------------------------
	CASE 2000 TO 2099
		
		;名古屋勢の面々が전투참가불가능になる또는매각とかされていなくなる
		SIF GETCHARA([[キャラ:아이리]]) < 0 || CFLAG:GETCHARA([[キャラ:아이리]]):전투참가불가능 > 0
			RETURN 0
		SIF GETCHARA([[キャラ:오토메]]) < 0 || CFLAG:GETCHARA([[キャラ:오토메]]):전투참가불가능 > 0
			RETURN 0
		SIF L_FLAG_EVENT_DIGIT >= 10 && (GETCHARA([[キャラ:죠]]) < 0 || CFLAG:(GETCHARA([[キャラ:죠]])):전투참가불가능 > 0)
			RETURN 0
		SIF L_FLAG_EVENT_DIGIT >= 10 && (GETCHARA([[キャラ:로나우도]]) < 0 || CFLAG:(GETCHARA([[キャラ:로나우도]])):전투참가불가능 > 0)
			RETURN 0
		
		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [00-09] 다이치戦
			CASE 00 TO 09
				RESULTS = 로나우도의 호출（6TH DAY）
				RETURN 1
			; [10-19] 후미마코토ケイタ戦
			CASE 10 TO 19
				RESULTS = 미지의 위협（6TH DAY）
				RETURN 1
			; [20-29] ヤマト戦
			CASE 20 TO 29
				RESULTS = 강자와의 결착（6TH DAY）
				RETURN 1
			; [30-39] 베네트나쉬
			CASE 30 TO 39
				RESULTS = 격파 수 7（6TH DAY）
				RETURN 1
			; [40-49] ラストバトル
			CASE 40 TO 49
				RESULTS = 아직 보지 못한 내일로（7TH DAY）
				RETURN 1
		ENDSELECT
	;-----------------------------
	;- [6TH DAY 다이치ルート]
	;-----------------------------
	CASE 3000 TO 3099
		
		;이오と아이리がいなくなる
		SIF GETCHARA([[キャラ:이오]]) < 0
			RETURN 0
		SIF GETCHARA([[キャラ:아이리]]) < 0 || CFLAG:GETCHARA([[キャラ:아이리]]):전투참가불가능 > 0
			RETURN 0
		;東京勢が仲間になった後、전투참가불가능になる또는매각とかされていなくなる
		SIF L_FLAG_EVENT_DIGIT >= 10 && GETCHARA([[キャラ:데빌서바２주인공]]) < 0
			RETURN 0
		SIF L_FLAG_EVENT_DIGIT >= 10 && (GETCHARA([[キャラ:다이치]]) < 0 || CFLAG:GETCHARA([[キャラ:다이치]]):전투참가불가능 > 0)
			RETURN 0
		SIF L_FLAG_EVENT_DIGIT >= 10 && (GETCHARA([[キャラ:히나코]]) < 0 || CFLAG:GETCHARA([[キャラ:히나코]]):전투참가불가능 > 0)
			RETURN 0
		SIF L_FLAG_EVENT_DIGIT >= 10 && (GETCHARA([[キャラ:쥰고]]) < 0 || CFLAG:GETCHARA([[キャラ:쥰고]]):전투참가불가능 > 0)
			RETURN 0
		
		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [00-09] 마코토戦
			CASE 00 TO 09
				RESULTS = 오사카 세력과의 싸움（6TH DAY）
				RETURN 1
			; [10-19] ロナウド戦
			CASE 10 TO 19
				RESULTS = 미지의 위협（6TH DAY）
				RETURN 1
			; [20-29] ヤマト戦
			CASE 20 TO 29
				RESULTS = 강자와의 결착（6TH DAY）
				RETURN 1
			; [30-49] 베네트나쉬戦
			CASE 30 TO 49
				RESULTS = 격파 수 7（6TH DAY）
				RETURN 1
		ENDSELECT
	;-----------------------------
	;- [6TH DAY 憂う者ルート]
	;-----------------------------
	CASE 4000 TO 4099
		
		;이오と아이리がいなくなる
		SIF GETCHARA([[キャラ:이오]]) < 0
			RETURN 0
		SIF GETCHARA([[キャラ:아이리]]) < 0 || CFLAG:GETCHARA([[キャラ:아이리]]):전투참가불가능 > 0
			RETURN 0
		;東京勢とサダクが仲間になった後、전투참가불가능になる또는매각とかされていなくなる
		SIF L_FLAG_EVENT_DIGIT >= 10 && GETCHARA([[キャラ:사다크]]) < 0
			RETURN 0
		SIF L_FLAG_EVENT_DIGIT >= 10 && GETCHARA([[キャラ:데빌서바２주인공]]) < 0
			RETURN 0
		SIF L_FLAG_EVENT_DIGIT >= 10 && ( GETCHARA([[キャラ:다이치]]) < 0 || CFLAG:GETCHARA([[キャラ:다이치]]):전투참가불가능 > 0)
			RETURN 0
		SIF L_FLAG_EVENT_DIGIT >= 10 && ( GETCHARA([[キャラ:히나코]]) < 0 || CFLAG:GETCHARA([[キャラ:히나코]]):전투참가불가능 > 0)
			RETURN 0
		SIF L_FLAG_EVENT_DIGIT >= 10 && ( GETCHARA([[キャラ:쥰고]]) < 0 || CFLAG:GETCHARA([[キャラ:쥰고]]):전투참가불가능 > 0)
			RETURN 0
		
		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [00-09] 다이치戦
			CASE 00 TO 09
				RESULTS = 도쿄 세력과의 싸움（6TH DAY）
				RETURN 1
			; [10-19] 후미戦
			CASE 10 TO 19
				RESULTS = 오사카 탈환（6TH DAY）
				RETURN 1
			; [20-29] 마코토ケイタ戦
			CASE 20 TO 29
				RESULTS = 종말의 나팔수（6TH DAY）
				RETURN 1
			; [30-39] ロナウド戦
			CASE 30 TO 39
				RESULTS = 로나우도의 호출（6TH DAY - 信念の決着）
				RETURN 1
			; [40-49] 베네트나쉬戦
			CASE 40 TO 49
				RESULTS = 격파 수 7（6TH DAY）
				RETURN 1
			; [50-59] 新しき明日へ
			CASE 50 TO 59 
				LOCAL = GETCHARA([[キャラ:사다크]])
				;サダクが함락してない
				SIF !TALENT:LOCAL:3 && !TALENT:LOCAL:4 && !TALENT:LOCAL:5 && !TALENT:LOCAL:6 && !TALENT:LOCAL:7 && !TALENT:LOCAL:8
					RETURN 0
				
				RESULTS = 새로운 내일로（7TH DAY）
				RETURN 1
		ENDSELECT
	;-----------------------------
	;- [7TH DAY 다이치ルート]
	;-----------------------------
	CASE 3100 TO 3199
		
		;東京勢と아이리が전투참가불가능になる또는매각とかされていなくなる
		SIF GETCHARA([[キャラ:이오]]) < 0 || GETCHARA([[キャラ:데빌서바２주인공]]) < 0
			RETURN 0
		SIF GETCHARA([[キャラ:아이리]]) < 0 || CFLAG:GETCHARA([[キャラ:아이리]]):전투참가불가능 > 0
			RETURN 0
		SIF GETCHARA([[キャラ:다이치]]) < 0 || CFLAG:GETCHARA([[キャラ:다이치]]):전투참가불가능 > 0
			RETURN 0
		SIF GETCHARA([[キャラ:히나코]]) < 0 || CFLAG:GETCHARA([[キャラ:히나코]]):전투참가불가능 > 0
			RETURN 0
		SIF GETCHARA([[キャラ:쥰고]]) < 0 || CFLAG:GETCHARA([[キャラ:쥰고]]):전투참가불가능 > 0
			RETURN 0
		
		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [00-09] 事件の元凶
			CASE 00 TO 09
				RESULTS = 사건의 원흉（7TH DAY）
				RETURN 1
			; [10-19] ルート分岐
			CASE 10 TO 19
				RESULTS = 보여진 길（7TH DAY）
				RETURN 1
			; [20-29] ポラリス撃破ルート
			CASE 20 TO 29
				RESULTS = 자유로운 내일로（7TH DAY）
				RETURN 1
			; [30-39] 세계回帰ルート
			CASE 30 TO 39
				;サダク以外のデビサバ２キャラが誰か一人でもいない
				SIF GETCHARA([[キャラ:야마토]]) < 0 || GETCHARA([[キャラ:후미]]) < 0 || GETCHARA([[キャラ:케이타]]) < 0
					RETURN 0
				SIF GETCHARA([[キャラ:티코]]) < 0 || GETCHARA([[キャラ:코하루]]) < 0
					RETURN 0
				SIF GETCHARA([[キャラ:오토메]]) < 0 || CFLAG:GETCHARA([[キャラ:오토메]]):전투참가불가능 > 0
					RETURN 0
				SIF GETCHARA([[キャラ:로나우도]]) < 0 || CFLAG:GETCHARA([[キャラ:로나우도]]):전투참가불가능 > 0
					RETURN 0
				SIF GETCHARA([[キャラ:죠]]) < 0 || CFLAG:GETCHARA([[キャラ:죠]]):전투참가불가능 > 0
					RETURN 0
				SIF GETCHARA([[キャラ:마코토]]) < 0 || CFLAG:GETCHARA([[キャラ:마코토]]):전투참가불가능 > 0
					RETURN 0
				RESULTS = 나아가야 할 내일로（7TH DAY）
				RETURN 1
		ENDSELECT
	;-----------------------------
	;- [BR 火曜日]
	;-----------------------------
	CASE 5000 TO 5099
		
		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [0-09] デネボラ戦
			CASE 0 TO 9
				RESULTS = 사자의 분노（ TUES DAY ）
				RETURN 1
		ENDSELECT
	;-----------------------------
	;- [BR 水曜日]
	;-----------------------------
	CASE 5100 TO 5199
		
		;- CHK:이벤트フラグ
		SELECTCASE L_FLAG_EVENT_DIGIT
			; [0-9] 雑물고기戦１
			CASE 0 TO 9
				RESULTS = 동료를 찾고（ WEB DAY ）
				RETURN 1
			; [10-19] 스피카
			CASE 10 TO 19
				RESULTS = 다중 쌍성의 왕（ WEB DAY ）
				RETURN 1
			; [20-29] 雑물고기戦２
			CASE 20 TO 29
				RESULTS = 탈출지원（ WEB DAY ）
				RETURN 1
		ENDSELECT
	;-----------------------------
	;- 종료
	;-----------------------------
	CASE 10000
		RETURN 0
	;- 作成中
	CASEELSE
		RESULTS = 집스로부터 연락(대기 및 전달)
		RETURN 1

ENDSELECT

;念の為
RETURN 0

;====================
;설명
;====================
@EXPLAIN_REQUEST_30
#DIM DYNAMIC L_FLAG_REQ
#DIM DYNAMIC L_FLAG_EVENT_FIRST_DIGIT
;- フラグをセット
L_FLAG_REQ = FLAG:デビサバ２進行度

;- フラグをセット(이벤트フラグ:１桁눈)
L_FLAG_EVENT_FIRST_DIGIT = L_FLAG_REQ % 10

SELECTCASE L_FLAG_REQ
	;-----------------------------
	;- 1ST DAY
	;-----------------------------

	;- 전투:두베
	CASE 000 TO 009
		PRINTL -메일이 도착했습니다-
