;===================================================================
;ＭＡＧ소비処理
;===================================================================
@CONSUME_MAG
FOR LOCAL:1,1,7
	LOCALS = 포지션{LOCAL:1}
	IF FLAG:LOCALS > -1
		;LOCALに소비する人の番号を突っ込む
		LOCAL = FLAG:LOCALS
		;CPが0なら飛ばす
		SIF BASE:LOCAL:CP == 0
			CONTINUE
		;종족が인간でも飛ばす
		SIF ABL:LOCAL:종족 == 0
			CONTINUE
;---- EDIT 042 ADD START -------------------------
		;저주의레드카펫起動中なら飛ばす
		SIF EQUIP:MASTER:저주의레드카펫
			CONTINUE
;---- EDIT 042 ADD END -------------------------
		IF CFLAG:LOCAL:ＭＡＧ自己소비 == 1
		;자기소비가 ＯＮ이라면 자신의 ＭＡＧ를 본다
			IF BASE:LOCAL:ＭＡＧ == 0
			;악마 자신의 MAG가 고갈될 경우
				IF BASE:DELE_MAG_INCHARGE:ＭＡＧ == 0
				;마그네타이트 공급요원의 MAG가 0이 될 경우
					IF DELE_MAG_INCHARGE == MASTER
					;마그네타이트 공급요원이 아나타일 경우, 악마는 체력 기력에 데미지를 받는다
						CALL VAR_HP,LOCAL,-(MAXBASE:LOCAL:ＨＰ/10),0
						CALL VAR_MP,LOCAL,-(MAXBASE:LOCAL:ＭＰ/10),0
					ELSE
					;마그네타이트 공급요원이 동료 서머너일 경우, 공급요원 역할이 아나타로 전환된다
						DELE_MAG_INCHARGE = MASTER
						IF BASE:DELE_MAG_INCHARGE:ＭＡＧ == 0
						;근데 아나타마저 MAG가 없다면, 악마는 체력 기력에 데미지를 받는다
							CALL VAR_HP,LOCAL,-(MAXBASE:LOCAL:ＨＰ/10),0
							CALL VAR_MP,LOCAL,-(MAXBASE:LOCAL:ＭＰ/10),0
						ELSE
						;여기서 아나타가 MAG를 갖고 있다면, 전환되는 그 즉시 아나타가 MAG를 지불한다
							CALL CONTROL_MAG, DELE_MAG_INCHARGE, -MAXBASE:LOCAL:CP * FLAG:MAG소비율 / 100
						ENDIF
					ENDIF
				ELSE
				;마그네타이트 공급요원의 MAG가 대신 지불된다
					CALL CONTROL_MAG, DELE_MAG_INCHARGE, -MAXBASE:LOCAL:CP * FLAG:MAG소비율 / 100
				ENDIF
			ELSE
			;악마 자신이 MAG를 소비
				CALL CONTROL_MAG,LOCAL, -MAXBASE:LOCAL:CP * FLAG:MAG소비율 / 100
			ENDIF
		ELSE
		;자기소비가 ＯＦＦ라면 주인의 ＭＡＧ를 본다
			IF BASE:DELE_MAG_INCHARGE:ＭＡＧ == 0
			;마그네타이트 공급요원의 MAG가 0이 될 경우
				IF BASE:LOCAL:ＭＡＧ == 0
				;악마 자신의 MAG가 고갈될 경우
					IF DELE_MAG_INCHARGE == MASTER
					;공급요원이 아나타일 경우(아나타마저 MAG가 없다는 얘기니까), 악마는 체력 기력에 데미지를 받는다
						CALL VAR_HP,LOCAL,-(MAXBASE:LOCAL:ＨＰ/10),0
						CALL VAR_MP,LOCAL,-(MAXBASE:LOCAL:ＭＰ/10),0
					ELSE
					;마그네타이트 공급요원이 동료 서머너일 경우, 공급요원 역할이 아나타로 전환된다
						DELE_MAG_INCHARGE = MASTER
						IF BASE:DELE_MAG_INCHARGE:ＭＡＧ == 0
						;근데 아나타마저 MAG가 없다면, 악마는 체력 기력에 데미지를 받는다
							CALL VAR_HP,LOCAL,-(MAXBASE:LOCAL:ＨＰ/10),0
							CALL VAR_MP,LOCAL,-(MAXBASE:LOCAL:ＭＰ/10),0
						ELSE
						;여기서 아나타가 MAG를 갖고 있다면, 전환되는 그 즉시 아나타가 MAG를 지불한다
							CALL CONTROL_MAG, DELE_MAG_INCHARGE, -MAXBASE:LOCAL:CP * FLAG:MAG소비율 / 100
						ENDIF
					ENDIF
				ELSE
				;악마가 지불하기 전에, 상황을 따져본다
					IF DELE_MAG_INCHARGE != MASTER
					;공급요원이 아나타가 아닐 경우, 일단 아나타한테 공급요원의 역할이 넘어간다
						DELE_MAG_INCHARGE = MASTER
						IF BASE:DELE_MAG_INCHARGE:ＭＡＧ == 0
						;근데 아나타마저 MAG가 없다면, 악마가 MAG를 지불한다
							CALL CONTROL_MAG, LOCAL,-MAXBASE:LOCAL:CP * FLAG:MAG소비율 / 100
						ELSE
						;여기서 아나타가 MAG를 갖고 있다면, 전환되는 그 즉시 아나타가 MAG를 지불한다
							CALL CONTROL_MAG, DELE_MAG_INCHARGE, -MAXBASE:LOCAL:CP * FLAG:MAG소비율 / 100
						ENDIF
					ELSE
					;공급요원이 아나타일 경우(아나타마저 MAG가 없다는 얘기니까), 악마가 MAG를 지불한다
						CALL CONTROL_MAG, LOCAL,-MAXBASE:LOCAL:CP * FLAG:MAG소비율 / 100
					ENDIF
				ENDIF
			ELSE
			;마그네타이트 공급요원이 MAG를 지불한다
				CALL CONTROL_MAG,DELE_MAG_INCHARGE,-MAXBASE:LOCAL:CP  * FLAG:MAG소비율 / 100
			ENDIF
		ENDIF
	ENDIF
NEXT

