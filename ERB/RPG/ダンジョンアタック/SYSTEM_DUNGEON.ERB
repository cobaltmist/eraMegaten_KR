@KARMAN
	LOCAL:1 = 0
	FOR LOCAL, 1, 7
		SIF POS(LOCAL) < 0
			CONTINUE
		SIF !TALENT:POS(LOCAL):식노
			CONTINUE
		;行動可能なキャラでないとダメ
		CALL INPUTABLE_CHARA, POS(LOCAL)
		SIF !RESULT
			CONTINUE
		LOCAL:1 = 1
		BREAK
	NEXT
