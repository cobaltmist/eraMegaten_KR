	$NO_COMMON
	TRYCCALLFORM TILE_MARK_ID_{LD}_{LM}_% \@TILE_VAL < 0 ? m # \@%{ABS(TILE_VAL)}
		LOCALS = %RESULTS%
	CATCH
		;- DG:タイル標準文字
