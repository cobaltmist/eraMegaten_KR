	KEY = D
ELSE
	KEY = {TILE_VAL % 10}
ENDIF
TRYCCALLFORM TILE_MARK_COMMON_{LD}_%KEY%, TILE_VAL, LX, LY
	SIF RESULT == 1
		GOTO NO_COMMON
	LOCALS = %RESULTS%
