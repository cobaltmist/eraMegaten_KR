;=======================================================================
;탈출時の이벤트
;=======================================================================
@EVENT_DUNGEON_END_8
IF 던전플래그:8:11 && !던전플래그:8:12
	CALL SHOW_PICTURE, "D"
	CALL MESSAGE_WINDOW_D, "", "＞경시청을 구한 보수로 100000엔을 입수했다/＞또한、200의 명성을 얻었다"
	MONEY += 100000
	FLAG:명성 += 200
	던전플래그:8:12 = 1
ENDIF

