	CALL TREASURE_8
	RETURN RESULT
	
;=======
;階段
;=======
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 6
	CALL UPSTAIRS_8
	RETURN RESULT

ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 7
	CALL DOWNSTAIRS_8
	RETURN RESULT


