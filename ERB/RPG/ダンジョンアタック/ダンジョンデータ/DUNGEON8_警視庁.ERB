;=======================================================================
;[5]調べる　実行時の이벤트
;=======================================================================
@EVENT_CHECK_DUNGEON_8

;==========
;出口の処理
;==========
IF DA:(FLAG:현X):(FLAG:현Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 3
	CALL TREASURE_8
	RETURN RESULT
	
;=======
;階段
;=======
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 6
	CALL UPSTAIRS_8
	RETURN RESULT

ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 7
	CALL DOWNSTAIRS_8
	RETURN RESULT


ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 9
	CALL DUNGEON_TERMINAL
	RETURN 1
ELSE
	CALL MESSAGE_WINDOW_D, , "＞何も無いようだ"
	RETURN 0
ENDIF

