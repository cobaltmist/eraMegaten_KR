				CALL HEALTH_CHARA,(CHARANUM-1)
				CALL BATTLE_START

				FLAG:도주불가플래그 = 0
				FLAG:회화불능플래그 = 0
				FLAG:선제기습취소 = 0
				CALL MESSAGE_WINDOW_D, "イサク", "闇の貴公子よ。炎の使徒に会いなさい…"
				CALL MESSAGE_WINDOW_D, "イサク", "光の力に従うあの方が、アミを望んだのですから…"
				던전플래그:98:0 = 1
			ENDIF
		ENDIF
	CASE  200
		IF 던전플래그:98:0 == 1
			IF 이벤트플래그:102:0 == 3 
				CALL MESSAGE_WINDOW_D, "声", "私と、相反する力を感じる"
