@ENEMY_BATTLE_MESSAGE_K900, ARG
IF CFLAG:ARG:입력행동 == [[스킬:돌핀킥]]
	IF CFLAG:ARG:데미지흡수량 < MAXBASE:ARG:ＨＰ/2 && BASE:ARG:ＨＰ > MAXBASE:ARG:ＨＰ/2
		CALL MESSAGE_B(1, "날 방해하는 거냐 큐！")
	ELSE
		CALL MESSAGE_B(1, "이、인간 따위 나에겐 이길 수 없다구 큐！")
	ENDIF
ELSEIF CFLAG:ARG:입력행동 == [[스킬:마하・부흐라]]
	IF CFLAG:ARG:데미지흡수량 < MAXBASE:ARG:ＨＰ/4
		CALL MESSAGE_B(1, "마법은 뭐든지 흡수하지 큐")
	ELSEIF CFLAG:ARG:데미지흡수량 < MAXBASE:ARG:ＨＰ/3
		CALL MESSAGE_B(1, "뭐、마법은 뭐든지 흡수해 버리지 큐")
	ELSE
		CALL MESSAGE_B(1, "적당히 소용없는 짓 좀 그만하라고 큐！")
	ENDIF
ELSEIF CFLAG:ARG:입력행동 == [[스킬:메가톤프레스]]
	IF CFLAG:ARG:데미지흡수량 < MAXBASE:ARG:ＨＰ/4
		CALL MESSAGE_B(1, "나에게 마법공격은 통하지 않는다고 말했잖아 큐")
	ELSEIF CFLAG:ARG:데미지흡수량 < MAXBASE:ARG:ＨＰ/3
		CALL MESSAGE_B(1, "마법은 통하지 않는다고 말했다고 큐！")
	ELSE
		CALL MESSAGE_B(1, "사、사람 말 좀 들어 큐～～～！")
	ENDIF
ELSEIF CFLAG:ARG:입력행동 == [[스킬:부흐라]] || CFLAG:ARG:입력행동 == [[스킬:섬머솔트]]
	IF BASE:ARG:ＨＰ < MAXBASE:ARG:ＨＰ/4
		CALL MESSAGE_B(1, "너、너희들 뇌근육에 이상있냐 큐！")
	ELSEIF BASE:ARG:ＨＰ < MAXBASE:ARG:ＨＰ/2
		CALL MESSAGE_B(1, "어、어라？ 뭔가 이상한걸 큐")
	ELSE
		CALL MESSAGE_B(1, "물리공격은 골칫거리 큐、무서워 큐～")
	ENDIF
ENDIF

;스내피の사고패턴
@SET_ACTION_스내피,ARG
IF FLAG:경과턴수 == 0
	CALL CHECK_ACTIONABLE,ARG,[[스킬:돌핀킥]]
	IF RESULT == 1
		CFLAG:ARG:입력행동 = [[스킬:돌핀킥]]
		CALL RANDOM_TARGET,ARG,[[스킬:돌핀킥]]
		RETURN 0
	ELSE
		GOTO DECIDE_ACTION
	ENDIF
ENDIF
SIF FLAG:경과턴수 > 0
	CFLAG:ARG:행동횟수 = 2

IF CFLAG:ARG:１more플래그 > 0
	IF CFLAG:ARG:행동한횟수 == 1
		CALL CHECK_ACTIONABLE,ARG,[[스킬:돌핀킥]]
		IF RESULT == 1
			CFLAG:ARG:입력행동 = [[스킬:돌핀킥]]
			CALL RANDOM_TARGET,ARG,[[스킬:돌핀킥]]
		ELSE
			GOTO DECIDE_ACTION
		ENDIF
	ELSE
			GOTO DECIDE_ACTION
	ENDIF
ELSEIF FLAG:경과턴수 % 4 == 0 && CFLAG:ARG:행동한횟수 == 0
	IF CFLAG:ARG:행동한횟수 == 1
		CALL CHECK_ACTIONABLE,ARG,[[스킬:돌핀킥]]
		IF RESULT == 1
			CFLAG:ARG:입력행동 = [[스킬:돌핀킥]]
			CALL RANDOM_TARGET,ARG,[[스킬:돌핀킥]]
		ELSE
			GOTO DECIDE_ACTION
		ENDIF
	ELSE
			GOTO DECIDE_ACTION
	ENDIF
ELSE
	$DECIDE_ACTION
	CALL SEARCH_ACT,ARG
	IF RESULT == 0
		CFLAG:ARG:입력행동 = -1
		CFLAG:ARG:방어플래그 = 1
		RETURN 0
	ENDIF
	LOCAL = RAND:RESULT
	;스킬番号決定
	LOCAL:1 = S:(10+LOCAL)
	$TARGET_LOOP
	
	CALL RANDOM_TARGET,ARG,LOCAL:1
	CALLFORM SKILL_TARGET_{LOCAL:1}
	;ここで스킬番号を行動内容に代入
	SIF CFLAG:ARG:타겟 == -1
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력행동 = LOCAL:1
ENDIF


