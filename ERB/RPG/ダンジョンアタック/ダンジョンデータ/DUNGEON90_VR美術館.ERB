;=======================================================================
;[5]조사　런타임 이벤트
;=======================================================================
@EVENT_CHECK_DUNGEON_90

;==========
;出口の処理
;==========
IF DA:(FLAG:현X):(FLAG:현Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
;% 10は10で割ったあまり、つまり下一桁がでてきます
;그리고 、下一桁はタイルの종류を表す
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 3
	CALL TREASURE_90
	RETURN RESULT
;=======
;階段
;=======
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 6
	CALL UPSTAIRS_90
	RETURN RESULT

ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 7
	CALL DOWNSTAIRS_90
	RETURN RESULT

ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 9
	CALL DUNGEON_TERMINAL
	RETURN 1


ELSEIF 던전플래그:90:2 == 1 && 던전플래그:90:3 == 1 && 던전플래그:90:4 == 1
	PRINTFORMW %조사처리(NAME:MASTER,"는")% 포켓 안의 말을 확인했다
	PRINTFORMW 필요한 말 3개를 모두 갖췄다……
	RETURN 0
ELSEIF 던전플래그:90:2 == 1 && 던전플래그:90:3 == 1
	PRINTFORMW %조사처리(NAME:MASTER,"는")% 포켓 안의 말을 확인했다
	PRINTFORMW 필요한 말 중에서 나이트와 룩을 가지고 있다……
	RETURN 0
ELSEIF 던전플래그:90:2 == 1 && 던전플래그:90:4 == 1
	PRINTFORMW %조사처리(NAME:MASTER,"는")% 포켓 안의 말을 확인했다
	PRINTFORMW 필요한 말 중에서 나이트와 비숍을 가지고 있다……
	RETURN 0
ELSEIF 던전플래그:90:3 == 1 && 던전플래그:90:4 == 1
	PRINTFORMW %조사처리(NAME:MASTER,"는")% 포켓 안의 말을 확인했다
	PRINTFORMW 필요한 말 중에서 룩과 비숍을 가지고 있다……
	RETURN 0
ELSEIF 던전플래그:90:2 == 1
	PRINTFORMW %조사처리(NAME:MASTER,"는")% 포켓 안의 말을 확인했다
	PRINTFORMW 필요한 말 중에서 나이트만을 가지고 있다……
	RETURN 0
ELSEIF 던전플래그:90:3 == 1
	PRINTFORMW %조사처리(NAME:MASTER,"는")% 포켓 안의 말을 확인했다
	PRINTFORMW 필요한 말 중에서 룩만을 가지고 있다……
	RETURN 0
ELSEIF 던전플래그:90:4 == 1
	PRINTFORMW %조사처리(NAME:MASTER,"는")% 포켓 안의 말을 확인했다
	PRINTFORMW 필요한 말 중에서 비숍만을 가지고 있다……
	RETURN 0

ELSE
	PRINTW 아무 것도 없는 것 같다
	RETURN 0
ENDIF

;=======================================================================
;タイル進入時の이벤트
;=======================================================================
@EVENT_ENTER_DUNGEON_90,ARG
;ARG = 進入時の向き　0の場合ワープなど
;==========
;出口の処理
;==========
IF DA:(FLAG:현X):(FLAG:현Y) % 10 == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 3
	CALL TREASURE_90
	RETURN RESULT
;=======
;階段
;=======
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 6
	CALL UPSTAIRS_90
	RETURN RESULT

ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 7
	CALL DOWNSTAIRS_90
	RETURN RESULT
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 9
	CALL DUNGEON_TERMINAL
	RETURN 1

;========
;ワープの処理
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 4
	SELECTCASE FLAG:현M
		CASE 0
			IF FLAG:현X == 15 && FLAG:현Y == 7
				FLAG:현M = 2
				FLAG:현X = 8
				FLAG:현Y = 16
			ENDIF
		CASE 1
			IF FLAG:현X == 15 && FLAG:현Y == 10
				FLAG:현M = 4
				FLAG:현X = 16
				FLAG:현Y = 16
			ELSEIF FLAG:현X == 1 && FLAG:현Y == 10
				FLAG:현M = 3
				FLAG:현X = 7
				FLAG:현Y = 6
			ENDIF
		CASE 2
			IF FLAG:현X == 8 && FLAG:현Y == 16
				FLAG:현M = 0
				FLAG:현X = 15
				FLAG:현Y = 7
			ENDIF
		CASE 3
			IF FLAG:현X == 7 && FLAG:현Y == 6
				FLAG:현M = 1
				FLAG:현X = 1
				FLAG:현Y = 10
			ELSEIF FLAG:현X == 7 && FLAG:현Y == 5
				FLAG:현X = 9
				FLAG:현Y = 6
			ELSEIF FLAG:현X == 9 && FLAG:현Y == 6
				FLAG:현X = 7
				FLAG:현Y = 5
			ELSEIF FLAG:현X == 6 && FLAG:현Y == 7
				FLAG:현X = 4
				FLAG:현Y = 13
			ELSEIF FLAG:현X == 4 && FLAG:현Y == 13
				FLAG:현X = 6
				FLAG:현Y = 7
			ELSEIF FLAG:현X == 5 && FLAG:현Y == 14
				FLAG:현X = 16
				FLAG:현Y = 13
			ELSEIF FLAG:현X == 16 && FLAG:현Y == 13
				FLAG:현X = 5
				FLAG:현Y = 14
			ELSEIF FLAG:현X == 3 && FLAG:현Y == 14
				FLAG:현X = 14
				FLAG:현Y = 1
			ELSEIF FLAG:현X == 14 && FLAG:현Y == 1
				FLAG:현X = 3
				FLAG:현Y = 14
			ELSEIF FLAG:현X == 14 && FLAG:현Y == 14
				FLAG:현X = 16
				FLAG:현Y = 4
			ELSEIF FLAG:현X == 16 && FLAG:현Y == 4
