;RETURN CSV番号 , 人数 , 레벨
SELECTCASE RAND:2
	CASE 0
		RETURN [[キャラ:아귀]] , 3 + RAND:10 , CSVBASE([[キャラ:아귀]] , GETNUM(BASE , "LV") , 0) + RAND:3
	CASE 1
		RETURN [[キャラ:좀비아미]] , 3 + RAND:10 , CSVBASE([[キャラ:좀비아미]] , GETNUM(BASE , "LV") , 0) + RAND:3
ENDSELECT





