SELECTCASE ARG %10
	CASE 1
		IF ARG:1 == 7 && ARG:2 == 10
			RESULTS = 上
		ELSEIF ARG:1 == 5 && ARG:2 == 13
			RESULTS = 兵
			
		ELSEIF ARG:1 == 9 && ARG:2 == 21
			RESULTS = 兵
		ELSEIF ARG:1 == 6 && ARG:2 == 24
			RESULTS = 兵
		ELSEIF ARG:1 == 6 && ARG:2 == 22 && (FLAG:미조우걸음수 / 3) % 2 == 0
			RESULTS = 上
		ELSEIF ARG:1 == 6 && ARG:2 == 22
			RESULTS = 左
			
		ELSEIF ARG:1 == 4 && ARG:2 == 35 && FLAG:미조우걸음수 % 2 == 0
			RESULTS = 左
		ELSEIF ARG:1 == 4 && ARG:2 == 35
			RESULTS = 右
		ELSEIF ARG:1 == 4 && ARG:2 == 36 && (FLAG:미조우걸음수 / 2) % 2 == 0
			RESULTS = 左
		ELSEIF ARG:1 == 4 && ARG:2 == 36
			RESULTS = 右
		ELSEIF ARG:1 == 6 && ARG:2 == 36 && FLAG:미조우걸음수 % 2 == 0
			RESULTS = 上
		ELSEIF ARG:1 == 6 && ARG:2 == 36
			RESULTS = 下
		ELSEIF ARG:1 == 6 && ARG:2 == 38
			RESULTS = 兵
		ELSE
			RESULTS = □
		ENDIF
	CASE 3
		SETCOLOR 0xffff00
		IF ARG:1 == 8 && ARG:2 == 23
			RESULTS = 兵
		ELSE
			RESULTS = １
		ENDIF
	CASE 5
		SETCOLOR 0xffb900
		IF ARG:1 == 8 && ARG:2 == 23
			RESULTS = 兵
		ELSEIF ARG:1 == 4 && ARG:2 == 34 && (FLAG:미조우걸음수 / 2) % 2 == 0
