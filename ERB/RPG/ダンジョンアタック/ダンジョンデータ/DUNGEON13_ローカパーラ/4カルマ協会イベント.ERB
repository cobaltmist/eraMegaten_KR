					RETURN 1
				ENDIF
			ELSE
				FOR LOCAL:1, 4, 16
					LOCALS = %GET_TYPE(LOCAL:1)%
					IF MAXBASE:POS(LOCAL):LOCALS > 100 && MAXBASE:POS(LOCAL):LOCALS != 999
						SIF LOCAL:1 > 7 && LOCAL:1 < 12
