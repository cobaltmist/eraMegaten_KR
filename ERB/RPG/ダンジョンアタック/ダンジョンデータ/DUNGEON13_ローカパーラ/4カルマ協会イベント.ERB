								RETURN 1
							ELSE
								CFLAG:ARG:타겟 = 18
								RETURN 1
							ENDIF
						ENDIF
					ENDIF
				NEXT
				IF MAXBASE:POS(LOCAL):핵열 > 100 && MAXBASE:POS(LOCAL):핵열 != 999
					CFLAG:ARG:입력행동 = [[스킬:마하기가리온]]
					IF CFLAG:ARG:입력행동 > 0
						IF LOCAL < 4
							CFLAG:ARG:타겟 = 17
							RETURN 1
						ELSE
							CFLAG:ARG:타겟 = 18
							RETURN 1
						ENDIF
					ENDIF
				ENDIF
			ENDIF
		ENDIF
