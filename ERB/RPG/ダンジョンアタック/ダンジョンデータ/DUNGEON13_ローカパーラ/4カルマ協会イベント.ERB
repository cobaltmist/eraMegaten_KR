;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@kyoukai_ENTER,ARG,ARG:1
;DA:ARG:(ARG:1)
;ARG、ARG:1はそれぞれ대상のマスのX、Y座標を表しています。
;この던전ではエレベーターの扉が一度右側から開けておかないと左側からは開けられない、という処理に사용しています。
SELECTCASE FLAG:현M
	CASE 23
ENDSELECT
RETURN 1

;=======================================================================
;協会タイル
;=======================================================================
;이벤트タイル特殊표시4
@kyoukai_TILE, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 1600,1601,1602,2500,2501
		SETCOLOR 0x00ff00
		RESULTS = ｍ
	CASE 1603
		SETCOLOR 0x00ff00
		RESULTS = 杏
	CASE 1604
		SETCOLOR 0x00ff00
		RESULTS = 竜
	CASE 1605
		SETCOLOR 0x00ff00
		RESULTS = サ
	CASE 1606
		SETCOLOR 0x00ff00
		RESULTS = ロ
	CASE 1607
		SETCOLOR 0x00ff00
		RESULTS = ゲ
	CASE 1700
		SETCOLOR 0x00ff00
		RESULTS = ゲ
	CASE 1800
		SETCOLOR 0x00ff00
		RESULTS = ｍ
	CASE 2001
		IF 던전플래그:13:12 % 5 > 0
			SETCOLOR 0x00ff00
			RESULTS = Ｈ
		ELSE
			RESULTS = □
		ENDIF
	CASE 2000
		SETCOLOR 0x00ff00
		RESULTS = 竜
	CASE 2200
			RESULTS = □
	CASE 2300
		IF 던전플래그:13:11 < 4
			SETCOLOR 0x00ff00
			RESULTS = サ
		ELSE
			RESULTS = □
		ENDIF
	CASE 2301
		IF 던전플래그:13:12 % 11 > 0
			SETCOLOR 0x00ff00
			RESULTS = Ｈ
		ELSE
			RESULTS = □
		ENDIF
	CASE 2303
		IF 던전플래그:13:11 < 5
			RESULTS = □
		ELSE
			SETCOLOR 0x00ff00
			RESULTS = ゲ
		ENDIF
	CASE 2302
		IF 던전플래그:13:11 < 5
			RESULTS = □
		ELSE
			SETCOLOR 0x00ff00
			RESULTS = 竜
		ENDIF
	CASE 2304
		IF 던전플래그:13:11 < 5
			RESULTS = □
		ELSE
			SETCOLOR 0x00ff00
			RESULTS = ロ
		ENDIF
	CASE 2305
		IF 던전플래그:13:11 < 5
			RESULTS = □
		ELSE
			SETCOLOR 0x00ff00
			RESULTS = 杏
		ENDIF
	CASE 2306
		IF 던전플래그:13:11 < 5
			RESULTS = □
		ELSE
			SETCOLOR 0x00ff00
			RESULTS = サ
		ENDIF
	CASEELSE
		RESULTS = ！
ENDSELECT

;이벤트タイル特殊표시9
@kyoukai_TILE2, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 1700,1701
		RESULTS = □
	CASE 1900
		IF 던전플래그:13:11 < 3
			RESULTS = ！
		ELSE
			RESULTS = □
		ENDIF
	CASE 2200
		RESULTS = ↑
	CASE 2300
		RESULTS = 문
	CASE 1301
		RESULTS = ↓
	CASEELSE
		RESULTS = □
ENDSELECT

