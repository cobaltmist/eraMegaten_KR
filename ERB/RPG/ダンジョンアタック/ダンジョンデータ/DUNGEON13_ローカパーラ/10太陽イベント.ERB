SELECTCASE ARG / 10
	;서프
	CASE 5401
		SETCOLOR 0x00ff00
		RESULTS = サ
	CASEELSE
		RESULTS = ！
		SETCOLOR 0x00ff00
		RESULTS = ｍ
ENDSELECT

