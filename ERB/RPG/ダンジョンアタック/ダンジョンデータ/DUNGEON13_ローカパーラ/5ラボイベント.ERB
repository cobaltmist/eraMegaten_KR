;이벤트タイル特殊표시9
@rabo_TILE2, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 100, 200, 300
		RESULTS = ↑
	CASE 3300
		IF 던전플래그:13:15 < 1
			RESULTS = ！
		ELSE
			RESULTS = □
		ENDIF
	CASE 3200
		IF 던전플래그:13:15 < 2
			RESULTS = ！
		ELSE
			RESULTS = □
		ENDIF
	CASE 3503,3502,3501,3400,3403,3402,3401
		RESULTS = □
	CASEELSE
		RESULTS = ！
ENDSELECT

