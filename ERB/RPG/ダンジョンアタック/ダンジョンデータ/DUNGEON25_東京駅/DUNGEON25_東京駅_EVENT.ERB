CALL MESSAGE_WINDOW_D, "",			 @"＞美しくデザインされた白い石畳と緑の植え込み。/計画的に設計された公園を思わせる港町が眼下に広がっている。"
[IF_DEBUG]
	CALL INPUT_YN_D("던전スキップ", "")
	IF RESULT == 0
		이벤트플래그:86:1 = 8
		CALL MESSAGE_WINDOW_D, "",					 "＞이벤트플래그:86:1 == 8"
		FLAG:탈출 = 1
	ENDIF
[ENDIF]

@EVENT_HARBOR_TOWN
SELECTCASE  FLAG:현M
	;港町
	CASE 20
		;自慢げな女性
		IF FLAG:현X == 21 && FLAG:현Y == 21
			CALL MESSAGE_WINDOW_D, "自慢げな女性",			 @"見て、あたしのカレ/背が高くて細くてモデルみたいでしょ？"
			CALL MESSAGE_WINDOW_D, "自慢げな女性",			 @"並んで立ってるだけで注目の的。もう大変。"
		;グッド・ルッキング・ガイ
		ELSEIF FLAG:현X == 22 && FLAG:현Y == 21
			CALL MESSAGE_WINDOW_D, "グッド・ルッキング・ガイ",			 @"ア、ゴメナサイ…/コトバ、アマリ…ニガテデス。"
			CALL MESSAGE_WINDOW_D, "グッド・ルッキング・ガイ",			 @"（チョロイもんだよなぁ、ここの女は外国人の男に弱くてよ。）"
			CALL MESSAGE_WINDOW_D, "グッド・ルッキング・ガイ",			 @"（次は思いっきり甘えてよ、新しい背広でも買ってもらうかな。）"
		;胸毛の豊かなガイ
		ELSEIF FLAG:현X == 22 && FLAG:현Y == 17
			CALL MESSAGE_WINDOW_D, "胸毛の豊かな水夫",		 @"I'm gonna run…/I'm gonna run…"
			CALL MESSAGE_WINDOW_D, "胸毛の豊かな水夫",		 @"（鳥デカスギ、トメラレナイヨ。早メニ、逃ゲルガ、賢イヨ。）"
		;ニヤケ面のガイ
		ELSEIF FLAG:현X ==  4 && FLAG:현Y == 15
			CALL MESSAGE_WINDOW_D, "ニヤケ面のガイ",		 @"ワタシ、ニ、聞イテモ/ムダムダムダぁ、ネ。"
			CALL MESSAGE_WINDOW_D, "ニヤケ面のガイ",		 @"（ワタシ、女ショーカイ/アノ人カラ、在留許可所チョーダイ。）"
			CALL MESSAGE_WINDOW_D, "ニヤケ面のガイ",		 @"（アノ人、ガッポリ、ワタシ、来日OK/マッタクモッテ、アノ人サマサマネ）"
		;豊満なレディ
		ELSEIF FLAG:현X == 13 && FLAG:현Y == 12
			CALL MESSAGE_WINDOW_D, "豊満なレディ",			 @"柵の向こうのだだっ広いのは私有地ネ。/入ったら怒られるよ。"
			CALL MESSAGE_WINDOW_D, "豊満なレディ",			 @"海、隔てちゃっテルから/入れっこないけどネ。"
		;柵越しに見る教会
		ELSEIF FLAG:현X == 12 && FLAG:현Y == 11
			CALL MESSAGE_WINDOW_D, ""			,			 @"＞運河を隔てた向かい岸に、巨大な教会が見える。/ここからは、行けそうにない。"
		;レストラン
		ELSEIF FLAG:현X == 21 && FLAG:현Y == 20
			CALL MESSAGE_WINDOW_D, ""			,			 @"＞イタリアンレストランだ。"
			CALL MESSAGE_WINDOW_D, ""			,			 @"＞店内からオリーブオイルとニンニクの香りがしてくる。"
		;サロン
		ELSEIF FLAG:현X == 16 && FLAG:현Y == 24
			CALL MESSAGE_WINDOW_D, ""			,			 @"＞サロンだ。"
			CALL MESSAGE_WINDOW_D, ""			,			 @"＞店内にはアンティークな家具が配置され/気品漂う空間を演出している。"
			CALL MESSAGE_WINDOW_D, ""			,			 @"＞教養のありそうな客同士が/談笑している様が伺える。"
		;お散歩ばあさん
		ELSEIF FLAG:현X == 10 && FLAG:현Y == 28
			CALL MESSAGE_WINDOW_D, "お散歩ばあさん",		 @"さっき原…/あぁ、今は女将さんだったねぇ。"
			CALL MESSAGE_WINDOW_D, "お散歩ばあさん",		 @"船着き場の方にいたわ、女将さん。/相変わらずキリっとした美人さんだったよ。"
		;カーニバル・レディ
		ELSEIF FLAG:현X == 17 && FLAG:현Y == 35
			CALL MESSAGE_WINDOW_D, "カーニバル・レディ",	 @"ニッポンのトノガタ、スケベーだし/イクラでも稼げるヨ。"
			CALL MESSAGE_WINDOW_D, "カーニバル・レディ",	 @"ホーリツ怖くナイヨ、助ットイルシ/マッタクモテ、アノ人サマサマネ。"
		;女学生
		ELSEIF FLAG:현X ==  3 && FLAG:현Y == 35
			CALL MESSAGE_WINDOW_D, "女学生",				 @"こないだ外国人の男の人に声かけられたの。"
			CALL MESSAGE_WINDOW_D, "女学生",				 @"…でもさ、ヒゲ面でしつこくて/なんか怪しげだったから無視したわ。"
		;女将
		ELSEIF FLAG:현X == 24 && FLAG:현Y == 15
			IF 던전플래그:25:35 == 0
				CALL MESSAGE_WINDOW_D, "女将",			 @"来てくれたね"
				CALL MESSAGE_WINDOW_D, "女将",			 @"今、船が沖合に到着して、これから荷揚げするよ。/準備はいいかい？"
				CALL INPUT_YN_D("예", "아니오")
				IF RESULT == 0
					CALL MESSAGE_WINDOW_D, "",			 @"＞荷揚げを緊張した面持で見守る女将/そこへ、息せき切って水夫が一人駆け付けた。"
					CALL MESSAGE_WINDOW_D, "水夫",		 @"…た、たいへんです！！/化け物が！？"
					CALL MESSAGE_WINDOW_D, "女将",		 @"来たね…"
					CALL MESSAGE_WINDOW_D, "",			 @"＞辺り一面は妙に薄暗く、周囲のMAG濃度も急上昇している。/何者かがこの辺りを異界にしたようだ。"
					CALL MESSAGE_WINDOW_D, "女将",		 @"%CALLNAME:MASTER%さん、仕事だよ/うちの水夫たちを化け物から守っておくれ！"
					던전플래그:25:35 = 1
					CALL DUNGEON_WORP, 21, 24, 15
				ENDIF
			ELSEIF 던전플래그:25:35 == 5
				CALL MESSAGE_WINDOW_D, "女将",			 @"やってくれたね%CALLNAME:MASTER%さん！/水夫たちも無事だし、積荷の方も無事さ、傷一つ、ついちゃいないよ。"
				CALL MESSAGE_WINDOW_D, "女将",			 @"ようやく原田商会の面目躍如だよ/取引先にも顔が立つってもんさ。"
				CALL MESSAGE_WINDOW_D, "女将",			 @"本当に…本当に恩に着るよ、%CALLNAME:MASTER%さん。"
				CALL MESSAGE_WINDOW_D, "女将",			 @"ところで…エルフマンに会いたいんだってね？/話は聞いてるよ。"
				CALL MESSAGE_WINDOW_D, "女将",			 @"兄さんが死んでから…すっかり疎遠になってしまったけど/エルフマンの私有地に入れるように紹介するくらいは出来るよ。"
				CALL MESSAGE_WINDOW_D, "女将",			 @"船で行くからね、ついて来ておくれ。"
				CALL DUNGEON_WORP, 20, 28, 6
			ENDIF
		ELSEIF FLAG:현X == 12 && FLAG:현Y ==  3
			CALL MESSAGE_WINDOW_D, "",			 @"＞天主教会の入り口だ。中に入りますか？"
			CALL INPUT_YN_D("예", "아니오")
			IF RESULT == 0
				CALL DUNGEON_WORP, 22,  8, 11
			ENDIF
		ELSEIF FLAG:현X ==  5 && FLAG:현Y ==  6
			CALL MESSAGE_WINDOW_D, "",			 @"←　外国人墓地"
		ELSEIF FLAG:현X ==  3 && FLAG:현Y ==  5
			CALL MESSAGE_WINDOW_D, "",			 @"＞目の前の霊魂が無念そうな表情で地面を見つめている…。"
			CALL MESSAGE_WINDOW_D, "",			 @"＞そこには、何かを埋めたような形跡がある。/掘り返してみますか？"
			CALL INPUT_YN_D("예", "아니오")
			IF RESULT == 0
				CALL MESSAGE_WINDOW_D, "",				 @"＞地面の下から白骨死体が現れた！"
				CALL MESSAGE_WINDOW_D, "",				 @"＞時間経過によって白骨化したものではない。"
				CALL MESSAGE_WINDOW_D, "",				 @"＞まるで悪魔に飲み込まれ、骨だけが吐き出されたような…/真新しく、美しい頭蓋骨だ。"
				CALL MESSAGE_WINDOW_D, "",				 @"＞…この霊魂と関係があるのだろう/%CALLNAME:MASTER%は霊魂の声に耳を傾けた…。"
				CALL MESSAGE_WINDOW_D, "無念そうな霊魂", @"…ァァ………アノ男、許サヌ…/誰カ、私ノ声ヲ…、聞イテクレ…"
				CALL MESSAGE_WINDOW_D, "",				 @"＞無念そうな亡霊はその声が%CALLNAME:MASTER%に届いたことに気付いた。"
				CALL MESSAGE_WINDOW_D, "無念そうな霊魂", @"オォ！君ハ、私ノ声ガ聞コエルノカネ！"
				CALL MESSAGE_WINDOW_D, "無念そうな霊魂", @"ワタシ、ハ、エルフマン/殺サレタノダ。怪シイ男ノ手デ…。"
				CALL MESSAGE_WINDOW_D, "無念そうな霊魂", @"ソノ男ハ、ワタシニ代ワリ、貿易ヲ行ナウ、ト、イッタ。"
				CALL MESSAGE_WINDOW_D, "無念そうな霊魂", @"ワタシ、ノ、名ヲ、カタリ/ワタシ、ニ、成済マシテ…。"
				CALL MESSAGE_WINDOW_D, "無念そうな霊魂", @"…ワタシ、ノ、仇、ヲ、トッテ欲シイ…。"
				CALL MESSAGE_WINDOW_D, "無念そうな霊魂", @"＞霊魂は語り終えると静かに消えていった。"
				던전플래그:25:39 = 1
				CALL MAKE_FLOOR, FLAG:현던전
				CALL AUTOMAP
			ENDIF
		ENDIF
	;港町(襲撃)
	CASE 21
		IF (FLAG:현X == 12 && FLAG:현Y == 22) || (FLAG:현X == 20 && FLAG:현Y == 34) || (FLAG:현X ==  2 && FLAG:현Y == 36)
			IF 던전플래그:25:35 == 1
				CALL MESSAGE_WINDOW_D, "",					 @"＞水夫が巨大な鉤爪を持った鳥の悪魔に襲われている。"
				CALL MESSAGE_WINDOW_D, "粋な水夫",			 @"ひ～助けてくれぇ"
				FLAG:도주불가플래그 = 1
				FLAG:회화불능플래그 = 1
				FOR LOCAL:2 ,7, 12
					CALL SET_ENEMY,LOCAL:2,[[キャラ:안주]],46+RAND:5,0,0
					CALL SYNC_STATUS,(CHARANUM-1)
					CALL HEALTH_CHARA,(CHARANUM-1)
				NEXT
				CALL BATTLE_START
				FLAG:도주불가플래그 = 0
				FLAG:회화불능플래그 = 0
				CALL MESSAGE_WINDOW_D, "粋な水夫",			 @"あんた強いなぁ！/その強さ、惚れ惚れするねぇ！"
				CALL MESSAGE_WINDOW_D, "粋な水夫",			 @"積荷を運んでいたら急に化け物に襲われて…。"
				CALL MESSAGE_WINDOW_D, "粋な水夫",			 @"一緒にいた仲間たちともはぐれちまって/どうなったかわかんねえんだ！"
				CALL MESSAGE_WINDOW_D, "粋な水夫",			 @"なぁ、俺の仲間も助けてくれよ！/町のどこかにいるはずなんだ、頼むよ。"
				던전플래그:25:35 += 1
			ELSEIF 던전플래그:25:35 == 2
				CALL MESSAGE_WINDOW_D, "",					 @"＞水夫が巨大な鉤爪を持った鳥の悪魔に襲われている。"
				CALL MESSAGE_WINDOW_D, "オドオドした水夫",	 @"ひゃ～助けてくれぇ"
				FLAG:도주불가플래그 = 1
				FLAG:회화불능플래그 = 1
				FOR LOCAL:2 ,7, 12
					CALL SET_ENEMY,LOCAL:2,[[キャラ:안주]],46+RAND:5,0,0
					CALL SYNC_STATUS,(CHARANUM-1)
					CALL HEALTH_CHARA,(CHARANUM-1)
				NEXT
				CALL BATTLE_START
				FLAG:도주불가플래그 = 0
				FLAG:회화불능플래그 = 0
				CALL MESSAGE_WINDOW_D, "オドオドした水夫",	 @"助かったよ！ありがとう！"
				CALL MESSAGE_WINDOW_D, "オドオドした水夫",	 @"襲われている仲間が、他にもいるんだ。/お願いだ、仲間をたすけてくれ！"
				던전플래그:25:35 += 1
			ELSEIF 던전플래그:25:35 == 3
				CALL MESSAGE_WINDOW_D, "",					 @"＞水夫が巨大な鉤爪を持った鳥の悪魔に襲われている。"
				CALL MESSAGE_WINDOW_D, "いなせな水夫",	 @"ひぇ～助けてくれぇ"
				FLAG:도주불가플래그 = 1
				FLAG:회화불능플래그 = 1
				FOR LOCAL:2 ,7, 12
					CALL SET_ENEMY,LOCAL:2,[[キャラ:안주]],46+RAND:5,0,0
					CALL SYNC_STATUS,(CHARANUM-1)
					CALL HEALTH_CHARA,(CHARANUM-1)
				NEXT
				CALL BATTLE_START
				FLAG:도주불가플래그 = 0
				FLAG:회화불능플래그 = 0
				CALL MESSAGE_WINDOW_D, "いなせな水夫",	 @"仲間達は全員助かったみたいだ。/ありがとう！ほんとうに助かったぜ！"
				CALL MESSAGE_WINDOW_D, "",				 @"＞あとはこの異界を生み出した張本人を倒せばいいだろう/おそらく、近辺にいるはずだ。"
				던전플래그:25:35 += 1
			ENDIF
			IF FLAG:현X == 12 && FLAG:현Y == 22
				던전플래그:25:36 = 1
			ELSEIF FLAG:현X == 20 && FLAG:현Y == 34
				던전플래그:25:37 = 1
			ELSEIF FLAG:현X ==  2 && FLAG:현Y == 36
				던전플래그:25:38 = 1
			ENDIF
			CALL MAKE_FLOOR, FLAG:현던전
			CALL AUTOMAP
		ELSEIF FLAG:현X == 22 && FLAG:현Y == 14
			CALL MESSAGE_WINDOW_D, "악마",			 @"ヒーホー！"
			CALL MESSAGE_WINDOW_D, "악마",			 @"最近、この辺りに、ならず者な/悪魔が増えて、困ってるホー。"
			CALL MESSAGE_WINDOW_D, "악마",			 @"怖いので、オイラもトットと/引っ越そうかと思ってるホー。"
			CALL MESSAGE_WINDOW_D, "악마",			 @"でも、あの暴れん坊たち/誰かの命令で動いている気がするホー。"
			CALL MESSAGE_WINDOW_D, "악마",			 @"オイラ、スルドイ観察眼/名探偵になれるかもだホー。"
		ELSEIF FLAG:현X == 15 && FLAG:현Y == 15
			CALL MESSAGE_WINDOW_D, "",				 @"＞道にラスプーチンが召喚に用いる魔トリョーシカが落ちている。/奴がこの襲撃に関わっているのは間違いなさそうだ。"
			CALL MESSAGE_WINDOW_D, "",				 @"＞突如、魔トリョーシカが滑るように%CALLNAME:MASTER%と距離を取り/クルクルと回転しながら浮かび上がった。"
			CALL MESSAGE_WINDOW_D, "",				 @"＞そして、その中から声が聞こえてくる…。"
			CALL MESSAGE_WINDOW_D, "악마",			 @"鳥たちの声が聞こえなくなったと思えば…。"
			CALL MESSAGE_WINDOW_D, "악마",			 @"また、我が主人の邪魔をするのかい？/サマナーさんよぅ。"
			CALL MESSAGE_WINDOW_D, "",				 @"＞地面を震わせる閃光と衝撃と共に、騎乗した悪魔が現れ/漆黒の馬上から%CALLNAME:MASTER%に剣を向けた。"
			CALL MESSAGE_WINDOW_D, "악마",			 @"フハハハ…、そうだよ俺が荷物を頂いてたってわけだ。"
			CALL MESSAGE_WINDOW_D, "악마",			 @"我が主人の思惑通りコトが運んでいるって寸法さ。"
			CALL MESSAGE_WINDOW_D, "악마",			 @"…というわけで商売の邪魔だ。/とっとと死んでくれよな！"
			FLAG:도주불가플래그 = 1
			FLAG:회화불능플래그 = 1
			
			CALL SET_ENEMY,8,[[キャラ:트리글라프]],50+RAND:5,0,0
			CALL SYNC_STATUS,(CHARANUM-1)
			CALL HEALTH_CHARA,(CHARANUM-1)
			
			CALL SET_ENEMY,10,[[キャラ:트리글라프]],50+RAND:5,0,0
			CALL SYNC_STATUS,(CHARANUM-1)
			CALL HEALTH_CHARA,(CHARANUM-1)
			CALL BATTLE_START
			
			FLAG:도주불가플래그 = 0
			FLAG:회화불능플래그 = 0
			CALL MESSAGE_WINDOW_D, "",			 @"＞トリグラフを倒すと異界化が解けた。"
			던전플래그:25:35 = 5
			CALL DUNGEON_WORP, 20, 12, 22
		ENDIF

	;天主教会
	CASE 22
		IF FLAG:현X ==  8 && FLAG:현Y == 12
			CALL MESSAGE_WINDOW_D, "",			 @"＞外に出ますか？"
			CALL INPUT_YN_D("예", "아니오")
			IF RESULT == 0
				CALL DUNGEON_WORP, 20, 14,  4
			ENDIF
		ELSEIF FLAG:현X ==  2 && FLAG:현Y == 10
			CALL MESSAGE_WINDOW_D, "",				 @"＞懺悔室の中から独自のイントネーションの声が聞こえてくる…"
			CALL MESSAGE_WINDOW_D, "男の声",		 @"…えエ、忘れハいませんヨ。安心してミーにお任せくださイ。"
			CALL MESSAGE_WINDOW_D, "男の声",		 @"歴史の特異点…/因果ヲ見極めるため鋭意調査中デ………。"
			CALL MESSAGE_WINDOW_D, "男の声",		 @"はイ、全てはミーにお任せくださイ。必ずヤ、修正してみせまス…。"
			CALL MESSAGE_WINDOW_D, "男の声",		 @"…ふゥ、何時でモ言われた通りニ動くと思ったら大間違いネ。"
			CALL MESSAGE_WINDOW_D, "男の声",		 @"人を人形扱いしてもらっては困ル。/ミーにはミーの考えがあるネ……。"
			CALL MESSAGE_WINDOW_D, "",				 @"＞中に居るのは間違いないようだ。/%CALLNAME:MASTER%がシャっとカーテンを開けるも、そこには誰もいなかった。"
			CALL MESSAGE_WINDOW_D, "",				 @"＞誰かいたという痕跡すらなく/淀んだ冷えた空気があるだけだった。"
			CALL MESSAGE_WINDOW_D, "",				 @"＞ギシ。"
			CALL MESSAGE_WINDOW_D, "",				 @"＞礼拝堂に設得られたベンチのきしむ音に、%CALLNAME:MASTER%が振り返ると/ラスプーチンの姿が目に入った。"
			던전플래그:25:35 = 6
			CALL MAKE_FLOOR, FLAG:현던전
			CALL AUTOMAP
			CALL MESSAGE_WINDOW_D, "ラスプーチン",	 @"…盗み聞きとはあまりいい趣味ではないネ。"
			CALL MESSAGE_WINDOW_D, "ラスプーチン",	 @"しかシ、ユーがここまで来れるとはハ…"
			CALL MESSAGE_WINDOW_D, "ラスプーチン",	 @"ミーの頭脳をもってしてモ、予想できなかっタ。"
			CALL MESSAGE_WINDOW_D, "",				 @"＞%CALLNAME:MASTER%を見つめるラスプーチンが、ゆっくりと言葉をつづけた。"
			CALL MESSAGE_WINDOW_D, "ラスプーチン",	 @"しかシ、これも運命…、ユーとの再会を喜ぶことにしよウ。"
			CALL MESSAGE_WINDOW_D, "",				 @"＞立ち上がり、%CALLNAME:MASTER%に近付きながら、ラスプーチンが問う。"
			던전플래그:25:35 = 7
			CALL DUNGEON_WORP, 22, 8, 8
			CALL MESSAGE_WINDOW_D, "ラスプーチン",	 @"さテ、ユーがここに訪れた本来の訳…。"
			CALL MESSAGE_WINDOW_D, "ラスプーチン",	 @"Ｍｒ.エルフマンを探しテ…という所かネ？"
