;=======================================================================
;タイル調整
;=======================================================================
@MAKE_FLOOR_LINE_COMMON_62, ARG, ARG:1, ARG:2
#LOCALSIZE 30
;LOCAL:3 宝箱 던전フラグ:70~99
;LOCAL:4 이벤트
;(0/0)でLOCALを소거
SIF ARG:1 == 0 && ARG:2 == 0
	VARSET LOCAL
SELECTCASE ARG
	CASE 3
		IF GETBIT(던전フラグ:(FLAG:現던전):(FLAG:現M+70), LOCAL:3)
			DA:(ARG:1):(ARG:2) = 1
		ELSE
			DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:3 * 10 + 3
		ENDIF
		LOCAL:3++
	CASE 4
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:4 * 10 + 4
		LOCAL:4++
	CASE 5
		;DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:5 * 10 + 5
		;LOCAL:5++
	CASE 6
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:6 * 10 + 6
		LOCAL:6++
	CASE 7
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:7 * 10 + 7
		LOCAL:7++
	CASE 8
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:8 * 10 + 8
		LOCAL:8++
	CASE 9
		DA:(ARG:1):(ARG:2) = FLAG:現M *1000 + LOCAL:9 * 10 + 9
		LOCAL:9++

ENDSELECT
;=======================================================================
;[5]조사　実行時の이벤트
;=======================================================================
@EVENT_CHECK_DUNGEON_62
CALL EVENT_DUNGEON_62
RETURN RESULT

;=======================================================================
;タイル進入時の이벤트
;=======================================================================
@EVENT_ENTER_DUNGEON_62,ARG
;ARG = 進入時の向き　0の場合ワープなど
CALL EVENT_DUNGEON_62, ARG , 0
RETURN RESULT

@EVENT_DUNGEON_62, ARG, ARG:1
SELECTCASE DA:(FLAG:現X):(FLAG:現Y) % 10
;==========
;ワープの処理
;==========
	CASE 1
	CALL FLOOR_62
	RETURN RESULT
;==========
;出입の処理
;==========
	CASE 8
		CALL DUNGEON_EXIT
	RETURN 1
;==========
;扉の処理
;==========
	CASE 2
		CALL DUNGEON_DOOR,ARG
		RETURN 1
;========
;宝の処理
;========
	CASE 3
		CALL TREASURE_62
		RETURN 1

;=======
;階段
;=======
	CASE 6
		CALL UPSTAIRS_62
		RETURN 1

	CASE 7
		CALL DOWNSTAIRS_62
		RETURN 1
;==========
;이벤트
;==========
	CASE 4
	SELECTCASE DA:(FLAG:現X):(FLAG:現Y) / 10
	CASE 10
		IF 던전フラグ:62:1 < 4
		CALL MESSAGE_WINDOW_D, "잭프로스트", "큰 소리로 말할 수는 없지만 호―, 그 헬이란 녀석이 온 뒤로 이 거리는 힘들다 호―"
		CALL MESSAGE_WINDOW_D, "잭프로스트", "거리에서 약탈하지, 걔 밑에 붙은 놈들도 제멋대로 굴지 엉망진창이다 호―"
		ENDIF
	CASE 20
		IF 던전フラグ:62:1 == 1
			CALL MESSAGE_WINDOW_D, "설인", "이 마을의 헬이란 녀석은 지독한 녀석이라, 약탈하거나 마구 날뛰는 등 맘대로 굴고 있어"
		ELSEIF 던전フラグ:62:1 == 2 && 던전フラグ:62:2 == 0
			CALL MESSAGE_WINDOW_D, "더스트", "난 도적단의 부두목인 더스트라고 하지"
			CALL MESSAGE_WINDOW_D, "더스트", "헬을 쓰러뜨리는데 협력해주겠다면 동료마가 되어주마!"
		CALL INPUT_YN_D
