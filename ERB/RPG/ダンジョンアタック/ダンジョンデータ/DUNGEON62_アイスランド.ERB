;=======================================================================
;攻略조건
;=======================================================================
@PREREQUISITE_DUNGEON_62
SIF 의뢰フラグ:12:0 == 1
	RETURN 1
RETURN 0

;=======================================================================
;던전名
;=======================================================================
@GET_DUNGEON_NAME_62
RESULTS = 아이스랜드

;=======================================================================
;스타ト地点입력
;=======================================================================
@START_DUNGEON_62, ARG
FLAG:現M = 0
FLAG:現X = 7
FLAG:現Y =15
;CALL AUTOMAP
RETURN 0
;=======================================================================
;스타ト時이벤트
;=======================================================================
@EVENT_DUNGEON_START_62
IF 던전フラグ:62:5 == 0
	CALL MESSAGE_WINDOW_D, "", "＞얼음의 마계 아이스 랜드"
	IF 던전フラグ:61:1 == 1
		CALL MESSAGE_WINDOW_D, "","＞이곳에 있는 마왕 헬이 메시아의 뿔을 가지고 있다고 한다"
	ELSEIF 던전フラグ:61:1 == 2
		CALL MESSAGE_WINDOW_D, "","＞이곳에 있는 마왕 헬이 메시아의 눈동자를 가지고 있다고 한다"
	ENDIF
		CALL MESSAGE_WINDOW_D, "","＞당신은 헬을 만나보기로 했다"
던전フラグ:62:5 = 1
ENDIF
;=======================================================================
;フ로어名표시
;=======================================================================
@FLOORNAME_62
	PRINT 아이스 랜드　
SELECTCASE FLAG:現M
	CASE 0
		PRINTL 프로스트 타운
	CASE 1
		PRINTL 엘류드니르・바깥
	CASE 2
		PRINTL 엘류드니르・１Ｆ
	CASE 3
		PRINTL 엘류드니르・２Ｆ
	CASE 4
		PRINTL 엘류드니르・３Ｆ
	CASE 5
		PRINTL 엘류드니르・마왕의 방
	CASE 6
		PRINTL 엘류드니르・감옥
	CASE 7
		PRINTL 탈출 통로
	CASE 8
		PRINTL 감춰진 평원
	CASE 9
		PRINTL 감춰진 통로
	CASEELSE
ENDSELECT
;=======================================================================
;指定したマスに進入가능か
;=======================================================================
@CAN_ENTER_62, ARG, ARG:1
SELECTCASE DA:ARG:(ARG:1)
	CASE 12
		;上にしか進めない＝上からは無理
		;上である時＝現在のYがARG:1より小さいとき
	IF 던전フラグ:62:1 == 1
		IF ARG:1 < FLAG:現Y
			CALL SET_FLOOR_ANALYZE(2, FLAG:現던전, FLAG:現M, ARG, ARG:1)
			RETURN 0
		ENDIF
	ENDIF

	CASE 22
	IF 던전フラグ:62:6 == 0
		IF ARG:1 > FLAG:現Y
			CALL MESSAGE_WINDOW_D, "", "＞감옥의 문은 닫혀있다"
			CALL SET_FLOOR_ANALYZE(2, FLAG:現던전, FLAG:現M, ARG, ARG:1)
			RETURN 0
		ELSE
		CALL MESSAGE_WINDOW_D, "", "＞닫혀있던 문을 열었다"
		던전フラグ:62:6 =1
		ENDIF
	ENDIF

	CASE 32
	IF 던전フラグ:62:4 == 0
		IF ARG:1 < FLAG:現Y
			CALL MESSAGE_WINDOW_D, "", "＞문은 닫혀있다"
			CALL SET_FLOOR_ANALYZE(2, FLAG:現던전, FLAG:現M, ARG, ARG:1)
			RETURN 0
		ELSE
			CALL MESSAGE_WINDOW_D, "", "＞닫혀있던 문을 열었다"
			던전フラグ:62:4 = 1
			던전フラグ:62:1 = 3
			FLAG:탈출스킬금지 = 0
		ENDIF
	ENDIF
	CASEELSE
ENDSELECT
CALL CAN_ENTER,ARG,ARG:1
RETURN RESULT

;=======================================================================
;特殊タイル표시
;=======================================================================
@TILE_MARK_COMMON_62_1, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 30
	IF 던전フラグ:62:1 >= 2
			RESULTS = ↑
	ELSE
			RESULTS = ■
	ENDIF
	CASE 10 , 20 , 41 , 50 , 60
		RESULTS = ↑
	CASE 11 , 21 , 31 , 40 , 51 , 61
		RESULTS = ↓
	CASEELSE
		RETURN 1
ENDSELECT

@TILE_MARK_COMMON_62_2, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 1
	IF 던전フラグ:62:1 >= 2
			RESULTS = □
	ELSE
			RESULTS = ■
	ENDIF
	CASEELSE
		RETURN 1
ENDSELECT

@TILE_MARK_COMMON_62_4, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE  60
		SETCOLOR 0x00ff00
		RESULTS = ｍ
	CASE  10 , 70
		IF 던전フラグ:62:1 < 4
		SETCOLOR 0x00ff00
		RESULTS = ｍ
		ELSE
		RESULTS = □
		ENDIF
	CASE  20
	IF 던전フラグ:62:1 <= 2 && 던전フラグ:62:2 == 0
		SETCOLOR 0x00ff00
		RESULTS = ｍ
		ELSE
		RESULTS = □
		ENDIF
	CASE  30
	IF 던전フラグ:62:1 <= 2 && 던전フラグ:62:3 == 0
		SETCOLOR 0x00ff00
		RESULTS = ｍ
		ELSE
		RESULTS = □
		ENDIF
	CASE  40
	IF 던전フラグ:62:1 == 3 && 던전フラグ:62:2 == 0
		SETCOLOR 0x00ff00
		RESULTS = ｍ
		ELSE
		RESULTS = □
		ENDIF
	CASE  50
	IF 던전フラグ:62:1 == 3 && 던전フラグ:62:3 == 0
		SETCOLOR 0x00ff00
		RESULTS = ｍ
		ELSE
		RESULTS = □
		ENDIF
	CASE  444
	IF 던전フラグ:62:1 == 1
		SETCOLOR 0x00ff00
		RESULTS = ｍ
		ELSE
		RESULTS = □
		ENDIF
	CASEELSE
		RETURN 1
ENDSELECT


@TILE_MARK_COMMON_62_9, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 222
	IF 던전フラグ:62:1 == 0
			RESULTS = ！
	ELSE
			RESULTS = □
	ENDIF
