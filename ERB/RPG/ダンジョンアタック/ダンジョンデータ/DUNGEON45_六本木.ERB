;=======================================================================
;[5]조사　実行時の이벤트
;=======================================================================
@EVENT_CHECK_DUNGEON_45
CALL EVENT_DUNGEON_45
RETURN RESULT

;=======================================================================
;タイル進入時の이벤트
;=======================================================================
@EVENT_ENTER_DUNGEON_45,ARG
;ARG = 進入時の向き　0の場合ワープなど
CALL EVENT_DUNGEON_45, ARG , 0
RETURN RESULT

@EVENT_DUNGEON_45, ARG, ARG:1
SELECTCASE DA:(FLAG:현X):(FLAG:현Y) % 10

;==========
;出口の処理
;==========
CASE 8
	CALL DUNGEON_EXIT
	RETURN 1
;==========
;扉の処理
;==========
CASE 2
	CALL DUNGEON_DOOR,ARG
	RETURN 1
;========
;宝の処理
;========
CASE 3
	CALL TREASURE_45, DA:(FLAG:현X):(FLAG:현Y) / 10
	RETURN 1
;=======
;階段
;=======
CASE 9
	SELECTCASE DA:(FLAG:현X):(FLAG:현Y)  / 10
	CASE 1
		PRINTL 올라가는 계단이 있다. 올라가겠습니까?
		CALL INPUT_YN,"Yes","No"
		SIF RESULT == 0
			CALL DUNGEON_WORP,2,26,7
	CASE 2
	PRINTL 내려가는 계단이 있다. 내려가겠습니까?
		CALL INPUT_YN,"Yes","No"
		SIF RESULT == 0
			CALL DUNGEON_WORP,0,1,3
	ENDSELECT


CASE 6
	CALL UPSTAIRS_45
	RETURN RESULT

CASE 7
	CALL DOWNSTAIRS_45
	RETURN RESULT
;========
;罠の処理
;========
CASE 5
SELECTCASE DA:(FLAG:현X):(FLAG:현Y)
CASE 15
	CALL DUNGEON_TRAP_HP,10
CASE 25
	CALL DUNGEON_TRAP_MP,10
CASE 35
	CALL DUNGEON_TRAP_POISON,40
ENDSELECT
;==========
;이벤트
;==========
CASE 4
SELECTCASE DA:(FLAG:현X):(FLAG:현Y) / 10
	CASE 100
	;一階　ALICE
	IF 던전플래그:45:10 == 0  
		CALL MESSAGE_WINDOW_D, "", "＞인형을 안은 사랑스러운 여자아이가 있다"
		CALL MESSAGE_WINDOW_D, "여자아이", @"저기저기 \@던전플래그:45:4 ?오빠야#언니야\@/어디서 온 거야?"
			CALL INPUT_SELECT_D, @"[0]다른 거리/[1]팬텀 소사이어티/[2]마계/[3]무시한다"
				SELECTCASE RESULT
					CASE 0
						CALL MESSAGE_WINDOW_D, "여자아이", "다른 거리에서 온 거야?/좋겠다아"
						CALL MESSAGE_WINDOW_D, "여자아이", "난 이 거리에서 나오면 안 돼…"
						CALL MESSAGE_WINDOW_D, "여자아이", "아저씨들한테 혼나거든…"
					CASE 1
						CALL MESSAGE_WINDOW_D, "여자아이", "팬텀 소사이어티?"
						CALL MESSAGE_WINDOW_D, "여자아이", "이상한 이름 붙은 곳에서 왔구나"
					CASE 2
						CALL MESSAGE_WINDOW_D, "여자아이", "헤에― 마계에서 온 거야?"
						CALL MESSAGE_WINDOW_D, "여자아이",@"\@던전플래그:45:4 ?오빠#언니\@는 어째서 인간 모습을 하고 있는 거야?"
					CASE 3
						CALL MESSAGE_WINDOW_D, "여자아이", "왜 그래?"
						CALL MESSAGE_WINDOW_D, "여자아이",@"\@던전플래그:45:4 ?오빠#언니\@도 어디서 왔는지 모르는 거야?"
				ENDSELECT

		CALL MESSAGE_WINDOW_D, "앨리스", "내 이름은 앨리스"
		CALL MESSAGE_WINDOW_D, "앨리스", @"저기 있지…/\@던전플래그:45:4 ?오빠#언니\@한테 부탁이 있어"
		CALL MESSAGE_WINDOW_D, "앨리스", @"\@던전플래그:45:4 ?오빠야#언니야\@、친구가 되어줄래?"
			$INPUT_LOOP_ALICE
				CALL INPUT_YN_D
				IF RESULT == 0
						CALL MESSAGE_WINDOW_D, "앨리스", "고마워♪"
						CALL MESSAGE_WINDOW_D, "앨리스", @"\@던전플래그:45:4 ?오빠#언니\@를 빨간 아저씨 검은 아저씨한테 소개시켜주고 싶어"
						던전플래그:45:10 = 1
				ELSE
						CALL MESSAGE_WINDOW_D, "앨리스", @"\@RAND:2 == 0 ? 내가 싫은 거야?# 사실은 친구가 되고 싶은 거 맞지?\@"
						GOTO INPUT_LOOP_ALICE
				ENDIF
	ENDIF

	IF 던전플래그:45:10 == 1 
		CALL MESSAGE_WINDOW_D, "앨리스", "검은 아저씨는 지하에서 일하고 있어/ 빨간 아저씨는 3층에 있을 테니 가봤으면 좋겠는데"
	ENDIF

	IF 던전플래그:45:10 == 2 && !던전플래그:45:6
		CALL MESSAGE_WINDOW_D, "", "＞인형을 안은 사랑스러운 여자아이 앨리스가 있다"
		CALL MESSAGE_WINDOW_D, "앨리스", @"저기저기、\@던전플래그:45:4 ?오빠야#언니야\@"
		IF FLAG:무후후전개 || 던전플래그:45:5 == 1 ||  던전플래그:45:40 == 2
			CALL MESSAGE_WINDOW_D, "앨리스", @"나와 야한 놀이 할래?/\@던전플래그:45:4 ?오빠#언니\@는 노예 역할이야♪"
				CALL INPUT_YN_D
					IF RESULT == 0
						CALL ALICE_TRAINING
					ELSE
						CALL MESSAGE_WINDOW_D, "앨리스", @"에―엥!/\@던전플래그:45:4 ?오빠#언니\@는 심술쟁이!"
						던전플래그:45:6 = 1
					ENDIF
		ENDIF
	ENDIF
	IF 던전플래그:45:10 == 2
		CALL MESSAGE_WINDOW_D, "앨리스", "챠크라포트를 가지고 싶어/나 주라♪"
		IF ITEM:챠크라포트 >= 1 
			CALL INPUT_YN_D
			IF RESULT == 0
					CALL MESSAGE_WINDOW_D, "앨리스",@"고마워/착한 \@던전플래그:45:4 ?오빠#언니\@"
					던전플래그:45:10 = 1
					ITEM:챠크라포트 -= 1 
					던전플래그:45:10 = 3
			ELSE
					CALL MESSAGE_WINDOW_D, "앨리스", @"\@던전플래그:45:4 ?오빠#언니\@、깍쟁이구나"
			ENDIF
		ELSE
			CALL MESSAGE_WINDOW_D, "앨리스",  "없는 거야?/날 위해 가져와줬으면 좋겠는데"
