;=======================================================================
;攻略조건
;=======================================================================
@PREREQUISITE_DUNGEON_45
IF 던전플래그:18:22 == 1
	RETURN 1
ELSE
	RETURN 0
ENDIF
;=======================================================================
;던전名
;=======================================================================
@GET_DUNGEON_NAME_45
RESULTS = 롯폰기

;=======================================================================
;플로어名표시
;=======================================================================
@FLOORNAME_45
PRINT 롯폰기
SELECTCASE FLAG:현M
	CASE 0
		PRINTL 연결통로
	CASE 1
		PRINTL Ｂ１Ｆ
	CASE 2
		PRINTL １Ｆ
	CASE 3
		PRINTL ２Ｆ
	CASE 4
		PRINTL ３Ｆ
ENDSELECT

;=======================================================================
;스타ト地点입력
;=======================================================================
@START_DUNGEON_45,ARG
;階数
FLAG:현M = 0
FLAG:현X = 5
FLAG:현Y = 5

;=======================================================================
;스타ト時이벤트
;=======================================================================
@EVENT_DUNGEON_START_45
IF 던전플래그:45:1 == 0 
	CALL MESSAGE_WINDOW_D, "",@"＞메시아교、가이아교 어느 쪽에도 속하지 않은、결계에 둘러싸인 중립지대 롯폰기/＞팬텀 소사이어티 상층부의 지령에 따라 %조사처리(CALLNAME:MASTER,"는")% 수사를 명령 받아 그 땅을 찾아갔다"
	던전플래그:45:1 = 1
ENDIF

IF 던전플래그:45:2 == 2
	던전플래그:45:2 = 1
ENDIF

IF 던전플래그:45:0 == 1
	던전플래그:45:0 = 2
ENDIF

;마인포인트の決定
던전플래그:45:3 = RAND(1, 37)

;男女フラグ
SIF TALENT:MASTER:140 == 1 && TALENT:MASTER:157 == 0
	던전플래그:45:4 = 1
;=======================================================================
;指定したマスに進入가능か
;=======================================================================
@CAN_ENTER_45,ARG,ARG:1
SELECTCASE DA:ARG:(ARG:1)
CASE 12
	IF 던전플래그:45:0 == 0
		CALL MESSAGE_WINDOW_D, "", "＞문은 강한 마력으로 닫혀 있다"
		RETURN 0
	ELSEIF 던전플래그:45:0 >= 1 
		RETURN 1
	ENDIF
CASE 22
	IF 던전플래그:45:10 == 0
		CALL MESSAGE_WINDOW_D, "", "＞문은 강한 마력으로 닫혀 있다"
		RETURN 0
	ELSE
		RETURN 1
	ENDIF
CASE 32
	IF ARG:1 < FLAG:현Y
		CALL MESSAGE_WINDOW_D, "", "＞디스코"
	ENDIF
ENDSELECT
CALL CAN_ENTER,ARG,ARG:1
RETURN RESULT
