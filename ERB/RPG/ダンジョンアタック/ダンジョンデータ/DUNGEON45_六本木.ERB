@TREASURE_45, ARG
SELECTCASE DA:(FLAG:현X):(FLAG:현Y) / 10
	CASE 100
		LOCALS = ＨＰ
		LOCAL = 10
	CASE 101
		LOCALS = ＨＰ
		LOCAL = 10
	CASE 200
		LOCALS = 디스포이즌
		LOCAL = 2
	CASE 201
		LOCALS = 챠크라포트
		LOCAL = 1
	CASE 202
		LOCALS = ＨＰ
		LOCAL = 10
	CASE 203
		LOCALS = 반혼향
		LOCAL = 1
	CASE 300
		LOCALS = 속도의향
		LOCAL = 1
	CASE 400
		LOCALS = 힘의향
		LOCAL = 1
	CASE 401
		LOCALS = ＨＰ
		LOCAL = 10
	CASE 402
		IF  FLAG:월령 == 8
			LOCALS = 토파즈
		ELSE
			LOCALS = ＨＰ
			LOCAL = 10
		ENDIF
ENDSELECT
IF LOCALS == "￥"
	CALL GET_TREASURE_MONEY, 0, LOCAL, FLAG:현던전, FLAG:현M + 20, (DA:(FLAG:현X):(FLAG:현Y) / 10) % 100
ELSEIF LOCALS == "마카"
	CALL GET_TREASURE_MONEY, 1, LOCAL, FLAG:현던전, FLAG:현M + 20, (DA:(FLAG:현X):(FLAG:현Y) / 10) % 100
ELSEIF LOCALS == "MAG" || LOCALS == "ＭＡＧ"
	CALL GET_TREASURE_MAG, 0, LOCAL, FLAG:현던전, FLAG:현M + 20, (DA:(FLAG:현X):(FLAG:현Y) / 10) % 100
ELSEIF LOCALS == "ＨＰ" || LOCALS == "ＭＰ" || LOCALS == "POISON" || LOCALS == "PALYZE"
	;LOCALS:1 종류 LOCAL 데미지量or확률 LOCALS:2 相性
	CALL GET_TREASURE_TRAP, LOCALS, LOCAL, GET_TYPE_NUM(LOCALS:1), FLAG:현던전, FLAG:현M + 20, (DA:(FLAG:현X):(FLAG:현Y) / 10) % 100
ELSE
	IF GETNUM(ITEM, LOCALS) == -1
		PRINTL ＞아이템 이름의 지정이 잘못되었습니다
		PRINTW ＞아이템의 취득 처리를 중단합니다
	ELSE
		CALL GET_TREASURE, GETNUM(ITEM, LOCALS), LOCAL , FLAG:현던전, FLAG:현M + 20, (DA:(FLAG:현X):(FLAG:현Y) / 10) % 100
	ENDIF
ENDIF
RETURN 1
