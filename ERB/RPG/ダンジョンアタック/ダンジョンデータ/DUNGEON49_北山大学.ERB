@EVENT_DUNGEON_49, ARG, ARG:1
SELECTCASE DA:(FLAG:현X):(FLAG:현Y) % 10

;==========
;出口の処理
;==========
CASE 8
	CALL DUNGEON_EXIT
	RETURN 1
;==========
;扉の処理
;==========
CASE 2
	CALL DUNGEON_DOOR,ARG
	RETURN 1
;========
;宝の処理
;========
