IF CFLAG:ARG:１more플래그 > 0
	IF RAND:2 == 0
		;마구날뛰기
		CALL CHECK_ACTIONABLE,ARG,203
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = 203
		CALL RANDOM_TARGET,ARG,203
		RETURN 1
	ELSE
		;아라크노와이어
		CALL CHECK_ACTIONABLE,ARG,1124
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = 1124
		CALL RANDOM_TARGET,ARG,1124
		RETURN 1
	ENDIF
