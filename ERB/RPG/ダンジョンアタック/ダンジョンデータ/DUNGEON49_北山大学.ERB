@DEFINE_TILES_49, ARGS, ARG, ARG:1
SELECTCASE ARGS
	CASE "D"
		RETURN 1004
	CASE "R"
		RETURN 2004
	CASE "S"
		RETURN 3004

	CASE "W"
		RETURN 19
	CASE "w"
		RETURN 29

	CASE "t"
		RETURN 12
	CASE "s"
		RETURN 22

	CASE "H"
		RETURN 15
	CASE "P"
		RETURN 35

ENDSELECT

