	IF RAND:2 == 0
		CALL CHECK_ACTIONABLE,ARG,1124
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = 1124
		CALL RANDOM_TARGET,ARG,1124
		RETURN 1
	ELSE
		CALL CHECK_ACTIONABLE,ARG,602
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = 602
		CALL RANDOM_TARGET,ARG,602
		RETURN 1
	ENDIF
ELSEIF CFLAG:ARG:행동한횟수 == 1
	SELECTCASE RAND:100
		;50％で아라크노와이어
		CASE IS < 50
			CALL CHECK_ACTIONABLE,ARG,1124
			SIF RESULT == 0
				GOTO DECIDE_ACTION
			CFLAG:ARG:입력행동 = 1124
			CALL RANDOM_TARGET,ARG,1124
			RETURN 1
