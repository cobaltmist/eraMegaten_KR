@TILE_MARK_COMMON_49_4, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 100
		RESULTS = \@던전플래그:49:5 == 0 ?Dr#□\@
	CASE 200
		RESULTS = \@던전플래그:49:3 == 0 ?！#□\@
