	CALL TREASURE_49, DA:(FLAG:현X):(FLAG:현Y) / 10
	RETURN 1
;=======
;階段
;=======
CASE 6
	CALL UPSTAIRS_49
	RETURN RESULT

CASE 7
	CALL DOWNSTAIRS_49
	RETURN RESULT
;========
;罠の処理
;========
CASE 5
SELECTCASE DA:(FLAG:현X):(FLAG:현Y)
	CASE 15
		CALL DUNGEON_TRAP_HP,10
	CASE 35
		CALL DUNGEON_TRAP_POISON,40
ENDSELECT
