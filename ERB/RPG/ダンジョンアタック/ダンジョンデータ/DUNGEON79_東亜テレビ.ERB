SELECTCASE FLAG:현M
	CASE 0
		PRINTL 1F
	CASE 1
		PRINTL 2F
	CASE 2
		PRINTL 3F
	CASE 3
		PRINTL 4F
	CASE 4
		PRINTL 5F
	CASE 5
		PRINTL 6F
	CASE 6
