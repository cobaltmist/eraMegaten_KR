	CASE 2,3,4
	IF FLAG:라이트마 || (EQUIP:MASTER:다크스캐너 && NUM_SUMMONER())
		RESULTS = \@GET_FLOOR_ANALYZE_T(ARG:1, ARG:2) == 2 ?Ｗ#□\@
	ELSE
		SETCOLOR 0x404040
		RESULTS = □
	ENDIF
	CASE 0,1,109,200,201
		SETCOLOR 0x00ff00
		RESULTS = ｍ
	CASE 300
		RESULTS = ！
	CASEELSE
		RESULTS = \@GET_FLOOR_ANALYZE_T(ARG:1, ARG:2) == 2 ?Ｗ#□\@
ENDSELECT

