	SELECTCASE FLAG:현M
		CASE 0
			SIF FLAG:현X == 10 && FLAG:현Y == 1
				CALL DUNGEON_UPSTAIRS,1,6,9
		CASE 1
			SIF FLAG:현X == 4 && FLAG:현Y == 3
				CALL DUNGEON_UPSTAIRS,2,6,3
			SIF FLAG:현X == 8 && FLAG:현Y == 3
				CALL DUNGEON_UPSTAIRS,2,10,3
		CASE 3
