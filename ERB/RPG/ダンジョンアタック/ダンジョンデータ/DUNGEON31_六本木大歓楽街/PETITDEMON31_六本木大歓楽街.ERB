	;2回目
	TALENT:ARG:이형 = 1
	TALENT:ARG:짐승 = 0
	TALENT:ARG:새 = 0
	TALENT:ARG:파충류 = 0
	TALENT:ARG:물고기 = 0
	TALENT:ARG:인간화 = 0
	CSTR:ARG:타이틀 = 악마인(極限狂化)
	RETURN 1
ELSE
	;1回目
	TALENT:ARG:광기 = 1
	CSTR:ARG:타이틀 = 악마인(狂化)
	RETURN 1
ENDIF

