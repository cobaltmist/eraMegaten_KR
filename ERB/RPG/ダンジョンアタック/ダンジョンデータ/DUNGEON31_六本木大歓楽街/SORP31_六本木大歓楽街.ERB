IF 던전플래그:31:59 == 2
	CALL MESSAGE_WINDOW_D, "남자 간수", @"입장할 수 있는 것은 1일 1회까지다/얌전히 있다 가라고！"
	RETURN
ENDIF

$RANCHAR_LOOP_W
CALL INPUT_SELECT_D, "[1]입장료를 낸다/[2]설명을 듣는다/[3]잡담한다////[0]그만둔다" , , , 2
IF RESULT == 2
	;설명を聞く
	GOTO RANCHAR_INFO
ELSEIF RESULT == 3
	;雑談
	SELECTCASE RAND:3
		CASE 0
			CALL MESSAGE_WINDOW_D, "남자 간수", @"오 뇌물 상담이야?/…뭐지, 아닌가"
