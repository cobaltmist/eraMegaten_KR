LOCAL = 0
FOR LOCAL,1001, (1000 + MAX_DUNGEON_31_FLAGS)
	;所属している娼館
	TRYCALLFORM SORP_PLAY_{LOCAL}, "所属"
	SIF RESULT != ARG
		CONTINUE
	;指名できるか
	TRYCALLFORM SORP_PLAY_{LOCAL}, "判定"
	SIF RESULT == 1
