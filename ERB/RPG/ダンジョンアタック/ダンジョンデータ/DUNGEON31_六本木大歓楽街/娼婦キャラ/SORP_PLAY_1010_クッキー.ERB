;クッキー(ふたなりモー・ショボー)
@SORP_PLAY_1010, ARGS
#DIM CHARA, 1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "娼婦番号"
		RESULTS = [1010]
	CASE "キャラ名"
		RESULTS = 쿠키
	CASE "指名名"
		RESULTS = 쿠키(♂♀모・쇼보)
	CASE "所属"
		;表なら1、裏なら2
		RETURN 2
	CASE "判定"
		;指名可能なら0、不可なら1
		IF 롯폰기창관플래그:10 >= 3
			RETURN 1
		ELSE
			RETURN 0
		ENDIF
	CASE "価格"
		RETURN 300
	CASE "紹介文"
		IF 롯폰기창관플래그:10 == 0
			CALL MESSAGE_WINDOW_D, "접수 보는 남자", @"흠、쿠키양입니까/그녀는 재미있는 경험을 하게되었습니다"
			CALL MESSAGE_WINDOW_D, "접수 보는 남자", @"원래는 어디에나 있는 모·쇼보였던 것입니다만/어떠한 원인으로 갑자기 페니스가 돋친 듯하고/그 하락에 완전 사로잡혀 있습니다"
			CALL MESSAGE_WINDOW_D, "접수 보는 남자", @"어린 성정때문에 억제할 수도 없고/용서받지 못할 것이라고 이성이 이해하고 있는데 본능적으로 페니스를 계속 자극하는 것입니다/하지만、하지만 자신의 서투른 손가락 기술로는 가득낼 수 없는 악순환인거네요"
			CALL MESSAGE_WINDOW_D, "접수 보는 남자", @"고객님께도、아마 페니스에 대한 자극을 요구하겠죠/부디、용서해 주시기를…"
		ELSE
			CALL MESSAGE_WINDOW_D, "접수 보는 남자", @"흠、쿠키양입니까/고객님、독특한 감각에 빠지셨네요"
		ENDIF
	CASE "確認"
		CALL MESSAGE_WINDOW_D, "접수 보는 남자", @"그럼、쿠키양 말입니다만…/하룻밤에 ３００마카입니다"
	CASE "プレイ口上"
		CALL DUNGEON_WORP,2,5,10
		IF 롯폰기창관플래그:10 == 0
			롯폰기창관플래그:10 = 2
			CALL MESSAGE_WINDOW_D, "쿠키", @"우우…/자지…자지에서 정액 내고 싶어…"
			CALL MESSAGE_WINDOW_D, "", @"＞찾아온 귀여운 모・쇼보는/＞그러나 그 단정한 얼굴을 일그러트리며、오로지 검은 페니스를 문지르고 있다/＞쿠퍼액은 늘어져도、성욕의 소용돌이를 발산하는 낌새는 보이지 않는다"
			IF TALENT:MASTER:남자 && !TALENT:MASTER:오토코노코
				CALL MESSAGE_WINDOW_D, "쿠키", @"오니상、부탁해요/오니상의 엉덩이 안에 내 자지를 넣게해주세요/부탁입니다！"
			ELSE
				CALL MESSAGE_WINDOW_D, "쿠키", @"오네상、부탁해요/오네상의 보지 안에 내 자지를 넣게해주세요/부탁입니다！"
			ENDIF
			CALL MESSAGE_WINDOW_D, "", @"＞말하자마자 쿠키가 덤벼들어왔다！/＞분명히 이쪽을 범할 의도인 듯하다/＞뭐、별 문제도 없고 범해지겠다고 하자…"
			CALL MESSAGE_WINDOW_D, "", @"………………/…………/……"
			CALL SHOW_PICTURE, "EMPTY"
			IF TALENT:MASTER:남자 && !TALENT:MASTER:오토코노코
				CALL MESSAGE_WINDOW_D, "쿠키", @"오니상、이런 일 해서 미안해요、미안해요…/우우… 나는 어째서 이런게 나 버린거야…/미안해요…"
			ELSE
				CALL MESSAGE_WINDOW_D, "쿠키", @"오네상、이런 일 해서 미안해요、미안해요…/우우… 나는 어째서 이런게 나 버린거야…/미안해요…"
			ENDIF
			CALL MESSAGE_WINDOW_D, "쿠키", @"…그렇지만、만약 괜찮다면 또 불러주세요/그러면 더 정액 낼수 있으니까…"
			CALL MESSAGE_WINDOW_D, "", @"＞역시 아직 내지않은 것 같은 쿠키는、그런데도 방에서 나갔다…"
		ELSE
			CALL MESSAGE_WINDOW_D, "쿠키", @"우우…/자지…자지에서 정액 내고 싶어…"
			IF TALENT:MASTER:남자 && !TALENT:MASTER:오토코노코
				CALL MESSAGE_WINDOW_D, "쿠키", @"아、오니상이다/또 불러준거네…/더 엉덩이에 내버려도 좋은거지！"
			ELSEIF TALENT:MASTER:오토코노코
				CALL MESSAGE_WINDOW_D, "쿠키", @"아、오네상같은 오니상이다/또 불러준거네…/더 엉덩이에 내버려도 좋은거지！"
			ELSE
				CALL MESSAGE_WINDOW_D, "쿠키", @"아、오네상이다/또 불러준거네…/더 보지에 내버려도 좋은거지！"
			ENDIF
			CALL MESSAGE_WINDOW_D, "", @"＞말하자마자 쿠키가 덤벼들어왔다！/＞전회보다 망설임없이、" + CALLNAME:MASTER + "는 순식간에 옷을 벗겨져버렸다…"
			CALL MESSAGE_WINDOW_D, "", @"………………/…………/……"
			CALL SHOW_PICTURE, "EMPTY"
			IF TALENT:MASTER:남자 && !TALENT:MASTER:오토코노코
				CALL MESSAGE_WINDOW_D, "쿠키", @"오니상、이런 일 해서 미안해요、미안해요…/우우… 나는 어째서 이런게 나 버린거야…/미안해요…"
			ELSE
				CALL MESSAGE_WINDOW_D, "쿠키", @"오네상、이런 일 해서 미안해요、미안해요…/우우… 나는 어째서 이런게 나 버린거야…/미안해요…"
			ENDIF
			CALL MESSAGE_WINDOW_D, "쿠키", @"…그렇지만、만약 괜찮다면 또 불러주세요/그러면 더 정액 낼수 있으니까…"
			CALL MESSAGE_WINDOW_D, "", @"＞역시 아직 내지않은 것 같은 쿠키는、그런데도 방에서 나갔다…"
		ENDIF
		MONEY:1 -= 300
		TALENT:MASTER:처녀 = 0
		TALENT:MASTER:재생처녀 = 0
		EXP:MASTER:절정경험 += ABL:MASTER:욕망
		JUEL:MASTER:쾌Ｃ += ABL:MASTER:욕망 * (300) + 300
		JUEL:MASTER:쾌Ｂ += ABL:MASTER:욕망 * (300) + 300
		JUEL:MASTER:쾌Ａ += ABL:MASTER:욕망 * (300) + 300
		JUEL:MASTER:욕정 += ABL:MASTER:욕망 * (300) + 300
		EXP:MASTER:정액경험 += ABL:MASTER:욕망 * 10
		EXP:MASTER:펠라경험 += ABL:MASTER:욕망 * 10
		IF !TALENT:MASTER:남자
			EXP:MASTER:레즈경험 += ABL:MASTER:욕망 * 3
			JUEL:MASTER:쾌Ｖ += ABL:MASTER:욕망 * (500) + 500
			JUEL:MASTER:쾌Ａ += ABL:MASTER:욕망 * (500) + 500
			EXP:MASTER:Ａ경험 += ABL:MASTER:욕망 * 5
			EXP:MASTER:Ｖ경험 += ABL:MASTER:욕망 * 5
		ELSE
			JUEL:MASTER:쾌Ａ += ABL:MASTER:욕망 * (1000) + 1000
			EXP:MASTER:Ａ경험 += ABL:MASTER:욕망 * 10
		ENDIF
		IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
			EXP:MASTER:사정경험 += ABL:MASTER:욕망
			BASE:MASTER:ＭＡＧ -= ABL:MASTER:욕망 * 3000
		ENDIF
		IF TALENT:MASTER:모유체질
			EXP:MASTER:분유경험 += (ABL:MASTER:욕망 + 1) * 3
			JUEL:MASTER:쾌Ｂ += (ABL:MASTER:욕망 + 1) * 200
			BASE:MASTER:ＭＡＧ -= ABL:MASTER:욕망 * 2000 + 1000
		ENDIF
		CALL INCREASE_LC,5
		CALL DUNGEON_WORP,2,6,14
	CASE "身受け価格"
		RETURN 1000
	CASE "身受け確認"
		CALL MESSAGE_WINDOW_D, "접수 보는 남자", @"쿠키양을 낙적하시겠습니까？"
		CALL MESSAGE_WINDOW_D, "접수 보는 남자", @"그러시다면、１０００마카를 받도록 하겠습니다/괜찮으시겠습니까？"
	CASE "身受け口上"
		CALL ADD_NEW_COMPANION, 902, 6000
		CHARA = RESULT:1
		NAME:CHARA = 쿠키
		CALLNAME:CHARA = 쿠키
		CSTR:CHARA:일인칭 = 나
		IF TALENT:MASTER:남자
			CSTR:CHARA:이인칭 = 오니상
		ELSE
			CSTR:CHARA:이인칭 = 오네상
		ENDIF
		;即戦力として使えるように色々底上げ
		CFLAG:CHARA:매춘인기 = 200
		CFLAG:CHARA:매각가능 = 2
		CFLAG:CHARA:의존도 = 1000
		;ベース
		MAXBASE:CHARA:체력 += 5000
		BASE:CHARA:체력 = MAXBASE:CHARA:체력
		MAXBASE:CHARA:기력 += 5000
		BASE:CHARA:기력 = MAXBASE:CHARA:기력
		BASE:CHARA:ＭＡＧ = MAXBASE:CHARA:ＭＡＧ
		;素質
		TALENT:CHARA:불결무시 = 1
		TALENT:CHARA:중독되기쉬움 = 1
		TALENT:CHARA:후타나리 = 1
		TALENT:CHARA:절륜 = 1
		TALENT:CHARA:습득느림 = 1
		TALENT:CHARA:Ｃ민감 = 0
		TALENT:CHARA:처녀 = 0
		TALENT:CHARA:해방 = 1
		TALENT:CHARA:젖기쉬움 = 1
		TALENT:CHARA:자위광 = 1
		TALENT:CHARA:도착적 = 1
		TALENT:CHARA:회복빠름 = 1
		TALENT:CHARA:인기 = 1
		TALENT:CHARA:수수께끼의매력 = 1
		;能力
		ABL:CHARA:순종 = 2
		ABL:CHARA:욕망 = 4
		ABL:CHARA:기교 = 0
		ABL:CHARA:노출증 = 3
		ABL:CHARA:Ｖ감각 = 3
		ABL:CHARA:Ｂ감각 = 3
		ABL:CHARA:Ｃ감각 = 0
		ABL:CHARA:스킬4 = [[스킬:섹시댄스]]
		ABL:CHARA:스킬5 = [[스킬:캔디보이스]]
		;経験
		EXP:CHARA:Ｖ경험 = 100
		EXP:CHARA:절정경험 = 50
		EXP:CHARA:정액경험 = 100
		EXP:CHARA:성교경험 = 200
		EXP:CHARA:사정경험 = 1000
		EXP:CHARA:매춘경험 = 200
		;その他
		CALL ADDS_ABNORMAL_EXP , "사정" , CHARA
		EXP:CHARA:이상경험 = 2
		;口上
		IF TALENT:MASTER:남자
			CALL MESSAGE_WINDOW_D, "쿠키", @"오니상이 쿠키의 자지 매일 괴롭혀주는거야？/고마워、오니상！/이제 나도 매일 기분 좋게 될 수 있구나！"
			CALL MESSAGE_WINDOW_D, "", @"＞쿠키는" + CALLNAME:MASTER + "에게 천사와 같은 미소를 지어 보였다/＞…페니스를 만지작거리며"
			CALL MESSAGE_WINDOW_D, "쿠키", @"앞으로 나와 나의의 자지를 돌보는일 잘 부탁해、오니상！"
		ELSE
			CALL MESSAGE_WINDOW_D, "쿠키", @"오네상이 쿠키의 자지 매일 괴롭혀주는거야？/고마워、오네상！/이제 나도 매일 기분 좋게 될 수 있구나！"
			CALL MESSAGE_WINDOW_D, "", @"＞쿠키는" + CALLNAME:MASTER + "에게 천사와 같은 미소를 지어 보였다/＞…페니스를 만지작거리며"
			CALL MESSAGE_WINDOW_D, "쿠키", @"앞으로 나와 나의의 자지를 돌보는일 잘 부탁해、오네상！"
		ENDIF
		MONEY:1 -= 1000
		롯폰기창관플래그:10 = 3
	CASE "宣伝"
		PRINTL
		PRINTL 쿠키
		SETCOLOR COLOR("ハートピンク")
		PRINTL 『기분좋게 해주세요。더이상 참을 수 없어요…』
		RESETCOLOR
		PRINTL 이용자：（노 코멘트）
		PRINTL 　　　　（편집자 주。이용자 전원이 입을 다물고 있어 코멘트를 수록할 수 없었습니다。
		PRINTW 　　　　　아마 입에 담을 수 없을 정도로 훌륭한 체험을 한 듯 합니다）
ENDSELECT
