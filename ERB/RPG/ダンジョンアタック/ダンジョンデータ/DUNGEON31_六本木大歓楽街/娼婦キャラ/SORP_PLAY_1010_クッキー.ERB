;クッキー(ふたなりモー・ショボー)
@SORP_PLAY_1010, ARGS
#DIM CHARA, 1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "娼婦番号"
		RESULTS = [1010]
	CASE "キャラ名"
		RESULTS = クッキー
	CASE "指名名"
		RESULTS = クッキー(♂♀モー・ショボー)
	CASE "所属"
		;表なら1、裏なら2
		RETURN 2
	CASE "判定"
		;指名可能なら0、不可なら1
		IF 롯폰기창관플래그:10 >= 3
			RETURN 1
		ELSE
			RETURN 0
		ENDIF
	CASE "価格"
		RETURN 300
	CASE "紹介文"
		IF 롯폰기창관플래그:10 == 0
			CALL MESSAGE_WINDOW_D, "受付の男", @"ふむ、クッキー嬢ですか/彼女は面白い経験をしていましてね"
			CALL MESSAGE_WINDOW_D, "受付の男", @"元々はどこにでもいるモー・ショボーだったのですが/何らかの原因で突如ペニスが生えたようでして/その快楽に完全に取りつかれているのですよ"
			CALL MESSAGE_WINDOW_D, "受付の男", @"幼い性情故抑えも効かず/許されざることだと理性が理解しているのに本能でペニスを刺激し続けるのです/しかし、自身の拙い指技では満足できないという悪循環ですがね"
			CALL MESSAGE_WINDOW_D, "受付の男", @"お客様にも、恐らくペニスへの刺激を要求してくることでしょう/何卒、ご容赦のほどを…"
		ELSE
			CALL MESSAGE_WINDOW_D, "受付の男", @"ふむ、クッキー嬢ですか/お客様、独特の感覚に嵌りましたね"
		ENDIF
	CASE "確認"
		CALL MESSAGE_WINDOW_D, "受付の男", @"さて、クッキー嬢ですが…/一晩で３００マッカになります"
	CASE "プレイ口上"
		CALL DUNGEON_WORP,2,5,10
		IF 롯폰기창관플래그:10 == 0
			롯폰기창관플래그:10 = 2
			CALL MESSAGE_WINDOW_D, "クッキー", @"うう…/おチンポ…おチンポからせーえき出したいよぉ…"
			CALL MESSAGE_WINDOW_D, "", @"＞やってきた可愛らしいモー・ショボーは/＞しかしその端正な顔を歪めて、ひたすらに黒々としたペニスを擦っている/＞カウパーこそ垂れてはきても、性の滾りを放つ様子は見えない"
			IF TALENT:MASTER:남자 && !TALENT:MASTER:오토코노코
				CALL MESSAGE_WINDOW_D, "クッキー", @"おにーさん、おねがい/おにーさんのお尻に私のおチンポ入れさせてください/お願いしますっ！"
			ELSE
				CALL MESSAGE_WINDOW_D, "クッキー", @"おねーさん、おねがい/おねーさんのおマンコに私のおチンポ入れさせてください/お願いしますっ！"
			ENDIF
			CALL MESSAGE_WINDOW_D, "", @"＞言うなりクッキーは飛び掛かってきた！/＞どうやら此方を犯すつもりらしい/＞まあ、特に問題もない 犯されてやるとしよう…"
			CALL MESSAGE_WINDOW_D, "", @"………………/…………/……"
			CALL SHOW_PICTURE, "EMPTY"
			IF TALENT:MASTER:남자 && !TALENT:MASTER:오토코노코
				CALL MESSAGE_WINDOW_D, "クッキー", @"おにーさん、こんなことしてごめんなさい、ごめんなさい…/うう… 私なんでこんなの生えちゃったんだろう…/ごめんなさい…"
			ELSE
				CALL MESSAGE_WINDOW_D, "クッキー", @"おねーさん、こんなことしてごめんなさい、ごめんなさい…/うう… 私なんでこんなの生えちゃったんだろう…/ごめんなさい…"
			ENDIF
			CALL MESSAGE_WINDOW_D, "クッキー", @"…でも、もしよかったらまた呼んでください/そしたらもっとせーえき出せるから…"
			CALL MESSAGE_WINDOW_D, "", @"＞やはりまだ出したりないらしいクッキーは、それでも部屋から出ていった…"
		ELSE
			CALL MESSAGE_WINDOW_D, "クッキー", @"うう…/おチンポ…おチンポからせーえき出したいよぉ…"
			IF TALENT:MASTER:남자 && !TALENT:MASTER:오토코노코
				CALL MESSAGE_WINDOW_D, "クッキー", @"あ、おにーさんだ/また呼んでくれたってことは…/もっとお尻に出しちゃっていいんだよね！"
			ELSEIF TALENT:MASTER:오토코노코
				CALL MESSAGE_WINDOW_D, "クッキー", @"あ、おねーさんみたいなおにーさんだ/また呼んでくれたってことは…/もっとお尻に出しちゃっていいんだよね！"
			ELSE
				CALL MESSAGE_WINDOW_D, "クッキー", @"あ、おねーさんだ/また呼んでくれたってことは…/もっとおマンコに出しちゃっていいんだよね！"
			ENDIF
			CALL MESSAGE_WINDOW_D, "", @"＞言うなりクッキーは飛び掛かってきた！/＞前回よりもさらに迷いがなく、" + CALLNAME:MASTER + "はあっという間に服を脱がされてしまった…"
			CALL MESSAGE_WINDOW_D, "", @"………………/…………/……"
			CALL SHOW_PICTURE, "EMPTY"
			IF TALENT:MASTER:남자 && !TALENT:MASTER:오토코노코
				CALL MESSAGE_WINDOW_D, "クッキー", @"おにーさん、こんなことしてごめんなさい、ごめんなさい…/うう… 私なんでこんなの生えちゃったんだろう…/ごめんなさい…"
			ELSE
				CALL MESSAGE_WINDOW_D, "クッキー", @"おねーさん、こんなことしてごめんなさい、ごめんなさい…/うう… 私なんでこんなの生えちゃったんだろう…/ごめんなさい…"
			ENDIF
			CALL MESSAGE_WINDOW_D, "クッキー", @"…でも、もしよかったらまた呼んでください/そしたらもっとせーえき出せるから…"
			CALL MESSAGE_WINDOW_D, "", @"＞やはりまだ出したりないらしいクッキーは、それでも部屋から出ていった…"
		ENDIF
		MONEY:1 -= 300
		TALENT:MASTER:처녀 = 0
		TALENT:MASTER:재생처녀 = 0
		EXP:MASTER:절정경험 += ABL:MASTER:욕망
		JUEL:MASTER:쾌Ｃ += ABL:MASTER:욕망 * (300) + 300
		JUEL:MASTER:쾌Ｂ += ABL:MASTER:욕망 * (300) + 300
		JUEL:MASTER:쾌Ａ += ABL:MASTER:욕망 * (300) + 300
		JUEL:MASTER:욕정 += ABL:MASTER:욕망 * (300) + 300
		EXP:MASTER:정액경험 += ABL:MASTER:욕망 * 10
		EXP:MASTER:펠라경험 += ABL:MASTER:욕망 * 10
		IF !TALENT:MASTER:남자
			EXP:MASTER:레즈경험 += ABL:MASTER:욕망 * 3
			JUEL:MASTER:쾌Ｖ += ABL:MASTER:욕망 * (500) + 500
			JUEL:MASTER:쾌Ａ += ABL:MASTER:욕망 * (500) + 500
			EXP:MASTER:Ａ경험 += ABL:MASTER:욕망 * 5
			EXP:MASTER:Ｖ경험 += ABL:MASTER:욕망 * 5
		ELSE
			JUEL:MASTER:쾌Ａ += ABL:MASTER:욕망 * (1000) + 1000
			EXP:MASTER:Ａ경험 += ABL:MASTER:욕망 * 10
		ENDIF
		IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
			EXP:MASTER:사정경험 += ABL:MASTER:욕망
			BASE:MASTER:ＭＡＧ -= ABL:MASTER:욕망 * 3000
		ENDIF
		IF TALENT:MASTER:모유체질
			EXP:MASTER:분유경험 += (ABL:MASTER:욕망 + 1) * 3
			JUEL:MASTER:쾌Ｂ += (ABL:MASTER:욕망 + 1) * 200
			BASE:MASTER:ＭＡＧ -= ABL:MASTER:욕망 * 2000 + 1000
		ENDIF
		CALL INCREASE_LC,5
		CALL DUNGEON_WORP,2,6,14
	CASE "身受け価格"
		RETURN 1000
	CASE "身受け確認"
		CALL MESSAGE_WINDOW_D, "受付の男", @"クッキー嬢を身請けされるのですかな？"
		CALL MESSAGE_WINDOW_D, "受付の男", @"でしたら、１０００マッカ頂く事になります/よろしいですかな？"
	CASE "身受け口上"
		CALL ADD_NEW_COMPANION, 902, 6000
		CHARA = RESULT:1
		NAME:CHARA = クッキー
		CALLNAME:CHARA = クッキー
		CSTR:CHARA:일인칭 = 私
		IF TALENT:MASTER:남자
			CSTR:CHARA:이인칭 = おにーさん
		ELSE
			CSTR:CHARA:이인칭 = おねーさん
		ENDIF
		;即戦力として使えるように色々底上げ
		CFLAG:CHARA:매춘인기 = 200
		CFLAG:CHARA:매각가능 = 2
		CFLAG:CHARA:의존도 = 1000
		;ベース
		MAXBASE:CHARA:체력 += 5000
		BASE:CHARA:체력 = MAXBASE:CHARA:체력
		MAXBASE:CHARA:기력 += 5000
		BASE:CHARA:기력 = MAXBASE:CHARA:기력
		BASE:CHARA:ＭＡＧ = MAXBASE:CHARA:ＭＡＧ
		;素質
		TALENT:CHARA:불결무시 = 1
		TALENT:CHARA:중독되기쉬움 = 1
		TALENT:CHARA:후타나리 = 1
		TALENT:CHARA:절륜 = 1
		TALENT:CHARA:습득느림 = 1
		TALENT:CHARA:Ｃ민감 = 0
		TALENT:CHARA:처녀 = 0
		TALENT:CHARA:해방 = 1
		TALENT:CHARA:젖기쉬움 = 1
		TALENT:CHARA:자위광 = 1
		TALENT:CHARA:도착적 = 1
		TALENT:CHARA:회복빠름 = 1
		TALENT:CHARA:인기 = 1
		TALENT:CHARA:수수께끼의매력 = 1
		;能力
		ABL:CHARA:순종 = 2
		ABL:CHARA:욕망 = 4
		ABL:CHARA:기교 = 0
		ABL:CHARA:노출증 = 3
		ABL:CHARA:Ｖ감각 = 3
		ABL:CHARA:Ｂ감각 = 3
		ABL:CHARA:Ｃ감각 = 0
		ABL:CHARA:스킬4 = [[스킬:섹시댄스]]
		ABL:CHARA:스킬5 = [[스킬:캔디보이스]]
		;経験
		EXP:CHARA:Ｖ경험 = 100
		EXP:CHARA:절정경험 = 50
		EXP:CHARA:정액경험 = 100
		EXP:CHARA:성교경험 = 200
		EXP:CHARA:사정경험 = 1000
		EXP:CHARA:매춘경험 = 200
		;その他
		CALL ADDS_ABNORMAL_EXP , "사정" , CHARA
		EXP:CHARA:이상경험 = 2
		;口上
		IF TALENT:MASTER:남자
			CALL MESSAGE_WINDOW_D, "クッキー", @"おにーさんがクッキーのおチンポ毎日いじめてくれるの？/ありがとう、おにーさん！/これで私も毎日気持ちよくなれるんだね！"
			CALL MESSAGE_WINDOW_D, "", @"＞クッキーは" + CALLNAME:MASTER + "に天使のような笑顔を見せつけてくれた/＞…ペニスを弄りながら"
			CALL MESSAGE_WINDOW_D, "クッキー", @"これからも私と私のおチンポのおせわよろしくね、おにーさん！"
		ELSE
			CALL MESSAGE_WINDOW_D, "クッキー", @"おねーさんがクッキーのおチンポ毎日いじめてくれるの？/ありがとう、おねーさん！/これで私も毎日気持ちよくなれるんだね！"
			CALL MESSAGE_WINDOW_D, "", @"＞クッキーは" + CALLNAME:MASTER + "に天使のような笑顔を見せつけてくれた/＞…ペニスを弄りながら"
			CALL MESSAGE_WINDOW_D, "クッキー", @"これからも私と私のおチンポのおせわよろしくね、おねーさん！"
		ENDIF
		MONEY:1 -= 1000
		롯폰기창관플래그:10 = 3
	CASE "宣伝"
		PRINTL
		PRINTL クッキー
		SETCOLOR COLOR("ハートピンク")
		PRINTL 『きもちよくしてください。もうガマンできないんです…』
		RESETCOLOR
		PRINTL 利用者：（ノーコメント）
		PRINTL 　　　　（編集注記。利用者全員が口を噤んでおりコメントを収録できませんでした。
		PRINTW 　　　　　おそらく口に出せないほどに素晴らしい体験をしたのだと思われます）
ENDSELECT
