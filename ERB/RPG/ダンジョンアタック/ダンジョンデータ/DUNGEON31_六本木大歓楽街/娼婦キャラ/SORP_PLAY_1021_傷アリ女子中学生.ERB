;傷アリの女子中学生です
;身請けはできず援助交際のみです
@SORP_PLAY_1021, ARGS
#DIM CHARA, 1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "娼婦番号"
		RESULTS = [1021]
	CASE "キャラ名"
		RESULTS = 傷アリ女子中学生
	CASE "指名名"
		RESULTS = 傷アリ女子中学生
	CASE "所属"
		;表なら1、裏なら2、援助なら3
		RETURN 3
	CASE "判定"
		;指名可能なら0、不可なら1
		IF 롯폰기창관플래그:21 >= 3
			RETURN 1
		ELSEIF 투기장플래그:10:0 == 1 && 투기장플래그:10:1 == 0
			RETURN 0
		ELSE
			RETURN 1
		ENDIF
	CASE "価格"
		RETURN 50
	CASE "紹介文"
		IF 롯폰기창관플래그:21 == 0
			CALL MESSAGE_WINDOW_D, "露出したウェイトレス", @"彼女はー/お口えっちで援助して欲しいみたいですー"
			CALL MESSAGE_WINDOW_D, "露出したウェイトレス", @"お顔に傷が入ってるからー/避ける人も多いんですけどー/えっち結構上手ですよー"
			CALL MESSAGE_WINDOW_D, "露出したウェイトレス", @"なんだか訳アリのようでー/どこかから逃げてきたとか―/お金がいっぱいいるんだとかー"
		ELSE
			CALL MESSAGE_WINDOW_D, "露出したウェイトレス", @"あの子のお口―/クセになっちゃいましたー？"
			CALL MESSAGE_WINDOW_D, "露出したウェイトレス", @"壊れたCOMPを直してー/仲魔を集めなおすためにー/まだまだお金がいるんだとかー"
		ENDIF
	CASE "確認"
		CALL MESSAGE_WINDOW_D, "露出したウェイトレス", @"５０マッカでの援助を希望しておりますがー/どうしますかー？"
	CASE "プレイ口上"
		IF 롯폰기창관플래그:21 == 0
			CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"…………"
			CALL MESSAGE_WINDOW_D, "", @"＞現れたのは顔に傷がついた、ローティーンぐらいの不愛想な女の子だ/＞傷こそついているが美麗な顔と程よく発育した体が期待できる/＞……どこかで見たことがある気がした"
			CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"…………え？/……なんで、ここに"
			CALL MESSAGE_WINDOW_D, "", @"＞どこかで会ったんだったか。少女に尋ねてみるが、少女は不快そうに首を振った"
			CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"……なんでもない/……早く、しましょ"
			CALL MESSAGE_WINDOW_D, "", @"………………/…………/……"
			CALL SHOW_PICTURE, "EMPTY"
					IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
						CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"……んっ、ちゅぷ、んむ……ちゅぱ……/……っ、んっ、く……ごきゅ……っ/けほっ、うえ……っ"
						CALL MESSAGE_WINDOW_D, "", @"＞傷アリ女子中学生は" + CALLNAME:MASTER + "が口内に吐き出した精液を吐き出した/＞その表情は苦しそうで慣れているようには見えなかった…"
						CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"…………満足した？/……じゃあ、早く、マッカちょうだい/うぇ、まだ味残ってる……"
					ELSE
						CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"……んっ、れろ、んむ……ちゅむ……/……っ、んっ、く……う……っ/む、ぐ……っ"
						CALL MESSAGE_WINDOW_D, "", @"＞傷アリ女子中学生は" + CALLNAME:MASTER + "の愛液で汚れた唇を袖で拭った/＞その表情は苦しそうで慣れているようには見えなかった…"
						CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"…………満足した？/……じゃあ、早く、マッカちょうだい/なんで、こんなとこ舐めさせたがるんだか……"
					ENDIF
			CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"……ちゃんとマッカくれるなら、また来ていいよ/だいたい毎晩ここにいるから……"
			;経験フラグを設定
			롯폰기창관플래그:21 = 1
		ELSE
			CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"……また来たの？/……別にいいけど/ちゃんと、マッカあるなら"
			CALL MESSAGE_WINDOW_D, "", @"………………/…………/……"
			CALL SHOW_PICTURE, "EMPTY"
					IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
						CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"……んっ、ちゅぷ、んむ……ちゅぱ……/……っ、んっ、く……ごきゅ……っ/ぷは……っ"
						CALL MESSAGE_WINDOW_D, "", @"＞傷アリ女子中学生は" + CALLNAME:MASTER + "が口内に吐き出した精液を飲み込んだ/＞態度は不愛想だが、吸い取るような丁寧な奉仕だった…"
						CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"…………満足した？/……じゃあ、早く、マッカちょうだい/なんで、こんなもの飲ませたがるんだか……"
					ELSE
						CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"……んっ、れろ、んむ……ちゅむ……/……っ、んっ、く……う……っ/ぷは……っ"
						CALL MESSAGE_WINDOW_D, "", @"＞傷アリ女子中学生は" + CALLNAME:MASTER + "の愛液で汚れた唇を舌で舐めとった/＞態度は不愛想だが、手慣れた丁寧な奉仕だった…"
						CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"…………満足した？/……じゃあ、早く、マッカちょうだい/気持ちよかったなら、いいけど……"
					ENDIF
			;見受け可能フラグを設定
			롯폰기창관플래그:21 = 2
		ENDIF
		;プレイ経験(任意の経験を追加可)
		EXP:MASTER:절정경험 += ABL:MASTER:기교
		JUEL:MASTER:쾌Ｃ += ABL:MASTER:기교 * (50) + 50
		JUEL:MASTER:욕정 += ABL:MASTER:기교 * (50) + 50
		IF !TALENT:MASTER:남자
			EXP:MASTER:레즈경험 += ABL:MASTER:기교 * 1 + 1
		ENDIF
		IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
			EXP:MASTER:사정경험 += ABL:MASTER:기교
			BASE:MASTER:ＭＡＧ -= ABL:MASTER:기교 * 500
		ENDIF
		CALL INCREASE_LC,5
	CASE "身受け価格"
		RETURN 200
	CASE "身受け確認"
		CALL MESSAGE_WINDOW_D, "露出したウェイトレス", @"あの子のおまんこ、興味あります―？/お金がもっと必要みたいで―/ちょっと前から本番始めたんですよー"
		CALL MESSAGE_WINDOW_D, "露出したウェイトレス", @"固定客も何人かいるみたいでー/締まりがいいって結構評判ですよー？"
		CALL MESSAGE_WINDOW_D, "露出したウェイトレス", @"２００マッカ出せば本番をしても良いとのことですがー/いかがいたしますかー？"
	CASE "身受け口上"
	;キャラ加入はしません。遊べるだけです。
		IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
			CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"……そう、あなたなの/……別に/……誰でも変わらないよ"
			CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"……えっち、するんでしょ？/……はやく、行きましょ"
			CALL MESSAGE_WINDOW_D, "", @"………………/…………/……"
			CALL SHOW_PICTURE, "EMPTY"
			SELECTCASE RAND:3
				CASE 0
					;路地裏エッチ
					CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"うぁ……っ、ダメ、だめぇ……っ、こんな、とこで……っ！？"
					CALL MESSAGE_WINDOW_D, "", @"＞" + CALLNAME:MASTER + "はホテルに行く間も惜しがって傷アリ女子中学生を路地裏で犯した/＞言葉とは裏腹に、その膣は具合よく肉棒を咥えこんでいた…"
					CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"っ！？　やっ、見られて、……る、から……あっ……！？"
					CALL MESSAGE_WINDOW_D, "", @"＞ニヤニヤと見物するギャラリーたちに見せつけるように" + CALLNAME:MASTER + "は傷アリ女子中学生の膣内を精液で汚した…"
				CASE 1
					;ラブホエッチ
					CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"あ……っ、く……っ、はぁ……っ……んぅ……っ"
					CALL MESSAGE_WINDOW_D, "", @"＞" + CALLNAME:MASTER + "はラブホテルの部屋に傷アリ女子中学生を連れ込むなりその場で押し倒した/＞少女の膣内は年齢に見合わない熟れた具合の良さで肉棒を受け入れた…"
					CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"……もう、出るの……っ、最後は、口に……っ……"
					CALL MESSAGE_WINDOW_D, "", @"＞その言葉に従って" + CALLNAME:MASTER + "は傷アリ女子中学生の膣内から肉棒を抜くと、少女の顔を引き寄せ口内で射精した…"
				CASE 2
					;トイレエッチ
					CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"や……っ、いや、いやぁ……っ、こんな、きたない……っ！？"
					CALL MESSAGE_WINDOW_D, "", @"＞" + CALLNAME:MASTER + "はホテルに行く間も惜しがって傷アリ女子中学生を男子トイレで犯した/＞言葉とは裏腹に、その膣は具合よく肉棒を咥えこんでいた…"
					CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"っ！？　あ、くぅ……なか、だめ……っ……！？"
					CALL MESSAGE_WINDOW_D, "", @"＞その言葉を無視して" + CALLNAME:MASTER + "は傷アリ女子中学生の膣内を精液で汚した…"
				ENDSELECT
		ELSE
			;レズエッチ
			CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"こ、これで、いい……？　気持ちいいの……？"
			CALL MESSAGE_WINDOW_D, "", @"＞" + CALLNAME:MASTER + "はラブホテルの一室で傷アリ女子中学生にペニスバンドを装着させて腰を振らせた/＞戸惑うような言葉とは裏腹にどこか興奮しているようだ…"
			CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"っ、も、もうイくの……？　こんなので、イっちゃうの……っ？"
			CALL MESSAGE_WINDOW_D, "", @"＞初々しい傷アリ女子中学生の腰振りがいじらしく感じられて" + CALLNAME:MASTER + "は小さく絶頂した…"
		ENDIF
		CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"…………/……べつに、怒ってない/……いいから、早くマッカちょうだい"
		CALL MESSAGE_WINDOW_D, "傷アリ女子中学生", @"ん、マッカ、ありがと……/……またマッカくれるなら、来てもいいよ/……でも、あんまり変なの、嫌だから"
		EXP:MASTER:성교경험 += ABL:MASTER:기교 * ABL:MASTER:욕망 * 2 + 1
		EXP:MASTER:절정경험 += ABL:MASTER:기교 * ABL:MASTER:욕망
		JUEL:MASTER:쾌Ｃ += ABL:MASTER:기교 * ABL:MASTER:욕망 * 300 + 500
		JUEL:MASTER:욕정 += ABL:MASTER:기교 * ABL:MASTER:욕망 * 200 + 500
		JUEL:MASTER:습득 += ABL:MASTER:기교 * ABL:MASTER:욕망 * 200 + 500
		IF !TALENT:MASTER:남자
			EXP:MASTER:레즈경험 += ABL:MASTER:기교 * ABL:MASTER:욕망 * 2 + 1
			JUEL:MASTER:쾌Ｖ +=  ABL:MASTER:기교 * ABL:MASTER:욕망 * 100 + 300
		ENDIF
		IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
			EXP:MASTER:질사경험 += ABL:MASTER:욕망
			EXP:MASTER:사정경험 += ABL:MASTER:욕망 + 1
			BASE:MASTER:ＭＡＧ -=  ABL:MASTER:기교 * ABL:MASTER:욕망 * 2000 + 1000
		ENDIF
	CASE "宣伝"
		;
ENDSELECT

