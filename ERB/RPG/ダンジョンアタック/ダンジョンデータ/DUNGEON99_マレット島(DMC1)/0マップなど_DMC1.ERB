@EVENT_ENTER_DUNGEON_99,ARG
;ARG = 進入時の向き　0の場合ワープなど
SELECTCASE DA:(FLAG:現X):(FLAG:現Y) % 10
	CASE 1
		;도리グ
		IF DA:(FLAG:現X):(FLAG:現Y) == 91
			IF !GETBIT(던전フラグ:99:4,27)
				CALL D_LEAGUE
			ENDIF
		;レッドオーブ취득処理
		ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 191
			CALL ENTER_DUNGEON_99_ORB,ARG
			RETURN 1
		;落下/移動処理
		ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 171
			SELECTCASE FLAG:現M
				CASE 15
					CALL DUNGEON_WORP,FLAG:現M-1,FLAG:現X+26,FLAG:現Y+24
				CASE 18
					CALL DUNGEON_WORP,FLAG:現M,FLAG:現X-11,FLAG:現Y
				CASE 20
					CALL DUNGEON_WORP,FLAG:現M,FLAG:現X,FLAG:現Y+7
				CASE 21
					CALL DUNGEON_WORP,FLAG:現M,FLAG:現X,FLAG:現Y-7
				CASE 23
					CALL DUNGEON_WORP,FLAG:現M,FLAG:現X+21,FLAG:現Y+5
				CASE 24
					CALL DUNGEON_WORP,FLAG:現M+1,FLAG:現X-5,FLAG:現Y-14
				CASE 26
					;古城→古城前
					IF FLAG:現X == 8 && FLAG:現Y == 42
						CALL DUNGEON_WORP,13,4,3
					;古城→水中回廊
					ELSEIF FLAG:現X == 30 && FLAG:現Y == 5
						CALL DUNGEON_WORP,7,1,5
					;聖堂前へジャンプ
					ELSEIF FLAG:現X == 31 && FLAG:現Y == 5
						CALL DUNGEON_WORP,26,29,5
					;古城→小部屋の絵
					ELSEIF FLAG:現X == 39 && FLAG:現Y == 3
						CALL DUNGEON_WORP,27,2,13
					;古城→寝室の絵
					ELSEIF FLAG:現X == 39 && FLAG:現Y == 7
						CALL DUNGEON_WORP,32,16,3
					ENDIF
				CASE 27
					IF FLAG:現X == 2 && FLAG:現Y == 1
						CALL DUNGEON_WORP,27,10,2
					;小部屋の絵→古城
					ELSEIF FLAG:現X == 2 && FLAG:現Y == 14
						CALL DUNGEON_WORP,26,39,4
					ELSEIF FLAG:現X == 8 && FLAG:現Y == 1
						CALL DUNGEON_WORP,27,17,2
					ELSEIF FLAG:現X == 10 && FLAG:現Y == 1
						CALL DUNGEON_WORP,27,1,1
					;小部屋の絵→浸水牢獄
					ELSEIF FLAG:現X == 15 && FLAG:現Y == 13 
						SIF GETBIT(던전フラグ:99:4,29)
							CALL DUNGEON_WORP,29,19,2
					ELSEIF FLAG:現X == 17 && FLAG:現Y == 1
						CALL DUNGEON_WORP,27,8,2
					;小部屋の絵→闘技場の絵
					ELSEIF FLAG:現X == 17 && FLAG:現Y == 13
						CALL DUNGEON_WORP,28,13,27
					ENDIF
				CASE 28
					;竜の間　下段→上段へ
					IF FLAG:現X == 8 && FLAG:現Y == 10
						CALL DUNGEON_WORP,28,9,3
					;闘技場の絵→小部屋の絵
					ELSEIF FLAG:現X == 13 && FLAG:現Y == 28
						CALL DUNGEON_WORP,27,16,13
					ELSE
						CALL DUNGEON_WORP,FLAG:現M,FLAG:現X,FLAG:現Y+7
					ENDIF
				CASE 29
					;浸水牢獄→吹き抜け階段2
