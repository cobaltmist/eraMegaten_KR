					SETBIT 던전フラグ:99:6,35
					CALL DUNGEON_WORP,10,13,9
				ENDIF
			;■太陽の紋章先
			CASE 1107
				IF !GETBIT(던전フラグ:99:6,39)
					PRINTFORML レッドオーブ +5
					MONEY:99 += 5
					SETBIT 던전フラグ:99:6,39
					CALL DUNGEON_WORP,11,9,16
				ENDIF
			
		ENDSELECT
ENDSELECT


