			IF FLAG:현X == 4 && FLAG:현Y == 6
				FLAG:현X = 3
			ELSEIF FLAG:현X == 15 && FLAG:현Y == 10
				FLAG:현Y = 11
			ELSEIF FLAG:현X == 17 && FLAG:현Y == 12
				FLAG:현Y = 11
			ELSEIF FLAG:현X == 11 && FLAG:현Y == 13
