;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@CAN_ENTER_96,ARG,ARG:1
;ARG、ARG:1はそれぞれ대상のマスのX、Y座標を表しています。
;この던전ではエレベーターの扉が一度右側から開けておかないと左側からは開けられない、という処理に사용しています。
;どのマップでも座標(2,7)がエレベータの扉。던전フラグ:(FLAG:現던전):13 ＝ エレベータフラグ管理用変数の{現在のマップ番号}番눈のビットが立っていなければ開けられるかどうかの판정を行う

SELECTCASE DA:ARG:(ARG:1)
	;ミ
	CASE 39
		RETURN 0
	;이벤트扉
	CASE 12
		IF 0
		;12カオス
		ELSEIF FLAG:現M == 1 && (던전フラグ:(FLAG:現던전):12 == 3 || 던전フラグ:96:10 == 2)
			RETURN 1
		;13아리오크
		ELSEIF FLAG:現M == 4 && (던전フラグ:(FLAG:現던전):13 == 3 || 던전フラグ:96:10 == 2)
			RETURN 1
		;14아스타로트
		ELSEIF FLAG:現M == 5 && (던전フラグ:(FLAG:現던전):14 == 3 || 던전フラグ:96:10 == 2)
			RETURN 1
		;15수르트
		ELSEIF FLAG:現M == 6 && (던전フラグ:(FLAG:現던전):15 == 3 || 던전フラグ:96:10 == 2)
			RETURN 1
		;:16우리엘
		ELSEIF FLAG:現M == 9 && (던전フラグ:(FLAG:現던전):16 == 3 || 던전フラグ:96:10 == 1)
			RETURN 1
		;:17가브리엘
		ELSEIF FLAG:現M == 10 && (던전フラグ:(FLAG:現던전):17 == 3 || 던전フラグ:96:10 == 1)
			RETURN 1
		;:18라파엘
		ELSEIF FLAG:現M == 11 && (던전フラグ:(FLAG:現던전):18 == 3 || 던전フラグ:96:10 == 1)
			RETURN 1
		;:19ロウ
		ELSEIF FLAG:現M == 14 && (던전フラグ:(FLAG:現던전):19 == 3 || 던전フラグ:96:10 == 1)
			RETURN 1
		ELSEIF FLAG:現M == 7 && 던전フラグ:(FLAG:現던전):23
			RETURN 1
		ELSEIF FLAG:現M == 19 && 던전フラグ:(FLAG:現던전):33
			RETURN 1
		ELSE
			CALL MESSAGE_WINDOW_D, "", "/＞扉は不思議な힘で閉ざされている/" , , , , , , , , , "CENTER"
			RETURN 0
		ENDIF
	;↑
	CASE 22
		RETURN ARG:1 < FLAG:現Y
	;↓
	CASE 32
		RETURN ARG:1 > FLAG:現Y
	;→
	CASE 42
		RETURN ARG > FLAG:現X
	;←
	CASE 52
		RETURN ARG < FLAG:現X
	;ロ
	CASE 102
		SIF 던전フラグ:96:31 < 2
			RETURN 0
	;カ
	CASE 112
		SIF 던전フラグ:96:32 < 2
			RETURN 0
ENDSELECT

CALL CAN_ENTER,ARG,ARG:1
RETURN RESULT

