;----------------------------------
;쇼고스　罰ルート前座
;一斉자폭テロ子ちゃん
;----------------------------------
@SET_ACTION_쇼고스, ARG
;입력行動を-1とターゲットを-1にしておく
CFLAG:ARG:입력行動 = -1
CFLAG:ARG:ターゲット = -1
SELECTCASE RAND:100
	CASE IS < 20
		IF CFLAG:ARG:BS付着率강화 < 2
			CFLAG:ARG:입력行動 = [[스킬:무거운공기]]
		ELSE
			CFLAG:ARG:입력行動 = [[스킬:혼탁의장기]]
		ENDIF
	CASE IS < 40
		CFLAG:ARG:입력行動 = [[스킬:바이에이하]]
	CASE IS < 60
		CFLAG:ARG:입력行動 = [[스킬:돌격]]
	CASE IS < 80
		CFLAG:ARG:입력行動 = [[스킬:라쿤・다]]
	CASEELSE
		CFLAG:ARG:입력行動 = 0
ENDSELECT
;HP20%以下の쇼고스がでたら一斉に자폭（死亡も含む）
FOR LOCAL, 7, 12
	SIF 現HP割合(POS(LOCAL)) < 20
		CFLAG:ARG:입력行動 = [[스킬:옥쇄파]]
NEXT
;アクション実行不能なら自動的に통상공격にする
CALL CHECK_ACTIONABLE, ARG, CFLAG:ARG:입력行動
IF RESULT == 0
	CFLAG:ARG:입력行動 = 0
	CALL RANDOM_TARGET, ARG, CFLAG:ARG:입력行動
ENDIF
;ここまでターゲットが決まってなかったら、랜덤ターゲットをよんで決定
SIF CFLAG:ARG:ターゲット == -1
	CALL RANDOM_TARGET, ARG, CFLAG:ARG:입력行動
RETURN 1

