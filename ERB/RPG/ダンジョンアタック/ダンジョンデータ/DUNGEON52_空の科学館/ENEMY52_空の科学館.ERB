;-----------------------------------
;キングレオ(罪ルートボス)
;-----------------------------------
@SET_ACTION_킹・레오, ARG
;HP減少による확률보정を作成
LOCAL = (100 - 現HP割合(ARG)) / 5
;입력行動を-1とターゲットを-1にしておく
CFLAG:ARG:입력行動 = -1
CFLAG:ARG:ターゲット = -1
IF CFLAG:ARG:１moreフラグ > 0
	SELECTCASE RAND:100
		CASE 0 TO LOCAL * 2
			CFLAG:ARG:입력行動 = [[스킬:열화의곡옥]]
		CASE LOCAL * 2 TO LOCAL * 4
			CFLAG:ARG:입력行動 = [[스킬:암흑광염난무]]
	ENDSELECT
;1ターン눈は補助的な行動を取る
ELSEIF CFLAG:ARG:行動した횟수 == 0
		IF MAX(100 - CFLAG:ARG:공격강화 * 3, 80) > RAND:100
			IF CFLAG:ARG:공격강화 < 16
				CFLAG:ARG:입력行動 = [[스킬:타루・카쟈]]
			ELSE
				CFLAG:ARG:입력行動 = [[스킬:히트라이저]]
			ENDIF
		ELSE
			SELECTCASE RAND:100
				CASE 0 TO 70
					IF CFLAG:ARG:빙결무효화횟수 == 0
						CFLAG:ARG:입력行動 = [[스킬:명왕신부]]
					ELSE
						CFLAG:ARG:입력行動 = [[스킬:포효]]
					ENDIF
				CASEELSE
					CFLAG:ARG:입력行動 = [[스킬:액화질소봄베]]
			ENDSELECT
		ENDIF
ELSEIF CFLAG:ARG:行動した횟수 == 1
	SELECTCASE RAND:100
		CASE 0 TO 19 +LOCAL
			CFLAG:ARG:입력行動 = [[스킬:산산조각베기]]
		CASE 20 TO 39 +LOCAL
			CFLAG:ARG:입력行動 = [[스킬:암흑광염난무]]
	ENDSELECT
ELSE
	;ニャルルートの場合、水晶髑髏공격을してくる
	IF STRFLAG_NUM_EV("ニャル進行", , , , [[이벤트:페르소나２]]) == 3 && RAND:2 && EQUIP:ARG:특수탄1
		CALL RANDOM_TARGET, ARG, 0
		TSTR = 水晶髑髏
		CFLAG:ARG:입력行動 = [[스킬:ＤＵＭＭＹ]]
		RETURN 1
	ENDIF
	SELECTCASE RAND:100
		CASE 0 TO 19 +LOCAL
			CFLAG:ARG:입력行動 = [[스킬:프레이라]]
		CASE 20 TO 49 +LOCAL
			CFLAG:ARG:입력行動 = [[스킬:마하・라기온]]
		CASE 50 TO 54 +LOCAL * 2
			CFLAG:ARG:입력行動 = [[스킬:열화의곡옥]]
	ENDSELECT
ENDIF
;ここまで입력行動が決まってなかったら、汎用3種に行く
;GUNは특수탄でも撃って来やすいので減らしとく
IF CFLAG:ARG:입력行動 == -1
	SELECTCASE RAND:100
		CASE 0 TO 34
			CFLAG:ARG:입력行動 = [[스킬:아기다인]]
		CASE 35 TO 69 
			CFLAG:ARG:입력行動 = 0
		CASEELSE
			CFLAG:ARG:입력行動 = [[스킬:화염격]]
	ENDSELECT
ENDIF
;アクション実行不能なら自動的に통상공격にする
CALL CHECK_ACTIONABLE, ARG, CFLAG:ARG:입력行動
IF RESULT == 0
	CFLAG:ARG:입력行動 = 0
	CALL RANDOM_TARGET, ARG, CFLAG:ARG:입력行動
ENDIF
;ここまでターゲットが決まってなかったら、랜덤ターゲットをよんで決定
SIF CFLAG:ARG:ターゲット == -1
	CALL RANDOM_TARGET, ARG, CFLAG:ARG:입력行動
RETURN 1

;구상
@ENEMY_BATTLE_MESSAGE_K4010, ARG
SKIPDISP 0
LOCAL:1 = 1
LOCAL = CFLAG:ARG:입력行動
LOCALS = EMPTY
IF CFLAG:ARG:입력行動 == [[스킬:ＤＵＭＭＹ]]
	IF RAND:3 == 0
		CALL MESSAGE_B(1, "그분으로부터 받은 힘을…、너희들 따위가 견딜 수 있을 리가 없어！！")
	ELSEIF RAND:2
		CALL MESSAGE_B(1, "수정해골에 의해 죽는 것을 영광으로 알아라！！")
	ELSE
		CALL MESSAGE_B(1, "위해한 마이아의 힘이여！！")
	ENDIF
	WHILE 1
		LOCAL:1 = RAND:6 + 1
		LOCALS = 포지션{LOCAL:1}
		SIF FLAG:LOCALS >= 0 && GET_STATE(CFLAG:(FLAG:LOCALS):ステート) != "DYING"
			BREAK
	WEND
	LOCAL:1 = FLAG:LOCALS
	LOCALS = %GET_TYPE(EQUIP:ARG:특수탄1)%
	EQUIP:ARG:특수탄1 = 0
	IF MAXBASE:(LOCAL:1):LOCALS == 999 || MAXBASE:(LOCAL:1):LOCALS <= 0 || LOCAL:1 == 0
		LOCALS =  "하늘의 수정해골이 빛을 내뿜어 %플레이어를()% 지켰다！
		SIF LOCAL:1 == 0
			CALL MESSAGE_B(1, LOCALS)
		LOCALS = BLOCK
		LOCAL:3 = 0
	ELSE
		LOCAL:3 = (MAXBASE:(LOCAL:1):ＨＰ * MAXBASE:(LOCAL:1):LOCALS) / 100
		SIF CFLAG:(LOCAL:1):방어フラグ
			LOCAL:3 /= 2
		LOCALS = {LOCAL:3} 데미지
	ENDIF
	PRINTFORMW TARGET:[{LOCAL:1,2}] %CALLNAME:(LOCAL:1),20,LEFT%　>>>>>>　%LOCALS%
	CALL VAR_HP, LOCAL:1, -(LOCAL:3), 0
	RETURN 0
ENDIF
;実は1MOREとるたびに徐々に공격력があがる
IF CFLAG:ARG:１moreフラグ
	MAXBASE:ARG:공격 += 5
	MAXBASE:ARG:마법위력 += 5
	CFLAG:ARG:공격강화 += 5
	CFLAG:ARG:마법위력강화 += 5
	LOCALS = 좋아！ 힘이 솟는다…！ 너희들을 죽일 힘이！！
ELSE
	;숯덩어리로 만들어 주마
	;페르소나ー
	SELECTCASE LOCAL
		CASE 0, [[스킬:산산조각베기]]
			IF !RAND:3
				LOCALS = 그 분을 방해하게 둘까보냐！
			ELSEIF RAND:2
				LOCALS = 햐하！！ 잘게 썰어주마！
			ELSE
				LOCALS = 죽어라！ 죽어라！ 죽어버려어어！！
			ENDIF
		CASE [[스킬:마하・라기온]], [[스킬:아기다인]]
			IF !RAND:3
				LOCALS = 바짝 익혀주마！！
			ELSEIF RAND:2
				LOCALS = 햐하하！！ 타오르는거야！！
			ELSE
				LOCALS = 불타라 불타라！ 불타버려라라！！
			ENDIF
		CASE [[스킬:프레이라]]
			LOCALS = 시끄럽다！ 시끄러워！ 모두 날아가버려！！
		CASE [[스킬:암흑광염난무]], [[스킬:열화의곡옥]]
			IF !RAND:3
				LOCALS = 모두 한꺼번에 몰살이다아아！！
			ELSEIF RAND:2
				LOCALS = …히…이히…햐하！히야ー 하하하아아아아！！
			ELSE
				LOCALS = 숯덩어리로 만들어 주마！
			ENDIF
		CASE [[스킬:명왕신부]], [[스킬:액화질소봄베]]
			LOCALS = 이히…히히…히야아！ 이런 것도 있다！！
		CASE [[스킬:타루・카쟈]], [[스킬:히트라이저]]
			IF RAND:2
				LOCALS = 전파전파전파！！ 전파가 몸에 가득 찼다！
			ELSE
				LOCALS = 히야하아！！ 불타라！！
			ENDIF
		CASE [[스킬:포효]]
			IF RAND:2
				LOCALS = 히ー얏핫핫핫하！
			ELSE
				LOCALS = 나는 자유롭다！ 자유로워！！
			ENDIF
	ENDSELECT
ENDIF
SIF LOCALS != "EMPTY"
	CALL MESSAGE_B(LOCAL:1, LOCALS, LOCALS:1, LOCALS:2, LOCALS:3, LOCALS:4)

