;=======================================================================
;エンカウント処理
;=======================================================================
@CHECK_ENCOUNT_52
IF RAND:100 < FLAG:エンカウント率
	IF STRFLAG_EV("罪ルート", , [[이벤트:페르소나２]])
		FOR LOCAL, 7, 17
			LOCAL:LOCAL = -1
		NEXT
		LOCAL:20 = 0
		$ENCOUNT_LOOP
		;소문の状況では가면당원が出てくる
		IF RAND:2 && STRFLAG_NUM_EV("아지트의 소문", , , , [[이벤트:소문]]) == 2
			SELECTCASE RAND:100
				;4色
				CASE 0 TO 19
					LOCAL:7 = 21
					LOCAL:8 = 21
					LOCAL:10 = 22
					LOCAL:11 = 22
					LOCAL:12 = 23
					LOCAL:13 = 23
					LOCAL:15 = 24
					LOCAL:16 = 24
				;前衛後衛2色 10体
				CASE 10 TO 29
					LOCAL:1 = RAND:2 + 21
					LOCAL:2 = RAND:2 + 23
					FOR LOCAL, 7, 17
						IF LOCAL > 11
							LOCAL:LOCAL = LOCAL:1
						ELSE
							LOCAL:LOCAL = LOCAL:2
						ENDIF
					NEXT
				;1色１열
				CASE 30 TO 49
					LOCAL:1 = RAND:4 + 21
					FOR LOCAL, 7, 17
						LOCAL:LOCAL = LOCAL:1
					NEXT
				;40%の확률로랜덤配置、1枠50%
				CASEELSE
					FOR LOCAL, 7, 17
						SIF RAND:2
							LOCAL:LOCAL = RAND:4 + 21
					NEXT
			ENDSELECT
		ELSE
			SELECTCASE RAND:100
				;一種
				CASE 0 TO 29
					;모스맨は少なめ
					IF RAND:6 == 0
						LOCAL:1 = 4
					ELSE
						LOCAL:1 = RAND:3 + 1
					ENDIF
					FOR LOCAL, 7, 17
						IF RAND:2
							LOCAL:LOCAL = LOCAL:1
						ENDIF
					NEXT
				;二種
				CASE 30 TO 49
					;모스맨は少なめ
					DO
						IF RAND:6 == 0
							LOCAL:2 = 4
						ELSE
							LOCAL:2 = RAND:3 + 1
						ENDIF
					LOOP LOCAL:1 == LOCAL:2
					FOR LOCAL, 7, 17
						SIF RAND:2
							LOCAL:LOCAL = LOCAL:(RAND:2 + 1)
					NEXT
				;四種
				CASE 50 TO 59
					FOR LOCAL, 7, 17
						SIF RAND:2
							LOCAL:LOCAL = RAND:4 + 1
					NEXT
				;빙결のみテーブル
				CASE 69 TO 89
					FOR LOCAL, 7, 17
						IF RAND:2
							LOCAL:LOCAL = RAND:2 + 11
						ENDIF
					NEXT
				;混合テーブル
				CASEELSE
					FOR LOCAL, 7, 17
						IF RAND:2
							IF RAND:3
								LOCAL:LOCAL = RAND:4 + 1
							ELSE
								LOCAL:LOCAL = RAND:2 + 11
							ENDIF
						ENDIF
					NEXT
			ENDSELECT
		ENDIF
		FOR LOCAL, 7, 17
			SIF LOCAL:LOCAL >= 0
				CALL SET_DUNGEON_ENEMY_52, LOCAL, LOCAL:LOCAL / 10, LOCAL:LOCAL % 10
		NEXT
		FOR LOCAL,7,17
			SIF POS(LOCAL) > -1
				LOCAL:20 += 1
		NEXT
		SIF LOCAL:20 == 0
			GOTO ENCOUNT_LOOP
		RETURN 1
	ELSE
		CALL ENEMY_TABLE
		RETURN 1
	ENDIF
ELSE
	FLAG:未遭遇歩数 += 1
	RETURN 0
ENDIF

;=======================================================================
;적キャラの中身
;=======================================================================
@SET_DUNGEON_ENEMY_52, ARG, ARG:1, ARG:2
;ARG 포지션
;ARG:1 テーブル패턴
;ARG:2 キャラ指定
;LOCAL 作った적の등록번호
;LOCAL;1 キャラ번호
;LOCAL:2 랭크
LOCAL:2 = 0
;통상テーブル
IF ARG:1 == 0
	ARG:2 = ARG:2 ? ARG:2 # RAND:4 + 1
	SELECTCASE ARG:2
		CASE 1
			LOCAL:1 = [[キャラ:켈라이노]]
			LOCAL:2 = 28
		CASE 2
			LOCAL:1 = [[キャラ:카라스텐구]]
			LOCAL:2 = 28
		CASE 3
			LOCAL:1 = [[キャラ:프린시펄리티]]
			LOCAL:2 = 28
		CASE 4
			LOCAL:1 = [[キャラ:모스맨]]
			LOCAL:2 = 30
	ENDSELECT
	LOCAL:2 += ((FLAG:現M - 1) * 10 > RAND:100) * 3
;빙결持ちテーブル
ELSEIF ARG:1 == 1
	ARG:2 = ARG:2 ? ARG:2 # RAND:2 + 1
	SELECTCASE ARG:2
		CASE 1
			LOCAL:1 = [[キャラ:하우르바타트]]
			LOCAL:2 = 28
		CASE 2
			LOCAL:1 = [[キャラ:아에로]]
			LOCAL:2 = 28
	ENDSELECT
	LOCAL:2 += ((FLAG:現M - 1) * 10 > RAND:100)
;仮面党テーブル
ELSEIF ARG:1 == 2
	ARG:2 = ARG:2 ? ARG:2 # RAND:4 + 1
	LOCAL:2 = 28 + ((FLAG:現M - 1) * 10 > RAND:100) * 2 + ((FLAG:現M - 2) * 10 > RAND:100) * 2
	SELECTCASE ARG:2
		CASE 1
			LOCAL:1 = [[キャラ:황소가면]]
		CASE 2
			LOCAL:1 = [[キャラ:사자가면]]
		CASE 3
			LOCAL:1 = [[キャラ:전갈가면]]
		CASE 4
			LOCAL:1 = [[キャラ:물병가면]]
	ENDSELECT
ENDIF
CALL SET_ENEMY, ARG, LOCAL:1, LOCAL:2
LOCAL = CHARANUM - 1
SELECTCASE LOCAL:1
	CASE [[キャラ:프린시펄리티]]
		ABL:LOCAL:스킬1 = [[스킬:아기라오]]
	CASE [[キャラ:모스맨]]
		IF LOCAL:2 == 1
			ABL:LOCAL:스킬6 = [[스킬:파이어브레스]]
		ELSE
			ABL:LOCAL:스킬4 = [[스킬:파이어브레스]]
		ENDIF
	CASE [[キャラ:카라스텐구]]
		ABL:LOCAL:스킬4 = [[스킬:마하・라기온]]
		ABL:LOCAL:스킬5 = [[스킬:천구신부]]
	CASE [[キャラ:하우르바타트]]
		MAXBASE:LOCAL:충격 = 150
		BASE:LOCAL:충격 = 150
	CASE [[キャラ:켈라이노]]
		ABL:LOCAL:스킬2 = [[스킬:마하・라기]]
		ABL:LOCAL:스킬4 = [[스킬:아기라오]]
	CASE [[キャラ:황소가면]]
		ABL:LOCAL:스킬2 = [[스킬:마하・마그나스]]
		ABL:LOCAL:스킬3 = [[스킬:섬머솔트]]
	CASE [[キャラ:사자가면]]
		ABL:LOCAL:스킬2 = [[스킬:마하・라기온]]
		ABL:LOCAL:스킬3 = [[스킬:회전베기]]
	CASE [[キャラ:전갈가면]]
		ABL:LOCAL:스킬2 = [[스킬:마하・아쿠에스]]
		ABL:LOCAL:스킬3 = [[스킬:메・디아라마]]
	CASE [[キャラ:물병가면]]
		ABL:LOCAL:스킬2 = [[스킬:마하・갈라]]
ENDSELECT

