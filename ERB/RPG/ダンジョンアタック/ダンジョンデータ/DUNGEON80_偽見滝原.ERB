					DA:2:10 = 1
				ENDIF
			ELSEIF FLAG:현X == 7 && FLAG:현Y ==13
				CALL DUNGEON_WORP,3,7,1
			ENDIF
		CASE 1
			IF FLAG:현X == 5
				CALL DUNGEON_WORP,0,5,1
			ELSE
				CALL DUNGEON_WORP,0,9,1
			ENDIF
		CASE 2
			IF FLAG:현X == 14
				CALL DUNGEON_WORP,0,1,5
			ELSEIF FLAG:현X == 0 && FLAG:현Y == 2
				IF 던전플래그:80:2 == 0
					던전플래그:80:2 = 1
					DA:2:2 = 4
					DA:2:10 = 4
				ENDIF
				CALL DUNGEON_WORP,2,1,10
			ELSEIF FLAG:현X == 0 && FLAG:현Y == 10
				IF 던전플래그:80:2 == 0
					던전플래그:80:2 = 1
					DA:2:2 = 4
					DA:2:10 = 4
				ENDIF
				CALL DUNGEON_WORP,2,1,2
			ENDIF
