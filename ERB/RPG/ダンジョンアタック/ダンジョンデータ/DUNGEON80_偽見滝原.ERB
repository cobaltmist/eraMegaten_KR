	SELECTCASE FLAG:현M
		CASE 0
			IF FLAG:현X == 5 && FLAG:현Y == 0 
				CALL DUNGEON_WORP,1,5,12
			ELSEIF FLAG:현X == 9 && FLAG:현Y == 0
