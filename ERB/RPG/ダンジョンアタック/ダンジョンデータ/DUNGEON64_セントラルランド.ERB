RETURN RESULT

;=======================================================================
;特殊タイル표시
;=======================================================================
@TILE_MARK_COMMON_64_1, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 10 , 20 , 30 , 40 , 50 , 60 , 70 , 80 , 90, 160 , 170 , 171
		SETCOLOR 0xFFFF00
		RESULTS = ◎
	CASE 101 , 111 , 121 , 131 , 141 , 151
		RESULTS = 穴
;	CASE 11 , 21 , 31 , 41 , 51 , 61 , 71 , 81 , 91 , 160 , 161 , 100 , 110 , 120 , 130 , 140 , 150
;		RESULTS = □
	CASE 180
	RESULTS = ↑
	CASE 181
	RESULTS = ↓
	CASEELSE
		RETURN 1
ENDSELECT

@TILE_MARK_COMMON_64_4, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 100
		SIF 던전플래그:64:0 == 0
			RESULTS = ！
	CASE 200
		SIF 던전플래그:64:0 == 1
			RESULTS = ！
	CASE 300
		SIF 던전플래그:64:0 == 2
			RESULTS = ！
	CASE 400
		SIF 던전플래그:64:0 == 3
			RESULTS = ！
	CASE 500
		SIF 던전플래그:64:0 == 4
			RESULTS = ！
		SIF 던전플래그:64:1 > 0
			RESULTS = ！
ENDSELECT


@TILE_MARK_COMMON_64_9, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 1
		SETCOLOR 0xFFFF00
		RESULTS = ◎
	CASE 2
	IF 던전플래그:64:0 >= 2
		SETCOLOR 0xFFFF00
		RESULTS = ◎
	ENDIF
ENDSELECT
