;=======================================================================
;タイル進入時の이벤트
;=======================================================================
@EVENT_ENTER_DUNGEON_97,ARG
;ARG = 進入時の向き　0の場合ワープなど
;==========
;出입の処理
;==========
IF DA:(FLAG:현X):(FLAG:현Y) % 10 == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 3
	CALL TREASURE_97
	RETURN RESULT
	
;=======
;階段
;=======
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 6
	CALL UPSTAIRS_97
	RETURN RESULT

ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 7
	CALL DOWNSTAIRS_97
	RETURN RESULT
	
	
;========
;이벤트
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 4
	SELECTCASE DA:(FLAG:현X):(FLAG:현Y) / 10
		CASE 1
			CALL MESSAGE_WINDOW_D, "라벤차", "얄다바오트가 쓰러진 이후에도 팰리스는 계속 나타나는군요…"
			CALL MESSAGE_WINDOW_D, "라벤차", "그래도 계속 싸워나가시는 수밖에 없겠죠/제가 도울 일이 있을까요?"
			CALL INPUT_SELECT_D, "[1]동료를 교체한다/[2]라벤차를 갖고 싶다/[0]아무것도 아니다 "
			IF RESULT == 1
				CALL RECONSTRUCTION
			ELSEIF RESULT == 2
				CALL MESSAGE_WINDOW_D, "라벤차", "…………예? 이해가 잘 가지 않습니다"
				CALL MESSAGE_WINDOW_D, "라벤차", "저를 일행에 받아들이시고 싶으신 거라고요? "
				IF ABL:MASTER:속성LD == 1
				;당신がライト
					CALL MESSAGE_WINDOW_D, "라벤차", "지금까지 그대의 선한 면을 줄곧 보아 왔습니다/그대라면 분명 믿을 수 있겠지요"
					CALL MESSAGE_WINDOW_D, "라벤차", "좋습니다, 그대와 함께 하겠습니다"
					CALL ADD_NEW_COMPANION(7802, 500)
					CFLAG:(CHARANUM - 1):전투참가불가능 = -1
						;新페르소나処理
						IF BATTLE_SETTING_IS_PERSONA_NEW_FUNCTION()
						;旧페르소나処理
						ELSE
							LOCAL:1 = CFLAG:GETCHARA(7802):초기페르소나넘버
							DITEMTYPE:(LOCAL:1):페르소나("LV") = 50
							DITEMTYPE:(LOCAL:1):페르소나("힘") = 16
							DITEMTYPE:(LOCAL:1):페르소나("지혜") = 16
							DITEMTYPE:(LOCAL:1):페르소나("마력") = 16
							DITEMTYPE:(LOCAL:1):페르소나("인내력") = 16
							DITEMTYPE:(LOCAL:1):페르소나("속도") = 16
							DITEMTYPE:(LOCAL:1):페르소나("운") = 16
							DITEMTYPE:(LOCAL:1):페르소나("스킬1") = [[스킬:디아라마]]
							DITEMTYPE:(LOCAL:1):페르소나("스킬2") = [[스킬:방전]]
							DITEMTYPE:(LOCAL:1):페르소나("스킬3") = [[스킬:해피스텝]]
							DITEMTYPE:(LOCAL:1):페르소나("스킬4") = [[스킬:메・디아라마]]
							DITEMTYPE:(LOCAL:1):페르소나("스킬5") = [[스킬:암리타]]
							DITEMTYPE:(LOCAL:1):페르소나("습득스킬1") = [[스킬:메기도라온]]
							DITEMTYPE:(LOCAL:1):페르소나("습득LV1") = 60
						ENDIF
						CALL SYNC_STATUS, LOCAL
						CALL HEALTH_CHARA, LOCAL
						DA:(FLAG:현X):(FLAG:현Y) = 1
						던전フラグ:97:4 = 1
				ELSE
				;Light가 아님
					CALL MESSAGE_WINDOW_D, "라벤차", "죄송합니다, 저는 제 임무가 있기 때문에…/저보다 더 좋은 인연을 만나시기를 바라겠습니다"
				ENDIF
			ENDIF
			RETURN 1
				
		CASE 2
			CALL MESSAGE_WINDOW_D, "아케치 고로", "팰리스 조사는 계속 되고 있는 모양이네/나 역시 너희와 함께 다니긴 하겠지만, 협력은 비전투 조사만이다/섀도와 만나면 난 뒤로 빠질 거야"
			CALL MESSAGE_WINDOW_D, "아케치 고로", "…혹시 말을 건 이유라도 있어? 정보 교환이라면 얼마든지 OK다."
			CALL INPUT_SELECT_D, "[0]너는 누구지?/[1]괴도단에 가입할 생각 있나?/[2]아무것도 아니다 "
			IF RESULT == 0
				IF FINDCHARA_B(4663) >= 1 && CFLAG:GETCHARA(4663):전투참가불가능 != 1
					CALL MESSAGE_WINDOW_D, "모르가나", "분명 이 몸이 아는 아케치 고로는 시도 팰리스에서 죽었다/로카파라에서 만난 아케치 고로는…평행우주에서 온 다른 아케치 고로였고"
					CALL MESSAGE_WINDOW_D, "모르가나", "그렇다면 눈 앞에 있는 너는 누구지?"
					CALL MESSAGE_WINDOW_D, "아케치 고로", "…시도 팰리스에서 죽었다라, 그럴 수도 있지만/「사실은 죽지 않았다」라도 나쁘지 않잖아?"
					CALL MESSAGE_WINDOW_D, "아케치 고로", "아니면, 혹시 내가 죽기를 바랐나?"
					CALL MESSAGE_WINDOW_D, "모르가나", "너 때문에 가족을 잃은 누아르나 내비라면 그걸 바랐을 가능성은 있다만/뭐 이 몸은 그 정도까지 네가 죽기를 바라진 않았으니 안심해라"
					CALL MESSAGE_WINDOW_D, "아케치 고로", "이거 참, 살아 돌아와서 미안하게 됐군, 고양이"
					CALL MESSAGE_WINDOW_D, "모르가나", "고양이 아니야!"
				ELSE
					CALL MESSAGE_WINDOW_D, "아케치 고로", "탐정왕자의 재림이라 불린 고등학생 탐정 아케치 고로…TV에서 본 적 없나?"
					CALL MESSAGE_WINDOW_D, "아케치 고로", "특별히 네게 해 주고 싶은 말은 별로 없군/편하게 정보 주고 받을 사이는 아니잖아?"
				ENDIF
			ELSEIF RESULT == 1
				;괴도단 가입 조건(아케치 고로의 파티 합류 조건)
				;1. 마음의 괴도단 가입을 요청 + 조커가 있을 것
				;2. 석양의 괴도단 가입을 요청 + 미타케 란의 지혜가 25 이상 + 미타케 란이 연모계 함락 상태
				;원작(마음의 괴도단)을 따라가는 것이 훨씬 쉽고, 간편하다
				;아케치 고로가 원작에서 가입한 적 없던 석양의 괴도단(뱅드림)에 합류하는 건 까다롭고 복잡하다.
				
				CALL MESSAGE_WINDOW_D, "아케치 고로", "괴도단이라! 하하, 재미있는 이야기를 하는 군/내가 한 때 괴도단 놀이에 함께 했다는 걸 알고 물은 건가?"
				
				$INPUT_AKECHI_LOOP
				IF 의뢰플래그:85:2 == 1 && 의뢰플래그:85:3 == 1
					CALL INPUT_SELECT_D, "[0]마음의 괴도단에 돌아가는 건?/[1]석양의 괴도단에 가입하는 건?/[2]아무것도 아니다"
				ELSEIF 의뢰플래그:85:2 == 1 && 의뢰플래그:85:3 == 0
					CALL INPUT_SELECT_D, "[1]석양의 괴도단에 가입하는 건?/[2]아무것도 아니다"
				ELSEIF 의뢰플래그:85:2 == 0 && 의뢰플래그:85:3 == 1
					CALL INPUT_SELECT_D, "[0]마음의 괴도단에 돌아가는 건?/[2]아무것도 아니다"
				ENDIF
				IF RESULT == 0
