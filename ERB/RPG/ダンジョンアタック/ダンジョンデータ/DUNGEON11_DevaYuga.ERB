			CALL DUNGEON_UPSTAIRS,4,6,17
		SIF FLAG:現X == 11 && FLAG:現Y == 13
			CALL DUNGEON_UPSTAIRS,4,11,13
	CASE 4
		SIF FLAG:現X == 10 && FLAG:現Y == 17
			CALL DUNGEON_UPSTAIRS,5,10,17
	CASEELSE
