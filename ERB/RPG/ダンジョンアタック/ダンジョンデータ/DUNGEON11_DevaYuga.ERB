;=======================================================================
;[5]調べる　実行時の이벤트
;=======================================================================
@EVENT_CHECK_DUNGEON_11
;==========
;出입の処理
;==========
IF DA:(FLAG:現X):(FLAG:現Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 3
	CALL TREASURE_11
	RETURN 1
;=======
;階段
;=======
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 6
	CALL UPSTAIRS_11
	RETURN RESULT
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 7
	CALL DOWNSTAIRS_11
	RETURN RESULT
ELSEIF DA:(FLAG:現X):(FLAG:現Y) % 10 == 9
	CALL DUNGEON_TERMINAL
	RETURN 1
;========
;이벤트の処理
;========
ELSEIF DA:(FLAG:現X):(FLAG:現Y) == 4
	IF FLAG:現M == 4 && FLAG:現X == 17 && FLAG:現Y == 5
		CALL MESSAGE_WINDOW_D, "", "＞스위치가 있다"
		CALL INPUT_YN_D, "누른다", "누르지 않는다"
		IF RESULT == 0
			CALL MESSAGE_WINDOW_D, "", "＞스위치를 눌렀다"
			던전フラグ:(FLAG:現던전):5 = 1
		ENDIF
	ELSEIF FLAG:現M == 4 && FLAG:現X == 6 && FLAG:現Y == 8
		CALL MESSAGE_WINDOW_D, "", "＞스위치가 있다"
		CALL INPUT_YN_D, "누른다", "누르지 않는다"
		IF RESULT == 0
			CALL MESSAGE_WINDOW_D, "", "＞스위치를 눌렀다"
			던전フラグ:(FLAG:現던전):6 = 1
		ENDIF
	ENDIF
	CALL MAKE_FLOOR, FLAG:現던전
	CALL AUTOMAP
ELSE
	CALL MESSAGE_WINDOW_D, "", "＞아무 일도 일어나지 않았다"
	RETURN 0
ENDIF
