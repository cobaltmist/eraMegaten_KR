		IF 던전플래그:42:신주쿠플래그("오자와") > 1
			SETCOLOR 0x00ff00
			RESULTS = ｍ
		ELSE
			RESULTS = ！
		ENDIF
	CASEELSE
		;特に설정しない場合、"！"と표시されます
		RETURN 1
ENDSELECT


