@TILE_MARK_COMMON_88_4, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	;落とし穴を"●"と표시
	CASE 100 , 101 , 103 , 104 , 105 , 301 , 302 , 303 , 305 , 306
