;=======================================================================
;エンカウント処理
;=======================================================================
@CHECK_ENCOUNT_20
IF RAND:100 < FLAG:인카운트율
	;低확률で레어エネミーと遭遇。타르타로스の赤악마処理とほぼ同様
	IF RAND:100 < 3 && FLAG:현M != 3
		PRINTW ＞일격에 쓰러진 듯 악마의 무참한 시체가 나뒹굴고 있다。
		PRINTW ＞부근에 강력한 무장 집단이 있는 것 같다…
		CALL INPUT_YN("싸움을 건다", "앞서 가게 한다")
		IF RESULT == 0
			SELECTCASE RAND:3
				CASE 0
					;リーダーを個別定義。取り巻きはまとめて랜덤配置処理。
					;「下水掃除屋」
					;터미네이터
					CALL SET_ENEMY,14, 3125,65,0,1
					CFLAG:(CHARANUM-1):행동횟수 = 2
					CFLAG:(CHARANUM-1):ＨＰ보정 += 1500
					;터미네이터
					LOCAL:1 = 3125
					;T95메두사
					LOCAL:2 = 1114
					;모리안
					LOCAL:3 = 908
					LOCAL:4 = 60
				CASE 1
					;카테드랄の巡回神官団
					;달인
					CALL SET_ENEMY,14, 3126,65,0,1
					CFLAG:(CHARANUM-1):행동횟수 = 2
					CFLAG:(CHARANUM-1):ＨＰ보정 += 1500
					;스캐너
					LOCAL:1 = 3124
					;템플나이트
					LOCAL:2 = 3110
					;파워
					LOCAL:3 = 360
					LOCAL:4 = 60
				CASEELSE
					;가이아교도の捜索隊
					;やみほうし
					CALL SET_ENEMY,14, 3121,65,0,1
					CFLAG:(CHARANUM-1):행동횟수 = 2
					CFLAG:(CHARANUM-1):ＨＰ보정 += 1500
					;가면성
					LOCAL:1 = 3114
					;다키니
					LOCAL:2 = 607
					;기리메칼라（반사象さん）
					LOCAL:3 = 1084
					LOCAL:4 = 60
			ENDSELECT
			;こちらは14이외に取り巻きを配置する処理。
			FOR LOCAL,7,17,1
				SIF LOCAL != 14
					CALL SET_ENEMY,LOCAL, LOCAL:(RAND:3+1),LOCAL:4,0,0
			NEXT
			FLAG:회화불능플래그 = 1
			FLAG:선제기습취소 = 1
			CALL BATTLE_START
			FLAG:회화불능플래그 = 0
			FLAG:선제기습취소 = 0
		ELSE
			PRINTW ＞…삼엄한 기색은 멀어져 갔다。
		ENDIF
		FLAG:미조우걸음수 = 0
		RETURN 0
	ELSE
		CALL ENEMY_TABLE
		RETURN 1
	ENDIF
ELSE
	FLAG:미조우걸음수 += 1
	RETURN 0
ENDIF
