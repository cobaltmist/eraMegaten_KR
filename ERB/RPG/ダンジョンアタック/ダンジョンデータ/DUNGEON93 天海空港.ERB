	CASE 1
		RESULTS = ↑
	CASE 2
		RESULTS = ↓
	CASEELSE
		RETURN 1
ENDSELECT

@TILE_MARK_COMMON_93_2, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 1
		RESULTS = ↑
	CASE 2
		RESULTS = ↓
	CASEELSE
		RETURN 1
ENDSELECT

@TILE_MARK_COMMON_93_4, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE  2 , 3 
		RESULTS = ↑
	CASE  1 , 4
		RESULTS = ↓
	CASE  5
	IF 던전플래그:(FLAG:현던전):1 == 1 || 던전플래그:(FLAG:현던전):1 == 2
		SETCOLOR 0x00ff00
		RESULTS = ｍ
		ELSE
		RESULTS = □
		ENDIF
	CASE  6
	IF 던전플래그:(FLAG:현던전):1 == 4
		IF 던전플래그:(FLAG:현던전):8 == 0
		RESULTS = ！
		ENDIF
	ELSE
	RESULTS = □
	ENDIF
	CASEELSE
		RETURN 1
ENDSELECT

