@START_DUNGEON_30,ARG
FLAG:現M = 0
FLAG:現X = 22
FLAG:現Y = 1
;CALL AUTOMAP

;=======================================================================
;スタート時이벤트
;=======================================================================
@EVENT_DUNGEON_START_30
FLAG:탈출스킬禁止 = 1
;コドクノマレビトmod、凪を仲魔にした場合
IF 이벤트フラグ:41:0 == 4 && 이벤트フラグ:41:1 == 1
	CALL MESSAGE_WINDOW_D, "" , 	@"＞串蛇媛を保護している刑務所に赴くと/＞突如、濃密なMAGの감각と血の臭いが漂ってきた/＞しかも、中からは총声や爆発音が聞こえてくる。"
	CALL MESSAGE_WINDOW_D, "나기" , 	@"「まさかコドクノマレビトの襲撃のプロセスでしょうか！？/　%CALLNAME:MASTER%殿、串蛇媛のいる奥へ急ぎましょう！」"
;コドクノマレビトmod、凪を拒否した場合
ELSEIF 이벤트フラグ:41:0 == 4 && 이벤트フラグ:41:1 == 2
	CALL MESSAGE_WINDOW_D, "" , 	@"＞串蛇媛が保護されているという刑務所に赴くと/＞突如、濃密なMAGの감각と血の臭いが漂ってきた/＞しかも、中からは총声や爆発音が聞こえてくる。"
	CALL MESSAGE_WINDOW_D, "" , 	@"＞すでに襲撃は始まっているようだ。/＞刑務所の奥へ進み、串蛇媛を確保しよう。"
ELSE
	CALLF FINDCHARA_B([[キャラ:아인]])
	LOCAL = RESULT:1
	CALL MESSAGE_WINDOW_D, , "＞" + CALLNAME:MASTER + "는 " + CALLNAME:LOCAL + "을 구출하기 위해 메시아교 수용소에 도달했다/＞현재 팬텀의 손에 의해 양동 작전이 펼쳐지고 있다/＞때문에 이쪽의 경비는 허술하게 된 것 같다"
	CALL MESSAGE_WINDOW_D, , "＞하지만、실패하면 다음은 없다/＞단판 승부로" + CALLNAME:LOCAL + "를 구출하지 않으면 안된다…"
ENDIF
;=======================================================================
;탈출時の이벤트
;=======================================================================
@EVENT_DUNGEON_END_30
;コドクノマレビトmod、凪を仲魔にした場合
IF 이벤트フラグ:41:0 == 4 && 이벤트フラグ:41:1 == 1
	CALL EVENT_41_4_2
;나기を拒否した場合
ELSEIF 이벤트フラグ:41:0 == 4 && 이벤트フラグ:41:1 == 2
	CALL EVENT_41_4_3
;통상
ELSE
	CALLF FINDCHARA_B([[キャラ:아인]])
	LOCAL = RESULT:1
	CALL MESSAGE_WINDOW_D, , "＞" + CALLNAME:MASTER + "는 무사히 " + CALLNAME:LOCAL + "을 구출 할 수 있었다/＞많이 불안했는지、" + CALLNAME:LOCAL + "은 " + CALLNAME:MASTER + "의 등에 엎혀 떨어지지 않겠다고 말하듯 찰싹 달라붙어 있다"
	CALL MESSAGE_WINDOW_D, , "＞하지만、" + CALLNAME:LOCAL + "을 노리는 세력의 수는 많다/＞그것은 펜텀도 예외가 아니다　이능자의 양산은 큰 리턴이 되기 때문이다/＞…" + CALLNAME:MASTER + "도 차후의 처신을 생각할 필요가 있을지도 모른다…"
	CALL MESSAGE_WINDOW_D, , "＞…그렇다 쳐도、다/＞나는 왜" + CALLNAME:LOCAL + "이 신경 쓰이는 것일까"
	CALL MESSAGE_WINDOW_D, , "＞아니、대답은 뻔하다/＞나는" + CALLNAME:LOCAL + "이 좋은 것이다"
	IF (TALENT:(LOCAL):연모 || TALENT:(LOCAL):친애)
		CALL MESSAGE_WINDOW_D, , "＞그리고 또한、" + CALLNAME:LOCAL + "도 나를 좋아하고 있다/＞정말 기쁜 일이다"
	ELSE
		CALL MESSAGE_WINDOW_D, , "＞" + CALLNAME:LOCAL + "가 나를 어떻게 생각하는지는 모르지만.…/＞솔직히 부끄러워서、물어 보고 싶진 않다"
	ENDIF
	CALL MESSAGE_WINDOW_D, , "＞그럼、소중한" + CALLNAME:LOCAL + "을 지키기 위해、" + CALLNAME:MASTER + "는 어떻게 해야 할 까…"
	의뢰フラグ:70:0 = 7
ENDIF
;=======================================================================
;指定したマスに進入가능か
;=======================================================================
@CAN_ENTER_30,ARG,ARG:1
;コドクノマレビトmod
IF 이벤트フラグ:41:0 == 4
	IF FLAG:現M == 1 && ARG == 7 && ARG:1 == 8
		IF 던전フラグ:30:10 == 0
			CALL MESSAGE_WINDOW_D, "", "＞문이 잠겨있다"
			RETURN 0
		ELSE
			RETURN 1
		ENDIF
	ELSEIF FLAG:現M == 1 && ARG == 1 && ARG:1 == 8
		IF 던전フラグ:30:11 == 0
			CALL MESSAGE_WINDOW_D, "", "＞문이 잠겨있다"
			RETURN 0
		ELSE
			RETURN 1
		ENDIF
	ELSEIF FLAG:現M == 1 && ARG == 3 && ARG:1 == 6
		IF FLAG:現X <= 6 && FLAG:現Y <= 6
			RETURN 1
		ELSE
			CALL MESSAGE_WINDOW_D, "", "＞문이 잠겨있다"
			RETURN 0
		ENDIF
	ELSEIF FLAG:現M == 2 && ARG == 8 && ARG:1 == 7
		IF 던전フラグ:30:3 == 0
			CALL MESSAGE_WINDOW_D, "", "＞아무래도 전자 자물쇠가 걸려있는 것 같다"
			RETURN 0
		ELSEIF 던전フラグ:30:13 == 0
			CALL MESSAGE_WINDOW_D, "", "＞ＩＤカードで扉を開けますか？"
			CALL INPUT_YN_D
			IF RESULT == 0
				CALL MESSAGE_WINDOW_D, "", "＞" + CALLNAME:LOCAL + "はＩＤカードを使った"
				던전フラグ:30:13 = 1
			ENDIF
			RETURN 1
		ELSE
			RETURN 1
		ENDIF
	ELSE
		CALL CAN_ENTER,ARG,ARG:1
		RETURN RESULT
	ENDIF
;通常
ELSE
	IF FLAG:現M == 1 && ARG == 7 && ARG:1 == 8
		IF 던전フラグ:30:10 == 0
			CALL MESSAGE_WINDOW_D, "", "＞문이 잠겨있다"
			RETURN 0
		ELSE
			RETURN 1
		ENDIF
	ELSEIF FLAG:現M == 1 && ARG == 1 && ARG:1 == 8
		IF 던전フラグ:30:11 == 0
			CALL MESSAGE_WINDOW_D, "", "＞문이 잠겨있다"
			RETURN 0
		ELSE
			RETURN 1
		ENDIF
	ELSEIF FLAG:現M == 1 && ARG == 3 && ARG:1 == 6
		IF FLAG:現X <= 6 && FLAG:現Y <= 6
			RETURN 1
		ELSE
			CALL MESSAGE_WINDOW_D, "", "＞문이 잠겨있다"
			RETURN 0
		ENDIF
	ELSEIF FLAG:現M == 2 && ARG == 8 && ARG:1 == 7
		IF 던전フラグ:30:3 == 0
			CALL MESSAGE_WINDOW_D, "", "＞아무래도 전자 자물쇠가 걸려있는 것 같다"
			RETURN 0
		ELSEIF 던전フラグ:30:13 == 0
			CALL MESSAGE_WINDOW_D, "", "＞ＩＤ:게헨나로 문을 여시겠습니까？"
			CALL INPUT_YN_D
			IF RESULT == 0
				CALL MESSAGE_WINDOW_D, "", "＞" + CALLNAME:LOCAL + "는ＩＤ:게헨나를 사용했다"
				던전フラグ:30:13 = 1
			ENDIF
			RETURN 1
		ELSE
			RETURN 1
		ENDIF
	ELSE
		CALL CAN_ENTER,ARG,ARG:1
		RETURN RESULT
	ENDIF
ENDIF
