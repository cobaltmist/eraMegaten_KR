;=======================================================================
;攻略조건
;=======================================================================
@PREREQUISITE_DUNGEON_30
;コドクノマレビトmod、凪の同行が필요 or 最初に凪を拒否した
IF (이벤트フラグ:41:0 == 4 && FINDCHARA_B([[キャラ:나기]]) == 2) || (이벤트フラグ:41:0 == 4 && 이벤트フラグ:41:1 == 2)
	RETURN 1
ELSEIF 의뢰フラグ:70:0 != 6
	RETURN 0
ELSE
	RETURN 1
ENDIF

;=======================================================================
;던전名
;=======================================================================
@GET_DUNGEON_NAME_30
;コドクノマレビトmod
IF 이벤트フラグ:41:0 == 4
	RESULTS = 刑務所
ELSE
	RESULTS = 이단 수용소
ENDIF

;=======================================================================
;フ로어名표시
;=======================================================================
@FLOORNAME_30
;コドクノマレビトmod
IF 이벤트フラグ:41:0 == 4
	PRINT 刑務所
ELSE
	PRINT 이단 수용소
ENDIF
	SELECTCASE FLAG:現M
		CASE 0
			PRINTL １Ｆ
		CASE 1
			PRINTL ２Ｆ
		CASE 2
			PRINTL ３Ｆ
	ENDSELECT

