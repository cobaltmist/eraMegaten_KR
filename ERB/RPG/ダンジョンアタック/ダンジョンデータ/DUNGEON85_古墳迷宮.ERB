;=======================================================================
;特殊タイル표시
;=======================================================================
@TILE_MARK_COMMON_85_1, ARG , ARG:1 , ARG:2
IF FLAG:DEBUG
SELECTCASE ARG / 10
	CASE  0
		RETURN 1
	CASEELSE
		RESULTS = ！
ENDSELECT
ELSE
SELECTCASE ARG / 10
	CASE  140 , 150 , 160 , 170 , 180 , 190
		RESULTS = 穴
	CASEELSE
		RETURN 1
ENDSELECT
ENDIF

@TILE_MARK_COMMON_85_2, ARG , ARG:1 , ARG:2
IF EQUIP:MASTER:게이트서치 == 1
	SELECTCASE DA:(ARG:1):(ARG:2)
		CASE 12
			RESULTS = ↑
		CASE 22
			RESULTS = ↓
		CASE 32
			RESULTS = ←
		CASE 42
			RESULTS = →
		CASEELSE
			RESULTS = 문
	ENDSELECT
ELSE
	RESULTS = 문
ENDIF


@TILE_MARK_COMMON_85_4, ARG , ARG:1 , ARG:2
SELECTCASE ARG / 10
	CASE 1
		RESULTS = ↑
	CASE 2
		RESULTS = ↓
	CASE 3
		RESULTS = ←
	CASE 4
		RESULTS = →
ENDSELECT

