						CALL SET_ENEMY,8,LOCAL:4, 18,0
						CALL SET_ENEMY,10,LOCAL:4, 18,0
						CALL SET_ENEMY,13,LOCAL:4, 18,0
						CALL SET_ENEMY,15,LOCAL:4, 18,0
