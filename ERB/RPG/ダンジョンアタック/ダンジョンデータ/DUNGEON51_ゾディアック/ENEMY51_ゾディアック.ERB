;가면당원共通사고패턴
@SET_ACTION_KAMEN(ARG)
#DIM LCOUNT
;이슈킥不在
IF POS(9) == -1
	IF  CFLAG:ARG:포지션 >= 12
		CFLAG:ARG:입력行動 = -1
		CFLAG:ARG:방어フラグ = 1
	ELSE
		CFLAG:ARG:입력行動 = 0
		CALL RANDOM_TARGET,ARG,0
	ENDIF
	RETURN 3
ENDIF

;ターンに1回のみ
IF LOCAL:99 == FLAG:経過ターン数
	LOCAL:99 += 1
	;健在な가면당원を체크していく
	LOCAL = 0
	FOR LCOUNT,77,87
		;이슈킥は除外
		SIF LCOUNT == 79
			CONTINUE
		SIF FLAG:LCOUNT == -1
			CONTINUE
		LOCAL += 1
		LOCAL:(LOCAL) = FLAG:LCOUNT
	NEXT
	;健在な가면당원をシャッフルしつつ、余った枠を整理
	FOR LCOUNT,1,10
		IF LCOUNT <= LOCAL
			SWAP LOCAL:LCOUNT,LOCAL:(RAND:LOCAL+1)
		ELSE
			LOCAL:LCOUNT = 0
		ENDIF
	NEXT
ENDIF
;経過ターン/2＋2人が마법공격을してくる
LOCAL:20 = (FLAG:経過ターン数)/2 + 2
FOR LCOUNT,1,LOCAL
	IF LOCAL:LCOUNT == ARG
		IF LCOUNT <= LOCAL:20
			;1명마법
			IF LCOUNT % 2
				RETURN 1
			;범위마법
			ELSE
				RETURN 2
			ENDIF
		ELSE
			BREAK
		ENDIF
	ENDIF
NEXT
RETURN 0

;레이디の사고패턴
;아쿠에스を撃った場合、대상の수격の相性を学習する。
;学習後は相性悪い相손に아쿠에스をうたなくなり、ターゲットを振り変える
;相性がいい相손が発깨달음してる場合に、아쿠에스が選択された場合はその相손にタゲ確定
;약점もちは出してるとあっさり死亡まで持って行かれる…のはまぁそんなやらないとしても
;내성持ちを増やすとその分、내성持ってないキャラに아쿠에스が多くいくので注意か
;とくにHP600미만からは、マハ아쿠에스はあんまり連打しないように調整されてるマハ아쿠에스が行動횟수に関わらず出てくるので注意
;
;デバッグの感触だと、火약점は割と致命的というか、COOP連打しやすい部類にはいるんで、脆さが눈立つ
;Lv20強から、엘프・유키죠로・운디네が作れるんでそこまでいけば割と楽勝感
@SET_ACTION_레이디, ARG
#DIM LCOUNT
#LOCALSIZE 300
;フラグ초기化
IF LOCAL:9 == 0
	VARSET LOCAL,-1
	LOCAL:1 = 0
	LOCAL:2 = 0
ENDIF
LOCAL = RAND:100
;ターン限定공격、というか、マハ아쿠에스と디아라마の횟수制御
IF CFLAG:ARG:行動した횟수 == 0
	IF BASE:ARG:ＨＰ <= 1500 && LOCAL < 50
		;ＨＰ1500を割ってからは初回行動で디아라마を使い出す
		CALL CHECK_ACTIONABLE,ARG,402
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력行動 = 402
		CALL RANDOM_TARGET,ARG,402
		RETURN 0
	ENDIF
ELSEIF CFLAG:ARG:行動した횟수 == 2
	;20%でマハ아쿠에스,ＨＰ1200を割った단계で30％に
	IF LOCAL < 20 || (BASE:ARG:ＨＰ <= 1200 && LOCAL < 30)
		CALL CHECK_ACTIONABLE,ARG,1605
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력行動 = 1605
		CALL RANDOM_TARGET,ARG,1605
		RETURN 0
	;10%で暗黒マハ아쿠에스、HP1200を割った단계で15％
	ELSEIF LOCAL < 30 || (BASE:ARG:ＨＰ <= 1200 && LOCAL < 50)
		CALL CHECK_ACTIONABLE, ARG,
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력行動 = [[스킬:암흑마하・아쿠에스]]
		CALL RANDOM_TARGET,ARG, [[스킬:암흑마하・아쿠에스]]
		RETURN 0
	ENDIF
ENDIF
;아쿠에스のチェンジ횟수を초기化
LOCAL:6 = -1
;以下1명공격
$DECIDE_ACTION
LOCAL = RAND:100
;공격대상のNOを先にとっておく
;1/4の확률로最もHP低い적を狙いに行く
IF RAND:4 == 0
	LOCAL:7 = -1
	LOCAL:4 = 1
	FOR LCOUNT,1,7
		IF POS(LCOUNT) >= 0 && GET_STATE(CFLAG:(POS(LCOUNT)):ステート) != "DYING"
			IF LOCAL:7 == -1
				LOCAL:7 = BASE:(POS(LCOUNT)):ＨＰ
				LOCAL:4 = LCOUNT
			ENDIF
			IF BASE:(POS(LCOUNT)):ＨＰ <= LOCAL:7
				 LOCAL:7 = BASE:(POS(LCOUNT)):ＨＰ
				 LOCAL:4 = LCOUNT
			ENDIF
		ENDIF
	NEXT
	CFLAG:ARG:ターゲット = LOCAL:4
ELSE
	CALL RANDOM_TARGET,ARG
ENDIF
;대상のキャラ번호をLOCAL:5にいれておく
LOCAL:5 = FLAG:(CFLAG:ARG:ターゲット+70)
;20％の확률로마린카린
IF LOCAL < 20
	;대상が매료상태の場合、마린카린をやめる
	SIF GET_STATE(CFLAG:(LOCAL:5):ステート) == "CHARM"	
		GOTO DECIDE_ACTION
	CALL CHECK_ACTIONABLE,ARG,1211
	SIF RESULT == 0
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력行動 = 1211
;40％の확률로아쿠에스
ELSEIF ((LOCAL < 60 && BASE:ARG:ＨＰ >= 600) || (LOCAL < 50 && BASE:ARG:ＨＰ <= 600)) && LOCAL:6 < 10
	;수격약점の相손を깨달음えている場合、아쿠에스はその相손を狙う。
	LOCAL:8 = 0
	FOR LCOUNT,0,LOCAL:2
		IF CFLAG:(LOCAL:(LCOUNT+30)):포지션 && GET_STATE(CFLAG:(LOCAL:(LCOUNT+30)):ステート) != "DYING"
			CFLAG:ARG:ターゲット = CFLAG:(LOCAL:(LCOUNT+30)):포지션
			LOCAL:5 = FLAG:(CFLAG:ARG:ターゲット+70)
			LOCAL:8 = 1
		ENDIF
	NEXT
	$CHANGE_TARGET
	;チェンジ횟수記録
	LOCAL:6 += 1
	IF LOCAL:6 > 0
		;아군6人のキャラナンバーを취득する
		FOR LCOUNT,71,77
			LOCAL:LCOUNT = FLAG:LCOUNT
		NEXT
		;등록した配列をシャッフルする（こうすることで次の探査で最初にひっかかるキャラが랜덤になる）
		FOR LCOUNT,71,77
			SWAP LOCAL:LCOUNT,LOCAL:(RAND:6 + 71)
		NEXT
		FOR LCOUNT,71,77
			;生きてて、수격が効く相손をさがす
			IF LOCAL:LCOUNT >= 0 && MAXBASE:(LOCAL:LCOUNT):수격 >= 100 && MAXBASE:(LOCAL:LCOUNT):수격 < 999 && GET_STATE(CFLAG:(LOCAL:LCOUNT):ステート) != "DYING"
				CFLAG:ARG:ターゲット = CFLAG:(LOCAL:LCOUNT):포지션
				LOCAL:5 = FLAG:(CFLAG:ARG:ターゲット+70)
				BREAK
			ENDIF
		NEXT
	ENDIF
	;대상が매료상태の場合、2/3の확률로行動を選定しなおす
	IF GET_STATE(CFLAG:(LOCAL:5):ステート) == "CHARM" && RAND:3 >= 1	
		GOTO DECIDE_ACTION
	ENDIF
	CALL CHECK_ACTIONABLE,ARG,1602
	SIF RESULT == 0
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력行動 = 1602
	;수격が効かない相손を아쿠에스を1回打ち込んだら깨달음えて、次からは撃たなくなる
	IF MAXBASE:(LOCAL:5):수격 <= 75 || MAXBASE:(LOCAL:5):수격 == 999
		FOR LCOUNT,0,LOCAL:1
			IF LOCAL:5 == LOCAL:(10+LCOUNT)
				;10回チェンジしたら아쿠에스を諦める
				IF LOCAL:6 < 10
					GOTO CHANGE_TARGET
				ELSE
					GOTO DECIDE_ACTION
				ENDIF
			ENDIF
		NEXT
		IF LOCAL:1 < 20
			LOCAL:(10 + LOCAL:1) = LOCAL:5
			LOCAL:1 += 1
		ENDIF
	;수격약점の相손を깨달음える
	ELSEIF MAXBASE:(LOCAL:5):수격 > 100 && LOCAL:8 == 0
		LOCAL:(30+LOCAL:2) = LOCAL:5
		LOCAL:2 += 1
	ENDIF
;HP600미만からは20％の확률로マハ아쿠에스
ELSEIF LOCAL < 70 && BASE:ARG:ＨＰ <= 600
	CALL CHECK_ACTIONABLE,ARG,1605
	SIF RESULT == 0
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력行動 = 1605
	CALL RANDOM_TARGET,ARG,1605
;40％の확률로타격
ELSE
	;대상が매료상태の場合、2/3の확률로行動を選定しなおす
	SIF GET_STATE(CFLAG:(LOCAL:5):ステート) == "CHARM" && RAND:3 >= 1	
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력行動 = 0
ENDIF
LOCAL:6 = -1


