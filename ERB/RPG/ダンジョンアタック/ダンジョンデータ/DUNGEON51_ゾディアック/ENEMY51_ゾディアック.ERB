;=======================================================================
;エンカウント率
;=======================================================================
@ENCOUNT_RATE_51
SELECTCASE FLAG:現M
	;１Fは적がでない
	CASE 0
		FLAG:エンカウント率 = 0
	;１F奥から적出現開始,악마出現フラグがたってない場合、エンカウントが少ない
	CASEELSE
		FLAG:エンカウント率 = FLAG:未遭遇歩数 * 5
ENDSELECT
SIF FLAG:未遭遇歩数 < 5
	FLAG:エンカウント率 = 0
