;=======================================================================
;[5]조사　実行時の이벤트
;=======================================================================
@EVENT_CHECK_DUNGEON_33

;==========
;出口の処理
;==========
IF DA:(FLAG:현X):(FLAG:현Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 3
	RETURN 1
;=======
;階段
;=======
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 6
RETURN 1

ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 7
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 9
	CALL DUNGEON_TERMINAL
	RETURN 1
RETURN 1

ELSE
	PRINTW 아무것도 없는 것 같다
	RETURN 0
ENDIF

;=======================================================================
;タイル進入時の이벤트
;=======================================================================
@EVENT_ENTER_DUNGEON_33,ARG
;ARG = 進入時の向き　0の場合ワープなど
;==========
;出口の処理
;==========
IF DA:(FLAG:현X):(FLAG:현Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 3
	RETURN 1
;=======
;階段
;=======
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 6

RETURN 1

ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 7
	RETURN 1


ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 9
RETURN 1

;==========
;扉の処理
;==========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 2
	CALL DUNGEON_DOOR,ARG
	RETURN 1


;========================
;이벤트
;========================
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 4
	IF FLAG:현M == 0 && FLAG:현X == 13 && FLAG:현Y == 4 
		IF 던전플래그:33:0 == 0
			CUSTOMDRAWLINE =
			PRINTW 인적없는 공원묘지 입구에 악마의 모습이 보인다….
			PRINTW 「나는 마수 케르베로스… 이곳은 내 세력권이다. 썩 나가라」
			PRINTW 「…고 말하고 싶지만, 소환사여…. 들리는가? 이 원한 어린 소리가…」
			PRINTW 케르베로스의 말대로 땅에서 목소리가 들려 왔다.
			PRINTW 「이 소리는 소환사… 네게 복수를 바라는 망자들의 소리다」
			PRINTW 「날이 갈수록 심해지고 있다…. 이 소리 때문에 나는 만족스럽게 잠을 잘 수가 없다」
			PRINTW 「따라서 네놈에게 책임을 지우려고 한다. 이것을 받아라」
			PRINTW 케르베로스는 무언가를 입에 물고 이쪽으로 던졌다.
			ITEM:흑의바이저 += 1
			PRINTW 흑의바이저를 손에 넣었다!
			PRINTW 「이것을 사용해 묘비를 조사해라. 설명은 이상이다. 뒷일은 맡기지」
			PRINTW 케르베로스는 그 자리에 누워서 배를 긁기 시작했다.
			PRINTW 가만히 놔 두자….
			던전플래그:33:0 = 1
			DA:(FLAG:현X):(FLAG:현Y) = 1
		ELSE
			CUSTOMDRAWLINE =
			PRINTL 「소환사인가… 변함없이 나는 제대로 잘 수가 없다…」
			PRINTW 가만히 놔 두자….
			DA:(FLAG:현X):(FLAG:현Y) = 1
		ENDIF
	ELSEIF FLAG:현M == 0 && FLAG:현X == 11 && FLAG:현Y == 3 && 던전플래그:33:0 == 1
		CALL TOMB_STONE,0
	ELSEIF FLAG:현M == 0 && FLAG:현X == 9 && FLAG:현Y == 3 && 던전플래그:33:0 == 1
		CALL TOMB_STONE,10
	ELSEIF FLAG:현M == 0 && FLAG:현X == 7 && FLAG:현Y == 3 && 던전플래그:33:0 == 1
		CALL TOMB_STONE,20
	ELSEIF FLAG:현M == 0 && FLAG:현X == 5 && FLAG:현Y == 3 && 던전플래그:33:0 == 1
		CALL TOMB_STONE,30
	ELSEIF FLAG:현M == 0 && FLAG:현X == 3 && FLAG:현Y == 3 && 던전플래그:33:0 == 1
		CALL TOMB_STONE,40
	ELSEIF FLAG:현M == 0 && FLAG:현X == 11 && FLAG:현Y == 6 && 던전플래그:33:0 == 1
		CALL TOMB_STONE,50
	ELSEIF FLAG:현M == 0 && FLAG:현X == 9 && FLAG:현Y == 6 && 던전플래그:33:0 == 1
		CALL TOMB_STONE,60
	ELSEIF FLAG:현M == 0 && FLAG:현X == 7 && FLAG:현Y == 6 && 던전플래그:33:0 == 1
		CALL TOMB_STONE,70
	ELSEIF FLAG:현M == 0 && FLAG:현X == 5 && FLAG:현Y == 6 && 던전플래그:33:0 == 1
		CALL TOMB_STONE,80
	ELSEIF FLAG:현M == 0 && FLAG:현X == 3 && FLAG:현Y == 6 && 던전플래그:33:0 == 1
		CALL TOMB_STONE,90
	ENDIF
ENDIF
RETURN 0
;========================
;墓標이벤트
;========================
@TOMB_STONE,ARG
	CUSTOMDRAWLINE =
	$BACK_LOOP
	PRINTW 이름이 없는 묘비가 있다….
	PRINTL [1] 흑의바이저를 사용한다
;	SIF ITEM:후생의토령 > 0
;		PRINTL [2] 후생의토령을 사용한다
	PRINTL [0] 떠난다
	$INPUT_LOOP
	INPUT
	IF RESULT == 1
		CALL BLACKVISOR_USE,ARG
		GOTO BACK_LOOP
;	ELSEIF RESULT == 2
		;SIF ITEM:후생의토령 > 0
		;	CALL BELL_USE
	ELSEIF RESULT == 0
		RETURN 0
	ELSE
		GOTO INPUT_LOOP
	ENDIF
;========================
;흑의바이저이벤트
;========================
@BLACKVISOR_USE,ARG
#DIM LCOUNT

	PRINTL 누구와 싸우겠습니까?
	X = ARG
	FOR LCOUNT, 0, 10
		TRYCCALLFORM BOSS_NAME_{X}
			IF RESULT != 0
				IF CSVCSTR(RESULT,20,0) != ""
					;CSTRで종족명が指定されているならそれを표시
					PRINTFORML [{X}] %CSVCSTR(RESULT,20,0)% %CSVNAME(RESULT,0)%
				ELSE
					PRINTFORML [{X}] %STR:(CSVABL(RESULT,GETNUM(ABL,"종족"),0))% %CSVNAME(RESULT,0)%
				ENDIF
			ENDIF
		CATCH
		ENDCATCH
		X = X + 1
	NEXT
	
	PRINTW [0] 돌아간다
	$INPUT_SUMMON_LOOP
	INPUT
	IF RESULT != 0
		X = RESULT
		TRYCCALLFORM BOSS_NAME_{X}
			IF RESULT != 0
				TRYCCALLFORM BOSS_SUMMON_{X}
				CATCH
					PRINTW 그러나 아무 일도 일어나지 않았다….
				ENDCATCH
			ELSE
				GOTO INPUT_SUMMON_LOOP
			ENDIF
		CATCH
			GOTO INPUT_SUMMON_LOOP
		ENDCATCH
	ELSE
		RETURN 0
	ENDIF 
;========================
;후생의토령이벤트
;========================
@BELL_USE
#DIM LCOUNT
IF ITEM:후생의토령 > 0
	PRINTL 누구를 전생시키겠습니까?
	X = 1
	FOR LCOUNT, 0, 10
		TRYCCALLFORM BOSS_BELLCHECK_{X}
			IF RESULT == 1
				TRYCCALLFORM BOSS_NAME_{X}
				IF RESULT != 0
					IF CSVCSTR(RESULT,20,0) != ""
						;CSTRで종족명が指定されているならそれを표시
						PRINTFORML [{X}] %CSVCSTR(RESULT,20,0)% %CSVNAME(RESULT,0)%
					ELSE
						PRINTFORML [{X}] %STR:(CSVABL(RESULT,GETNUM(ABL,"종족"),0))% %CSVNAME(RESULT,0)%
					ENDIF
				ENDIF
				CATCH
				ENDCATCH
			ENDIF
		CATCH
			
		ENDCATCH

		X = X + 1
	NEXT	
	PRINTW [0] 돌아간다
	
	$INPUT_BELL_LOOP
	INPUT
	IF RESULT == 0
		
	ELSE
		X = RESULT
		TRYCCALLFORM BOSS_NAME_{X}
			IF RESULT != 0
				TRYCCALLFORM BOSS_BELL_{X}
				
				CATCH
					GOTO INPUT_BELL_LOOP
				ENDCATCH
			ELSE
				GOTO INPUT_BELL_LOOP
			ENDIF
		CATCH
			GOTO INPUT_BELL_LOOP
		ENDCATCH
	ENDIF 							
ENDIF


