IF ARG == -11
	DA:(ARG:1):(ARG:2) = -(FLAG:현M * 1000 + LOCAL:29 * 10 + 1)
	LOCAL:29++
	SELECTCASE -DA:(ARG:1):(ARG:2) / 10
		
	ENDSELECT
ENDIF
