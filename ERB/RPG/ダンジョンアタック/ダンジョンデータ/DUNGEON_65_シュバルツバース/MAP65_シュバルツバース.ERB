	IF GETBIT(던전フラグ:(FLAG:현던전):(FLAG:현M+25), LOCAL:3)
		DA:(ARG:1):(ARG:2) = 1
	ELSE
		DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:3 * 10 + 3
	ENDIF
	LOCAL:3++

ENDIF
IF ARG == 4
	DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:4 * 10 + 4
	LOCAL:4++
	SELECTCASE DA:(ARG:1):(ARG:2) / 10

	ENDSELECT
ENDIF
IF ARG == 6
	DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:6 * 10 + 6
	LOCAL:6++
ENDIF
IF ARG == 7
	DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:7 * 10 + 7
	LOCAL:7++
ENDIF
IF ARG == 8
	DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:8 * 10 + 8
	LOCAL:8++
	SELECTCASE DA:(ARG:1):(ARG:2) / 10

	ENDSELECT
ENDIF

IF ARG == 19
	DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:9 * 10 + 9
	LOCAL:9++
ENDIF


;隠し扉
IF ARG == 21
	DA:(ARG:1):(ARG:2) = (FLAG:현M+1)*1000 + LOCAL:21 * 10 + 100001
	LOCAL:21++
ENDIF


IF ARG == 22
	DA:(ARG:1):(ARG:2) = (FLAG:현M+1)*1000 + LOCAL:22 * 10 + 100002
	LOCAL:22++
ENDIF

;隠し場用
IF ARG >= 1001
	IF STRFLAG_NUM_D("カリーナ숨겨진 장소") == 0
		DA:(ARG:1):(ARG:2) = (ARG - 1000) / 100
	ELSEIF STRFLAG_NUM_D("カリーナ숨겨진 장소") == 1
		DA:(ARG:1):(ARG:2) = ((ARG - 1000) % 100) / 10
	ELSE
		DA:(ARG:1):(ARG:2) = (ARG - 1000) % 10
	ENDIF
ENDIF

