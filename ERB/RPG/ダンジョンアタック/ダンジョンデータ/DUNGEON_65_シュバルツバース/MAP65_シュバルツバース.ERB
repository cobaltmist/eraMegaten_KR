;=======================================================================
;攻略조건
;=======================================================================
@PREREQUISITE_DUNGEON_65
IF 이벤트플래그:11:0 > 0 
	RETURN 1
ELSE
	RETURN 0
ENDIF

;=======================================================================
;던전名
;=======================================================================
@GET_DUNGEON_NAME_65
RESULTS = 슈바르츠바스

;=======================================================================
;フ로어名표시
;=======================================================================
@FLOORNAME_65
PRINT 슈바르츠바스
SELECTCASE FLAG:현M
	CASE 0
		PRINT   조사선（
		SELECTCASE STRFLAG_NUM_D("조사선位置")
			CASE 0
				PRINT 지상
			CASE 1
				PRINT 섹터・안트리아
			CASE 2
				PRINT 섹터・보티즈
			CASE 3
				PRINT 섹터・카리나
			CASE 4
				PRINT 섹터・델파이너스
			CASE 5
				PRINT 섹터・에리다누스
		ENDSELECT
		PRINTL ）
	CASE IS < 4
		PRINTL 섹터・안트리아
	CASE IS < 12
		PRINTL 섹터・보티즈
	CASE 12
		PRINTL 섹터・카리나 Ｂ１Ｆ
	CASE 13
		PRINTL 섹터・카리나 １Ｆ
	CASE 14
		PRINT 섹터・카리나 ２Ｆ
		SELECTCASE STRFLAG_NUM_D("カリーナ숨겨진 장소")
			CASE 0
				PRINTL 
			CASE 1
				PRINTL （숨겨진 장소Ａ）
			CASE 2
				PRINTL （숨겨진 장소Ｂ）
		ENDSELECT
	CASE 15
		PRINT 섹터・카리나 ３Ｆ
		SELECTCASE STRFLAG_NUM_D("カリーナ숨겨진 장소")
			CASE 0
				PRINTL 
			CASE 1
				PRINTL （숨겨진 장소Ａ）
			CASE 2
				PRINTL （숨겨진 장소Ｂ）
		ENDSELECT
	CASE 16
		PRINTL 섹터・카리나 ４Ｆ
	CASE IS < 32
		PRINTL 섹터・델파이너스
ENDSELECT




;=======================================================================
;スタート地点입력
;=======================================================================
@START_DUNGEON_65,ARG
FLAG:현M = 0
FLAG:현X = 11
FLAG:현Y = 7


;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@CAN_ENTER_65,ARG,ARG:1
SELECTCASE DA:ARG:(ARG:1)
	CASE 1001
		SIF STRFLAG_NUM_D("조사선位置") == 0
			RETURN 0
	;∧
	CASE 902, 912
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		IF FLAG:현Y < ARG:1
			PRINTW ＞扉は開かない
			RETURN 0
		ENDIF
		FLAG:현Y -= 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:현Y += 2
	;＞
	CASE 922, 932
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		IF FLAG:현X > ARG
			PRINTW ＞扉は開かない
			RETURN 0
		ENDIF
		FLAG:현X += 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:현X -= 2
	;∨
	CASE 942, 952
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		IF FLAG:현Y > ARG:1
			PRINTW ＞扉は開かない
			RETURN 0
		ENDIF
		FLAG:현Y += 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:현Y -= 2
	;＜
	CASE 962, 972
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		IF FLAG:현X < ARG
			PRINTW ＞扉は開かない
			RETURN 0
		ENDIF
		FLAG:현X -= 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:현X += 2
	CASE 992	
		PRINTW ＞문이 열리지 않는다
		RETURN 0
	CASEELSE
		;2以外の扉はドア이벤트へ
		IF DA:ARG:(ARG:1) % 10 == 2 && DA:ARG:(ARG:1) > 2
			CALL DOOR_65, DA:ARG:(ARG:1) / 10
			RETURN RESULT
		ENDIF
ENDSELECT

CALL CAN_ENTER,ARG,ARG:1
RETURN RESULT

