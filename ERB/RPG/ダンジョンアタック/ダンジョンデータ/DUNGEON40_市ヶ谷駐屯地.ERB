		CALL MAKE_FLOOR_LINE, 4," 1 2 2 1 2 2 1 2 "
		CALL MAKE_FLOOR_LINE, 5," 111111111111111 "
		CALL MAKE_FLOOR_LINE, 6," 1 2 2 1 2 2 2 2 "
