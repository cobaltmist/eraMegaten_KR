		CALL SET_TILE,1,6,던전플래그:40:22,1,4

	CASE 3
		;三階
		FLAG:최대X = 17 
		FLAG:최대Y = 9 
;								 01234567890123456
		CALL MAKE_FLOOR_LINE, 0,"                 "
