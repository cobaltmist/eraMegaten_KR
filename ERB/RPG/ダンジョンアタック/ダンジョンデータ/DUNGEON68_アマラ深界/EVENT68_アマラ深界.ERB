		SIF GET_STATE(CFLAG:(FLAG:LOCALS):ステート) == "DYING"
			CONTINUE
		FOR LOCAL:1,0,6
			IF CFLAG:(FLAG:LOCALS):(GET_BATTLESTATUS(LOCAL:1) + "강화") > 0 && RAND:3 == 1
				;デカジャ
				CFLAG:ARG:입력행동 = [[스킬:데・카쟈]]
				CALL RANDOM_TARGET,ARG,CFLAG:ARG:입력행동
				RETURN 0
			ENDIF
		NEXT
	NEXT
ENDIF

LOCAL = RAND:15
	;통상공격
	IF LOCAL <= 2
		CFLAG:ARG:입력행동 = 0
	;태초에숨은악
	ELSEIF 3 <= LOCAL && LOCAL <= 4
		CFLAG:ARG:입력행동 = [[스킬:태초에숨은악]]
	;왕중의왕
	ELSEIF 5 <= LOCAL && LOCAL <= 6
		CFLAG:ARG:입력행동 = [[스킬:왕중의왕]]
	;マハ라기다인
	ELSEIF 7 <= LOCAL && LOCAL <= 8
		CFLAG:ARG:입력행동 = [[스킬:마하・라기다인]]
	;マハ부흐다인
	ELSEIF 9 <= LOCAL && LOCAL <= 10
		CFLAG:ARG:입력행동 = [[스킬:마하・부흐다인]]
	;메기도라온
	ELSEIF 11 <= LOCAL && LOCAL <= 12
		CFLAG:ARG:입력행동 = [[스킬:메기도라온]]
	ELSE
		;M様ルートなら메기도라온
		IF 던전フラグ:68:51 == 2
			CFLAG:ARG:입력행동 = [[스킬:메기도라온]]
		;통상공격、HP50%以下なら디아라마、MANIACS이상だと디아라한
		;HARDはまだぎりぎり통상레벨だし・・・
		ELSEIF RAND:4 < 1 && FLAG:전투난이도 > 4
			CFLAG:ARG:입력행동 = [[스킬:디아라한]]
		ELSEIF RAND:2 < 1 && FLAG:전투난이도 <= 4
			CFLAG:ARG:입력행동 = [[스킬:디아라마]]
		ELSE
			CFLAG:ARG:입력행동 = 0
		ENDIF
	ENDIF

CALL CHECK_ACTIONABLE,ARG,CFLAG:ARG:입력행동
IF RESULT == 1
	CALL RANDOM_TARGET,ARG,CFLAG:ARG:입력행동
ELSE
	;なんか엘라ーあれば通常공격に
	CFLAG:ARG:입력행동 = 0
	CALL RANDOM_TARGET,ARG,CFLAG:ARG:입력행동
ENDIF
RETURN 0





