;=======================================================================
;던전突入時の이벤트
;=======================================================================
@START_DUNGEON_53, ARG
IF !STRFLAG_D("初回", 1)
	PRINTW ＞총성이 들린다
	PRINTL ＞정말로 라스트 바탈리온의 병사가 와서、
	PRINTW ＞누군가와 교전하고 있는 것 같다
	PRINTW ＞카라콜의 입구를 찾자
ENDIF
FLAG:現M = 0
FLAG:現X = 2
FLAG:現Y = 10
;=======================================================================
;[5]조사　実行時の이벤트
;=======================================================================
@EVENT_CHECK_DUNGEON_53
SELECTCASE DA:(FLAG:現X):(FLAG:現Y)% 10
	;========
	;宝の処理
	;========
	CASE 3
		CALL TREASURE_53
		RETURN 1
	;========================
	;이벤트
	;========================
	CASE 4
		CALL EVENT_53
		RETURN 1
	;=======
	;ワープゾーン
	;=======
	CASE 6,7
		CALL WORP_53
		RETURN 1
	;=======
	;メッセンジャー
	;=======
	CASE 8
		CALL MESSAGE_53, 0
		RETURN 1
	;=======
	;特殊タイル
	;=======
	CASE 9
		CALL SPECIAL_53
		RETURN 1
	;=======
	;ダークゾーン
	;=======
	CASE -1
		IF DA:(FLAG:現X):(FLAG:現Y) < -10
			CALL DARK_53
			RETURN 1
		ENDIF
	CASEELSE
	PRINTW 아무 것도 없는 것같다
	RETURN 0
ENDSELECT
;=======================================================================
;タイル進入時の이벤트
;=======================================================================
@EVENT_ENTER_DUNGEON_53,ARG
;ARG = 進入時の向き　0の場合ワープなど
SELECTCASE DA:(FLAG:現X):(FLAG:現Y) % 10
	;==========
	;扉の処理
	;==========
	CASE 2
		CALL DUNGEON_DOOR,ARG
		RETURN 1
;========
;宝の処理
;========
	CASE 3
		CALL TREASURE_53
		RETURN 1
;========================
;이벤트
;========================
	CASE 4
		CALL EVENT_53, 1
		RETURN 0
;=======
;ワープゾーン
;=======
	CASE 6,7
		CALL WORP_53
		RETURN 1
;=======
;メッセンジャー
;=======
	CASE 8
		CALL MESSAGE_53, 1
		RETURN 1
;=======
;特殊タイル
;=======
	CASE 9
		CALL SPECIAL_53
		RETURN 1
;=======
;ダークゾーン
;=======
	CASE -1
		CALL DARK_53, 1
		RETURN 1
ENDSELECT
;=======
;主に扉
;=======
;扉のタイル표시
@TILE_MARK_COMMON_53_2, ARG, ARG:1, ARG:2
SELECTCASE ARG / 10
	CASEELSE
		RESULTS = 문
ENDSELECT
@CAN_ENTER_53, ARG, ARG:1
SELECTCASE DA:ARG:(ARG:1)
	CASE 9012
		IF !STRFLAG_D("조커撃破")
			PRINTW ＞안에서 시끄러운 소리가 난다
			PRINTW ＞방 안으로 들어가겠습니까?
			CALL INPUT_YN
			SIF RESULT == 1
				RETURN 0
		ENDIF
	CASE 12012
		IF !STRFLAG_D("M12ドアオープン")
			IF ARG:1 > FLAG:現Y
				PRINTW ＞문은 열리지 않는다.
				PRINTW ＞반대 쪽에서 자물쇠로 잠겨 있는 것 같다.
				RETURN 0
			ELSE
				PRINTW ＞문의 자물쇠를 풀었다.
				CALLF STRFLAG_D("M12ドアオープン", 1)
			ENDIF
		ENDIF
	CASE 12022
		IF !STRFLAG_D("성창騎士団撃破")
			PRINTW ＞안에 누군가 있는 것 같다
			PRINTW ＞방 안으로 들어가겠습니까?
			CALL INPUT_YN
			SIF RESULT == 1
				RETURN 0
		ENDIF
	CASE 15012
		IF !STRFLAG_D("M15ドアオープン")
			IF ARG:1 > FLAG:現Y
				PRINTW ＞문은 열리지 않는다.
				PRINTW ＞반대 쪽에서 자물쇠로 잠겨 있는 것 같다.
				RETURN 0
			ELSE
				PRINTW ＞문의 자물쇠를 풀었다.
				CALLF STRFLAG_D("M15ドアオープン", 1)
			ENDIF
		ENDIF
	CASE 18012
		IF !STRFLAG_D("카라코르クリア")
			PRINTW ＞여기가 최심부인 것 같다
			PRINTW ＞방 안으로 들어가겠습니까?
			CALL INPUT_YN
			SIF RESULT == 1
				RETURN 0
		ENDIF
ENDSELECT
CALL CAN_ENTER,ARG,ARG:1
RETURN RESULT

@TILE_MARK_COMMON_53_9, ARG, ARG:1, ARG:2
SELECTCASE ARG / 10
	CASE 98
		RESULTS = Ｃ
	CASE 99
		RESULTS = 출
ENDSELECT

@SPECIAL_53
SELECTCASE DA:(FLAG:現X):(FLAG:現Y) / 10
	;ケーブルカー
	CASE 98
		PRINTW ＞케이블카가 있다
		IF FLAG:現M == 8 && !STRFLAG_D("ケーブルカー起動", 1)
			PRINTW ＞주위를 탐색하니 전원이 있어 기동시켰다
			PRINTW ＞이것으로 이 다음부터는 케이블카를 사용할 수 있다
		ENDIF
		IF !STRFLAG_D("ケーブルカー起動")
			PRINTW ＞그러나 전원이 공급되지 않아 움직이지 않는다
		ELSE
			PRINTL ＞케이블카를 타겠습니까?
			CALL INPUT_YN
			SIF RESULT == 0
				CALL DUNGEON_WORP_ID, ABS(FLAG:現M - 8), 989, 1
		ENDIF
	;出입
	CASE 99
		CALL DUNGEON_EXIT
ENDSELECT
