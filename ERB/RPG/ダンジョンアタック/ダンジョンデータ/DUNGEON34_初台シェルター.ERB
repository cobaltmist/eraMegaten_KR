;=======================================================================
;인카운터율
;=======================================================================
@ENCOUNT_RATE_34
IF FLAG:現M < 8
	FLAG:인카운터율 = FLAG:未遭遇歩数 > 5 ? FLAG:未遭遇歩数*5 # 0
ELSEIF FLAG:現M == 9
	FLAG:인카운터율 = FLAG:未遭遇歩数 > 5 ? FLAG:未遭遇歩数*5 + 10 # 0
ELSE
	FLAG:인카운터율 = FLAG:未遭遇歩数 > 5 ? FLAG:未遭遇歩数*5 + 20 # 0
ENDIF
;=======================================================================
;エンカウント処理
;=======================================================================
@CHECK_ENCOUNT_34
IF RAND:100 < FLAG:인카운터율
	IF 던전フラグ:34:32
		FLAG:선제기습취소 = 1
		던전フラグ:34:18 = 0
	ENDIF
	CALL ENEMY_TABLE
	RETURN 1
ELSE
	던전フラグ:34:18 = 0
	FLAG:未遭遇歩数 += 1
	RETURN 0
ENDIF
;=======================================================================
;적の종족の종류数
