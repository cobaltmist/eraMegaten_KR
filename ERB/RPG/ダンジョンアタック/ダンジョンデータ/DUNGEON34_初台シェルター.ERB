@CLASS_NUM_34
;チュートリアル
IF 던전フラグ:34:30
	SELECTCASE RAND:100
		CASE IS < 50
			RETURN 1
		CASEELSE
			RETURN 2
	ENDSELECT
ELSE
	SELECTCASE RAND:100
		CASE IS < 40
			RETURN 1
		CASE IS < 95
			RETURN 2
		CASEELSE
			RETURN 3
	ENDSELECT
ENDIF
;=======================================================================
;적の総数
;=======================================================================
@ENEMY_NUM_34, ARG
;チュートリアル
IF 던전フラグ:34:30
	SELECTCASE ARG
		CASE 1
			RETURN RAND(1, 4)
		CASE 2
			RETURN RAND(2, 5)
	ENDSELECT
;통상
ELSE
	SELECTCASE ARG
		CASE 1
			RETURN RAND(1, 3)
		CASE 2
			RETURN RAND(2, 5)
		CASE 3
			RETURN RAND(3, 6)
	ENDSELECT
ENDIF
