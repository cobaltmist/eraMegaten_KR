;CFLAG:ARG:１more플래그 > 0 || CFLAG:ARG:집중플래그 == 0
;CFLAG:ARG:행동한횟수 == 0
;CHECK_WEAKNESS(ARG)
;(CHECK_WEAKNESS(MEMORIZE_WEAKNESS(ARG, LOCAL:10, "〇〇")) < -1)
@SET_ACTION_미츠루,ARG
#LOCALSIZE 2
; 一人でもPANICがいたら即퍼니시먼트
FOR LOCAL,1,7
	IF POS(LOCAL) > -1 && CFLAG:(POS(LOCAL)):상태이상 == GET_STATE_NUM("PANIC")
		CALL CHECK_ACTIONABLE,ARG,[[스킬:퍼니시먼트]]
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = [[스킬:퍼니시먼트]]
		CALL RANDOM_TARGET,ARG,[[스킬:퍼니시먼트]]
		CFLAG:ARG:행동한횟수 -= 1
		RETURN 1
	ENDIF
NEXT
;一回目の行動
IF CFLAG:ARG:행동한횟수 == 0 || CFLAG:ARG:１more플래그 >= 1
	IF CFLAG:ARG:집중플래그 == 0 && RAND:2
		CALL CHECK_ACTIONABLE,ARG,[[스킬:컨센트레이트]]
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = [[스킬:컨센트레이트]]
		CALL RANDOM_TARGET,ARG,[[스킬:컨센트레이트]]
		RETURN 1
	ELSEIF 현HP비율(ARG) < 30 && RAND:4 == 0
		CALL CHECK_ACTIONABLE,ARG,[[스킬:디아라한]]
		SIF RESULT == 0
			GOTO DECIDE_ACTION
		CFLAG:ARG:입력행동 = [[스킬:디아라한]]
		CALL RANDOM_TARGET,ARG,[[스킬:디아라한]]
		RETURN 1
	ELSE
		FOR LOCAL,1,7
			SIF POS(LOCAL) == -1
				CONTINUE
			;SIF CFLAG:(POS(LOCAL)):상태이상 >= GET_STATE_NUM("STONE")
			;	CONTINUE
			SIF CFLAG:(POS(LOCAL)):상태이상 >= GET_STATE_NUM("DYING")
				CONTINUE
			;빙결無効-2/반사-4/흡수-3がいたらそいつ1체に
			IF CHECK_WEAKNESS(MAXBASE:(POS(LOCAL)):빙결) <= -2 && CFLAG:POS(LOCAL):빙결가드킬 == 0
				CALL CHECK_ACTIONABLE,ARG,[[스킬:빙결가드킬]]
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력행동 = [[스킬:빙결가드킬]]
				CFLAG:ARG:타겟 = POS(LOCAL)
				RETURN 1
			;いなかったら전체に
			ELSE
				CALL CHECK_ACTIONABLE,ARG,[[스킬:빙결가드브레이크]]
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력행동 = [[스킬:빙결가드브레이크]]
				CALL RANDOM_TARGET,ARG,[[스킬:빙결가드브레이크]]
				RETURN 1
			ENDIF
		NEXT
	ENDIF
;１more
ELSEIF CFLAG:ARG:집중플래그
	FOR LOCAL,1,7
		SIF POS(LOCAL) == -1
			CONTINUE
		;SIF CFLAG:(POS(LOCAL)):상태이상 >= GET_STATE_NUM("STONE")
		;	CONTINUE
		SIF CFLAG:(POS(LOCAL)):상태이상 >= GET_STATE_NUM("DYING")
			CONTINUE
		;生きてる정신弱点1がいたら텐타라후
		IF CHECK_WEAKNESS(MAXBASE:(POS(LOCAL)):정신) >= 1
			CALL CHECK_ACTIONABLE,ARG,[[스킬:텐타라후]]
			SIF RESULT == 0
				GOTO DECIDE_ACTION
			CFLAG:ARG:입력행동 = [[스킬:텐타라후]]
			CALL RANDOM_TARGET,ARG,[[스킬:텐타라후]]
			RETURN 1
		ENDIF
	NEXT
	;いなかったら절대영도
	CALL CHECK_ACTIONABLE,ARG,[[스킬:절대영도]]
	SIF RESULT == 0
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력행동 = [[스킬:절대영도]]
	CALL RANDOM_TARGET,ARG,[[스킬:절대영도]]
	RETURN 1
;2/3で빙결공격
ELSEIF RAND:3
	FOR LOCAL,1,7
		SIF POS(LOCAL) == -1
			CONTINUE
		;SIF CFLAG:(POS(LOCAL)):상태이상 >= GET_STATE_NUM("STONE")
		;	CONTINUE
		SIF CFLAG:(POS(LOCAL)):상태이상 >= GET_STATE_NUM("DYING")
			CONTINUE
		;빙결弱点がいたらそいつに
		IF CHECK_WEAKNESS(MAXBASE:(POS(LOCAL)):빙결) >= 1 && CFLAG:POS(LOCAL):빙결가드킬
			IF RAND:2
				LOCAL:1 = [[스킬:마하・부흐다인]]
			ELSE
				LOCAL:1 = [[스킬:절대영도]]
			ENDIF
			CALL CHECK_ACTIONABLE,ARG,[[스킬:마하・부흐다인]]
			SIF RESULT == 0
				GOTO DECIDE_ACTION
			CFLAG:ARG:입력행동 = [[스킬:마하・부흐다인]]
			CFLAG:ARG:타겟 = LOCAL < 4 ? 17 # 18
			RETURN 1
		ENDIF
	NEXT
;それ以外は텐타라후
ELSE
	CALL CHECK_ACTIONABLE,ARG,[[스킬:텐타라후]]
	SIF RESULT == 0
		GOTO DECIDE_ACTION
	CFLAG:ARG:입력행동 = [[스킬:텐타라후]]
	CALL RANDOM_TARGET,ARG,[[스킬:텐타라후]]
	RETURN 1
ENDIF
