;=======================================================================
;F.O.E.『刈り取るもの』関連
;=======================================================================
;『刈り取るもの』出現チェック
@DUNGEON_70_FOE_REAPER_CHECK
;-------------
;変数定義
;-------------
#DIM POINT_X			;X座標
#DIM POINT_Y			;Y座標

;-------------
;変数の初期化
;-------------
POINT_X = 0				;X座標
POINT_Y = 0				;Y座標

;-------------
;ここから本処理
;-------------
;現フロアの滞在時間に応じて処理を行う
IF CD_TIME_AND_TURN:3 < (1000 * 60 * 10)
	;10分未満：何もなし
	던전플래그:70:(67) = 0	;警告フラグを「0:警告未発生」にする

ELSEIF (1000 * 60 * 10) <= CD_TIME_AND_TURN:3 && CD_TIME_AND_TURN:3 < (1000 * 60 * 13) && 던전플래그:70:(67) == 0
	;10分経過：警告(1回目)
	CALL MESSAGE_WINDOW, "" , "＞どこかで鎖が鳴る音がした"
	던전플래그:70:(67) = 1	;警告フラグを「1:1回目の警告済」にする

ELSEIF (1000 * 60 * 13) <= CD_TIME_AND_TURN:3 && CD_TIME_AND_TURN:3 < (1000 * 60 * 15) && 던전플래그:70:(67) <= 1
	;13分経過：警告(2回目)
	CALL MESSAGE_WINDOW, "" , "＞少しずつ　不吉な気配が強くなっていく"
	CALL MESSAGE_WINDOW, "" , "＞この階層に長居しない方がいいかもしれない"
	던전플래그:70:(67) = 2	;警告フラグを「2:2回目の警告済」にする

ELSEIF (1000 * 60 * 15) <= CD_TIME_AND_TURN:3 && 던전플래그:70:(67) <= 2
	;15分経過：F.O.E.『刈り取るもの』出現！
	SETBGCOLOR 0xff0000				;バックグラウンドカラーを赤色にする
	SETCOLOR 0x000000				;黒文字にする
	CALL MESSAGE_WINDOW, "" , "＞どこかに死神タイプが出現した！　あなたを追いかけてくる！"
	RESETBGCOLOR					;バックグラウンドカラーを元に戻す
	RESETCOLOR						;文字色を元に戻す

	;F.O.Eを配置する
	CALL CD_GET_POINT, POINT_X, POINT_Y, 0, 1, -1								;座標を取得する
	CALL SEF_ADD_SYMBOL, 2, 3201, 2, POINT_X, POINT_Y, 99, 2, 1, 2, 70, 1, 1, 1	;シンボルエンカウントを設定する

	던전플래그:70:(67) = 3	;警告フラグを「3:F.O.E.出現済」にする

ENDIF
RETURN 1

