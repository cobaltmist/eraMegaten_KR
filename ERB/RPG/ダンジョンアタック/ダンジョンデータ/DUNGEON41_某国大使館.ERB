		CALL SET_TILE,1,1,던전フラグ:41:1,1,-9

	CASEELSE
ENDSELECT


;=======================================================================
;[5]調べる　実行時の이벤트
;=======================================================================
@EVENT_CHECK_DUNGEON_41

;==========
;出입の処理
;==========
IF DA:(FLAG:현X):(FLAG:현Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 3
	SELECTCASE FLAG:현M
		;地下一階
		CASE 0
		;一階
		CASE 1
			;X8 && Y5座標の宝箱
			IF FLAG:현X == 8 && FLAG:현Y == 5
				;ボスを倒していない
				IF 던전フラグ:41:1 == 0
					;1005,메디컬파우더,15,아군全員のHPを50회복する薬品
					CALL GET_TREASURE,1005,1,FLAG:현던전,10
				;ボスを倒している
				ELSE
					;79,황색사탕,20000,;임신中に使うと産卵し、임신を失う。임신キャンセルが可能だが、ハイリスク,
					CALL GET_TREASURE,79,1,FLAG:현던전,12
				ENDIF
			;X1 && Y1座標の宝箱
			ELSEIF FLAG:현X == 1 && FLAG:현Y == 1
				;ボスを倒していない
				IF 던전フラグ:41:1 == 0
					;1001,외상약,50,;HPを30회복する薬品
					CALL GET_TREASURE,1001,2,FLAG:현던전,11
				;ボスを倒している
				ELSE
					;1008,도반옥,3000,리캄
					CALL GET_TREASURE,1008,2,FLAG:현던전,13
				ENDIF
			ENDIF
		;二階
		CASE 2
	ENDSELECT
	RETURN 1

;=======
;階段
;=======
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 6
	SELECTCASE FLAG:현M
		;地下一階の6を調べた
		CASE 0
			;地下一階　6の座標を指定　X3/Y3
			SIF FLAG:현X == 3 && FLAG:현Y == 3
				;一階　7の座標に移動　X10/Y7
				CALL DUNGEON_UPSTAIRS,1,10,7
		;一階の6を調べた
		CASE 1
			;一階　6の座標を指定
			SIF FLAG:현X == 8 && FLAG:현Y == 7
				;ニ階　7の座標に移動
				CALL DUNGEON_UPSTAIRS,2,1,7
	ENDSELECT
	RETURN 1
	
ELSEIF DA:(FLAG:현X):(FLAG:현Y) == 7
	SELECTCASE FLAG:현M
		;上記昇り階段の数値を入れ替える
		;一階の7を調べた
		CASE 1
			;一階　MAP7の座標を指定
			SIF FLAG:현X == 10 && FLAG:현Y == 7
				;地下一階　6の座標に移動
				CALL DUNGEON_DOWNSTAIRS,0,3,3
		;二階の7を調べた
		CASE 2
			;ニ階　MAP7の座標を指定
			SIF FLAG:현X == 1 && FLAG:현Y == 7
				;一階　MAP6の座標に移動
				CALL DUNGEON_DOWNSTAIRS,1,8,7
	ENDSELECT
	RETURN 1

ELSE
	PRINTW 아무것도 없었다
	RETURN 0
ENDIF


