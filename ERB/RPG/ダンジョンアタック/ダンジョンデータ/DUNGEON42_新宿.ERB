				CASE IS < 100
					CALL CHECK_ACTIONABLE,ARG,0
					SIF RESULT == 0
						GOTO DECIDE_ACTION
					CFLAG:ARG:입력행동 = 0
					CALL RANDOM_TARGET,ARG,0
					RETURN 1
			ENDSELECT
		ENDIF

	ELSEIF CFLAG:ARG:행동한횟수 == 1
		SELECTCASE RAND:100
			;20％で아기
			CASE IS < 20
				CALL CHECK_ACTIONABLE,ARG,701
				SIF RESULT == 0
					GOTO DECIDE_ACTION
				CFLAG:ARG:입력행동 = 701
				CALL RANDOM_TARGET,ARG,701
