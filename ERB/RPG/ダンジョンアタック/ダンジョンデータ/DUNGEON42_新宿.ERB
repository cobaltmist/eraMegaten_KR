			ELSEIF FLAG:현X == 13 && FLAG:현Y == 18
				CALL DUNGEON_TERMINAL
				RETURN 1
			ENDIF
		CASE 1
			IF FLAG:현X == 1 && FLAG:현Y == 0
				CALL DUNGEON_WORP,0,1,18
			ELSEIF FLAG:현X == 7 && FLAG:현Y == 0
				CALL DUNGEON_WORP,0,7,18
			ELSEIF FLAG:현X == 11 && FLAG:현Y == 0
				CALL DUNGEON_WORP,0,11,18
			ELSEIF FLAG:현X == 18 && FLAG:현Y == 0
