	IF CFLAG:ARG:행동한횟수 == 0
		IF 던전플래그:42:30 == 0
			;初回セクシー
			CALL CHECK_ACTIONABLE,ARG,1212
			SIF RESULT == 0
				GOTO DECIDE_ACTION
			CFLAG:ARG:입력행동 = 1212
