;=======================================================================
@DUNGEON_RAPE_DEVIL_82
;랜덤にどの악마が出るか決定
;RETURN CSV번호 , 人数 , 레벨
SELECTCASE RAND:3
	CASE 0 , 1 
		RETURN [[キャラ:아귀]] , 3 + RAND:10 , CSVBASE([[キャラ:아귀]] , GETNUM(BASE , "LV") , 0) + RAND:3
	CASE 2 ,3 ,4 ,5 ,
		RETURN [[キャラ:잭랜턴]] , 3 + RAND:10 , CSVBASE([[キャラ:잭랜턴]] , GETNUM(BASE , "LV") , 0) + RAND:3
ENDSELECT
