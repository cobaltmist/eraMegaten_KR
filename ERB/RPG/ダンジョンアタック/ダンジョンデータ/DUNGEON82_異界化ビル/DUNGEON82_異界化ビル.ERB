;=======================================================================
;攻略조건
;=======================================================================
@PREREQUISITE_DUNGEON_82
;의뢰플래그:29:0 = 1
;의뢰を受けているか종료済み
IF 의뢰플래그:29:0 == 1 || 의뢰플래그:29:0 == 2
	RETURN 1
ENDIF
RETURN 0

;=======================================================================
;던전名
;=======================================================================
@GET_DUNGEON_NAME_82
RESULTS = 이계화빌딩

;=======================================================================
;플로어名표시
;=======================================================================
@FLOORNAME_82
PRINT 이계화빌딩
SELECTCASE FLAG:현M
	CASE 0
		PRINTL １Ｆ
	CASE 1
		PRINTL ２Ｆ
	CASE 2
		PRINTL ３Ｆ
	CASE 3
		PRINTL ４Ｆ
	CASE 4
		PRINTL ５Ｆ
	CASE 5
		PRINTL ６Ｆ
	CASEELSE
		PRINTL
ENDSELECT

;=======================================================================
;スタート地点입력
;=======================================================================
@START_DUNGEON_82, ARG
FLAG:현M = 0
FLAG:현X = 8
FLAG:현Y = 13
;CALL AUTOMAP
RETURN 0

;=======================================================================
;攻略開始時の이벤트
;=======================================================================
@EVENT_DUNGEON_START_82

;쇼우지の상태
IF 던전플래그:82:1 > -1 && 의뢰플래그:29:0 == 1
	PRINTFORML 　
	SELECTCASE DAY - STRFLAG_NUM_REQ("빌딩내부상황변화" , , , , 29)
		;要調整
		;３日以内は捕獲前（本当は７日としたいがさすがにありえないので）
		;０～３日눈
		CASE IS <= 3
			PRINTFORMW 이계화된 빌딩에 침입했다
			PRINTFORMW 이 빌딩 어딘가에 쇼우지가 있다、그녀를 찾는 것이 의뢰 완수로 이어질 것이다……
			
			;쇼우지の상태、捕獲前へ
			던전플래그:82:1 = 1
		;４日눈に捕獲されて레이프이벤트
		CASE IS <= 4
			PRINTFORMW 의뢰를 받고나서 상당히 시간이 지나고 말았다
			PRINTFORMW 쇼우지는 어디에 있는 걸까？
			
			;쇼우지の상태、捕獲へ
			던전플래그:82:1 = 10
			;１Ｆにボスいないフラグオンへ
			던전플래그:82:8 = 1
		;５日눈に윤간される（ボスいる）
		CASE IS <= 5
			;쇼우지を見たことある
			IF 던전플래그:82:3 == 1
				PRINTFORMW 이전에는 철수하고 말았지만、쇼우지는 어떻게 되어있을까？
				PRINTFORMW 어떻게 해서든지 의뢰 품목은 찾아내야 한다……
			ELSE
				PRINTFORMW 의뢰를 받고나서 상당히 시간이 지나고 말았다
				PRINTFORMW 쇼우지는 어디에 있는 걸까？
			ENDIF
			
			;쇼우지の상태、윤간中１へ
			던전플래그:82:1 = 20
			;１Ｆにボスいないフラグオンへ
			던전플래그:82:8 = 1
		;６日に윤간される（ボスいない）
		CASE IS <= 6
			;쇼우지を見たことある
			IF 던전플래그:82:3 == 1
				PRINTFORMW 전에는 철수하고 말았지만、쇼우지는 어떻게 되어있을까？
				PRINTFORMW 어떻게 해서든지 의뢰 품목은 찾아내야 한다……
			ELSE
				PRINTFORMW 의뢰를 받고나서 상당히 시간이 지나고 말았다
				PRINTFORMW 쇼우지는 어디에 있는 걸까？
			ENDIF
			
			;쇼우지の상태、윤간中２へ
			던전플래그:82:1 = 30
			;１Ｆにボスいないフラグオンへ
			던전플래그:82:8 = 1
		;７、８日눈は붕괴윤간
		CASE IS == 7
			;쇼우지を見たことある
			IF 던전플래그:82:3 == 1
				PRINTFORMW 전에는 철수하고 말았지만、쇼우지는 어떻게 되어있을까？
				PRINTFORMW 어떻게 해서든지 의뢰 품목은 찾아내야 한다……
			ELSE
				PRINTFORMW 의뢰를 받고나서 상당히 시간이 지나고 말았다
				PRINTFORMW 쇼우지는 어디에 있는 걸까？
			ENDIF
			
			;쇼우지の상태、붕괴マグロ化へ
			던전플래그:82:1 = 40
			;１Ｆにボスいないフラグオンへ
			던전플래그:82:8 = 1
		;７、８日눈は붕괴윤간
		CASE IS == 8
			;쇼우지を見たことある
			IF 던전플래그:82:3 == 1
				PRINTFORMW 전에는 철수하고 말았지만、쇼우지는 어떻게 되어있을까？
				PRINTFORMW 어떻게 해서든지 의뢰 품목은 찾아내야 한다……
			ELSE
				PRINTFORMW 의뢰를 받고나서 상당히 시간이 지나고 말았다
				PRINTFORMW 쇼우지는 어디에 있는 걸까？
			ENDIF
			
			;７日눈に見てなければ쇼우지の상태、붕괴マグロ化へ
			IF 던전플래그:82:1 != 41
				던전플래그:82:1 = 40
			ENDIF
			;１Ｆにボスいないフラグオンへ
			던전플래그:82:8 = 1
		;それ以後は변기化
		CASEELSE
			;쇼우지を見たことある
			IF 던전플래그:82:3 == 1
