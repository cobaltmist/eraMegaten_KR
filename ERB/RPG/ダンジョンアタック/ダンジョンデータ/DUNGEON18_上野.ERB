;=======================================================================
;[5]調べる　実行時の이벤트
;=======================================================================
@EVENT_CHECK_DUNGEON_18
;==========
;出口の処理
;==========
IF DA:(FLAG:현X):(FLAG:현Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 3
	CALL TREASURE_18
	RETURN RESULT
;==========
;이벤트の処理
;==========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 4
	CALL EVENT_18
	RETURN RESULT
;=======
;ワープゾーン
;=======
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 6 || DA:(FLAG:현X):(FLAG:현Y) % 10 == 7
	CALL WORP_16
	RETURN RESULT
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 9
	RETURN 0
ENDIF

;=======================================================================
;タイル進入時の이벤트
;=======================================================================
@EVENT_ENTER_DUNGEON_18,ARG
;ARG = 進入時の向き　0の場合ワープなど
;==========
;出口の処理
;==========
IF DA:(FLAG:현X):(FLAG:현Y) == 8
	CALL DUNGEON_EXIT
	RETURN 1
;========
;宝の処理
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 3
	CALL TREASURE_18
	RETURN RESULT
;=======
;階段
;=======
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 6 || DA:(FLAG:현X):(FLAG:현Y) % 10 == 7
	CALL WORP_18
	RETURN RESULT
;========
;이벤트の処理
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 4
	CALL EVENT_18
	RETURN RESULT
;========
;罠の処理
;========
ELSEIF DA:(FLAG:현X):(FLAG:현Y) % 10 == 5
	CALL TRAP_18
	RETURN RESULT
;==========
;扉の処理
;==========
ELSEIF DA:(FLAG:현X):(FLAG:현Y)  % 10 == 2
	CALL DUNGEON_DOOR,ARG
	RETURN 1
;==========
;特殊床
;==========
ELSEIF DA:(FLAG:현X):(FLAG:현Y)  % 10 == 1
	CALL FLOOR_18
	RETURN RESULT
ENDIF
RETURN 0

