;=======================================================================
;던전名
;=======================================================================
@GET_DUNGEON_NAME_55
RESULTS = 시바르바

;=======================================================================
;指定したマスに進入가능か
;=======================================================================
@CAN_ENTER_55, ARG, ARG:1
#LOCALSIZE 2
SELECTCASE DA:ARG:(ARG:1)
	CASE 4002
		IF !STRFLAG_D("扉開放")
			IF FLAG:現Y == ARG:1 - 1
				CALL MESSAGE_WINDOW_D, "", "＞문이 열리지 않는다！"
				RETURN 0
			ELSE
				CALL MESSAGE_WINDOW_D, "", "＞문의 자물쇠를 해제했다"
				CALLF STRFLAG_D("扉開放", 1)
				RETURN 1
			ENDIF
		ELSE
			RETURN 1
		ENDIF
	CASE 4012
		IF !STRFLAG_D("롱기누스5撃破")
			CALL MESSAGE_WINDOW_D, "", "＞문 너머에서 모터 구동음이 들린다/＞문을 열고 진행하시겠습니까？"
			CALL INPUT_YN_D
			IF RESULT == 1
				RETURN 0
			ELSE
				FOR LOCAL:1, 3, 13
					FOR LOCAL, 0, 7
						CALL SET_FLOOR_ANALYZE(2, FLAG:現던전, 4, LOCAL, LOCAL:1)
					NEXT
				NEXT
				CALL MESSAGE_WINDOW_D, "", "＞문을 열고 방에 들어선다"
				RETURN 1
			ENDIF
		ELSE
			RETURN 1
		ENDIF
	CASE 7002
		IF !STRFLAG_D("롱기누스1撃破")
			CALL MESSAGE_WINDOW_D, "", "＞문 너머에서 모터 구동음이 들린다/＞문을 열고 진행하시겠습니까？"
			CALL INPUT_YN_D
			IF RESULT == 0
				FOR LOCAL:1, 9, 16
					FOR LOCAL, 3, 8
						CALL SET_FLOOR_ANALYZE(2, FLAG:現던전, 7, LOCAL, LOCAL:1)
					NEXT
				NEXT
				CALL MESSAGE_WINDOW_D, "", "＞문을 열고 방에 들어선다"
				RETURN 1
			ELSE
				RETURN 0
			ENDIF
		ELSE
			RETURN 1
		ENDIF
	CASE 8002
		CALL MESSAGE_WINDOW_D, "", "＞문 너머에서 괴상한 기색이…/＞문을 열고 진행하시겠습니까？"
		CALL INPUT_YN_D
		IF RESULT == 0
			CALL DUNGEON_WORP_ID, 9, 9014
			RETURN 0
		ELSE
			RETURN 0
		ENDIF
	CASE 8012
		IF !STRFLAG_D("포론틱撃破")
			CALL MESSAGE_WINDOW_D, "", "＞문에는 수수께끼의 문자가 새겨져 있다…/＞어쩐지 이상한 기색이 있는 것 같지만…/＞문을 열고 진행하시겠습니까？"
			CALL INPUT_YN_D
			IF RESULT == 0
				FOR LOCAL:1, 8, 21
					FOR LOCAL, 18, 27
						CALL SET_FLOOR_ANALYZE(2, FLAG:現던전, 8, LOCAL, LOCAL:1)
					NEXT
				NEXT
				CALL MESSAGE_WINDOW_D, "", "＞문을 열고 방에 들어선다"
				RETURN 1
			ELSE
				RETURN 0
			ENDIF
		ELSE
			RETURN 1
		ENDIF
	CASEELSE
		CALL CAN_ENTER, ARG, ARG:1
		RETURN RESULT
ENDSELECT

