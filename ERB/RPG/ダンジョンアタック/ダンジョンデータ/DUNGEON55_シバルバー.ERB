;----------------------------------------------
@SET_ACTION_퓌러, ARG
#LOCALSIZE 3
;입력행동を-1と타겟を-1にしておく
CFLAG:ARG:입력행동 = -1
CFLAG:ARG:타겟 = -1
SIF 現HP割合(ARG) <= 25
	CFLAG:ARG:행동횟수 = 3
IF !TFLAG:적오메가클러스터
	CFLAG:ARG:입력행동 = [[스킬:오메가클러스터]]
ELSEIF CFLAG:ARG:１more플래그 > 0
	CFLAG:ARG:입력행동 = [[스킬:성궤]]
ELSEIF CFLAG:ARG:행동한횟수 == 0
	;성배の쿨制御
	TFLAG:435++
	;波紋체크
	LOCAL:1 = 0
	;ンダは-8まで容認
	SIF MINARRAY(CFLAG:ARG:0, GETNUM(CFLAG, "공격강화"), GETNUM(CFLAG, "BS부착률강화") + 1) < -8
		LOCAL:1 = 1
	;아군のうち一人から판정する
	LOCAL = POS(RAND(1, 7))
	IF LOCAL >= 0
		;カジャは8まで容認
		SIF MAXARRAY(CFLAG:LOCAL:0, GETNUM(CFLAG, "공격강화"), GETNUM(CFLAG, "BS부착률강화") + 1) > 8
			LOCAL:1 = 1
		;반사も波紋대상
		SIF CFLAG:LOCAL:물리반사플래그 || CFLAG:LOCAL:마법반사플래그
			LOCAL:1 = 2
	ENDIF
	;波紋フラグが立っている時
	IF (LOCAL:1 == 1 && RAND:2) || (LOCAL:1 == 2 && !RAND:3)
		CFLAG:ARG:입력행동 = [[스킬:포말의파문]]
		TFLAG:435 += 3
	;1/3で반사は성궤送り
	ELSEIF LOCAL:1 == 2 && RAND:2
		CFLAG:ARG:입력행동 = [[스킬:성궤]]
	ELSE
		SELECTCASE RAND:100
			CASE IS < 50
				CFLAG:ARG:입력행동 = [[스킬:성창]]
			CASE IS < 75
				CFLAG:ARG:입력행동 = [[스킬:ＭＧ３４]]
			CASEELSE
				CFLAG:ARG:입력행동 = [[스킬:성궤]]
		ENDSELECT
	ENDIF
ELSEIF CFLAG:ARG:행동한횟수 == 1
	SELECTCASE RAND:100
		CASE IS < 50
			;성배は쿨期間がある
			IF TFLAG:435 < 3
				CFLAG:ARG:입력행동 = [[스킬:성창]]
			ELSE
				CFLAG:ARG:입력행동 = [[스킬:성배]]
				TFLAG:435 = 0
			ENDIF
		CASE IS < 75
			CFLAG:ARG:입력행동 = [[스킬:ＭＧ３４]]
		CASEELSE
			CFLAG:ARG:입력행동 = [[스킬:브릴블리츠]]
	ENDSELECT
ELSE
	SELECTCASE RAND:100
		CASE IS < 25
			CFLAG:ARG:입력행동 = [[스킬:성창]]
		CASE IS < 50
			CFLAG:ARG:입력행동 = [[스킬:ＭＧ３４]]
		CASE IS < 75
			CFLAG:ARG:입력행동 = [[스킬:성궤]]
		CASEELSE
			CFLAG:ARG:입력행동 = [[스킬:브릴블리츠]]
	ENDSELECT
ENDIF

;アクション実行不能
CALL CHECK_ACTIONABLE, ARG, CFLAG:ARG:입력행동
SIF RESULT == 0
	CFLAG:ARG:입력행동 = 0
;ここまで타겟が決まってなかったら、랜덤타겟をよんで決定
SIF CFLAG:ARG:타겟 == -1
	CALL RANDOM_TARGET, ARG, CFLAG:ARG:입력행동
