	CASE 201
		LOCALS = 선행의돌
		LOCAL = 1
	CASE 202
		LOCALS = 디스톤
		LOCAL = 2
	CASE 100
		LOCALS = 무명의도
		LOCAL = 1
	CASE 101
		LOCALS = 마법の箱
		LOCALS:1 = 진주
		LOCALS:2 = 마석
		LOCAL = 8
		LOCAL:1 = 1
	CASE 102
		LOCALS = 메탈카드
		LOCAL = 1
	CASE 103
		LOCALS = 마법の箱
		LOCALS:1 = 루비
		LOCALS:2 = 마석
		LOCAL = 8
		LOCAL:1 = 1
	CASEELSE
ENDSELECT
IF LOCALS == "￥"
	CALL GET_TREASURE_MONEY, 0, LOCAL, FLAG:現던전, FLAG:現M + 70, (DA:(FLAG:現X):(FLAG:現Y) / 10) % 100
ELSEIF LOCALS == "마카"
	CALL GET_TREASURE_MONEY, 1, LOCAL, FLAG:現던전, FLAG:現M + 70, (DA:(FLAG:現X):(FLAG:現Y) / 10) % 100
ELSEIF LOCALS == "MAG" || LOCALS == "ＭＡＧ"
	CALL GET_TREASURE_MAG, 0, LOCAL, FLAG:現던전, FLAG:現M + 70, (DA:(FLAG:現X):(FLAG:現Y) / 10) % 100
ELSEIF LOCALS == "덫"
	;LOCALS:1 종류 LOCAL 데미지量or확률 LOCALS:2 相性
	CALL GET_TREASURE_TRAP, LOCALS:1, LOCAL, GET_TYPE_NUM(LOCALS:2), FLAG:現던전, FLAG:現M + 70, (DA:(FLAG:現X):(FLAG:現Y) / 10) % 100
ELSEIF LOCALS == "마법の箱"
	IF GETNUM(ITEM, LOCALS:1) == -1 || GETNUM(ITEM, LOCALS:2) == -1
		PRINTL ＞아이템 이름의 지정이 잘못되었습니다
		PRINTW ＞아이템 이름의 지정이 잘못되었습니다
	ELSE
		CALL MAGIC_BOX , LOCAL , GETNUM(ITEM, LOCALS:1) , GETNUM(ITEM, LOCALS:2) , LOCAL:1 , FLAG:現던전, FLAG:現M + 70, (DA:(FLAG:現X):(FLAG:現Y) / 10) % 100
	ENDIF
ELSE
	IF GETNUM(ITEM, LOCALS) == -1
		PRINTL ＞아이템 이름의 지정이 잘못되었습니다
		PRINTW ＞아이템 이름의 지정이 잘못되었습니다
	ELSE
		CALL GET_TREASURE, GETNUM(ITEM, LOCALS), LOCAL , FLAG:現던전, FLAG:現M + 70, (DA:(FLAG:現X):(FLAG:現Y) / 10) % 100
	ENDIF
ENDIF
RETURN 1

