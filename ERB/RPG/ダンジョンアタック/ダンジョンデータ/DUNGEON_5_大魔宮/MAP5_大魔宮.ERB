@MAKE_FLOOR_LINE_COMMON_5, ARG, ARG:1, ARG:2
#LOCALSIZE 30
;LOCAL:3 宝箱 던전플래그:75~99
;LOCAL:4 이벤트
;LOCAL:8 メッセンジャー
;(0/0)でLOCALを소거
SIF ARG:1 == 0 && ARG:2 == 0
	VARSET LOCAL
	
IF ARG == 1
	SIF RANGE(FLAG:현M, 60, 64)
		DA:(ARG:1):(ARG:2) = -1
	;→
	IF ARG:1 == (FLAG:최대X -1)
		DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:16 * 100 + 16
		LOCAL:16++
	;←
	ELSEIF ARG:1 == 0
		DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:17 * 100 + 17
		LOCAL:17++
	;↓
	ELSEIF ARG:2 == (FLAG:최대Y - 1)
		DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:26 * 100 + 26
		LOCAL:26++
	;↑
	ELSEIF ARG:2 == 0
		DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:27 * 100 + 27
		LOCAL:27++
	ENDIF
ENDIF
;이벤트ダークゾーン
IF ARG == -11
	DA:(ARG:1):(ARG:2) = -(FLAG:현M * 1000 + LOCAL:29 * 10 + 1)
	LOCAL:29++
	SELECTCASE -DA:(ARG:1):(ARG:2) / 10
		CASE 700
			;SIF STRFLAG_D("盾ゲット")
			SIF ITEM:갈의방패
				DA:(ARG:1):(ARG:2) = -1
		;ガイアは은릴리프획득で姿を消す
		CASE 2501
			SIF ITEM:은릴리프
				DA:(ARG:1):(ARG:2) = -1
		CASE 3801
			SIF STRFLAG_D("세트격파")
				DA:(ARG:1):(ARG:2) = -1
		CASE 6001
			SIF STRFLAG_D("Y코")
				DA:(ARG:1):(ARG:2) = -1
	ENDSELECT
ENDIF
;이벤트床
IF ARG == 11
	DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:1 * 10 + 1
	LOCAL:1++
ENDIF
;이벤트扉
IF ARG == 12
	DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:2 * 10 + 2
	LOCAL:2++
ENDIF
IF ARG == 3
	IF GETBIT(던전플래그:(FLAG:현던전):(FLAG:현M+25), LOCAL:3)
		DA:(ARG:1):(ARG:2) = 1
	ELSE
		DA:(ARG:1):(ARG:2) = FLAG:현M *1000 + LOCAL:3 * 10 + 3
	ENDIF
	LOCAL:3++
