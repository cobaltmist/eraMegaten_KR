;=======================================================================
;指定したマスに進入가능か
;=======================================================================
@CAN_ENTER_5,ARG,ARG:1
SELECTCASE DA:ARG:(ARG:1)
	;∧
	CASE 902, 912
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		SIF FLAG:현Y < ARG:1
			RETURN 0
		FLAG:현Y -= 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:현Y += 2
	;＞
	CASE 922, 932
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		SIF FLAG:현X > ARG
			RETURN 0
		FLAG:현X += 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:현X -= 2
	;∨
	CASE 942, 952
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		SIF FLAG:현Y > ARG:1
			RETURN 0
		FLAG:현Y += 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:현Y -= 2
	;＜
	CASE 962, 972
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		SIF FLAG:현X < ARG
			RETURN 0
		FLAG:현X -= 2
		CALL ONE_WAY(902, 912, 922, 932, 942, 952, 962, 972)
		FLAG:현X += 2
	CASE 992
		PRINTW ＞扉は開かない
		RETURN 0
	CASEELSE
		;2以外の扉はドア이벤트へ
		IF DA:ARG:(ARG:1) % 10 == 2 && DA:ARG:(ARG:1) > 2
			CALL DOOR_5, DA:ARG:(ARG:1) / 10
			RETURN RESULT
		ENDIF
ENDSELECT
CALL CAN_ENTER,ARG,ARG:1
RETURN RESULT

