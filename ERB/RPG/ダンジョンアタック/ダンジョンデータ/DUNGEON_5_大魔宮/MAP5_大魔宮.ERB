#LOCALSIZE 1
;地下1Fのワープゾーン
;処理簡略化の為に던전플래그:90~に座標をいれておく
SELECTCASE FLAG:현M
	CASE 12, 35, 36
		SIF ARGS == "w"
			RETURN 96
	CASE 14
		;ポイズンバー
		SIF ARGS == "b"
			RETURN 909
		;カジノ
		SIF ARGS == "c"
			RETURN 979
	;冥界
	CASE 60
		SELECTCASE ARGS
			CASE "B"
				LOCAL = -61
			CASE "b"
				LOCAL = -71
			CASE "C"
				LOCAL = -161
			CASE "c"
				LOCAL = -171
			CASE "J"
				LOCAL = -261
			CASE "j"
				LOCAL = -271
			CASE "K"
				LOCAL = -361
			CASE "k"
				LOCAL = -371
			CASE "L"
				LOCAL = -461
			CASE "l"
				LOCAL = -471
			CASE "N"
				LOCAL = -561
			CASE "n"
				LOCAL = -571
			CASE "O"
				LOCAL = -661
			CASE "o"
				LOCAL = -671
			CASEELSE
				LOCAL = 0
		ENDSELECT
		IF LOCAL % 100 == -71
			던전플래그:5:(99 + LOCAL / 100) = ARG + ARG:1 * 100
			RETURN LOCAL
		ENDIF
		SIF LOCAL % 100 == -61
			RETURN LOCAL
ENDSELECT
