ENDIF


;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@CAN_ENTER_81, ARG, ARG:1
IF 던전플래그:81:1 == 2
	SELECTCASE DA:ARG:(ARG:1)
		CASE 12
			;↑
			IF FLAG:현M == 7
				IF ARG:1 > FLAG:현Y
					CALL SET_FLOOR_ANALYZE(2, FLAG:현던전, FLAG:현M, ARG, ARG:1)
