	CASE 1
		CALL DUNGEON_EXIT
	CASE 200
		LOCAL = 0
		SELECTCASE 던전フラグ:(FLAG:현던전):1
			;初회화
			CASE 0
				던전フラグ:(FLAG:현던전):1 = 1
				CALL MESSAGE_WINDOW_D, "", "＞방 안에는 법의를 걸친 청년이 있었다"
