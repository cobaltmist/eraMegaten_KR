		RETURNF 1
	CASE 30;칠성
		RETURNF 0
	CASE 31;광인
		RETURNF 1
	CASE 32;괴이
		RETURNF 1
	CASE 33;ボス戦用
		RETURNF 0
	CASE 36;半魔
		RETURNF 0
	CASE 39;링크악마専用
		RETURNF 0
	CASE 40;페르소나전용
		RETURNF 0
	CASE 41;페르소나2
		RETURNF 0
	CASE 42;마인
		RETURNF 1
	CASE 43;초인
		RETURNF 0
	CASE 44;영웅
		RETURNF 1
	CASE 45;조마
		RETURNF 0
	CASE 73;악마인
		RETURNF 0
ENDSELECT