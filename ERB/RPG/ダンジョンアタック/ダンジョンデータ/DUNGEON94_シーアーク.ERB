	ENEMY:LOCAL = 出現악마:(LOCAL:2)
	ENEMYLV:LOCAL = CSVBASE(出現악마:(LOCAL:2),GETNUM(BASE, "LV"))
	LOCAL += 1
WEND

LOCAL = 0
