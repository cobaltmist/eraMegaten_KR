SELECTCASE ARG / 10
	CASE 2 , 4 , 5
	SETCOLOR 0x00ff00
	RESULTS = ｍ
	CASE 3
	IF 던전フラグ:61:1 == 1 && 던전フラグ:61:0 == 1
	SETCOLOR 0x00ff00
	RESULTS = ｍ
