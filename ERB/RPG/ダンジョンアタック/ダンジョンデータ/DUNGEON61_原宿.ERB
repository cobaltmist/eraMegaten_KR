;タイル定義
;=======================================================================
@DEFINE_TILES_61, ARGS, ARG, ARG:1
SELECTCASE ARGS
	CASE "!"
		RETURN 14
	CASE "'"
		RETURN 24
	CASE "#"
		RETURN 34
	CASE "$"
		RETURN 44
	CASE "%"
		RETURN 54
	CASE "C"
		RETURN 104
	CASE "&"
		RETURN 1004
	CASE "="
		RETURN 2004
	CASE "("
		RETURN 3004
	CASE ")"
		RETURN 4004
ENDSELECT

;=======================================================================
;指定したマスに進入가능か
;=======================================================================
@CAN_ENTER_61,ARG,ARG:1
IF FLAG:現M == 0 && ARG == 13 && ARG:1 == 14
	IF 던전フラグ:61:0 == 0 || 던전フラグ:61:1 == 2 
		RETURN 0
	ENDIF
;미라이
ELSEIF FLAG:現M == 4 && ARG == 10 && ARG:1 == 2
		CALL MESSAGE_WINDOW_D, "", "＞「카나메 미라이」라고 써져있다"
		IF 던전フラグ:61:1 == 2 
		CALL MESSAGE_WINDOW_D, "", "＞타겟의 집인 것 같다"
		CALL MESSAGE_WINDOW_D, "", "＞……아직 학교에 있는 것 같다"
		IF 던전フラグ:61:1 == 2 && 던전フラグ:61:0 == 0
		던전フラグ:61:0 = 1
		ENDIF
		ELSE
		CALL MESSAGE_WINDOW_D, "", "＞집을 비운 것 같다……"
		ENDIF

		RETURN 0
;세츠나
ELSEIF FLAG:現M == 4 && ARG == 7 && ARG:1 == 2
	IF 던전フラグ:61:1 == 1 && 던전フラグ:61:0 == 0
		CALL MESSAGE_WINDOW_D, "", "＞「카이 세츠나」라고 써져있다"
		CALL MESSAGE_WINDOW_D, "", "＞타겟의 집인 것 같다"
		CALL MESSAGE_WINDOW_D, "", "＞문을 엽니까？"
	CALL INPUT_YN_D
		IF RESULT == 0;y
			RETURN 1
		ELSEIF RESULT == 1;n
			RETURN 0
		ENDIF
	ELSEIF 던전フラグ:61:1 == 1 && 던전フラグ:61:0 >= 1
		RETURN 1
	ELSE
		CALL MESSAGE_WINDOW_D, "", "＞「카이 세츠나」라고 써져있다"
		CALL MESSAGE_WINDOW_D, "", "＞집을 비운 것 같다……"
		RETURN 0
	ENDIF

ELSEIF FLAG:現M == 5 && ARG == 6 && ARG:1 == 4
	IF  던전フラグ:61:1 == 2 && 던전フラグ:61:0 == 0 ||던전フラグ:61:1 == 1
		CALL MESSAGE_WINDOW_D, "", "＞소년이 길을 막고있다"
		CALL MESSAGE_WINDOW_D, "소년", "뭐야！ / 오늘이야말로 그 애와 사귀기로 했으니 방해하지마！"
		CALL MESSAGE_WINDOW_D, "", "＞아무래도 지나갈 수 없을 것 같다 /＞타겟의 집에 가보기로 하자"
		RETURN 0
	ELSE
		RETURN 1
	ENDIF
ENDIF
CALL CAN_ENTER,ARG,ARG:1
RETURN RESULT
