@SET_ACTION_데빌,ARG
;아군의中で一番ＨＰが低く、かつ비구相性が1미만or999ではないキャラにＧＵＮ공격

;全員の中で一番ＨＰが低い캐릭터を狙う
CFLAG:ARG:입력행동 = 2101
CFLAG:ARG:타겟 = -1
FOR LOCAL,1,7
	LOCALS = 포지션{LOCAL}
	SIF FLAG:LOCALS == -1
		CONTINUE
	SIF CFLAG:(FLAG:LOCALS):ステート >= GET_STATE_NUM("STONE")
		CONTINUE
	SIF MAXBASE:(FLAG:LOCALS):비구 < 1 || MAXBASE:(FLAG:LOCALS):비구 == 999
		CONTINUE
	IF FLAG:LOCALS > -1 && CFLAG:ARG:타겟 == -1 
		CFLAG:ARG:타겟 = LOCAL
		CONTINUE
	ENDIF
	IF FLAG:LOCALS > -1 && BASE:(FLAG:LOCALS):ＨＰ < BASE:((POS(CFLAG:ARG:타겟))):ＨＰ
		CFLAG:ARG:타겟 = LOCAL
	ENDIF
NEXT

IF CFLAG:ARG:타겟 == -1
	CFLAG:ARG:입력행동 = -1
	CFLAG:ARG:방어플래그 = 1
ENDIF

