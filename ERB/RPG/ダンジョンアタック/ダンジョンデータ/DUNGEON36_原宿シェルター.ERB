	CASE 2
		PRINTL   Ｂ２Ｆ
	CASE 3
		PRINTL   Ｂ３Ｆ
	CASE 4
		PRINTL   Ｂ４Ｆ
	CASE 5
		PRINTL   Ｂ５Ｆ
	CASE 6
		PRINTL   Ｂ６Ｆ
	CASE 7
		PRINTL   Ｂ７Ｆ
	CASE 8
