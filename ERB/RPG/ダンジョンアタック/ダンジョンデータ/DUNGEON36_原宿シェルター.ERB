;=======================================================================
;攻略조건
;=======================================================================

@PREREQUISITE_DUNGEON_36
SIF (이벤트フラグ:70:0 == 0 || 이벤트フラグ:70:0 == 99) || !(FINDCHARA_B(4761,0) == 2)
	RETURN 0
RETURN 1

