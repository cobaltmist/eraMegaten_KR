;=======================================================================
@GET_DUNGEON_NAME_76
RESULTS = 야토 빌딩

;=======================================================================
;플로어名표시
;=======================================================================
@FLOORNAME_76
PRINT 야토 빌딩
SELECTCASE FLAG:현M
	CASE 0
		PRINTL １Ｆ
	CASE 1
		PRINTL ２Ｆ
	CASE 2
		PRINTL ３Ｆ
ENDSELECT

;=======================================================================
;스타ト地点입력
;=======================================================================
@START_DUNGEON_76,ARG
FLAG:현M = 0 
