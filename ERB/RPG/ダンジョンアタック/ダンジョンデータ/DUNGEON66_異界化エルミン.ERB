	SELECTCASE FLAG:현M
		CASE 0
			IF FLAG:현Y == 0
				FLAG:현M = 1
				FLAG:현Y = 14
			ENDIF
		CASE 1
			IF FLAG:현Y == 14
				FLAG:현M = 0
				FLAG:현Y = 0
			ELSE
				FLAG:현M = 2
				FLAG:현X = 0
			ENDIF
		CASE 2
			IF FLAG:현X == 0
				FLAG:현M = 1
				FLAG:현X = 14
			ENDIF
		CASE 3
			IF FLAG:현Y == 14
				FLAG:현M = 4
				FLAG:현Y = 0
			ENDIF
		CASE 4
			IF FLAG:현Y == 0
				FLAG:현M = 3
				FLAG:현Y = 14
