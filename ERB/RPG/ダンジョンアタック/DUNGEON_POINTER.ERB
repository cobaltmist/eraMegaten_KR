;-------------------------------------------------------------------------------
;	ポインタ内部処理2
;-------------------------------------------------------------------------------
;	ARGにARGSでARG:1と加減乗除する
;===============================================================================
;	ARG = 元値  ARGS = 計算式(+-*/=....)  ARG:1 = 計算する数値

@POINTER_SUB_CALC(ARG,ARGS,ARG:1)
#FUNCTION
SELECTCASE ARGS
	CASE "","GET"
		RETURNF ARG
	CASE "=="
		RETURNF (ARG == ARG:1)
	CASE "+", "+="
		RETURNF (ARG + ARG:1)
	CASE "-", "-="
		RETURNF (ARG - ARG:1)
	CASE "*", "*="
		RETURNF (ARG * ARG:1)
	CASE "/", "/="
		RETURNF (ARG / ARG:1)
	CASE "<"
		RETURNF (ARG < ARG:1)
	CASE "<=","=<"
		RETURNF (ARG <= ARG:1)
	CASE ">"
		RETURNF (ARG > ARG:1)
	CASE ">=","=>"
		RETURNF (ARG >= ARG:1)
ENDSELECT
;CALLF PRINT_ERROR(@"POINTER_SUB_CALC({ARG}, %ARGS%,{ARG:1}) 31_引数が不正です(%ARGS%という計算式は存在しません)")
THROW 引数が不正です


