;-----------------------------------------------------
;성욕처리関連
;-----------------------------------------------------
;성욕처리の計算
@성욕처리, ARG , ARG:1 = -1 , ARG:2 = -1
#DIM CHARA , 1
#DIM 元욕정 , 2
#DIM LCOUNT
;ARG = 処理するキャラ
;ARG:1 = 相손
;ARG:2 = プ레이内容（1:仲魔と섹스をする 2:仲魔に봉사してもらう 3:この場で자위をする 4:物陰で자위をしてくる 5:불결を落とす）
;초기化
FOR LOCAL , 0 , 1 + (ARG:1 > -1)
	CFLAG:(ARG:LOCAL):挿入횟수 = 0
	CFLAG:(ARG:LOCAL):本番횟수 = 0
	CFLAG:(ARG:LOCAL):中出し횟수 = 0
	CFLAG:(ARG:LOCAL):애널횟수 = 0
	CFLAG:(ARG:LOCAL):펠라횟수 = 0
	CFLAG:(ARG:LOCAL):클리자극횟수 = 0
	CFLAG:(ARG:LOCAL):가슴責め횟수 = 0
	NOWEX:(ARG:LOCAL):Ｃ절정 = 0
	NOWEX:(ARG:LOCAL):Ｖ절정 = 0
	NOWEX:(ARG:LOCAL):Ａ절정 = 0
	NOWEX:(ARG:LOCAL):Ｂ절정 = 0
	NOWEX:(ARG:LOCAL):사정 = 0
	NOWEX:(ARG:LOCAL):분유 = 0
	TCVAR:(ARG:LOCAL):획득자위경험 = 0
	元욕정:LOCAL = CFLAG:(ARG:LOCAL):던전内発情用욕정치
	DOWNBASE:(ARG:LOCAL):체력 = 0
	DOWNBASE:(ARG:LOCAL):기력 = 0
NEXT
;プ레이횟수用
LOCAL:1 = 0
;애무횟수計算用
LOCAL:2 = 0
;윤간악마の번호격납用
LOCAL:3 = 0
;윤간人数
LOCAL:4 = 0
;CFLAG:던전内発情用욕정치の표시用
LOCAL:5 = 0
;プ레이횟수
IF ARG:2 == 1
	LOCAL:1 = 2 + (ABL:(ARG):기교 + ABL:(ARG:1):봉사정신)
ELSEIF ARG:2 == 2
	LOCAL:1 = 2 + (ABL:(ARG:1):기교 + ABL:(ARG:1):봉사정신) / 2
ELSEIF ARG:2 == 3 || ARG:2 == 4
	LOCAL:1 = 2 + ((ABL:(ARG):기교 + ABL:(ARG):노출증) / 2 + ABL:(ARG):자위중독) / 2
ENDIF
;절륜をもっている場合は횟수が増える
LOCAL:1 += TALENT:ARG:절륜 * 2
SIF ARG:1 > 0 && TALENT:(ARG:1):절륜
	LOCAL:1 += 2
;プ레이内容
IF ARG:2 == 1
	FOR LCOUNT , 0 , LOCAL:1
		$AIBU
		;前戯は3回まで
		IF RAND:100 < 30 && LOCAL:2 < 3
			FOR LOCAL , 0 , 2
				IF HAVE_PENIS(ARG:LOCAL)
					CFLAG:(ARG:(!LOCAL)):펠라횟수 += 1
				ELSE
					IF RAND:2 == 0
						CFLAG:(ARG:LOCAL):가슴責め횟수 += 1
					ELSE
						CFLAG:(ARG:LOCAL):클리자극횟수 += 1
					ENDIF
				ENDIF
			NEXT
			LOCAL:2 += 1
			GOTO AIBU
		ELSEIF RAND:100 < 60
			IF HAVE_PENIS(ARG) && HAVE_VAGINA(ARG:1)
				CFLAG:(ARG:1):本番횟수 += 1
				CFLAG:(ARG):挿入횟수 += 1
			ELSEIF HAVE_PENIS(ARG)
				CFLAG:(ARG:1):애널횟수 += 1
				CFLAG:(ARG):挿入횟수 += 1
			ELSEIF HAVE_PENIS(ARG:1)
				CFLAG:(ARG):本番횟수 += 1
				CFLAG:(ARG:1):挿入횟수 += 1
			ELSE
				CFLAG:(ARG:1):클리자극횟수 += 1
			ENDIF
		ELSEIF RAND:100 < 50
			FOR LOCAL , 0 , 2
				IF HAVE_PENIS(ARG:LOCAL)
					CFLAG:(ARG:(!LOCAL)):펠라횟수 += 2
				ELSE
					CFLAG:(ARG:LOCAL):클리자극횟수 += 2
				ENDIF
			NEXT
		ELSE
			IF HAVE_PENIS(ARG)
				CFLAG:(ARG:1):애널횟수 += 1
				CFLAG:(ARG):挿入횟수 += 1
			ELSEIF HAVE_PENIS(ARG:1)
				CFLAG:(ARG):애널횟수 += 1
				CFLAG:(ARG:1):挿入횟수 += 1
			ELSE
				CFLAG:(ARG:1):클리자극횟수 += 1
				CFLAG:(ARG:1):가슴責め횟수 += 1
				CFLAG:(ARG):가슴責め횟수 += 1
				CFLAG:(ARG):클리자극횟수 += 1
			ENDIF
		ENDIF
	NEXT
	CFLAG:ARG:中出し횟수 = CFLAG:ARG:本番횟수
	CFLAG:(ARG:1):中出し횟수 = CFLAG:(ARG:1):本番횟수
ELSEIF ARG:2 == 2
	IF HAVE_PENIS(ARG)
		CFLAG:(ARG:1):펠라횟수 += LOCAL:1
	ELSE
		CFLAG:(ARG):클리자극횟수 = 1 + RAND:(MAX(1, LOCAL:1 - 1))
		CFLAG:(ARG):가슴責め횟수 = LOCAL:1 - CFLAG:(ARG):가슴責め횟수
	ENDIF
ELSEIF ARG:2 == 3
	TCVAR:ARG:획득자위경험 += LOCAL:1
	CFLAG:ARG:클리자극횟수 = 1 + RAND:(MAX(1 , TCVAR:ARG:획득자위경험 - 1))
	CFLAG:ARG:가슴責め횟수 = TCVAR:ARG:획득자위경험 - CFLAG:ARG:클리자극횟수
ELSEIF ARG:2 == 4
	PRINTFORML ・
	PRINTFORML ・・
	PRINTFORML ・・・
	PRINTL 
	;윤간확률
	IF 위험일(ARG) >= 2
		LOCAL:2 = 40
	ELSEIF CFLAG:ARG:던전内発情用욕정치 >= 10000
		LOCAL:2 = 20
	ELSEIF CFLAG:ARG:던전内発情用욕정치 >= 5000
		LOCAL:2 = 10
	ELSE
		LOCAL:2 = 1
	ENDIF
	;満月時やられやすくなる
	SIF FLAG:月齢 == 8
		LOCAL:2 += 10
	;월하향머리장식を付けていると大幅アップ
	SIF CFLAG:ARG:모자 == 30
		LOCAL:2 += 50
	TCVAR:ARG:획득자위경험 = LOCAL:2 / 10 + LOCAL:1
	CFLAG:ARG:가슴責め횟수 = 1 + RAND:(TCVAR:ARG:획득자위경험 +1)
	CFLAG:ARG:클리자극횟수 = TCVAR:ARG:획득자위경험 - CFLAG:ARG:가슴責め횟수
	;던전内に설정がされている場合、악마に윤간される가능性がある
	TRYCCALLFORM DUNGEON_RAPE_DEVIL_{FLAG:現던전}
		IF RAND:100 < LOCAL:2 && !IS_LOOKSLIKE_MALE(ARG)
			;윤간されちゃうかも판정へ
			CALL 성욕처리_윤간, ARG , RESULT
			LOCAL:3 = RESULT ;횟수
			LOCAL:4 = RESULT:1 ;人数
		ENDIF
	CATCH
	ENDCATCH
ELSEIF ARG:2 == 5
	PALAM:ARG:윤활 = 0
	;질내以外의 불결を綺麗に
	FOR LCOUNT, 0, 8
		SIF LCOUNT == 6
			CONTINUE
		STAIN:ARG:LCOUNT = 0
	NEXT
	RETURN 0
ENDIF

FOR LOCAL , 0 , 1 + (ARG:1 > -1)
	IF TALENT:(ARG:LOCAL):남자 == 0
		CALL SET_STAIN("질내" , "애액" , (ARG:LOCAL))
		CALL SET_STAIN("바기나" , "애액" , (ARG:LOCAL))
		IF CFLAG:(ARG:LOCAL):本番횟수 > 0
			CALL SET_STAIN("질내" , "정액" , (ARG:LOCAL))
			CALL SET_STAIN("바기나" , "정액" , (ARG:LOCAL))
		ENDIF
		IF CFLAG:(ARG:LOCAL):펠라횟수 > 0
			CALL SET_STAIN("입" , "정액" , (ARG:LOCAL))
			CALL SET_STAIN("손" , "정액" , (ARG:LOCAL))
			SIF TEQUIP:(ARG:LOCAL):유방노출
				CALL SET_STAIN("가슴" , "정액" , (ARG:LOCAL))
		ENDIF
		IF CFLAG:(ARG:LOCAL):애널횟수 > 0
			CALL SET_STAIN("애널" , "정액" , (ARG:LOCAL))
		ENDIF
		;윤간された場合は머리카락も불결る
		SIF LOCAL:3 > 0
			CALL SET_STAIN("머리카락" , "정액" , ARG)
	ENDIF
NEXT


;誰が犯したか판정
SIF LOCAL:3 > 0
	ARG:1 = POS(14)

FOR LOCAL , 0 , 1 + (ARG:1 > -1 && ARG:1 != POS(14))
	TCVAR:(ARG:LOCAL):획득펠라경험 += CFLAG:(ARG:LOCAL):펠라횟수
	SIF ARG:1 > -1 && ARG:1 != POS(14) && CFLAG:(ARG:LOCAL):함락キャラ == CFLAG:(ARG:(!LOCAL)):キャラ固有の번호
		TCVAR:(ARG:LOCAL):획득봉사쾌락경험 += CFLAG:(ARG:LOCAL):펠라횟수
	TCVAR:(ARG:LOCAL):획득Ｖ경험 += CFLAG:(ARG:LOCAL):本番횟수
	TCVAR:(ARG:LOCAL):획득Ａ경험 += CFLAG:(ARG:LOCAL):애널횟수
	TCVAR:(ARG:LOCAL):획득성교경험 += CFLAG:(ARG:LOCAL):本番횟수 + CFLAG:(ARG:LOCAL):애널횟수
	SIF ARG:1 == POS(14)
		TCVAR:(ARG:LOCAL):획득윤간경험 += (LOCAL:4 > 1 ? 1 # 0)
	SIF ARG:1 > -1 && !(TALENT:(ARG:LOCAL):짐승 || TALENT:(ARG:LOCAL):새) && (TALENT:(ARG:(!LOCAL)):짐승 || TALENT:(ARG:(!LOCAL)):새)
		TCVAR:(ARG:LOCAL):획득수간경험 += TCVAR:(ARG:LOCAL):획득성교경험
	IF ARG:1 > -1 && !LOCAL:3
		TCVAR:(ARG:LOCAL):획득성교경험 += CFLAG:(ARG:(!LOCAL)):本番횟수 + CFLAG:(ARG:(!LOCAL)):애널횟수
	ENDIF
	
	;ソースの획득
	IF ARG:1 > -1
		SOURCE:(ARG:LOCAL):쾌Ｃ += CFLAG:(ARG:LOCAL):클리자극횟수 * (200)
		SOURCE:(ARG:LOCAL):쾌Ｃ += (CFLAG:(ARG:(!LOCAL)):本番횟수 + CFLAG:(ARG:(!LOCAL)):애널횟수 + CFLAG:(ARG:(!LOCAL)):펠라횟수) * (250)
		CUP:(ARG:LOCAL):쾌Ｃ += 300 * (CFLAG:(ARG:LOCAL):클리자극횟수 > 0)
		SOURCE:(ARG:LOCAL):쾌Ｖ += CFLAG:(ARG:LOCAL):本番횟수 * (250 + ABL:(ARG:LOCAL):섹스중독 * 25)
		CUP:(ARG:LOCAL):쾌Ｖ +=  + MIN(1 , CFLAG:(ARG:LOCAL):本番횟수) * 350
		SOURCE:(ARG:LOCAL):쾌Ａ += CFLAG:(ARG:LOCAL):애널횟수 * (250 + ABL:(ARG:LOCAL):섹스중독 * 25)
		CUP:(ARG:LOCAL):쾌Ａ +=  + MIN(1 , CFLAG:(ARG:LOCAL):애널횟수) * 350
		SOURCE:(ARG:LOCAL):쾌Ｂ += CFLAG:(ARG:LOCAL):가슴責め횟수 * (225) + 300
		CUP:(ARG:LOCAL):쾌Ｂ += 300 * (CFLAG:(ARG:LOCAL):가슴責め횟수 > 0)
		SOURCE:(ARG:LOCAL):욕정 += (CFLAG:(ARG:LOCAL):펠라횟수 + CFLAG:(ARG:LOCAL):클리자극횟수 + CFLAG:(ARG:LOCAL):가슴責め횟수) * 25
	;자위用
	ELSEIF ARG:2 == 3
		SOURCE:(ARG:LOCAL):쾌Ｃ += CFLAG:(ARG:LOCAL):클리자극횟수 * (25 * (10 + ABL:(ARG:LOCAL):자위중독 + ABL:(ARG:LOCAL):노출증))
		CUP:(ARG:LOCAL):쾌Ｃ += 300 * (CFLAG:(ARG:LOCAL):가슴責め횟수 > 0)
		SOURCE:(ARG:LOCAL):쾌Ｂ += CFLAG:(ARG:LOCAL):가슴責め횟수 * (25 * (10 + ABL:(ARG:LOCAL):자위중독 + ABL:(ARG:LOCAL):노출증))
		CUP:(ARG:LOCAL):쾌Ｂ += 300 * (CFLAG:(ARG:LOCAL):가슴責め횟수 > 0)
		SOURCE:(ARG:LOCAL):욕정 += (CFLAG:(ARG:LOCAL):클리자극횟수 + CFLAG:(ARG:LOCAL):가슴責め횟수 + ABL:(ARG:LOCAL):노출증) * 25
	ELSEIF ARG:2 == 4
		SOURCE:(ARG:LOCAL):쾌Ｃ += CFLAG:(ARG:LOCAL):클리자극횟수 * (25 * (ABL:(ARG:LOCAL):자위중독 + 10)) + 300 * (CFLAG:(ARG:LOCAL):클리자극횟수 > 0)
		CUP:(ARG:LOCAL):쾌Ｃ += 300 * (CFLAG:(ARG:LOCAL):가슴責め횟수 > 0)
		SOURCE:(ARG:LOCAL):쾌Ｂ += CFLAG:(ARG:LOCAL):가슴責め횟수 * (25 * (ABL:(ARG:LOCAL):자위중독 + 10)) + 300 * (CFLAG:(ARG:LOCAL):가슴責め횟수 > 0)
		CUP:(ARG:LOCAL):쾌Ｂ += 300 * (CFLAG:(ARG:LOCAL):가슴責め횟수 > 0)
		SOURCE:(ARG:LOCAL):욕정 += (CFLAG:(ARG:LOCAL):클리자극횟수 + CFLAG:(ARG:LOCAL):가슴責め횟수) * 25
	ENDIF
	
	IF ARG:1 > -1
		CALL TECHNIQUE_CHECK, ARG:(!LOCAL), ARG:LOCAL
		TCVAR:(ARG:(!LOCAL)):획득조교경험 = 0
	ELSE
		CALL TECHNIQUE_CHECK, ARG:LOCAL, ARG:LOCAL
		TCVAR:(ARG:LOCAL):획득조교경험 = 0
		
	ENDIF
	CALL SOURCE_CHECK_DUNGEON , ARG:LOCAL
NEXT

IF LOCAL:3 > 0
	TCVAR:(ARG):획득질사경험 += CFLAG:(ARG):中出し횟수
	TCVAR:(ARG):획득정액경험 += CFLAG:(ARG):本番횟수 + CFLAG:(ARG):애널횟수 + CFLAG:(ARG):펠라횟수
ELSEIF ARG:1 > -1
	SIF CFLAG:(ARG):本番횟수
		CFLAG:(ARG):中出し횟수 += NOWEX:(ARG:1):사정 * CFLAG:(ARG):本番횟수 / (CFLAG:(ARG):本番횟수 + CFLAG:(ARG):애널횟수 + CFLAG:(ARG):펠라횟수)
	TCVAR:(ARG):획득질사경험 += CFLAG:(ARG):中出し횟수
	SIF CFLAG:(ARG:1):本番횟수
		CFLAG:(ARG:1):中出し횟수 += NOWEX:(ARG):사정 * CFLAG:(ARG:1):本番횟수 / (CFLAG:(ARG:1):本番횟수 + CFLAG:(ARG:1):애널횟수 + CFLAG:(ARG:1):펠라횟수)
	TCVAR:(ARG:1):획득질사경험 += CFLAG:(ARG:1):中出し횟수
ENDIF
PRINTL 


IF CFLAG:(ARG):中出し횟수
	SELECTCASE CFLAG:(ARG):中出し횟수
		CASE 1 TO 10
			CFLAG:(ARG):子宮現在容量 += 10
		CASE 11 TO 20
			CFLAG:(ARG):子宮現在容量 += 20
		CASE 21 TO 30
			CFLAG:(ARG):子宮現在容量 += 30
		CASE 31 TO 50
			CFLAG:(ARG):子宮現在容量 += 50
		CASEELSE
			IF FLAG:ふたごちゃんフラグ > 0
				CFLAG:(ARG):子宮現在容量 += 100 + RAND:(CFLAG:(ARG):中出し횟수)
			ELSE
				CFLAG:(ARG):子宮現在容量 += 100
			ENDIF
	ENDSELECT
	;디버그용
	DEBUGPRINTFORML %CALLNAME:(ARG)%中出し횟수{CFLAG:(ARG):中出し횟수}
	DEBUGPRINTFORML %CALLNAME:(ARG)%子宮現在容量{CFLAG:(ARG):子宮現在容量}
ENDIF

IF ARG:1 > -1
	IF CFLAG:(ARG:1):中出し횟수
		SELECTCASE CFLAG:(ARG:1):中出し횟수
			CASE 1 TO 10
				CFLAG:(ARG:1):子宮現在容量 += 10
			CASE 11 TO 20
				CFLAG:(ARG:1):子宮現在容量 += 20
			CASE 21 TO 30
				CFLAG:(ARG:1):子宮現在容量 += 30
			CASE 31 TO 50
				CFLAG:(ARG:1):子宮現在容量 += 50
			CASEELSE
				IF FLAG:ふたごちゃんフラグ > 0
					CFLAG:(ARG:1):子宮現在容量 += 100 + RAND:(CFLAG:(ARG:1):中出し횟수)
				ELSE
					CFLAG:(ARG:1):子宮現在容量 += 100
				ENDIF
		ENDSELECT
		;디버그용
		DEBUGPRINTFORML %CALLNAME:(ARG:1)%中出し횟수{CFLAG:(ARG:1):中出し횟수}
		DEBUGPRINTFORML %CALLNAME:(ARG:1)%子宮現在容量{CFLAG:(ARG:1):子宮現在容量}
	ENDIF
ENDIF

;妊娠判定用
FOR LOCAL , 0 , 1 + (ARG:1 > -1 && !LOCAL:3)
	IF PREGNANCY_ABLE((ARG:LOCAL)) && TCVAR:(ARG:LOCAL):획득질사경험
		IF (ARG:(!LOCAL)) == MASTER
			CFLAG:(ARG:LOCAL):主人の中出氏による임신판정 = TCVAR:(ARG:LOCAL):획득질사경험*2
			CHARA = MASTER
		ELSEIF !LOCAL:3 && (ARG:(!LOCAL)) == ASSI
			CFLAG:(ARG:LOCAL):조수の中出氏による임신판정 = TCVAR:(ARG:LOCAL):획득질사경험*2
			CHARA = (ARG:(!LOCAL))
		ELSEIF !LOCAL:3 && (ARG:(!LOCAL)) == TARGET
			CFLAG:(ARG:LOCAL):奴隷の中出氏による임신판정 = TCVAR:(ARG:LOCAL):획득질사경험*2
			CHARA = (ARG:(!LOCAL))
		ELSEIF !LOCAL:3 && CFLAG:(ARG:(!LOCAL)):PTフラグ == 2
			CFLAG:(ARG:LOCAL):奴隷の中出氏による임신판정 = TCVAR:(ARG:LOCAL):획득질사경험*2
			CHARA = (ARG:(!LOCAL))
		ELSEIF !LOCAL:3
			CFLAG:(ARG:LOCAL):顧客の中出氏による임신판정 = TCVAR:(ARG:LOCAL):획득질사경험*2
			CHARA = (ARG:(!LOCAL))
		ELSEIF LOCAL:3 > 0
			CFLAG:(ARG:LOCAL):顧客の中出氏による임신판정 = TCVAR:(ARG:LOCAL):획득질사경험*2
			CHARA = -1
		ENDIF
		CALL PREGNANCY_CHECK, (ARG:LOCAL)
		;임신
		IF RESULT && CHARA > -1
			CALL SET_FATHERINFO, (ARG:LOCAL), NO:(ARG:(!LOCAL)), CFLAG:CHARA:キャラ固有の번호, CALLNAME:(ARG:(!LOCAL))
		ELSEIF RESULT
			CALL SET_FATHERINFO, (ARG:LOCAL), NO:(ARG:(!LOCAL)), CHARA, NAME:(ARG:(!LOCAL))
		ENDIF
	ENDIF
NEXT
FORCEWAIT

;구상呼び出し
SWAP TARGET , ARG:1
SWAP PLAYER , ARG
TFLAG:이벤트구상呼び出し用 = 78 + ARG:2
IF TARGET > -1
	IF LOCAL:3
		TFLAG:이벤트구상呼び出し用 = 85
		CALL KOJO_TALK, TARGET ,90
	ELSE
		CALL KOJO_MESSAGE_EVENT
	ENDIF
ENDIF
SIF ARG:2 == 1
	TFLAG:이벤트구상呼び出し用 = 83
SIF ARG:2 == 2
	TFLAG:이벤트구상呼び出し用 = 84
SWAP TARGET , PLAYER
SIF TARGET > -1
	CALL KOJO_MESSAGE_EVENT
SWAP TARGET , PLAYER


SWAP TARGET , ARG:1
SWAP PLAYER , ARG


;경험の加算と표시


FOR LOCAL , 0 ,1 + (ARG:1 > -1 && !LOCAL:3)
	DRAWLINE
	PRINTFORML ＲＥＳＵＬＴ(%CALLNAME:(ARG:LOCAL)%)
	IF CFLAG:(ARG:LOCAL):本番횟수
		IF TALENT:(ARG:LOCAL):처녀
			IF LOCAL:3
				CALL ADDS_ABNORMAL_EXP, "처녀상실（윤간）", ARG
				TALENT:(ARG:LOCAL):처녀 = 0
				DOWNBASE:(ARG:LOCAL):체력 += 500
				DOWNBASE:(ARG:LOCAL):기력 += 500
			ELSE
				PRINTFORML 처녀 상실
				TALENT:(ARG:LOCAL):처녀 = 0
				DOWNBASE:(ARG:LOCAL):체력 += 200
				DOWNBASE:(ARG:LOCAL):기력 += 100
			ENDIF
		ELSEIF TALENT:(ARG:LOCAL):재생처녀
			PRINTFORML 재생처녀 상실
			TALENT:(ARG:LOCAL):재생처녀 = 0
			DOWNBASE:(ARG:LOCAL):체력 += 200
			DOWNBASE:(ARG:LOCAL):기력 += 100
		ENDIF
	ENDIF
	CALL SHOW_ADD_EXP, ARG:LOCAL
	CALL SHOW_ABNORMAL_EXP, (ARG:LOCAL)
;체력・기력소비
	;던전内発情用욕정치は절정分減り、윤간された場合は増える、자위は少量減る
	SIF ARG:2 < 3
		CFLAG:(ARG:LOCAL):던전内発情用욕정치 -= LIMIT(SUMARRAY(NOWEX:(ARG:LOCAL):0, 0, 6) * 800, 0, 6000+(250*(RAND:8+1)))
	IF LOCAL:3 > 0
		CFLAG:(ARG:LOCAL):던전内発情用욕정치 +=SUMARRAY(NOWEX:(ARG:LOCAL):0, 0, 6) * 100
	ELSEIF ARG:2 == 4 && LOCAL:3 == 0
		CFLAG:(ARG:LOCAL):던전内発情用욕정치 -= LIMIT(SUMARRAY(NOWEX:(ARG:LOCAL):0, 0, 6) * 300, 0, 2000+(250*(RAND:4+1)))
	ENDIF
	
	;남자の場合はCFLAG:던전内発情用욕정치は0に
	SIF TALENT:(ARG:LOCAL):남자
		CFLAG:(ARG:LOCAL):던전内発情用욕정치 = 0
		
	IF SUMARRAY(NOWEX:(ARG:LOCAL):0, 0, 6) && LOCAL:3 == 0
		TIMES CFLAG:(ARG:LOCAL):던전内発情用욕정치 , 0.85
	ENDIF
	;CFLAG:던전内発情用욕정치の上限は15000
	SIF CFLAG:(ARG:LOCAL):던전内発情用욕정치 > 15000
		CFLAG:(ARG:LOCAL):던전内発情用욕정치 = 15000
	SIF CFLAG:(ARG:LOCAL):던전内発情用욕정치 < 0
		CFLAG:(ARG:LOCAL):던전内発情用욕정치 = 0
	IF CFLAG:(ARG:LOCAL):던전内発情用욕정치 != 元욕정:LOCAL
		PRINTFORM 욕정　
		LOCAL:5 = MIN(15000 , 元욕정:LOCAL)
		SETCOLOR 0x40 + (0xff - 0x40) * LOCAL:5 / 15000 , 0x40 - (0x40 * LOCAL:5 / 15000) , 0x40 + (0xff - 0x40) * LOCAL:5 / 15000
		SETCOLOR 0xff * LOCAL:5 / 15000 , 0 , 0xff * LOCAL:5 / 15000
		IF 元욕정:LOCAL >= 10000
			PRINTFORM %ハート()%ＭＡＸ%ハート()%
		ELSE
			PRINTFORM %ハート()%
		ENDIF
		RESETCOLOR
		PRINTFORM 　→　
		LOCAL:5 = MIN(15000 , CFLAG:(ARG:LOCAL):던전内発情用욕정치)
		SETCOLOR 0x40 + (0xff - 0x40) * LOCAL:5 / 15000 , 0x40 - (0x40 * LOCAL:5 / 15000) , 0x40 + (0xff - 0x40) * LOCAL:5 / 15000
		SETCOLOR 0xff * LOCAL:5 / 15000 , 0 , 0xff * LOCAL:5 / 15000
		IF CFLAG:(ARG:LOCAL):던전内発情用욕정치 >= 10000
			PRINTFORML %ハート()%ＭＡＸ%ハート()%
		ELSE
			PRINTFORML %ハート()%
		ENDIF
		RESETCOLOR
	ELSEIF CFLAG:(ARG:LOCAL):던전内発情用욕정치 >= 10000
		PRINTFORML 욕정　
		LOCAL:5 = MIN(15000 , CFLAG:(ARG:LOCAL):던전内発情用욕정치)
		SETCOLOR 0x40 + (0xff - 0x40) * LOCAL:5 / 15000 , 0x40 - (0x40 * LOCAL:5 / 15000) , 0x40 + (0xff - 0x40) * LOCAL:5 / 15000
		SETCOLOR 0xff * LOCAL:5 / 15000 , 0 , 0xff * LOCAL:5 / 15000
		PRINTFORML %ハート()%ＭＡＸ%ハート()%
		RESETCOLOR
	ENDIF
;		PRINTFORML 욕정(%CALLNAME:(ARG:LOCAL)%)　{元욕정:LOCAL , 6 , RIGHT}→{CFLAG:(ARG:LOCAL):던전内発情用욕정치 , 6 , RIGHT}
	IF LOCAL:3
		DOWNBASE:(ARG:LOCAL):체력 += (CFLAG:(ARG:LOCAL):本番횟수 + CFLAG:(ARG:LOCAL):애널횟수 + CFLAG:(ARG:LOCAL):펠라횟수) * 25
		DOWNBASE:(ARG:LOCAL):기력 += (CFLAG:(ARG:LOCAL):本番횟수 + CFLAG:(ARG:LOCAL):애널횟수 + CFLAG:(ARG:LOCAL):펠라횟수) * 20 + CFLAG:(ARG:LOCAL):中出し횟수 * 40
		CALL CONTROL_MAG, ARG , (CFLAG:ARG:本番횟수*2+CFLAG:ARG:애널횟수*2+CFLAG:ARG:펠라횟수*2+CFLAG:ARG:中出し횟수*5)*MAXBASE:(ARG:1):LV
		;윤간중독があると軽減される
		IF ABL:(ARG:LOCAL):윤간중독 > 0
			DOWNBASE:(ARG:LOCAL):체력 -= DOWNBASE:(ARG:LOCAL):체력 / (5 * ABL:(ARG:LOCAL):윤간중독)
			DOWNBASE:(ARG:LOCAL):기력 -= DOWNBASE:(ARG:LOCAL):기력 / (5 * ABL:(ARG:LOCAL):윤간중독)
		ENDIF
			
