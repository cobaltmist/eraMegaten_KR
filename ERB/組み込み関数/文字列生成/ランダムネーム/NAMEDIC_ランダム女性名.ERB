;========================================================================
;適当な女性の名前を指定した数だけランダム生成してRESULTS配列に代入
;========================================================================
@NAMEDIC_랜덤여성명,ARG = 1
#LOCALSIZE 1
#LOCALSSIZE 2
SIF ARG <= 0
	RETURN 
LOCALS = 西欧_東欧_イスラム_インド_モンゴル_意味불명
FOR LOCAL,1,ARG+1
	SELECTCASE AUTO_SPLIT(LOCALS,"_",RAND(0,6))
		CASE "西欧"
			CALL NAMEDIC_랜덤가타가나,1,RAND(1,3)
			LOCALS:1 = 디아_리스_리나_레토_레이테
		CASE "東欧"
			CALL NAMEDIC_랜덤가타가나,1,RAND(1,2)
			LOCALS:1 = 안누_시아_드라_리테_리크스
		CASE "イスラム"
			CALL NAMEDIC_랜덤가타가나,1,RAND(2,3)
			LOCALS:1 = 이다_자드_티마_린
		CASE "インド"
			CALL NAMEDIC_랜덤가타가나,1,RAND(1,3)
			LOCALS:1 = 샤나_티_하르_라타
		CASE "モンゴル"
			CALL NAMEDIC_랜덤가타가나,1,RAND(1,2)
			LOCALS:1 = 안_토리_란_리크_리츄_린_루크_루크나_루체_룬
		CASE "意味불명"
			CALL NAMEDIC_랜덤가타가나,1,RAND(2,8)
			LOCALS:1 = 
	ENDSELECT
	RESULTS:LOCAL = %RESULTS%%RANDOM_SPLIT(LOCALS:1,"_",1)%
NEXT
ARRAYREMOVE RESULTS,0,1
