FOR LOCAL,0,ARG
	DO
		RESULTS:LOCAL = %RANDOM_SPLIT(LOCALS,"_",ARG:1)%
	LOOP EQUALCHECK_STR(SUBSTRING(RESULTS:LOCAL,0,2),"ン","ー")
	REPLACE RESULTS:LOCAL,"_",""
NEXT
