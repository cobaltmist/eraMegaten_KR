@SHOW_AISYOU_COLOR_LIST, ARG
SELECTCASE ARG
	CASE 0
		;약점
		CALL SET_AISYOU_COLOR, 200
		LOCALS = WEAK
	CASE 1
		;普通
		CALL SET_AISYOU_COLOR, 100
		LOCALS = NORMAL
	CASE 2
		;내성
		CALL SET_AISYOU_COLOR, 50
		LOCALS = RESIST
	CASE 3
		;무효
		CALL SET_AISYOU_COLOR, 0
		LOCALS = BLOCK
	CASE 4
		;흡수
		CALL SET_AISYOU_COLOR, -50
		LOCALS = ABSORB
	CASE 5
		;반사
		CALL SET_AISYOU_COLOR, 999
		LOCALS = REFLECT
ENDSELECT
PRINT ■
RESETCOLOR
PRINTFORM %LOCALS, 7, LEFT%