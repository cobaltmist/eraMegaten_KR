@HAVE_SKILL_OVERLAP(ARG,ARG:1)
#FUNCTION
#LOCALSIZE 2
LOCAL = 0
LOCAL:1 = GETNUM(ABL , "스킬1")
LOCAL += MATCH(ABL:ARG:0, ARG:1, LOCAL:1, LOCAL:1 + CHARA_SKILLCOUNT(ARG)+1)
LOCAL:1 = GETNUM(ABL , "장비스킬1")
LOCAL += MATCH(ABL:ARG:0, ARG:1, LOCAL:1, LOCAL:1 + 22)
RETURNF LOCAL
