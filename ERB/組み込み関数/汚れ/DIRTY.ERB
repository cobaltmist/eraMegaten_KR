;-------------------------------------------------
;불결판정 함수
;ARGのARGSについている汚れを判定する
;愛液と母乳と元々その部位についていた汚れは対象外
;汚れの個数と、汚れの合計(つまり、愛液と母乳を除いた汚れ情報を返す)
;-------------------------------------------------
@DIRTY, ARGS, ARG = -99
#FUNCTION
RESULT = 0
RESULT:1 = 0
SIF ARG == -99
	ARG = TARGET
;IF ARGS != 바기나" && GET_STAIN(ARGS, "애액", ARG)
;	RESULT++
;	RESULT:1 |= 1
;ENDIF
IF ARGS != "페니스" && GET_STAIN(ARGS, "페니스", ARG)
	RESULT++
	RESULT:1 |= 2
ENDIF
IF GET_STAIN(ARGS, "정액", ARG)
	RESULT++
	RESULT:1 |= 4
ENDIF
IF ARGS != "애널" && GET_STAIN(ARGS, "애널", ARG)
	RESULT++
	RESULT:1 |= 8
ENDIF
;IF GET_STAIN(ARGS, "모유", ARG)
;	RESULT++ 
;	RESULT:1 |= 16
;ENDIF
IF GET_STAIN(ARGS, "점액", ARG)
	RESULT++
	RESULT:1 |= 32
ENDIF
IF GET_STAIN(ARGS, "파과혈", ARG)
	RESULT++
	RESULT:1 |= 64
ENDIF
RETURNF RESULT 