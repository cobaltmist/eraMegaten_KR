;-------------------------------------------------
;クリ사용판정기본
;-------------------------------------------------
@USE_CLI,ARG,ARG:1 = 0
#FUNCTION
;クリが無いとダメ
SIF !HAVE_CLITORIS(ARG)
	RETURNF 0

;下半身下着をつけている、스커트以外の下半身겉옷を履いている、下の空いてない全身겉옷を着ているとダメ
SIF TEQUIP:ARG:음순노출 != -1 && ((ARG:1 & 1 && (TEQUIP:ARG:Ｃ촉각 & 2)) || (ARG:1 & 2 && (TEQUIP:ARG:Ｃ촉각 & 4)) ) == 0
	RETURNF 0

;삼각목마騎乗中はダメ
SIF TEQUIP:ARG:41
	RETURNF 0
;カテーテル装着中はダメ
SIF TEQUIP:ARG:43
	RETURNF 0
;尿道プラグ装着中はダメ
SIF TEQUIP:ARG:44
	RETURNF 0
;클리캡사용중はダメ
SIF TEQUIP:ARG:11
	RETURNF 0

RETURNF 1