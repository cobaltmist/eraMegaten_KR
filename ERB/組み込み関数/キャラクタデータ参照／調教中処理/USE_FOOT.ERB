@USE_FOOT,ARG
#FUNCTION
SIF !IS_HUMAN(ARG) && !TALENT:ARG:발 && !TALENT:ARG:갈퀴발 
	RETURNF 0

;갈퀴발の조교者の場合は기교Lv2이상필요
SIF TALENT:ARG:갈퀴발 && ABL:ARG:2 < 2 && ARG == PLAYER
	RETURNF 0

SIF ITEM_FOOT(ARG) > 0
	RETURNF 0

RETURNF 1