
;-1미만,CHARANUM이상 :추가卑語なし
;-1                  :成人男性を想定した추가卑語
;0이상,CHARANUM미만  :指定번호キャラの性別や体格で추가卑語
@卑語_정액(ARG=-1)
#FUNCTIONS
IF LOCAL == 0
	LOCALS:(LOCAL++) = 정액
	LOCALS:(LOCAL++) = 子種汁
	LOCALS:(LOCAL++) = ザー汁
	LOCALS:(LOCAL++) = チンポミルク
	LOCALS:(LOCAL++) = チンポエ키스
	LOCAL:1 = LOCAL
ENDIF
LOCAL = LOCAL:1
IF ARG == -1
	LOCALS:(LOCAL++) = ザーメン
	LOCALS:(LOCAL++) = チンポ汁
	LOCALS:(LOCAL++) = オスミルク
	LOCALS:(LOCAL++) = オス汁	
ELSEIF INRANGE(ARG,0,CHARANUM-1)
	;짐승
	IF IS_BEAST(ARG)
		LOCALS:(LOCAL++) = 野生ザーメン
		IF TALENT:ARG:짐승
			LOCALS:(LOCAL++) = 수간정액
			LOCALS:(LOCAL++) = 수간チンポ汁
			LOCALS:(LOCAL++) = ケモミルク
			LOCALS:(LOCAL++) = ケダモノ汁
		ENDIF
		IF TALENT:ARG:파충류
			LOCALS:(LOCAL++) = 파충류エ키스
			LOCALS:(LOCAL++) = 파충류ミルク
			LOCALS:(LOCAL++) = トカゲ汁
		ENDIF
		IF TALENT:ARG:부정형
			LOCALS:(LOCAL++) = 体液
			LOCALS:(LOCAL++) = 슬라임汁
		ENDIF
		;IF TALENT:ARG:물고기
		;	;LOCALS:(LOCAL++) = 물고기汁
		;ENDIF
	ELSE
		LOCALS:(LOCAL++) = ザーメン
		;一般的な男性
		IF IS_LOOKSLIKE_MALE(ARG) && GROUPMATCH(体格(ARG),0,1)
			LOCALS:(LOCAL++) = チンポ汁
			LOCALS:(LOCAL++) = オスミルク
			LOCALS:(LOCAL++) = オス汁
		;それ以外の男(男の子、오토코노코)
		ELSEIF IS_MALE(ARG) && ( TALENT:ARG:오토코노코 || GROUPMATCH(体格(ARG),-1) )
			LOCALS:(LOCAL++) = ショタエ키스
			LOCALS:(LOCAL++) = ショタミルク
			LOCALS:(LOCAL++) = ショタ汁
		ELSEIF !IS_MALE(ARG)
			LOCALS:(LOCAL++) = 牝チンポ汁
			LOCALS:(LOCAL++) = 牝チンポミルク
			LOCALS:(LOCAL++) = 후타나리汁
			LOCALS:(LOCAL++) = 후타나리エ키스
		ENDIF
		;小人
		IF 体格(ARG) <= -10
			LOCALS:(LOCAL++) = 요정ミルク
			LOCALS:(LOCAL++) = 요정チンポ汁
			LOCALS:(LOCAL++) = 요정ミルク
			LOCALS:(LOCAL++) = 요정チンポ汁
		;거체
		ELSEIF 体格(ARG) >= 10
			LOCALS:(LOCAL++) = デカチンポ汁
			LOCALS:(LOCAL++) = デカマラ汁
			LOCALS:(LOCAL++) = デカマラミルク
		ENDIF
		;절륜
		SIF TALENT:ARG:절륜
			LOCALS:(LOCAL++) = 無限ザーメン
		;早漏(早漏がないのでＣ민감)
		IF TALENT:ARG:Ｃ민감
			LOCALS:(LOCAL++) = 早漏ザーメン
			LOCALS:(LOCAL++) = 早漏정액
		ENDIF
	ENDIF
ENDIF

LOCALS:LOCAL = %LOCALS:(RAND:LOCAL)%
SIF ARG == -1
	GOTO 랜덤修飾

;修飾
IF EXP:ARG:사정경험 == 0
	LOCALS:LOCAL = 初物%LOCALS:LOCAL%
ELSEIF EXP:ARG:성교경험 == 0
	LOCALS:LOCAL = 동정%LOCALS:LOCAL%
ELSE
	$랜덤修飾
	SELECTCASE RAND:100
	CASE IS < 3
		LOCALS:LOCAL = 濃厚%LOCALS:LOCAL%
	CASE IS < 6
		LOCALS:LOCAL = 特濃%LOCALS:LOCAL%
	ENDSELECT
ENDIF

RETURNF LOCALS:LOCAL