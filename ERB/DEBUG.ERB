	PRINTFORM 회피 {RESULT,3}　
	LOCAL:2 = 0
	FOR LOCAL:1, 0, 19
		TRYCALLFORM 방어상성_{LOCAL}, LOCAL:1
		IF RESULT != 100
			LOCAL:2 += 1
			IF LOCAL:2 % 7 == 1
				PRINTL 
				PRINT 　　　　
			ENDIF
			IF RESULT < 100 || RESULT == 999
				SETCOLOR 51, 255, 255
			ELSE
				SETCOLOR 204, 0, 0
			ENDIF
			IF RESULT == 999
				PRINTFORM [%GET_TYPE(LOCAL:1),4%:반사]　
			ELSEIF RESULT == 50
				PRINTFORM [%GET_TYPE(LOCAL:1),4%:반감]　
			ELSEIF RESULT == 0
				PRINTFORM [%GET_TYPE(LOCAL:1),4%:무효]　
			ELSE
				PRINTFORM [%GET_TYPE(LOCAL:1),4%:{RESULT,4}]　
			ENDIF
		ENDIF
		RESETCOLOR
	NEXT
	PRINTL 
NEXT

@ARMOR_TYPE
FOR LOCAL:1, 0, 19
	PRINTL 
	PRINTFORML %GET_TYPE(LOCAL:1),4%
	FOR LOCAL, 3000, 5000
		TRYCALLFORM 방어상성_{LOCAL}, LOCAL:1
		SIF RESULT == 100
			CONTINUE
		IF RESULT < 100 || RESULT == 999
			SETCOLOR 51, 255, 255
		ELSE
			SETCOLOR 204, 0, 0
		ENDIF
		IF RESULT == 999
			LOCALS = 반사
		ELSEIF RESULT == 50
			LOCALS = 반감
		ELSEIF RESULT == 0
			LOCALS = 무효
		ELSE
			LOCALS = {RESULT}
		ENDIF
		PRINTL 
		PRINTFORMLC %ITEMNAME:LOCAL%　[%LOCALS,4%]
		PRINTL 
		LOCAL:4 = 0
		FOR LOCAL:3, 0, 19
			TRYCALLFORM 방어상성_{LOCAL}, LOCAL:3
			IF RESULT != 100
				IF RESULT < 100 || RESULT == 999
					SETCOLOR 51, 255, 255
				ELSE
					SETCOLOR 204, 0, 0
				ENDIF
				IF RESULT == 999
					PRINTFORM [%GET_TYPE(LOCAL:3),4%:반사]　
				ELSEIF RESULT == 50
					PRINTFORM [%GET_TYPE(LOCAL:3),4%:반감]　
				ELSEIF RESULT == 0
					PRINTFORM [%GET_TYPE(LOCAL:3),4%:무효]　
				ELSE
					PRINTFORM [%GET_TYPE(LOCAL:3),4%:{RESULT,4}]　
				ENDIF
				SIF LOCAL:4 % 7 == 6
					PRINTL 
				LOCAL:4 += 1
			ENDIF
			RESETCOLOR
		NEXT
		SIF LOCAL:4 % 7 > 0
			PRINTL 
	NEXT
NEXT
