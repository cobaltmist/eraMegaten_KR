			SETCOLOR 0xffff00
		PRINTFORM %UNICODE(0x263E)%
	CASE 5
		SIF ARG:1 == 0
			SETCOLOR 0xffa500
		PRINTFORM ★
	CASE 6
		SIF ARG:1 == 0
			SETCOLOR 0xf0f8ff
		PRINTFORM ●
	CASE 7
		SIF ARG:1 == 0
			SETCOLOR 0x008800
		PRINTFORM ◆
	CASE 8
		SIF ARG:1 == 0
			SETCOLOR COLOR("aqua")
		PRINTFORM JP
ENDSELECT
SETCOLOR RESULT
