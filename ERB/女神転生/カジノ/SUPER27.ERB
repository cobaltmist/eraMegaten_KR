					CASE 13, 14, 15
						LOCAL:3 = 7
				ENDSELECT
			ELSE
				SELECTCASE LOCAL
					;７　１００点
					CASE -1
						LOCAL:3 = 0
					;♀　５０点
					CASE 0, 1
						LOCAL:3 = 1
					;♂　５０点
					CASE 2, 3
						LOCAL:3 = 2
					;太陽　２０点
					CASE 4, 5
