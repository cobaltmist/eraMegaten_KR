@CODING
#LOCALSIZE 2
DRAWLINE
PRINTL 해석이 완료된 동료마에게서 플러그인 소프트를 코딩합니다
DRAWLINE
FOR LOCAL, 0, CHARANUM
	SIF FLAG:(NO:LOCAL+20000) < 1000
		CONTINUE
	LOCAL:1 = ABL:LOCAL:코딩 ? ABL:LOCAL:코딩 # CSVABL(NO:LOCAL, GETNUM(ABL, "코딩"),0)
	IF !LOCAL:1
		IF ABL:LOCAL:종족 == 1
			LOCAL:1 = 8056
 		ELSEIF ABL:LOCAL:종족 == 2
 			LOCAL:1 = 8067
 		ELSEIF ABL:LOCAL:종족 == 3
 			LOCAL:1 = 8071
 		ELSEIF ABL:LOCAL:종족 == 4
 			LOCAL:1 = 8058
 		ELSEIF ABL:LOCAL:종족 == 5
 			LOCAL:1 = 8065
 		ELSEIF ABL:LOCAL:종족 == 6 && TALENT:LOCAL:남자
 			LOCAL:1 = 8064
 		ELSEIF ABL:LOCAL:종족 == 6 && !TALENT:LOCAL:남자
 			LOCAL:1 = 8055
 		ELSEIF ABL:LOCAL:종족 == 7
 			LOCAL:1 = 8063
 		ELSEIF ABL:LOCAL:종족 == 8 && TALENT:LOCAL:남자
 			LOCAL:1 = 8066
 		ELSEIF ABL:LOCAL:종족 == 8 && !TALENT:LOCAL:남자
 			LOCAL:1 = 8060
 		ELSEIF ABL:LOCAL:종족 == 9
 			LOCAL:1 = 8069
 		ELSEIF ABL:LOCAL:종족 == 10
 			LOCAL:1 = 8068
 		ELSEIF ABL:LOCAL:종족 == 11 && TALENT:LOCAL:남자
 			LOCAL:1 = 8059
 		ELSEIF ABL:LOCAL:종족 == 11 && !TALENT:LOCAL:남자
 			LOCAL:1 = 8062
 		ELSEIF ABL:LOCAL:종족 == 15
 			LOCAL:1 = 8061
 		ELSEIF ABL:LOCAL:종족 == 16
 			LOCAL:1 = 8070
 		ELSEIF ABL:LOCAL:종족 == 18
 			LOCAL:1 = 8057
		ELSE
			CONTINUE
		ENDIF
	ENDIF
	SIF ITEM:(LOCAL:1)
		CONTINUE
	CALL MESSAGE_B(1, @"%CALLNAME:LOCAL%에게서【%PLUGINNAME(LOCAL:1)%】%조사만처리(PLUGINNAME(LOCAL:1),"를")% 추출했습니다")
	ITEM:(LOCAL:1) = 1
NEXT
