;==========================================================================
;소재の충성도による소질변경
;==========================================================================
@MAKE_TALENT,ARG,ARG:1
#DIM LCOUNT
;コピー作成
ADDVOIDCHARA
FOR LCOUNT, 0, 200
		TALENT:(CHARANUM-1):LCOUNT = TALENT:ARG:LCOUNT
NEXT

;ARG:1から포인트を취득
LOCAL = ARG:1/200
PRINTFORML {LOCAL}포인트의 범위내로 소질을 바꿔넣을 수 있습니다。
SIF ABL:ARG:종족 != 45
	PRINTL ※사용한만큼 합체후에는 충성도가 저하됩니다
WAIT
CUSTOMDRAWLINE =
LOCAL:1 = 0
$PRINT
PRINTFORML %STR:(ABL:ARG:종족)% %NAME:ARG% 
PRINTFORML 앞으로 {LOCAL}포인트
U = ARG
CALL NEW_PRINT_TALENT
DRAWLINE

CALLFORM PRINT_TALENT_{LOCAL:1},ARG
PRINTL
DRAWLINE

PRINTFORMLC [1007] \@ LOCAL:1 == 0 ? 육체・기술・버릇계소질 # 정신계소질 \@
PRINTLC [1000] 종료
PRINTLC [1009] 리셋
$INPUT_LOOP
INPUT
IF RESULT == 1000
	IF TALENT:ARG:남자 == 1 && TALENT:ARG:처녀 == 1
		TALENT:ARG:처녀 = 0
		LOCAL += 5
	ENDIF
	IF TALENT:ARG:처녀 && CSVTALENT(NO:ARG,0,0) == 0
		EXP:ARG:성교경험 -= EXP:ARG:Ｖ경험
		EXP:ARG:Ｖ경험 = 0
		EXP:ARG:질사경험 = 0
		EXP:ARG:Ｖ확장경험 = 0
		EXP:ARG:출산경험 = 0
		ABL:ARG:Ｖ감각 = 0
	ELSEIF TALENT:ARG:처녀 == 0 && EXP:ARG:Ｖ경험 == 0 && CSVTALENT(NO:ARG,0,0)
		EXP:ARG:성교경험 += 1
		EXP:ARG:Ｖ경험 += 1
	ENDIF
	DELCHARA CHARANUM-1
	RETURN (ARG:1 - ((ARG:1/200 -LOCAL)*200))
ELSEIF RESULT == 1007
	IF LOCAL:1 == 0
		LOCAL:1 = 1
	ELSE
		LOCAL:1 = 0
	ENDIF
	GOTO PRINT
ELSEIF RESULT == 1009
	FOR LCOUNT, 0, 200
		TALENT:ARG:LCOUNT = TALENT:(CHARANUM-1):LCOUNT
;		TALENT:ARG:LCOUNT = 0
;		SIF CSVTALENT(NO:ARG,LCOUNT,0)
;			TALENT:ARG:LCOUNT = 1
	NEXT
	DELCHARA CHARANUM-1
	RESTART
ELSEIF RESULT < 200  && RESULT >= 0 && T:RESULT > -100
	IF T:RESULT > LOCAL
		PRINTW 포인트가 부족합니다
		CLEARLINE 1
		GOTO INPUT_LOOP
	ENDIF
	IF TALENT:ARG:RESULT
		PRINTFORMW %조사처리(TALENTNAME:RESULT,"를")% 소거했습니다。
		TALENT:ARG:RESULT = 0
		LOCAL -= T:RESULT
		GOTO PRINT
	ELSE
		PRINTFORMW %조사처리(TALENTNAME:RESULT,"를")% 얻었습니다。
		TALENT:ARG:RESULT = 1
		LOCAL -= T:RESULT
		GOTO PRINT
	ENDIF
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF

