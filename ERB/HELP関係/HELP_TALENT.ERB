@DICTIONARY_TALENT_MAIN
P = 0
$PRINT_LIST
DRAWLINE
PRINTFORML 「소질」에 관한 대략적인 설명입니다. ＜page.{P+1}/3＞
DRAWLINE
;各ページを표시
FOR LOCAL:1, 0, 60
	LOCAL = LOCAL:1 + P*60
	STRLENS TALENTNAME:LOCAL
	SIF RESULT > 0
		PRINTFORMLC [{LOCAL, 3}]%TALENTNAME:LOCAL%
NEXT
PRINTL 
DRAWLINE
PRINTFORMLC \@(P <= 0) ? %" " * 16% # [1001]이전 페이지\@
PRINTLC [1000]돌아온다
PRINTFORMLC \@(P >= 2) ? %" " * 16% # [1009]다음 페이지\@

$INPUT_LOOP
INPUT
IF RESULT == 1000
	RETURN 0
ELSEIF RESULT == 1001 && P > 0
	P -= 1
	GOTO PRINT_LIST
ELSEIF RESULT == 1009 && P < 2
	P += 1
	GOTO PRINT_LIST
ELSE
	Q = RESULT
	STRLENS TALENTNAME:Q
	IF RESULT > 0
		PRINTFORML 【%TALENTNAME:Q%】
		TRYCALLFORM DICTIONARY_TALENT_{Q}
		GOTO PRINT_LIST
	ELSE
		GOTO INPUT_LOOP
	ENDIF
ENDIF

