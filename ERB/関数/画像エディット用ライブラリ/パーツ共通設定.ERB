;********************************************************************************************
@パーツデータ(パーツセット名)
#FUNCTIONS
#DIMS パーツセット名
SELECTCASE パーツセット名
	CASE "kaoru"
		RETURNF kaoru_パーツデータ
	CASE "seizon"
		RETURNF seizon_パーツデータ
	CASEELSE
		RETURNF default_パーツデータ
ENDSELECT

;********************************************************************************************
@パーツ選択肢
#FUNCTIONS
SELECTCASE 画像エディット_パーツセット名
	CASE "kaoru"
		RETURNF kaoru_パーツ選択肢
	CASE "seizon"
		RETURNF seizon_パーツ選択肢
	CASEELSE
		RETURNF default_パーツ選択肢
ENDSELECT

;********************************************************************************************
@パーツ初期設定
#FUNCTIONS
SELECTCASE 画像エディット_パーツセット名
	CASE "kaoru"
		RETURNF kaoru_パーツ初期設定
	CASE "seizon"
		RETURNF seizon_パーツ初期設定
	CASEELSE
		RETURNF default_パーツ初期設定
ENDSELECT

;********************************************************************************************
@パーツ画像サイズ(パーツセット名, 顔絵フラグ)
#FUNCTION
#DIMS パーツセット名
#DIM 顔絵フラグ
SELECTCASE パーツセット名
	CASEELSE
		RETURNF 画像サイズ_小
ENDSELECT

