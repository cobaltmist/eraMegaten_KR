;-------------------------------------------------
;PBAND사용가능판정
;ARG = 캐릭터 등록번호
;-------------------------------------------------
@USE_PBAND,ARG
#FUNCTION
;ペニスバンドを持っていないとダメ
SIF !ITEM:페니스밴드
	RETURNF 0

	;--------------------------------
	;射精関連の処理で
	;ペニスとペニスバンドを区別できるようになるまでは
	;--------------------------------
	;ペニスがあるとダメ
	SIF HAVE_PENIS(ARG)
		RETURNF 0
		
;クリキャップ使用中はダメ
SIF TEQUIP:ARG:11
	RETURNF 0

RETURNF 1
