@EVENT_10_2人掛け合いチェック(ARG,ARG:1,ARG:2=0)
#FUNCTION
;1人目がいなかったらパス
SIF !キャラ存在確かめ(ARG)
	RETURNF 0
;2人目がいなかったらパス
SIF !キャラ存在確かめ(ARG:1)
	RETURNF 0
;絆が指定値未満だったらパス
SIF キャラ絆確かめ(ARG,ARG:1) < ARG:2
	RETURNF 0
RETURNF 1
