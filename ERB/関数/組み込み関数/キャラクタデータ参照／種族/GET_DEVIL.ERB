;-------------------------------------------------
;@GET_DEVIL
;ARG의 캐릭터가 악마인지 아닌지를 반환한다
;ARG:1で指定していた場合はNOを見る
;-------------------------------------------------
@GET_DEVIL, ARG, ARG:1 = -1
#FUNCTION
ARG:2 = ARG:1 >= 0 ? CSVABL(ARG:1, GETNUM(ABL, "종족"), 0) # ABL:ARG:종족
;人間はのぞく
SIF ARG:2 == 0
	RETURNF 0
;半魔人はのぞく
SIF ARG:2 == 36
	RETURNF 0
;造魔はのぞく
SIF ARG:2 == 45
	RETURNF 0
RETURNF 1