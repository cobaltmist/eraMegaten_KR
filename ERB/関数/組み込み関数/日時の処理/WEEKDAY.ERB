;----------------------------------------------------------
;@WEEKDAY(ARG)
;ARG번의 요일을 반환한다
;----------------------------------------------------------
@WEEKDAY(ARG)
#FUNCTIONS
SELECTCASE ARG
	CASE 0
		RETURNF "일"
	CASE 1
		RETURNF "월"
	CASE 2
		RETURNF "화"
	CASE 3
		RETURNF "수"
	CASE 4
		RETURNF "목"
	CASE 5
		RETURNF "금"
	CASE 6
		RETURNF "토"
ENDSELECT
RETURNF "？"