SIF NO:LOCAL >= 3701 && NO:LOCAL <= 3707
	CALL TYEFEI_TENSEI,ARG,810
	;엔젤転生処理
SIF NO:LOCAL >= 3711 && NO:LOCAL <= 3718
	CALL TYEFEI_TENSEI,ARG,301

IF ABL:LOCAL:종족 == 0
	BASE:LOCAL:LV = 5
ELSE
	BASE:LOCAL:LV = 1
ENDIF

	;元が비전투원または랜덤キャラ(No.4901〜4920)ならベース스테이터스を保管
IF CSVTALENT(NO:LOCAL,230,0) == 1 || (4900 < NO:LOCAL  && 4921 > NO:LOCAL)
	FOR LOCAL:1,1,7
		BASE:LOCAL:GET_BASESTATUS(LOCAL:1) = 3
		CFLAG:LOCAL:(GET_BASESTATUS(LOCAL:1) + "강화횟수") = 0
	NEXT
	;元が비전투원または랜덤キャラ以外ならベース스테이터스をCSVから読み込み
ELSE
	FOR LOCAL:1,1,7
		BASE:LOCAL:GET_BASESTATUS(LOCAL:1) = CSVBASE(NO:LOCAL,GETNUM(BASE,GET_BASESTATUS(LOCAL:1)),0)
		CFLAG:LOCAL:(GET_BASESTATUS(LOCAL:1) + "강화횟수") = 0
	NEXT
ENDIF



	;能力値の합계を計算
LOCAL:2 = (18 + BASE:LOCAL:LV * 3 / 2)

FOR LOCAL:1,1,7
	;레벨に応じて能力値増減
	BASE:LOCAL:(GET_BASESTATUS(LOCAL:1)) *= LOCAL:2
	BASE:LOCAL:(GET_BASESTATUS(LOCAL:1)) /= (18 + CSVBASE(NO:LOCAL,GETNUM(BASE,"LV"),0) * 3 / 2)
NEXT

	;기본能力値합계が레벨分より低い場合差分を補填
