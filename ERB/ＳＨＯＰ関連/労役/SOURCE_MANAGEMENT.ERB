LOCAL:2 = 0
LOCAL:3 = 0

;절정Ｃ
IF CUP:ARG:0 + PALAM:ARG:쾌Ｃ >= PALAMLV:4 * 2
	LOCAL= 2
	CDOWN:ARG:0 = (PALAMLV:4 * 2 - 1000)
ELSEIF CUP:ARG:0 + PALAM:ARG:쾌Ｃ >= PALAMLV:4
	LOCAL= 1
	CDOWN:ARG:0 = (PALAMLV:4 - 1000)
ENDIF
;CDOWN:ARG:0で下げても絶頂以上なら
;その値-1になるように調整（10000で絶頂なら9999）
SIF CUP:ARG:0 + PALAM:ARG:쾌Ｃ - CDOWN:ARG:0 >= PALAMLV:4
	CDOWN:ARG:0 = CUP:ARG:0 + PALAM:ARG:쾌Ｃ - PALAMLV:4 + 1

;절정Ｖ
IF CUP:ARG:1 + PALAM:ARG:쾌Ｖ >= PALAMLV:4 * 2
	LOCAL:1 = 2
	CDOWN:ARG:1 = (PALAMLV:4 * 2 - 1000)
ELSEIF CUP:ARG:1 + PALAM:ARG:쾌Ｖ >= PALAMLV:4
	LOCAL:1 = 1
	CDOWN:ARG:1 = (PALAMLV:4 - 1000)
ENDIF
