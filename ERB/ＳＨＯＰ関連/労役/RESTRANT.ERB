;==========================================================
;@CORRECT_DIFFICULTY
; 노역단가／매각가격に難易度보정を掛ける
;<引数>
;	ARG   = 노역단가／매각가격
;	ARG:1 = 보정情報표시有無(0:非표시／1:표시)
;<戻り値>
;	보정後가격
;==========================================================
@CORRECT_DIFFICULTY, ARG, ARG:1 = 0
#LOCALSIZE 1
LOCAL = ARG
;EASY
IF FLAG:조교난이도 == 1
	SIF ARG:1
		PRINTFORML 난이도:EASY ×120％
	TIMES LOCAL , 1.20
;NORMAL
ELSEIF FLAG:조교난이도 == 2
	TIMES LOCAL , 1.00
;HARD
ELSEIF FLAG:조교난이도 == 3
	SIF ARG:1
		PRINTFORML 난이도:HARD ×80％
	TIMES LOCAL , 0.80
ENDIF
SIF ARG:1
	PRINTL

RETURN LOCAL

;==========================================================
;@CORRECT_PROCESSION_CHARA
; 노역단가／매각가격に引継ぎキャラ보정を掛ける
;<引数>
;	ARG   = 대상キャラ
;	ARG:1 = 노역단가／매각가격
;	ARG:2 = 보정情報표시有無(0:非표시／1:표시)
;<戻り値>
;	보정後가격
;==========================================================
@CORRECT_PROCESSION_CHARA, ARG, ARG:1, ARG:2 = 0
#LOCALSIZE 1
LOCAL = ARG:1
IF CFLAG:ARG:인계횟수 > 0 && FLAG:5 < 9 && DAY < 51
;MOD 015a 주회횟수で減額率を변경
	IF FLAG:주회횟수 > 11
		SIF ARG:2
			PRINTFORML 인계된 캐릭터： × 0.50
		LOCAL /= 2
	ELSEIF FLAG:주회횟수 > 8
		SIF ARG:2
			PRINTFORML 인계된 캐릭터： × 0.35
		LOCAL *= 7
		LOCAL /= 20
	ELSEIF FLAG:주회횟수 > 5
		SIF ARG:2
			PRINTFORML 인계된 캐릭터： × 0.20
		LOCAL /= 5
	ELSEIF FLAG:주회횟수 > 2
		SIF ARG:2
			PRINTFORML 인계된 캐릭터： × 0.10
		LOCAL /= 10
	ELSE
		SIF ARG:2
			PRINTFORML 인계된 캐릭터： × 0.05
		LOCAL /= 20
	ENDIF
;MOD 015a END
	SIF LOCAL < 1
		LOCAL = 1
ENDIF

RETURN LOCAL


;==========================================================
;@NTR_RESTRANT_SEX
; ＮＴＲ섹스計算
;<引数>
;	ARG   = 노역キャラの登録番号
;	ARG:1 = ＮＴＲ相手番号
;LOCAL:1 = 相手の사정횟수
;LOCAL:2 = 相手の성교횟수
;LOCAL:3 = プレイ内容
;LOCAL:4 = 윤간人数
;LOCAL:5 = 호감도
;LOCAL:6 = 음란도
;LOCAL:7 = 호감도減少処理用
;==========================================================
@NTR_RESTRANT_SEX, ARG, ARG:3, ARG:4
#LOCALSIZE 8
#DIM LCOUNT
CFLAG:ARG:펠라횟수 = 0
CFLAG:ARG:본방횟수 = 0
CFLAG:ARG:애널횟수 = 0
CFLAG:ARG:질내사정횟수 = 0
CFLAG:ARG:수음횟수 = 0
TCVAR:ARG:획득키스경험 = 0
LOCAL:3 = ARG:3 , ARG:4
;ＮＴＲ好きの客（本番）
IF LOCAL:3 == 1
	ARG:1 = -2
	CFLAG:ARG:질내사정횟수 = 1
	CFLAG:ARG:본방횟수 = 1
	LOCAL:1 = 2 + RAND:4
	LOCAL:2 = LOCAL:1 + RAND:(LOCAL:1)
	TCVAR:ARG:획득바람경험 = 1

;クレーム客（本番）
ELSEIF LOCAL:3 == 2
	ARG:1 = -3
	CFLAG:ARG:질내사정횟수 = LOCAL:4
	CFLAG:ARG:본방횟수 = LOCAL:4
	LOCAL:1 = LOCAL:4 + RAND:(LOCAL:4)
	LOCAL:2 = LOCAL:1 + RAND:(LOCAL:4)
	TCVAR:ARG:획득윤간경험 = 1

;ＮＴＲ好きの客（フェラ）
ELSEIF LOCAL:3 == 3
	ARG:1 = -2
	CFLAG:ARG:펠라횟수 = 1 + RAND:3
	TCVAR:ARG:획득바람경험 = 1

;クレーム客（フェラ）
ELSEIF LOCAL:3 == 4
	ARG:1 = -3
	CFLAG:ARG:펠라횟수 = 3 + RAND:3

ENDIF

IF LOCAL:3 < 3
	FOR LCOUNT , 0 , LOCAL:2
		IF RAND:100 < 60
			IF HAVE_PENIS(ARG) && HAVE_VAGINA(ARG)
				CFLAG:ARG:본방횟수 += 1
				CFLAG:ARG:수음횟수 += 1
			ELSEIF HAVE_PENIS(ARG)
				CFLAG:ARG:애널횟수 += 1
				CFLAG:ARG:수음횟수 += 1
			ELSE
				CFLAG:ARG:본방횟수 += 1
			ENDIF
		ELSEIF RAND:100 < 50
			IF HAVE_PENIS(ARG)
				CFLAG:ARG:수음횟수 += 1
			ELSE
				CFLAG:ARG:펠라횟수 += 1
			ENDIF
		ELSE
			IF HAVE_PENIS(ARG) && HAVE_VAGINA(ARG)
				CFLAG:ARG:애널횟수 += 1
				CFLAG:ARG:수음횟수 += 1
			ELSEIF HAVE_PENIS(ARG)
				CFLAG:ARG:애널횟수 += 1
				CFLAG:ARG:수음횟수 += 1
			ELSE
				CFLAG:ARG:애널횟수 += 1
			ENDIF
		ENDIF
	NEXT
	CFLAG:ARG:질내사정횟수 += LIMIT(RAND:(1+CFLAG:ARG:본방횟수) , 0, LOCAL:1)

	;키스경험処理：適当に랜덤で
	SIF RAND:2 == 0
		TCVAR:ARG:획득키스경험 += 1
ENDIF

;획득경험
TCVAR:ARG:획득펠라경험 = CFLAG:ARG:펠라횟수
TCVAR:ARG:획득봉사쾌락경험 = CFLAG:ARG:펠라횟수
TCVAR:ARG:획득Ｖ경험 = CFLAG:ARG:본방횟수
TCVAR:ARG:획득Ａ경험 = CFLAG:ARG:애널횟수
TCVAR:ARG:획득성교경험 = CFLAG:ARG:본방횟수 + CFLAG:ARG:애널횟수
TCVAR:ARG:획득질사경험 = CFLAG:ARG:질내사정횟수
TCVAR:ARG:획득정액경험 = LOCAL:1
IF TCVAR:ARG:획득윤간경험
	TCVAR:ARG:획득고통쾌락경험 = (CFLAG:ARG:본방횟수 + CFLAG:ARG:애널횟수) / 2
	TCVAR:ARG:획득피사경험 = (CFLAG:ARG:본방횟수 + CFLAG:ARG:애널횟수) / 2
ENDIF
SIF CFLAG:ARG:수음횟수 > 0
	TCVAR:ARG:획득사정경험 = RAND:(CFLAG:ARG:수음횟수)
SIF TCVAR:ARG:획득성교경험 + TCVAR:ARG:획득사정경험 > 0
	TCVAR:ARG:획득절정경험 = RAND:(TCVAR:ARG:획득성교경험 + TCVAR:ARG:획득사정경험)
;임신用
SIF CFLAG:ARG:자궁내피임결계 == 0
	CFLAG:ARG:고객의질내사정에의한임신판정 = TCVAR:ARG:획득질사경험*2
;처녀상실処理
IF TALENT:ARG:처녀 && CFLAG:ARG:본방횟수
	CALL ADDS_ABNORMAL_EXP, "처녀상실（윤간）", ARG
	CALL ADDS_ABNORMAL_EXP, "처녀상실（촬영）", ARG
	SETCOLOR COLOR("red")
	IF LOCAL:3 == 2
		PRINTFORML 【＋２】   처녀상실（윤간）
		PRINTFORML 【＋２】   처녀상실（촬영）
	ELSEIF LOCAL:3 == 1
		PRINTFORML 【＋２】   처녀상실(ＮＴＲ)
	ENDIF
	RESETCOLOR
	TALENT:ARG:처녀 = 0
ELSEIF TALENT:ARG:재생처녀
	PRINTFORML 재생처녀상실
	TALENT:ARG:재생처녀 = 0
ENDIF

LOCAL:5 = TCVAR:ARG:획득정액경험 * 5 + (TCVAR:ARG:획득절정경험 + TCVAR:ARG:획득질사경험) * 10 + CFLAG:ARG:방치일수
LOCAL:6 = CFLAG:ARG:본방횟수 + CFLAG:ARG:애널횟수 + TCVAR:ARG:획득절정경험 * 10
LOCAL:7 = CFLAG:ARG:질내사정횟수 * 3 + TCVAR:ARG:획득절정경험 * 5


;ＮＴＲ손님의설정処理
SELECTCASE FLAG:ＮＴＲ손님의설정
	CASE  0
		TIMES LOCAL:5 , 1.00
		TIMES LOCAL:6 , 1.00
		TIMES LOCAL:7 , 1.00
	CASE  1
		TIMES LOCAL:5 , 2.00
		TIMES LOCAL:6 , 2.00
		TIMES LOCAL:7 , 2.00
	CASE  2
		TIMES LOCAL:5 , 5.00
		TIMES LOCAL:6 , 5.00
		TIMES LOCAL:7 , 5.00
ENDSELECT

;자궁현재용량加算
IF TALENT:ARG:오토코노코
	CFLAG:(ARG):자궁현재용량 += LOCAL:7 * 2
ELSEIF !IS_MALE(ARG)
	CFLAG:(ARG):자궁현재용량 += LOCAL:7 * 2
ENDIF

;호감도
IF ARG:1 == -2
	CDFLAG:ARG:캐릭터간호감도:(98) += LOCAL:5
ELSEIF ARG:1 == -3
	CDFLAG:ARG:캐릭터간호감도:(97) -= LOCAL:5
ENDIF

;음란도上昇
;바람癖がある場合は음란도倍化
IF TALENT:ARG:바람기
	CFLAG:ARG:음란도 += LOCAL:6*2
ELSE
	CFLAG:ARG:음란도 += LOCAL:6
ENDIF

;호감도減少処理
CALL NTR_SHOOT , ARG, ARG:1, LOCAL:7

CALL SHOW_ADD_EXP, ARG


