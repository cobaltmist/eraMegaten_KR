@MouseUIStore_Is_Payable(aPrice, rCurrencies)
#FUNCTION
#LOCALSIZE 2
#DIMS REF rCurrencies, 0
#DIM aPrice
;그외
FOR LOCAL, 0, VARSIZE("rCurrencies")
	LOCAL:1 = MouseUIStore_CurMoney(rCurrencies:LOCAL)
	IF LOCAL:1 >= aPrice
		RETURNF 1
	ENDIF
	aPrice -= LOCAL:1
NEXT
RETURNF 0



