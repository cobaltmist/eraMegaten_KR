@KUZI,ARG
#DIM ITEMID = 3204,3608,5203,1402,1010,1011,1012,1013,1014,1015,1029,1025,1026,1027,1104,1106,1108,1122,1124,1126,1128,1130,1132,1007,1050,1051,1052,1053,1041,1047,1100,1030,1008,1009,1007,1004,1031,1032,1033,1034,1035,1036,1040,1103,1105,1107,1109,1110,1111,1112,1120,1115,1116,1117,1118,5512,5521,41,40,5503,5504,5509,5510,5505,5506,5507,5508,5515,5500,5501,5502,5511,5513,5514,5516,5517,5518,5519,5520,1412
#DIM ITEMNUM,80
#DIM LCOUNT

VARSET ITEMNUM,0
LOCAL = 0
FOR LCOUNT,0,ARG
	LOCAL = RAND:100
	SELECTCASE LOCAL
		CASE IS < 5
			;5/100으로 레어 장비
			LOCAL = RAND:3
			SELECTCASE LOCAL
				CASE 0
					;휴대용헤드폰
					ITEMNUM:0 += 1
				CASE 1
					;프로스트수트
					ITEMNUM:1 += 1
				CASEELSE
					;눈꽃
					ITEMNUM:2 += 1
			ENDSELECT
		CASE IS < 15
			;10/95 로 장비상자 
			ITEMNUM:3 += 1
		CASE IS < 65
			;50/100 으로 뭔가 아이템
			;25％로（＝총계10％로）레어아이템을 포함한 테이블
			IF RAND:5 == 0
				LOCAL = RAND:76 + 4
			ELSE
				LOCAL = RAND:52 + 28
			ENDIF

			SELECTCASE LOCAL
				CASE 55 TO 57
					;만능탄、천마의탄환、미약
					ITEMNUM:LOCAL += 10
				CASE 58 TO 67
					;로션、破魔呪殺、状態異常系弾丸
					ITEMNUM:LOCAL += 50
				CASE 68 TO 78
					;속성계 탄환
					ITEMNUM:LOCAL += 25
				CASEELSE
					;그 외  x1
					ITEMNUM:LOCAL += 1
			ENDSELECT
		;10/100で 1 MORE!
		CASE IS < 75
			PRINTW 1 MORE！
			LCOUNT -= 2
		CASEELSE
			;25/100ではずれくじ
			ITEMNUM:79 += 1
	ENDSELECT
NEXT


FOR LCOUNT,0,80
	IF ITEMNUM:LCOUNT > 0
		PRINTFORML %ITEMNAME:(ITEMID:LCOUNT)%ｘ{ITEMNUM:LCOUNT}が当たりました！
		CALL CHECK_ITEMMAX,ITEMID:LCOUNT
		SIF RESULT == 0
			CALL GET_ITEM,ITEMID:LCOUNT,ITEMNUM:LCOUNT
	ELSE
		CONTINUE
	ENDIF
NEXT
PRINTW
RETURN 0



;==========================================================
;@ITEM_GOUSEI_표시
; 아이템合成の표시用
;<引数>
;	ARGS:0 = 合成後のアイテム名
;	ARGS:1 = 一つ目の素材アイテム名
;	ARGS:2 = 二つ目の素材アイテム名
;	ARGS:3 = 三つ目の素材アイテム名
;	ARG:1  = 一つ目の素材アイテムの必要個数
;	ARG:2  = 二つ目の素材アイテムの必要個数
;	ARG:3  = 三つ目の素材アイテムの必要個数
;
;<戻り値>
;	なし
;==========================================================
@ITEM_GOUSEI_표시(ARGS:0, ARGS:1, ARG:1, ARGS:2 = "", ARG:2 = 0, ARGS:3 = "", ARG:3 = 0)
#DIM 아이템소지수, 4

;필요な소재が￥・마카だった場合は置き換える
FOR LOCAL, 1, 4
	SIF ARGS:LOCAL == ""
		BREAK
	IF ARGS:LOCAL == "마카"
		아이템소지수:LOCAL = MONEY:1
	ELSEIF ARGS:LOCAL == "￥"
		아이템소지수:LOCAL = MONEY
	ELSE
		아이템소지수:LOCAL = ITEM:(ARGS:LOCAL)
	ENDIF
NEXT

;合成後の아이템名표시
SETCOLOR COLOR("灰色")
IF (ARG:2 == 0 && 아이템소지수:1 >= ARG:1) || (ARG:2 && ARG:3 == 0 && 아이템소지수:1 >= ARG:1 && 아이템소지수:2 >= ARG:2) || (ARG:3 && 아이템소지수:1 >= ARG:1 && 아이템소지수:2 >= ARG:2 && 아이템소지수:3 >= ARG:3)
	RESETCOLOR
ENDIF
PRINTFORM %ARGS:0,26,LEFT%
RESETCOLOR

;一つ눈の소재아이템名표시
IF 아이템소지수:1 >= ARG:1
	SETCOLOR COLOR("하늘색")
ELSEIF 아이템소지수:1 < ARG:1
	SETCOLOR COLOR("赤")
ENDIF
;PRINTFORM (필요한 소재:%ARGS:1,30,LEFT%({아이템소지수:1}/{ARG:1})
PRINTFORM (%ARGS:1,30,LEFT%({아이템소지수:1}/{ARG:1})
IF ARGS:2 == ""
	PRINTL )
;	PRINTL 
ENDIF
RESETCOLOR

;二つ눈の소재がない場合は戻る
SIF ARGS:2 == ""
	RETURN

;二つ눈の소재아이템名표시
IF 아이템소지수:2 >= ARG:2
	SETCOLOR COLOR("하늘색")
ELSEIF 아이템소지수:2 < ARG:2
	SETCOLOR COLOR("赤")
ENDIF
PRINTFORM 　%ARGS:2,20,LEFT%({아이템소지수:2}/{ARG:2})
IF ARGS:3 == ""
	PRINTL )
;	PRINTL 
ENDIF
RESETCOLOR

;三つ눈の소재がない場合は戻る
SIF ARGS:3 == ""
	RETURN

;三つ눈の소재아이템名표시
IF 아이템소지수:3 >= ARG:3
	SETCOLOR COLOR("하늘색")
ELSEIF 아이템소지수:3 < ARG:3
	SETCOLOR COLOR("赤")
ENDIF
PRINTFORML 　%ARGS:3,20,LEFT%({아이템소지수:3}/{ARG:3}))
RESETCOLOR
