@CLOTHES_NAME_520
RESULTS = 니플피어스

;노출부위
@CLOTHES_EXPOSE_520,ARG
;ARG = 노출되는부위
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = 배꼽노출 7 = 음순노출 8 = 둔부노출
SELECTCASE ARG
	CASE 0
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비부위
@CLOTHES_PART_520
RETURN 5

;옷의 앞쪽이 열리는가
;1 열 수 있다、2 열린 채
@CLOTHES_OPENFRONT_520
RETURN 0

;고간의구조
;1 = 바지、2 = 비키니타입、3 = 스커트、4 = 구멍뚫림、5 앞열림、6 개폐가능
@CLOTHES_CROTCH_520
RETURN 0

;장비시의 조교효과
@CLOTHES_SOURCE_520,ARG
IF FLAG:SHOP커맨드 == 0
	SOURCE:ARG:아픔 += 25
	IF ARG == TARGET
		DOWNBASE:0 += 10
		DOWNBASE:1 += 10
	ENDIF
	IF SOURCE:ARG:쾌Ｂ > 0
		TIMES SOURCE:ARG:쾌Ｂ , 1.20
		IF ARG == TARGET
			TIMES DOWNBASE:ARG:0 , 1.30
			TIMES DOWNBASE:ARG:1 , 1.10
		ENDIF
	ENDIF
ELSE

ENDIF

;장비시의 경험추가
@CLOTHES_EXP_520,ARG
IF FLAG:SHOP커맨드 == 0
	SIF ARG == -1
		RETURN 0
	PRINTFORML <니플피어스착용중>\@ ARG == MASTER ? (주인) #\@\@ ARG == ASSI ? (조수) #\@
	IF TCVAR:ARG:획득고통쾌락경험
		TCVAR:ARG:획득고통쾌락경험 += MAX(TCVAR:ARG:획득고통쾌락경험*140/100 , 1)
		SIF ARG == TARGET
			TIMES DOWNBASE:ARG:1 , 1.10
	ENDIF
ELSE

ENDIF


;装備するための条件
@CLOTHES_PREREQUISITE_520,ARG
SIF ARG == MASTER
	RETURN 1
SIF MARK:ARG:고통각인 < 3
	RETURN 0
SIF ABL:ARG:순종 >= 3
	RETURN 1
SIF ABL:ARG:마조끼 >= 2
	RETURN 1
RETURN 0

@CLOTHES_PRICE_520
RETURN 1500
