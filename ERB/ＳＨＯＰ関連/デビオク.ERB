			ELSE
				TCVAR:ARG:획득성교경험 += 1
				;約７０％の確立で절정
				IF RAND:100 < 30
					TCVAR:ARG:획득사정경험 += 1
					TCVAR:ARG:획득절정경험 += 1
					TCVAR:ARG:획득윤간경험 += 1
				ENDIF
				
			ENDIF
