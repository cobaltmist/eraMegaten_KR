@MASTER_MUSUME
CALL INPUT_CHARA_LIST("마스터로 할 캐릭터를 골라주세요", "MASTER_BLOOD")
IF RESULT == 1000
	RETURN 0
ENDIF

LOCAL = RESULT
COPYCHARA LOCAL,0
CALL 캐릭터삭제, LOCAL

@MASTER_BLOOD(ARG)
SIF TALENT:ARG:마스터의혈통
	RETURN 1
SIF ARG == MASTER
	RETURN 0
SIF CFLAG:ARG:이자리에없는플래그 == 1
	RETURN 0
RETURN 0
