;ARGは生成キャラの등록번호
;2020/03/20　PS構成員はあんまり변경なし。
;이능자스킬のコンセプトは小学生を参照
@RANDOMCHARA_4901, ARGS, ARG, ARG:1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "일인칭"
		RESULTS = 나
	CASE "이인칭"
		RESULTS = %CALLNAME:MASTER%
	CASE "체력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1500, 3400)
		ELSE
			RETURN RAND(1000, 2400)
		ENDIF
	CASE "기력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1500, 2800)
		ELSE
			RETURN RAND(1000, 1800)
		ENDIF
	CASE "속성"
		RETURN RAND(0, 127), RAND(128, 255)
	CASE "종족"
		RETURN 0
	CASE "전투소질"
		IF RAND:4 > 1
			RESULTS = 서머너
			RESULTS:1 = 도구지식
			RESULTS:2 = 석세서
			RESULTS:3 = 건슬링거
			RETURN 5, 3, 1, 1
		ELSEIF RAND:4 == 0
			RESULTS = 이능자
			RESULTS:1 = 서머너
			RETURN 1, 1
		ELSE
			RESULTS = 비전투원
			RETURN 1
		ENDIF
	;戻り値は0:랜덤 それ이외は指定
	CASE "초기페르소나"
		RETURN 0
	;戻り値は0:랜덤 それ이외は指定
	CASE "링크악마"
		RETURN 0
	;戻り値は0:랜덤 それ이외は指定
	CASE "초기스테이터스비율"
		RETURN 10, 10, 8, 7, 7, 5
	CASE "스킬"
		SIF TALENT:ARG:이능자 == 0
			RETURN 0
		
		;이능자스킬
		;공격
		SELECTCASE RAND:6
			CASE 0
				RESULT = [[스킬:아기]]
				RESULT:1 = [[스킬:아기라오]]
				RESULT:2 = [[스킬:아기다인]]
				RESULT:3 = [[스킬:데들리번]]
			CASE 1
				RESULT = [[스킬:부흐]]
				RESULT:1 = [[스킬:부흐라]]
				RESULT:2 = [[스킬:부흐다인]]
				RESULT:3 = [[스킬:아이시클]]
			CASE 2
				RESULT = [[스킬:지오]]
				RESULT:1 = [[스킬:지온가]]
				RESULT:2 = [[스킬:지오다인]]
				RESULT:3 = [[스킬:나루카미]]
			CASE 3
				RESULT = [[스킬:잔]]
				RESULT:1 = [[스킬:잔마]]
				RESULT:2 = [[스킬:잔다인]]
				RESULT:3 = [[스킬:림도라온]]
			CASE 4
				RESULT = [[스킬:산바람]]
				RESULT:1 = [[스킬:히트웨이브]]
				RESULT:2 = [[스킬:데스바운드]]
				RESULT:3 = [[스킬:사마리캄]]
			CASE 5
				RESULT = [[스킬:베어내기]]
				RESULT:1 = [[스킬:히트웨이브]]
				RESULT:2 = [[스킬:무사차]]
				RESULT:3 = [[스킬:산산조각베기]]
		ENDSELECT
		;カジャ系
		SELECTCASE RAND:4
			CASE 0
				RESULT:4 = [[스킬:타루・카쟈]]
				RESULT:5 = [[스킬:다크브레스]]
			CASE 1
				RESULT:4 = [[스킬:스쿠・카쟈]]
				RESULT:5 = [[스킬:도발]]
			CASE 2
				RESULT:4 = [[스킬:라쿠・카쟈]]
				RESULT:5 = [[스킬:파워브레스]]
			CASE 3
				RESULT:4 = [[스킬:사마・카쟈]]
				RESULT:5 = [[스킬:위협]]
		ENDSELECT
		;魔脈/コンセ
		IF RAND:2 > 0
			RESULT:6 = [[스킬:일할의마맥]]
		ELSE
			RESULT:6 = [[스킬:컨센트레이트]]
		ENDIF
		;만능
		RESULT:7 = [[스킬:메기도]]
		RESULT:8 = [[스킬:메기도라온]]
		;PS構成員추가스킬
		SELECTCASE RAND:2
			CASE 0
				RESULT:9 = [[스킬:싱글샷]]
			CASE 1
				RESULT:9 = [[스킬:퀵드로]]
		ENDSELECT
		RESULT:10 = [[스킬:연속사격]]
		RESULT:11 = [[스킬:산탄사격]]
		RETURN RESULT
	CASE "상성"
		SELECTCASE GET_TYPE(ARG:1)
			CASE "파마"
				RETURN 0
			CASEELSE
				RETURN 100
		ENDSELECT
	CASE "소질"
		RETURN GETNUM(TALENT, "무관심"), GETNUM(TALENT, "자제심"), GETNUM(TALENT, "해방"), GETNUM(TALENT, "도착적"), GETNUM(TALENT, "바이"), GETNUM(TALENT, "소악마"), GETNUM(TALENT, "쾌감에솔직")
	CASE "제외소질"
		RETURN GETNUM(TALENT, "정조관념"), GETNUM(TALENT, "아픔에약함"), GETNUM(TALENT, "겁쟁이")
	;처녀は特別 0は랜덤(20%), 1なら처녀, 2なら非처녀
	CASE "처녀"
	;戻り値は0:랜덤 1:소인체형 2:유아체형 4:큰체형 5:거체
	CASE "체형"
		RETURN 0
	;戻り値は0:랜덤 1:절벽　2:빈유　3:並　4:거유　5:폭유
	CASE "가슴"
		RETURN 0
	;추가対応。戻り値は0:랜덤 1:小尻　2:並尻　3:巨尻　4:爆尻
	CASE "尻"
		RETURN 0
	;통상の인간と肉体部位が違う場合はここで直接代入してください
	CASE "육체부위"
		RETURN 0
	CASE "ABL"
		SELECTCASE ABLNAME:(ARG:1)
			CASE "순종", "욕망", "기교"
				RETURN !RAND:3+!RAND:5
			CASE "Ｃ감각", "Ｖ감각", "Ｂ감각"
				RETURN !RAND:3
			CASE "마조끼", "노출증", "봉사정신"
				RETURN !RAND:5
			CASE "새드끼"
				RETURN !RAND:4 * !ABL:ARG:마조끼
			CASE "Ａ감각", "자위중독", "정액중독", "레즈끼", "ＢＬ끼", "섹스중독"
				RETURN !RAND:10
		ENDSELECT
	;EXPはABLが1이상ある場合、ある程度調整される(ただし、ABL0の時は調整が動かないので適当に초기値をきめてください)
	CASE "EXP"
		;全く손付かずの場合
		SIF !GROUPMATCH(1, !TALENT:ARG:처녀, ABL:ARG:Ｃ감각, ABL:ARG:Ｖ감각, ABL:ARG:Ａ감각, ABL:ARG:Ｂ감각, ABL:ARG:기교)
			RETURN 0
		SELECTCASE EXPNAME:(ARG:1)
			CASE "Ｖ경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "Ａ경험"
				RETURN RAND(0, 20) * !RAND:5
			CASE "절정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "사정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "성교경험"
				RETURN 0
			CASE "질사경험"
				RETURN EXP:ARG:사정경험/2*EXP:ARG:성교경험 > 0
			CASE "자위경험"
				RETURN RAND(0, 20)
			CASE "조교자위경험"
				RETURN 0
			CASE "정액경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "봉사쾌락경험"
				RETURN 0
			CASE "펠라경험"
				RETURN RAND(0, 10) * RAND:2
			CASE "애정경험"
				RETURN 0
			CASE "고통쾌락경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "방뇨경험"
				RETURN 0
			CASE "가학쾌락경험"
				RETURN RAND(0, 10) * !RAND:4
			CASE "야외노출경험"
				RETURN 0
			CASE "레즈경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "ＢＬ경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "이상경험"
				RETURN 0
			CASE "긴박경험"
				RETURN RAND(0, 10) * EXP:ARG:고통쾌락경험 > 0
		ENDSELECT
	CASE "모자"
		IF IS_MALE(ARG)
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:모자]]
				CASE 1
					RETURN [[衣装:반다나]]
				CASE 2
					RETURN [[衣装:후드]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:모자]]
				CASE 1
					RETURN [[衣装:리본]]
				CASE 2
					RETURN [[衣装:헤어밴드]]
				CASE 3
					RETURN [[衣装:카츄샤]]
				CASE 4
					RETURN [[衣装:머리끈]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ENDIF
	CASE "옷"
		IF IS_MALE(ARG)
			SELECTCASE RAND:3
				CASE 0
					RETURN [[衣装:겉옷]]
				CASE 1
					RETURN [[衣装:와이셔츠]]
				CASE 2
					RETURN [[衣装:T셔츠]]
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:겉옷]]
				CASE 1
					RETURN [[衣装:터틀넥]]
				CASE 2
					RETURN [[衣装:캐미솔]]
				CASE 3
					RETURN [[衣装:블라우스]]
				CASE 4
					RETURN [[衣装:T셔츠]]
			ENDSELECT
		ENDIF
	CASE "하의"
		IF IS_MALE(ARG)
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:바지]]
				CASE 1
					RETURN [[衣装:레더팬츠]]
				CASE 2
					RETURN [[衣装:슬랙스]]
				CASE 3
					RETURN [[衣装:카고팬츠]]
				CASE 4
					RETURN [[衣装:치노팬츠]]
			ENDSELECT
		ELSE
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:스커트]]
				CASE 1
					RETURN [[衣装:롱스커트]]
				CASE 2
					RETURN [[衣装:미니스커트]]
				CASE 3
					RETURN [[衣装:타이트스커트]]
				CASE 4
					RETURN [[衣装:숏팬츠]]
				CASE 5
					RETURN [[衣装:플리츠스커트]]
				CASE 6
					RETURN [[衣装:플레어스커트]]
				CASE 7
					RETURN [[衣装:타이트미니스커트]]
				CASE 8
					RETURN [[衣装:핫팬츠]]
				CASE 9
					RETURN [[衣装:퀼로트]]
				CASEELSE
					RETURN [[衣装:바지]]
			ENDSELECT
		ENDIF
	CASE "전신복"
		RETURN 0
	CASE "손"
		RETURN 0
	CASE "속옷（상）"
		IF IS_MALE(ARG)
			RETURN 0
		ELSE
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:브래지어]]
				CASE 1
					RETURN [[衣装:브래지어(흑)]]
				CASE 2
					RETURN [[衣装:브래지어(적)]]
				CASE 3
					RETURN [[衣装:레이스달린브래지어]]
				CASE 4
					RETURN [[衣装:니플리스]]
			ENDSELECT
		ENDIF
	CASE "속옷（하）"
		IF IS_MALE(ARG)
			IF RAND:5
				RETURN [[衣装:트렁크스]]
			ELSE
				RETURN [[衣装:부메랑팬츠]]
			ENDIF
		ELSE
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:팬티]]
				CASE 1
					RETURN [[衣装:팬티(흑)]]
				CASE 2
					RETURN [[衣装:팬티(적)]]
				CASE 3
					RETURN [[衣装:레이스달린팬티]]
				CASE 4
					RETURN [[衣装:로우라이즈]]
			ENDSELECT
		ENDIF
	CASE "전신속옷"
		RETURN 0
	CASE "양말"
		IF IS_MALE(ARG)
			RETURN [[衣装:양말]]
		ELSE
			SELECTCASE RAND:7
				CASE 0
					RETURN [[衣装:하이삭스]]
				CASE 1
					RETURN [[衣装:양말]]
				CASE 2
					RETURN [[衣装:니삭스]]
				CASE 3
					RETURN [[衣装:사이하이삭스]]
				CASE 4
					RETURN [[衣装:가터스타킹]]
				CASE 5
					RETURN [[衣装:팬티스타킹]]
				CASE 6
					RETURN [[衣装:검은색타이즈]]
			ENDSELECT
		ENDIF
	CASE "신발"
		IF IS_MALE(ARG)
			IF RAND:2 == 0
				RETURN [[衣装:스니커]]
			ELSE
				RETURN [[衣装:가죽구두]]
			ENDIF
		ELSE
			SELECTCASE RAND:7
				CASE 0
					RETURN [[衣装:슈즈]]
				CASE 1
					RETURN [[衣装:부츠]]
				CASE 2
					RETURN [[衣装:샌들]]
				CASE 3
					RETURN [[衣装:뮬]]
				CASE 4
					RETURN [[衣装:펌프스]]
				CASE 6
					RETURN [[衣装:하이부츠]]
				CASE 7
					RETURN [[衣装:숏부츠]]
			ENDSELECT
		ENDIF
	CASE "회화타입"
	;악마汎用사용しています0:랜덤 1:女_아이　2:女_소녀　3:女_청소년　4:女_성인　5:女_여성　6:女_고풍　7:男_청소년　8:男_성인　9:짐승_짐승
		RETURN 0
	CASE "이름"
		$SET_NAME
		RESULTS = P・S구성원
	CASE "애칭"
		;이름と同じため생략
		GOTO SET_NAME
	CASE "해설"
		RESULTS = 팬텀 소사이어티의 구성원
		IF TALENT:ARG:이능자
			RESULTS:1 = 이능자이며, 그 힘으로 조직에 공헌해 왔다
		ELSEIF TALENT:ARG:비전투원
			RESULTS:1 = 비전투원이었고, 주로 백업을 담당했다
		ELSE
			RESULTS:1 = 다크서머너로 중마와 함께 조직에 공헌해 왔다
		ENDIF
	;最後に呼び出すので何らかの調整が필요한場合はここで
	CASE "最終調整"
		RETURN 0
ENDSELECT
