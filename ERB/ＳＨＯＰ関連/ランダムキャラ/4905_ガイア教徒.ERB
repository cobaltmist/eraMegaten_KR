@RANDOMCHARA_4905, ARGS, ARG, ARG:1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "일인칭"
		IF TALENT:ARG:남자 == 1
			SELECTCASE RAND:4
				CASE 0
					RESULTS = 이 몸
				CASE 1
					RESULTS = 나
				CASE 2
					RESULTS = 나
				CASE 3
					RESULTS = 나
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
				CASE 0
					RESULTS = 나
				CASE 1
					RESULTS = 저
				CASE 2
					RESULTS = 저
				CASE 3
					RESULTS = 본녀
				CASE 4
					RESULTS = 본녀
			ENDSELECT
		ENDIF
	CASE "이인칭"
		RESULTS = %CALLNAME:MASTER%
	CASE "체력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1700, 3400)
		ELSE
			RETURN RAND(1200, 2400)
		ENDIF
	CASE "기력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2500)
		ELSE
			RETURN RAND(800, 1500)
		ENDIF
	CASE "속성"
		RETURN RAND(50, 120), RAND(0, 85)
	CASE "종족"
		RETURN 0
	CASE "전투소질"
;데빌시프터、モヒカン系、닌자系、破戒僧系で大別 비전투원は存在しない
		SELECTCASE RAND:10
			CASE IS < 3
				RESULTS = 데빌시프터
				RESULTS:1 = 도구지식
				RETURN 1, 3
			CASE IS < 5
				RESULTS = 달인
				IF RAND:2 > 0
					RESULTS:1 = 석세서
				ELSE
					RESULTS:1 = 건슬링거
				ENDIF
				RETURN 1, 1
			CASE IS < 7
				RESULTS = 이능자
				RESULTS:1 = 도구지식
					;강화フラグがある場合、1/2で강화。도구지식を2に
					IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
						RETURN 1, 2
					ENDIF
				RETURN 1, 1
			CASEELSE
				RESULTS = 서머너
				RESULTS:1 = 이능자
				RETURN 1, 1
		ENDSELECT
	CASE "초기페르소나"
		RETURN 0
	CASE "링크악마"
		RETURN 0
;		IF TALENT:ARG:데빌시프터 == 1
;			IF RAND:2 == 0
;				;1/2で랜덤リンク
;				RETURN 0
;			ELSE
;				IF TALENT:ARG:남자 == 1
;					SELECTCASE RAND:5
;						CASE 0
;							RETURN [[キャラ:슬라임]]
;						CASE 1
;							RETURN [[キャラ:잭프로스트]]
;						CASE 2
;							RETURN [[キャラ:오니]]
;						CASE 3
;							RETURN [[キャラ:인큐버스]]
;						CASE 4
;							RETURN [[キャラ:이누가미]]
;					ENDSELECT
;				ELSE
;					SELECTCASE RAND:5
;						CASE 0
;							RETURN [[キャラ:보디코니언]]
;						CASE 1
;							RETURN [[キャラ:픽시]]
;						CASE 2
;							RETURN [[キャラ:유키죠로]]
;						CASE 3
;							RETURN [[キャラ:서큐버스]]
;						CASE 4
;							RETURN [[キャラ:네코마타]]
;					ENDSELECT
;				ENDIF
;			ENDIF
;		ELSE
;			RETURN 0
;		ENDIF
	CASE "초기스테이터스비율"
		RETURN 10, 10, 8, 7, 7, 5
	CASE "스킬"
		IF TALENT:ARG:달인 == 1 && TALENT:ARG:이능자 == 0 && TALENT:ARG:석세서 == 1;ローレシアのヒャッハー
				RESULT = [[스킬:관통]]
				RESULT:1 = [[스킬:기합]]
				RESULT:2 = [[스킬:레볼루션]]
				RESULT:3 = [[스킬:타루카쟈오토]]
				RESULT:4 = [[스킬:이신의잔영]]
				RESULT:5 = [[스킬:체인업]]
				RESULT:6 = [[스킬:끝없는도발]]
				RESULT:7 = [[스킬:맹반격]]
				RESULT:8 = [[스킬:삼할의활천]]
				RESULT:9 = [[스킬:추격의심득]]
				RESULT:10 = [[스킬:데스카운터]]
				RESULT:11 = [[스킬:불굴의투지]]
				RETURN RESULT
		ELSEIF  TALENT:ARG:달인 == 1 && TALENT:ARG:이능자 == 0 && TALENT:ARG:건슬링거 == 1;伝承者の落ちこぼれヒャッハー
				RESULT = [[스킬:해피트리거]]
				RESULT:1 = [[스킬:나한장]]
				RESULT:2 = [[스킬:돌려차기]]
				RESULT:3 = [[스킬:파이어브레스]]
				RESULT:4 = [[스킬:독가스브레스]]
				RESULT:5 = [[스킬:아카샤아츠]]
				RESULT:6 = [[스킬:비공찌르기]]
				RESULT:7 = [[스킬:도발]]
				RESULT:8 = [[스킬:플레어샷]]
				RESULT:9 = [[스킬:삼할의활천]]
				RESULT:10 = [[스킬:모탈지하드]]
				RESULT:11 = [[스킬:뉴클리어미사일]]
				RETURN RESULT
		ELSE
			;가이아ーズ스킬
			RESULT = [[스킬:독침]]
			RESULT:1 = [[스킬:마비침]]
			RESULT:2 = [[스킬:몽견침]]
			;이능자스킬
			;공격
			SELECTCASE RAND:5
				CASE 0
					RESULT:3 = [[스킬:번개찌르기]]
					RESULT:4 = [[스킬:베놈재퍼]]
					RESULT:5 = [[스킬:회전베기]]
					RESULT:6 = [[스킬:치유촉진(소)]]
				CASE 1
					RESULT:3  = [[스킬:돌격]]
					RESULT:4 = [[스킬:마구날뛰기]]
					RESULT:5 = [[스킬:돌려차기]]
					RESULT:6 = [[스킬:이악물기]]
				CASE 2
					RESULT:3 = [[스킬:테트라칸]]
					RESULT:4 = [[스킬:무드]]
					RESULT:5 = [[스킬:마하무드]]
					RESULT:6 = [[스킬:무드온]]
				CASE 3
					RESULT:3  = [[스킬:지오]]
					RESULT:4 = [[스킬:지온가]]
					RESULT:5 = [[스킬:지오다인]]
					RESULT:6 = [[스킬:마카라칸]]
				CASE 4
					RESULT:3  = [[스킬:잔]]
					RESULT:4 = [[스킬:잔마]]
					RESULT:5 = [[스킬:잔다인]]
					RESULT:6 = [[스킬:마카라칸]]
			ENDSELECT
			;カジャ系
			SELECTCASE RAND:4
				CASE 0
					RESULT:7 = [[스킬:타루・카쟈]]
					RESULT:8 = [[스킬:다크브레스]]
				CASE 1
					RESULT:7 = [[스킬:스쿠・카쟈]]
					RESULT:8 = [[스킬:도발]]
				CASE 2
					RESULT:7 = [[스킬:라쿠・카쟈]]
					RESULT:8 = [[스킬:파워브레스]]
				CASE 3
					RESULT:7 = [[스킬:사마・카쟈]]
					RESULT:8 = [[스킬:위협]]
			ENDSELECT
			;魔脈/コンセ
			IF RAND:2 > 0
				RESULT:9 = [[스킬:일할의마맥]]
			ELSE
				RESULT:9 = [[스킬:컨센트레이트]]
			ENDIF
			;만능
			RESULT:10 = [[스킬:메기도]]
			RESULT:11 = [[스킬:메기도라온]]
			;도구지식がLv2だと奥義的나나にかを깨달음えるように
			IF 이벤트플래그:88:0 > 3
				RESULT:12 = [[스킬:지옥의인두]]
			ENDIF
		RETURN RESULT
		ENDIF	
	CASE "상성"
		SELECTCASE GET_TYPE(ARG:1)
			CASE "파마"
				RETURN 0
			CASEELSE
				RETURN 100
		ENDSELECT
	CASE "소질"
		RETURN GETNUM(TALENT, "광기"), GETNUM(TALENT, "발정가능"), GETNUM(TALENT, "해방"), GETNUM(TALENT, "바늘다루기"), GETNUM(TALENT, "바이"), GETNUM(TALENT, "악취둔감"), GETNUM(TALENT, "쾌감에솔직")
	CASE "제외소질"
		RETURN GETNUM(TALENT, "정조관념"), GETNUM(TALENT, "아픔에약함"), GETNUM(TALENT, "겁쟁이"),GETNUM(TALENT, "헌신적")
	;처녀は特別 0は랜덤(20%), 1なら처녀, 2なら非처녀
	CASE "처녀"
		IF RAND:5 == 0
			RETURN 1
		ELSE
			RETURN 2
		ENDIF
	;戻り値は0:랜덤 1:소인체형 2:유아체형 4:큰체형 5:거체
	CASE "체형"
		RETURN 0
	;戻り値は0:랜덤 1:절벽　2:빈유　3:並　4:거유　5:폭유
	CASE "가슴"
		RETURN 0
	;추가対応。戻り値は0:랜덤 1:小尻　2:並尻　3:巨尻　4:爆尻
	CASE "尻"
		RETURN 0
	;통상の인간と肉体部位が違う場合はここで直接代入してください
	CASE "육체부위"
		RETURN 0
	CASE "ABL"
		SELECTCASE ABLNAME:(ARG:1)
			CASE "순종", "욕망", "기교"
				RETURN !RAND:3+!RAND:5
			CASE "Ｃ감각", "Ｖ감각", "Ｂ감각"
				RETURN !RAND:3
			CASE "마조끼", "노출증", "봉사정신"
				RETURN !RAND:5
			CASE "새드끼"
				RETURN !RAND:4 * !ABL:ARG:마조끼
			CASE "Ａ감각", "자위중독", "정액중독", "레즈끼", "ＢＬ끼", "섹스중독"
				RETURN !RAND:10
		ENDSELECT
	;EXPはABLが1이상ある場合、ある程度調整される(ただし、ABL0の時は調整が動かないので適当に초기値をきめてください)
	CASE "EXP"
		;全く손付かずの場合
		SIF !GROUPMATCH(1, !TALENT:ARG:처녀, ABL:ARG:Ｃ감각, ABL:ARG:Ｖ감각, ABL:ARG:Ａ감각, ABL:ARG:Ｂ감각, ABL:ARG:기교)
			RETURN 0
		SELECTCASE EXPNAME:(ARG:1)
			CASE "Ｖ경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "Ａ경험"
				RETURN RAND(0, 20) * !RAND:5
			CASE "절정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "사정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "성교경험"
				RETURN 0
			CASE "질사경험"
				RETURN EXP:ARG:사정경험/2*EXP:ARG:성교경험 > 0
			CASE "자위경험"
				RETURN RAND(0, 20)
			CASE "조교자위경험"
				RETURN 0
			CASE "정액경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "봉사쾌락경험"
				RETURN 0
			CASE "펠라경험"
				RETURN RAND(0, 10) * RAND:2
			CASE "애정경험"
				RETURN 0
			CASE "고통쾌락경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "방뇨경험"
				RETURN 0
			CASE "가학쾌락경험"
				RETURN RAND(0, 10) * !RAND:4
			CASE "야외노출경험"
				RETURN 0
			CASE "레즈경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "ＢＬ경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "이상경험"
				RETURN 0
			CASE "긴박경험"
				RETURN RAND(0, 10) * EXP:ARG:고통쾌락경험 > 0
		ENDSELECT
	CASE "모자"
		IF IS_MALE(ARG)
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:모자]]
				CASE 1
					RETURN [[衣装:카우보이모자]]
				CASE 2
					RETURN [[衣装:헬멧]]
				CASE 3
					RETURN [[衣装:삿갓]]
				CASE 4
					RETURN [[衣装:반다나]]
				CASE 5
					RETURN [[衣装:머리끈]]
				CASE 6
					RETURN [[衣装:에보시]]
				CASE 7
					RETURN [[衣装:헌팅캡]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ELSE
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:모자]]
				CASE 1
					RETURN [[衣装:반다나]]
				CASE 2
					RETURN [[衣装:비녀]]
				CASE 3
					RETURN [[衣装:머리장식]]
				CASE 4
					RETURN [[衣装:후드]]
				CASE 5
					RETURN [[衣装:머리끈]]
				CASE 6
					RETURN [[衣装:네코미미카츄샤]]
				CASE 7
					RETURN [[衣装:머리핀]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ENDIF
	CASE "옷"
		IF IS_MALE(ARG)
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:겉옷]]
				CASE 1
					RETURN [[衣装:탱크탑]]
				CASE 2
					RETURN [[衣装:언더셔츠]]
				CASE 3
					RETURN [[衣装:깃세운옷]]
				CASE 4
					RETURN [[衣装:카우보이셔츠]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ELSE
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:기모노]]
				CASE 1
					RETURN [[衣装:유카타]]
				CASE 2
					RETURN [[衣装:고스로리옷（흑）]]
				CASE 3
					RETURN [[衣装:비키니탑]]
				CASE 4
					RETURN [[衣装:캐미솔]]
				CASE 6
					RETURN [[衣装:튜브탑]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ENDIF
	CASE "하의"
		IF CFLAG:ARG:옷 == 0 && RAND:2 == 0
			;옷がなければ5割で하의もなし(後で전신복を설정)
			RETURN 0
		ELSEIF IS_MALE(ARG)
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:바지]]
				CASE 1
					RETURN [[衣装:하카마]]
				CASE 2
					RETURN [[衣装:레더팬츠]]
				CASE 3
					RETURN [[衣装:슬랙스]]
				CASE 4
					RETURN [[衣装:카고팬츠]]
			ENDSELECT
		ELSE
			IF CFLAG:ARG:옷 == [[衣装:기모노]] || CFLAG:ARG:옷 == [[衣装:유카타]]
				RETURN [[衣装:하카마]]
			ELSEIF CFLAG:ARG:옷 == [[衣装:고스로리옷（흑）]]
				SELECTCASE RAND:5
					CASE 0
						RETURN [[衣装:스커트]]
					CASE 1
						RETURN [[衣装:롱스커트]]
					CASE 2
						RETURN [[衣装:플리츠스커트]]
					CASE 3
						RETURN [[衣装:플레어스커트]]
					CASE 4
						RETURN [[衣装:미니스커트]]
				ENDSELECT
			ELSE
				SELECTCASE RAND:4
					CASE 0
						RETURN [[衣装:숏팬츠]]
					CASE 1
						RETURN [[衣装:핫팬츠]]
					CASE 2
						RETURN [[衣装:타이트미니스커트]]
					CASE 3
						RETURN [[衣装:비키니바텀]]
				ENDSELECT
			ENDIF
		ENDIF
	CASE "전신복"
		IF CFLAG:ARG:옷 == 0 && CFLAG:ARG:하의 == 0
			IF IS_MALE(ARG)
				SELECTCASE RAND:10
					CASE 0
						RETURN [[衣装:전신갑주]]
					CASE 1
						RETURN [[衣装:군복]]
					CASE 2
						RETURN [[衣装:민족의상]]
					CASE 3
						RETURN [[衣装:트라이브수트]]
					CASE 4
						RETURN [[衣装:도사복]]
					CASE 5
						RETURN [[衣装:도복]]
					CASE 6
						RETURN [[衣装:길리수트]]
					CASE 7
						RETURN [[衣装:사무에]]
					CASE 8
						RETURN [[衣装:죄수복]]
					CASE 9
						RETURN [[衣装:법의]]
				ENDSELECT
			ELSE
				SELECTCASE RAND:10
					CASE 0
						RETURN [[衣装:본디지수트]]
					CASE 1
						RETURN [[衣装:차이나드레스]]
					CASE 2
						RETURN [[衣装:군복]]
					CASE 3
						RETURN [[衣装:민족의상]]
					CASE 4
						RETURN [[衣装:도사복]]
					CASEELSE
						;女の場合、옷及び전신복がない場合を含む(あとで 전신속옷を설정)
						RETURN 0
				ENDSELECT
			ENDIF
		ELSE
			RETURN 0
		ENDIF
	CASE "손"
		RETURN 0
	CASE "속옷（상）"
		IF IS_MALE(ARG)
			RETURN 0
		ELSEIF CFLAG:ARG:옷 == 0 && CFLAG:ARG:하의 == 0 && CFLAG:ARG:전신복 == 0
			; 전신속옷を설정するためなし
			RETURN 0
		ELSE
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:브래지어]]
				CASE 1
					RETURN [[衣装:브래지어(흑)]]
				CASE 2
					RETURN [[衣装:브래지어(적)]]
				CASE 3
					RETURN [[衣装:무명천]]
				CASE 4
					RETURN [[衣装:니플리스]]
			ENDSELECT
		ENDIF
	CASE "속옷（하）"
		IF IS_MALE(ARG)
			IF RAND:5
				RETURN [[衣装:트렁크스]]
			ELSE
				RETURN [[衣装:훈도시]]
			ENDIF
		ELSEIF CFLAG:ARG:옷 == 0 && CFLAG:ARG:하의 == 0 && CFLAG:ARG:전신복 == 0
			; 전신속옷を설정するためなし
			RETURN 0
		ELSE
			SELECTCASE RAND:6
				CASE 0
					RETURN [[衣装:팬티]]
				CASE 1
					RETURN [[衣装:팬티(흑)]]
				CASE 2
					RETURN [[衣装:팬티(적)]]
				CASE 3
					RETURN [[衣装:레이스달린팬티]]
				CASE 4
					RETURN [[衣装:스패츠]]
				CASE 5
					RETURN [[衣装:로우라이즈]]
			ENDSELECT
		ENDIF
	CASE "전신속옷"
		IF CFLAG:ARG:옷 == 0 && CFLAG:ARG:하의 == 0 && CFLAG:ARG:전신복 == 0
			SELECTCASE RAND:3
				CASE 0
					RETURN [[衣装:타이트보디수트]]
				CASE 1
					RETURN [[衣装:레오타드]]
				CASE 2
					RETURN [[衣装:고무수트]]
			ENDSELECT
		ELSE
			RETURN 0
		ENDIF
	CASE "양말"
		IF IS_MALE(ARG)
			RETURN [[衣装:양말]]
		ELSEIF CFLAG:ARG:옷 == 0 && CFLAG:ARG:하의 == 0 && CFLAG:ARG:전신복 == 0
			RETURN [[衣装:검은색타이즈]]
