			SIF IS_ANTI_NTR_CLOTHES(LOCAL) == 1
				CONTINUE
			SIF CFLAG:LOCAL:상사이벤트진행도 < 7
				CONTINUE
			SIF CFLAG:LOCAL:상사이벤트진행도 == 10
				CONTINUE
			ＮＴＲリスト:(LOCAL:1) = LOCAL
			LOCAL:1 ++
		NEXT
		SIF LOCAL:1 == 0
			RETURN 0
		LOCAL = ＮＴＲリスト:(RAND:(LOCAL:1))
		SIF LOCAL < 1 && LOCAL >= CHARANUM
			RETURN 0
		;CFLAG:상사내습が２の場合は呼び出しに分岐
		CFLAG:LOCAL:상사내습 = 2
		;노역とパーティーを外す
		SIF TARGET == LOCAL
			TARGET = -1
		SIF ASSI == LOCAL
			ASSI = -1
		CFLAG:LOCAL:이자리에없는플래그 = 1
		CFLAG:LOCAL:노역플래그 = 0
		IF CFLAG:LOCAL:극장출연순서 != 0
			DE:(49+(CFLAG:LOCAL:극장출연순서)):(CFLAG:LOCAL:노역점포-1) = -1
			DE:(59+(CFLAG:LOCAL:극장출연순서)):(CFLAG:LOCAL:노역점포-1) = -1
			CFLAG:LOCAL:노역플래그 = 0
			CFLAG:LOCAL:극장출연순서 = 0
			CFLAG:LOCAL:노역점포 = 0
		ENDIF
		SIF CPOS(LOCAL) > 0
			CALL REMOVE_POSITION,CPOS(LOCAL)
		SETCOLOR COLOR("RED")
		IF CFLAG:LOCAL:상사이벤트진행도 >= 13
			PRINTFORMW %조사처리(CALLNAME:LOCAL,"는")% 어딘가 나간 모양이다
		ELSE
			PRINTFORMW %조사처리(CALLNAME:LOCAL,"는")% %CALLNAME:MASTER%에게 미소를 짓고는, 뭔가를 결의한 표정으로 나갔다
		ENDIF
		RESETCOLOR
	;上司が来るかどうかの판정
	ELSEIF RAND:60 < FLAG:상사내습률 && !LOCAL:3 == 1
		CALL JOUSHI_RAISYUU

	ENDIF
	;上司の来る확률は최대100％
	FLAG:상사내습률 += RAND:6
	SIF FLAG:상사내습률 > 100
		FLAG:상사내습률 = 100
ENDIF


