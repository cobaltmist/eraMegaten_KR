;--------
;상사 내습
;--------
@JOUSHI_RAISYUU
#LOCALSIZE 7
#DIM リスト, 500
;CFLAG:상사내습持ち用
#DIM 上司リスト, 500
VARSET リスト, -1
VARSET 上司リスト, -1
LOCAL:1 = 0
LOCAL:5 = 0
LOCAL:6 = 0
FOR LOCAL,1,CHARANUM
	LOCAL:2 = 0
	SIF CFLAG:LOCAL:이자리에없는플래그 == 1
		CONTINUE
	SIF TALENT:LOCAL:ＮＴＲ
		CONTINUE
	SIF !(TALENT:LOCAL:연모 || TALENT:LOCAL:친애)
		CONTINUE
	SIF TALENT:LOCAL:임신 || TALENT:LOCAL:육아중
		CONTINUE
	SIF CFLAG:LOCAL:노역플래그 > 0
		CONTINUE
	;専有奴隷の首輪系装備時は弾く
	SIF IS_ANTI_NTR_CLOTHES(LOCAL) == 1
		CONTINUE
	SIF TALENT:LOCAL:남자
		CONTINUE
	SIF TALENT:LOCAL:매력
		LOCAL:2 += 10
	SIF TALENT:LOCAL:거유 || TALENT:LOCAL:폭유
		LOCAL:2 += 30
	SIF TALENT:LOCAL:마유
		LOCAL:2 += 40
	SIF TALENT:LOCAL:아내
		LOCAL:2 += 30
	;背徳の指輪系装備時は弾く
	SIF IS_ANTI_NTR_CLOTHES(LOCAL) == -1
		LOCAL:2 += 50
	SIF (위험일(LOCAL)) > 0
		LOCAL:2 += 40

	IF CFLAG:LOCAL:상사이벤트진행도 > 0
		上司リスト:(LOCAL:6) = LOCAL
		LOCAL:5 = 1
		LOCAL:6 ++
	ELSEIF RAND:100 < 10 + LOCAL:2
		リスト:(LOCAL:1) = LOCAL
		LOCAL:1 ++
	ENDIF
NEXT
;LOCAL:1 == 0の場合、女要求率０
SIF LOCAL:1 == 0 && LOCAL:6 == 0
	GOTO YOUKYUUNASHI
;上司の餌食決定＞LOCAL
;CFLAG:상사이벤트진행도がある場合は優先
$LOOP_JOUSHILIST
IF LOCAL:5 == 1
	LOCAL = 上司リスト:(RAND:(LOCAL:6))
ELSE
	LOCAL = リスト:(RAND:(LOCAL:1))
ENDIF
;ありえないと思うけど念のため。LOCALがキャラ등록번호の범위外の場合なにもしない。
;CFLAG:상사이벤트진행도持ちがいる場合はループする
IF LOCAL < 1 && LOCAL >= CHARANUM
	SIF LOCAL:5 == 1
		GOTO LOOP_JOUSHILIST
	RETURN 0
ENDIF
;女要求率
LOCAL:2 = 100
;要求金額倍率
LOCAL:3 = 0
;要求額
LOCAL:4 = 0

;金のみ（소지額の３分の一）か、金（소지額の半分）か彼女かの分岐
;どちらかの소지金が목표金額の半分を超えると、選択肢に연모持ちの인간の女が加わる率が+40％
SIF MONEY > (MONEY:2 / 2) || (MONEY:1 * 50) > (MONEY:2 / 2)
	LOCAL:2 += 40
;以下の소질が有ると+30％
SIF TALENT:LOCAL:매력 || TALENT:LOCAL:거유 || TALENT:LOCAL:폭유 || TALENT:LOCAL:마유 || TALENT:LOCAL:아내
	LOCAL:2 += 30
;위험일は+40％
SIF (위험일(LOCAL)) > 0
	LOCAL:2 += 40
;要求金額倍率（選択肢）決定
IF RAND:100 < LOCAL:2
	LOCAL:3 = 2
ELSE
	LOCAL:3 = 5
ENDIF
;LOCAL:1 == 0の場合、女要求率０
$YOUKYUUNASHI
SIF LOCAL:1 == 0 && LOCAL:6 == 0
	LOCAL:3 = 5
;마카（換金した値）が￥の2倍より多い場合は마카要求
;최저要求額は￥１０万（2000마카）
;金も女も無い様な所には来ない
IF (MONEY:1 * 50) > MONEY * 2
	LOCAL:4 = MONEY:1 / LOCAL:3
	IF LOCAL:4 < 2000
		LOCAL:4 = 2000
		LOCAL:3 = 1
		SIF LOCAL:1 == 0
			RETURN 1
	ENDIF
ELSE
	LOCAL:4 = MONEY / LOCAL:3
	IF LOCAL:4 < 100000
		LOCAL:4 = 100000
		LOCAL:3 = 1
		SIF LOCAL:1 == 0
			RETURN 1
	ENDIF
ENDIF
;CFLAG:LOCAL:상사이벤트진행도が6の場合は最終단계へ
IF LOCAL:5 > 0
	IF CFLAG:LOCAL:상사이벤트진행도 == 10
		LOCAL:3 = 10
	ENDIF
ENDIF
IF LOCAL:3 == 10
	PRINTFORML 
	PRINTFORML 
	PRINTFORMW 팬텀 소사이어티의 간부남이 방문해왔다.
	PRINTFORML 
	PRINTFORML 『어제, 네놈이 조직의 정보를 외부로 흘리고 있다는 정보가 들어왔거든』
	PRINTFORML 『네놈을 좋게 생각하지 않는 녀석들이 처리하라고 말하기 시작했다』
	PRINTFORML 『나도 이런 시시한 일로 우수한 장기말을 잃고 싶지 않아서 말이지, 뒤처리를 맡아주지』
	LOCAL:4 = MONEY
	TIMES LOCAL:4,0.9
	LOCAL:4 = LIMIT(LOCAL:4 , 1000000, LOCAL:4)
	PRINTFORM 『뭐, 
	SETCOLOR COLOR("RED")
	PRINTFORM ￥{LOCAL:4} 
	RESETCOLOR
	PRINTFORMW 정도 있으면 충분하다만, 액수가 액수니』
	PRINTFORM 『없다면… 
	SETCOLOR COLOR("dark-pink")
	PRINTFORM %CALLNAME:LOCAL%
	RESETCOLOR
	PRINTFORMW %조사만처리(CALLNAME:LOCAL,"를")% 조수로 빌려갈까』
ELSE
	PRINTFORML 
	PRINTFORML 
	PRINTFORMW 팬텀 소사이어티의 간부남이 방문해왔다.
	PRINTFORML 
	PRINTFORML 『흥, 변함없이 맘에 안 드는 낮짝을 하고 있구만.』
	PRINTFORMW 『급하게, 재계쪽 사람을 대접하게 되어서 말이지, 네놈에게도 협력을 받게 되었다.』
	PRINTFORML 
	IF (MONEY:1 * 50) > MONEY * 2
		PRINTFORM 『
		SETCOLOR COLOR("RED")
		PRINTFORM {LOCAL:4}마카 
		RESETCOLOR
		PRINTFORMW 정도, 납부해주실까』
	ELSE
		PRINTFORM 『
		SETCOLOR COLOR("RED")
		PRINTFORM ￥{LOCAL:4} 
		RESETCOLOR
		PRINTFORMW 정도, 납부해주실까』
	ENDIF
	IF LOCAL:3 < 5
		PRINTFORML 『무리라면, 그렇군…』
		PRINTFORMW 남자의 시선이 %CALLNAME:MASTER% 뒤쪽으로 돌아갔다.
		PRINTFORML 
		;눈をつけられた場合
		IF CFLAG:LOCAL:상사이벤트진행도 > 0
			PRINTFORM 『대신 
			SETCOLOR COLOR("dark-pink")
			PRINTFORM %CALLNAME:LOCAL%
			RESETCOLOR
			PRINTFORML %조사만처리(CALLNAME:LOCAL,"를")% 맡겨주지 않겠나』
			PRINTFORMW 『저번에 호평이었거든, 큭큭큭』
		;通常
		ELSE
			PRINTFORML 『대신 저 여자를 맡겨주지 않겠나』
			PRINTFORM 히죽히죽 웃음을 띠면서 
			SETCOLOR COLOR("dark-pink")
			PRINTFORM %CALLNAME:LOCAL%
			RESETCOLOR
			PRINTFORML %조사만처리(CALLNAME:LOCAL,"를")% 핥듯이 쳐다보고 있다.
			PRINTFORML 
			PRINTFORML 『저 미모라면 분명 도움이 되겠지, 크크크』
		ENDIF
	ENDIF
ENDIF
PRINTFORML 
IF LOCAL:3 == 2 || LOCAL:3 == 10
	PRINTFORM 어찌할까?　　　소지금:￥{MONEY}　　{MONEY:1}마카　　　
	PRINTFORM %CALLNAME:LOCAL%　
	IF (위험일(LOCAL)) == 2
		SETCOLOR 0xCC33FF
		CALL HEARTB
		PRINTFORM 발정중
		CALL HEARTB
		RESETCOLOR
	ELSEIF (위험일(LOCAL)) == 1
		SETCOLOR 0xCC33FF
		CALL HEARTB
		PRINTFORM 위험일
		CALL HEARTB
		RESETCOLOR
	ELSE
		PRINTFORM 
	ENDIF
	PRINTFORMW 
ELSE
	PRINTFORMW 어찌할까?　　　소지금:￥{MONEY}　　{MONEY:1}마카
ENDIF

IF LOCAL:3 == 10
	IF MONEY < 1000000
		CALL INPUT_SELECT(2, "여자를 넘긴다")
	ELSE
		CALL INPUT_SELECT(1, "지불한다",2, "여자를 넘긴다")
	ENDIF
ELSEIF LOCAL:3 == 2
	CALL INPUT_SELECT(1, "지불한다",2, "여자를 넘긴다", 3, "거절한다")
ELSEIF LOCAL:3 == 1
	CALL INPUT_SELECT(2, "여자를 넘긴다", 3, "거절한다")
ELSE
	CALL INPUT_SELECT(1, "지불한다", 3, "거절한다")
ENDIF
SELECTCASE RESULT
	CASE 1
		IF (MONEY:1 * 50) > MONEY * 2
			MONEY:1 -= LOCAL:4
			PRINTFORMW {LOCAL:4}마카를 지불했다
		ELSE
			MONEY -= LOCAL:4
			PRINTFORMW ￥{LOCAL:4}을 지불했다
		ENDIF
		IF LOCAL:3 == 10
			PRINTFORML 『오오, 확실하게 받았다』
			PRINTFORML 『뒤는 내게 맡겨둬라』
			PRINTFORMW 남자는 입금을 확인하고, 조속히 떠나갔다.
			FLAG:상사내습률 = 0
			CFLAG:LOCAL:상사이벤트진행도 = 0
		ELSE
			PRINTFORML 
			PRINTFORML 『꽤 사리분별을 잘하는 녀석이군』
			PRINTFORML 『앞으로도 네놈 나름대로 조직에 도움이 되도록』
			PRINTFORMW 남자는 깔보면서 말을 툭던지고, 떠나갔다.
			FLAG:상사내습률 = FLAG:상사내습률 / 3
			;10％の확률로上司の타겟から外れる
			IF RAND:100 < 10 && LOCAL:3 == 2 && CFLAG:LOCAL:상사이벤트진행도 > 0
				PRINTFORMW %조사처리(CALLNAME:LOCAL,"는")% %조사처리(CALLNAME:MASTER,"를")% 살짝 껴안고 위로하고 있다.
				CFLAG:LOCAL:상사이벤트진행도 = 0
			ENDIF
			;CFLAG:상사이벤트진행도がある場合は10％の확률로3단계눈に移行する
			IF RAND:100 < 10 && LOCAL:3 == 2 && CFLAG:LOCAL:상사이벤트진행도 < 7
				PRINTFORMW %조사처리(CALLNAME:LOCAL,"는")% %조사처리(CALLNAME:MASTER,"를")% 걱정스럽게 바라보았다.
				PRINTFORMW 그리고 간부남에게 은밀히 전달받은 메모를 꼬옥 쥐었다…
				CFLAG:LOCAL:상사이벤트진행도 = 7
			ENDIF
		ENDIF
	CASE 2
		IF LOCAL:3 == 10
			PRINTFORML 
			PRINTFORML 『그러냐, 그럼 맡아두도록 하지… 조수로 말이야』
			PRINTFORML 『사태의 수습은 하룻밤이면 충분하겠지』
			PRINTFORML 『그럼… 기대해둬라』
			PRINTFORMW %조사처리(CALLNAME:LOCAL,"는")% 슬픈 표정으로 남자를 따라갔다
		ELSEIF CFLAG:LOCAL:상사이벤트진행도 > 3 && CFLAG:LOCAL:상사이벤트진행도 <= 6
			PRINTFORML 
			PRINTFORMW %조사처리(CALLNAME:MASTER,"가")% %조사처리(CALLNAME:LOCAL,"를")% 맡기겠다고 말하자, 남자는 히죽 뺨을 끌어올렸다
			PRINTFORML 『크큭, 그럼 맡아주도록 하지』
			PRINTFORML 『뭘, 내일 아침까지는 돌려보내주겠어』
			PRINTFORMW 『그건 그렇고…』
			IF TALENT:LOCAL:거유 || TALENT:LOCAL:폭유 || TALENT:LOCAL:마유
				PRINTFORML 남자는 %CALLNAME:LOCAL% 뒤로 돌아가더니
				PRINTFORML 풍만한 가슴을 만지기 시작하면서 %CALLNAME:LOCAL%의 가랑이에 손을 넣어 애무하기 시작했다…
				PRINTFORML 남자의 격한 손가락놀림에 %조사처리(CALLNAME:LOCAL,"는")% 허리가 풀어지고, 다리는 지금이라도 무너질듯이 떨리고 있었다
				PRINTFORMW 『하하하하, 변함없이 음란한 몸이군… 이건 이번 상대도 만족할 테지』
			ELSE
				PRINTFORML 남자는 %조사처리(CALLNAME:LOCAL,"를")% 끌어안고, %CALLNAME:LOCAL%의 입술에 키스를 하며
				PRINTFORML %CALLNAME:LOCAL%의 가랑이를 희롱하기 시작했다…
				PRINTFORML %조사처리(CALLNAME:LOCAL,"는")% 남자의 기교스런 손가락놀림에 농락당해, 황홀한 표정을 지으며 남자의 입술을 빨아들이고 있다
				PRINTFORMW 『크큭, 여전히 감도 좋은 몸이군… 이건 이번 상대도 만족할 테지』
			ENDIF
			PRINTFORML 
			PRINTFORMW %조사처리(CALLNAME:LOCAL,"는")% 쾌락으로 허리힘이 빠진 몸을 질질 끌어, 남자를 따라갔다
		ELSEIF CFLAG:LOCAL:상사이벤트진행도 > 6
			;섹스系
			IF RAND:2 == 0
				PRINTFORML 
				PRINTFORMW %조사처리(CALLNAME:MASTER,"가")% %조사처리(CALLNAME:LOCAL,"를")% 맡기겠다고 말하자, 남자는 히죽 뺨을 끌어올렸다
				PRINTFORML 『크큭, 그럼 맡아주도록 하지』
				PRINTFORML 『뭘, 내일 아침까지는 돌려보내주겠어』
				PRINTFORMW 『그건 그렇고…』
				IF TALENT:LOCAL:거유 || TALENT:LOCAL:폭유 || TALENT:LOCAL:마유
					PRINTFORML 남자는 %CALLNAME:LOCAL% 뒤로 돌아가서 지퍼를 내리고 페니스를 꺼내들어 
					PRINTFORML %CALLNAME:LOCAL%의 풍만한 가슴을 만지작대며 %CALLNAME:LOCAL%의 속옷을 비껴 질 안에 삽입했다
					PRINTFORML %조사처리(CALLNAME:LOCAL,"는")% 갑작스런 충격에 눈을 희번덕거리며 헐떡였다
					PRINTFORMW 『하하하, 여전히 조임 좋은 구멍이군. 상대가 집착하는 것도 이해된다』
					PRINTFORMW 
					PRINTFORMW %조사처리(CALLNAME:LOCAL,"는")% 남자에게 뒤로 찔리며 풍만한 가슴을 크게 흔들면서 황홀한 표정을 띠고 있다
					PRINTFORMW 남자가 한층 더 강하게 찌르고 %조사처리(CALLNAME:LOCAL,"가")% 환희의 비명을 지르자, 남자와 %CALLNAME:LOCAL%의 결합부에서 대량의 정액이 넘쳐나왔다…
					PRINTFORMW 『어이쿠, 너무 기분 좋아서 그만 질내사정해버렸군』
					PRINTFORMW 『뭐어, 딱히 임신해도 상관없잖아…?』
					PRINTFORML 
					PRINTFORMW 
					PRINTFORMW 『크크큭, 조금 더 맛보고 싶다만 슬슬 가야겠군』

;						PRINTFORMW 『크크크, 아무래도 %조사처리(CALLNAME:LOCAL,"는")% 너무 기분 좋아서 허리가 빠져버린 모양이군』
;						PRINTFORMW 『내가 이대로 오늘 회장까지 데려가주도록 하지』
				ELSE
					PRINTFORML 남자는 %조사처리(CALLNAME:LOCAL,"를")% 껴안고 입술을 훔치더니, 지퍼를 내려 페니스를 꺼내 %CALLNAME:LOCAL%의 비부에 들이밀어
					PRINTFORML 키스에 취한 표정을 짓고 있는 %CALLNAME:LOCAL%의 비부를 아래에서 단번에 찔러올렸다
					PRINTFORMW %조사처리(CALLNAME:LOCAL,"는")% 남자에게 껴안겨서 범해져, 멀쩡하지 않은 목소리를 내고 있다
					PRINTFORMW 『역시 %CALLNAME:LOCAL%의 몸은 매력적이야. 무심코 네놈 앞에서 덮치고 말았군』
					PRINTFORMW 
					PRINTFORMW %조사처리(CALLNAME:LOCAL,"는")% 떨어지지 않도록 남자의 허리에 발을 휘감으면서 남자와 열렬한 키스를 나누고 있다
					PRINTFORMW 남자는 여러번 격하게 찔러올린 다음, %조사처리(CALLNAME:LOCAL,"를")% 강하게 껴안은 자세로 사정하기 시작했다
					PRINTFORMW %CALLNAME:LOCAL% 또한 팔을 남자의 목에 두르고 허리를 휘감고 있는 다리에 힘을 주었다
					PRINTFORMW 여운에 잠긴 %조사처리(CALLNAME:LOCAL,"와")% 남자의 결합부에서 대량의 정액이 흘러내려 바닥에 하얀 웅덩이를 만들고 있다
					PRINTFORML 
					PRINTFORMW 
					PRINTFORMW 『크크큭, 조금 더 맛보고 싶다만 슬슬 가야겠군』

;						PRINTFORMW 『크크크, 아무래도 %조사처리(CALLNAME:LOCAL,"는")% 너무 기분 좋아서 허리가 빠져버린 모양이군』
;						PRINTFORMW 『내가 이대로 오늘 회장까지 데려가주도록 하지』
				ENDIF
				PRINTFORML 
				PRINTFORMW 흐트러진 의상을 익숙한 손놀림으로 정돈하고, %조사처리(CALLNAME:LOCAL,"는")% %조사처리(CALLNAME:MASTER,"를")% 돌아보지도 않고, 남자를 따라 방을 나갔다

;					PRINTFORMW %조사처리(CALLNAME:LOCAL,"는")% 남자와 연인처럼 교접한 채 따라갔다
			;봉사계
			ELSE
				PRINTFORML 
				PRINTFORMW %조사처리(CALLNAME:MASTER,"가")% %조사처리(CALLNAME:LOCAL,"를")% 맡기겠다고 말하자, 남자는 히죽 뺨을 끌어올렸다
				PRINTFORML 『크큭, 그럼 맡아주도록 하지』
				PRINTFORMW 『뭘, 내일 아침까지는 돌려보내주겠어』
				PRINTFORML 
				PRINTFORMW 『그건 그렇고…』
				PRINTFORML 『%CALLNAME:LOCAL%의 실력은 떨어지지 않았겠지?』
				PRINTFORML 『크큭, 중요한 접대다… 실수가 있어서는…』
				PRINTFORMW 일부러 말하는듯한 말과 함께 남자가 %CALLNAME:LOCAL%의 몸을 핥듯이 쳐다보았다
