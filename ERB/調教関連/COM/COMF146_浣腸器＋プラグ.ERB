;-------------------------------------------------
;관장기＋플러그
;ハードな조교コマンド、레벨4
;-------------------------------------------------
@COMNAME146
RESULTS = 관장기＋플러그
SIF TEQUIP:21
	RESULTS = 관장기해제
@ACT_COM146
IF TEQUIP:25
	PRINTL 촉수관장
	TSTR:0 = 촉수관장
	SELECTCOM = 167
ELSE
	PRINTL 관장기＋플러그
	TSTR:0 = 관장기＋플러그
ENDIF
;CALL KOJO_MESSAGE_COM

;着脱の処理
IF TEQUIP:21 == 0
	TEQUIP:21 = 1
	;촉수관장は촉수の불결を추가
	IF TEQUIP:25
		CALL SET_STAIN("애널", "점액", TARGET)
	ENDIF
ELSE
	TEQUIP:21 = 0
;		EXP:50 += 1
;		PRINTFORML %EXPNAME:50%+1
	CALL ADDS_ABNORMAL_EXP("관장배설", TARGET)
ENDIF

;조교자→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 60
DOWNBASE:TARGET:기력 += 150

SOURCE:TARGET:도구쾌Ａ = 300
SOURCE:TARGET:아픔 = 500
SOURCE:TARGET:노출 = 300
SOURCE:TARGET:굴종 = 200
SOURCE:TARGET:불결 = 500
SOURCE:TARGET:일탈 = 500
SOURCE:TARGET:반감 = 500

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 500

;Aへの고통は先に処理しておく
;EXP:Ａ경험をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:TARGET:아픔 , 3.00
	TIMES SOURCE:TARGET:반감 , 3.00
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:TARGET:아픔 , 1.20
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:TARGET:아픔 , 0.80
	TIMES SOURCE:TARGET:반감 , 1.50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:TARGET:아픔 , 0.60
	TIMES SOURCE:TARGET:반감 , 1.20
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 0.40
ELSEIF EXP:1 >= EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 0.20
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:TARGET:아픔 , 1.20
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:TARGET:아픔 , 1.00
	TIMES SOURCE:TARGET:반감 , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:TARGET:아픔 , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 0.20
ENDIF


;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;Ａ경험
CALL ADDS_EXP("Ａ경험", 25, TARGET)
;PRINTL Ａ경험＋5

;레즈경험
A = 1
IF IS_LESBIAN(TARGET,PLAYER)
	PRINTFORML %EXPNAME:40%+{A}
	CALL ADDS_EXP("레즈경험", A, TARGET)
ELSEIF IS_GAY(TARGET,PLAYER)
	PRINTFORML %EXPNAME:41%+{A}
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
ENDIF

;의존도ベクトル
TFLAG:33 = 5

;의존도보정値(복종で1、예속で2）
IF TALENT:5 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:8 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM146
;浣腸＋애널プラグ挿入中
IF TEQUIP:25
	PRINTL ＜관장촉수삽입중＞
ELSE
	PRINTL ＜관장＋애널플러그삽입중＞
ENDIF

DOWNBASE:TARGET:체력 += 60
DOWNBASE:TARGET:기력 += 150

SOURCE:TARGET:도구쾌Ａ += 150
SOURCE:TARGET:굴종 += 100
SOURCE:TARGET:불결 += 250
SOURCE:TARGET:일탈 += 250

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 250

;Aへの고통は先に処理しておく
;EXP:Ａ경험をみる
A = 250
B = 250
IF EXP:1 < EXPLV:1
	TIMES A , 3.00
	TIMES B , 3.00
ELSEIF EXP:1 < EXPLV:2
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF EXP:1 < EXPLV:3
	TIMES A , 0.80
	TIMES B , 1.50
ELSEIF EXP:1 < EXPLV:4
	TIMES A , 0.60
	TIMES B , 1.20
ELSEIF EXP:1 < EXPLV:5
	TIMES A , 0.40
ELSEIF EXP:1 >= EXPLV:5
	TIMES A , 0.20
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES A , 1.00
	TIMES B , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES A , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES A , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES A , 0.20
ENDIF
SOURCE:TARGET:아픔 += A
SOURCE:TARGET:반감 += B



;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;Ａ경험
CALL ADDS_EXP("Ａ경험", 3, TARGET)
;PRINTL Ａ경험＋3

;===================================================
;조교타입
;===================================================
@COMTYPE_146
RETURN COMTYPE("하드계")




