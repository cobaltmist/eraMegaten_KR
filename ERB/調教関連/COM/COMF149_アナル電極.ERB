;-------------------------------------------------
;애널전극
;ハードな조교コマンド、레벨4
;-------------------------------------------------
@COMNAME149
RESULTS = 애널전극
SIF TEQUIP:23
	RESULTS = 애널전극해제
@ACT_COM149
PRINTL 애널전극
TSTR:0 = 애널전극
;CALL KOJO_MESSAGE_COM

;着脱の処理
IF TEQUIP:23 == 0
	TEQUIP:23 = 1
ELSE
	TEQUIP:23 = 0
ENDIF

;조교자→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 300
DOWNBASE:TARGET:기력 += 450

SOURCE:TARGET:도구쾌Ａ = 500
SOURCE:TARGET:아픔 = 1500
SOURCE:TARGET:노출 = 300
SOURCE:TARGET:굴종 = 800
SOURCE:TARGET:일탈 = 1200
SOURCE:TARGET:반감 = 1200

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 1200

;フィストなどのAへの大きな고통は先に処理しておく
;EXP:Ａ경험をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:TARGET:아픔 , 3.00
	TIMES SOURCE:TARGET:반감 , 4.00
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:TARGET:아픔 , 2.50
	TIMES SOURCE:TARGET:반감 , 3.50
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:TARGET:아픔 , 2.40
	TIMES SOURCE:TARGET:반감 , 3.00
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:TARGET:아픔 , 2.00
	TIMES SOURCE:TARGET:반감 , 2.50
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 1.80
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF EXP:1 >= EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 1.60
	TIMES SOURCE:TARGET:반감 , 1.50
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:TARGET:아픔 , 3.00
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:TARGET:아픔 , 2.50
	TIMES SOURCE:TARGET:반감 , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:TARGET:아픔 , 2.00
	TIMES SOURCE:TARGET:반감 , 1.20
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 1.50
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 1.00
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;Ａ경험
CALL ADDS_EXP("Ａ경험", 25, TARGET)
;PRINTL Ａ경험＋５

;이상경험が5미만のとき、이상경험추가
IF EXP:50 < 5
;	EXP:50 += 1
;	PRINTL 이상경험＋１
	CALL ADDS_ABNORMAL_EXP("전극", TARGET)
ENDIF

;레즈경험
A = 1
IF IS_LESBIAN(TARGET,PLAYER)
	PRINTFORML %EXPNAME:40%+{A}
	CALL ADDS_EXP("레즈경험", A, TARGET)
ELSEIF IS_GAY(TARGET,PLAYER)
	PRINTFORML %EXPNAME:41%+{A}
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
ENDIF

;의존도ベクトル
TFLAG:33 = 5

;의존도보정値(복종で1、예속で2）
IF TALENT:5 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:8 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM149
;애널전극

PRINTL ＜애널전극삽입중＞

DOWNBASE:TARGET:체력 += 200
DOWNBASE:TARGET:기력 += 250

SOURCE:TARGET:도구쾌Ａ += 250
SOURCE:TARGET:굴종 += 400
SOURCE:TARGET:일탈 += 600


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 600

;フィストなどのAへの大きな고통は先に処理しておく
;EXP:Ａ경험をみる
A = 750
B = 600
IF EXP:1 < EXPLV:1
	TIMES A , 3.00
	TIMES B , 4.00
ELSEIF EXP:1 < EXPLV:2
	TIMES A , 2.50
	TIMES B , 3.50
ELSEIF EXP:1 < EXPLV:3
	TIMES A , 2.40
	TIMES B , 3.00
ELSEIF EXP:1 < EXPLV:4
	TIMES A , 2.00
	TIMES B , 2.50
ELSEIF EXP:1 < EXPLV:5
	TIMES A , 1.80
	TIMES B , 2.00
ELSEIF EXP:1 >= EXPLV:5
	TIMES A , 1.60
	TIMES B , 1.50
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES A , 3.00
	TIMES B , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES A , 2.50
	TIMES B , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES A , 2.00
	TIMES B , 1.20
ELSEIF PALAM:4 < PALAMLV:4
	TIMES A , 1.50
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES A , 1.00
ENDIF
SOURCE:TARGET:아픔 += A
SOURCE:TARGET:반감 += B

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;Ａ경험
CALL ADDS_EXP("Ａ경험", 5, TARGET)
;PRINTL Ａ경험＋５


RETURN 1

;===================================================
;조교타입
;===================================================
@COMTYPE_149
RETURN COMTYPE("하드계")




