;-------------------------------------------------
;이라마치오
;ハードな조교コマンド、레벨2
;-------------------------------------------------
@JUMP_COM140
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;전회の조교が3Pのとき3Pへ
;TFLAG:42 = 0
;전회の조교が정상위か후배위か애널섹스か3Pのときは3Pへ
IF GROUPMATCH(PREVCOM, 60, 61, 62, 63, 500 ,513, 514) && TEQUIP:TARGET:입삽입자 != SUBPLAYER()
	;3Ｐ系から来たのを考慮して、ここで代入しなおしておく
	TEQUIP:TARGET:입삽입자 = PLAYER
	IF TEQUIP:TARGET:Ｖ삽입자 == SUBPLAYER()
		CALL COM_ABLE_COMMON, 513
		SIF RESULT == 1
			RETURN 513
	ELSEIF TEQUIP:TARGET:Ａ삽입자 == SUBPLAYER()
		CALL COM_ABLE_COMMON, 514
		SIF RESULT == 1
			RETURN 514
	ENDIF
ENDIF
RETURN -1

@ACT_COM140
PRINTL 이라마치오


;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 이라마치오
;CALL KOJO_MESSAGE_COM

;実行者が奴隷であるフラグ
TFLAG:28 = 1

;-------------------------------------------------
;사정ゲージ체크
;-------------------------------------------------
B = 2000

;ABL:조교자の기교をみる
IF ABL:기교 == 0
	TIMES B , 1.00
ELSEIF ABL:기교 == 1
	TIMES B , 1.20
ELSEIF ABL:기교 == 2
	TIMES B , 1.40
ELSEIF ABL:기교 == 3
	TIMES B , 1.60
ELSEIF ABL:기교 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:순종 == 0
	TIMES B , 0.80
ELSEIF ABL:순종 == 1
	TIMES B , 0.90
ELSEIF ABL:순종 == 2
	TIMES B , 1.00
ELSEIF ABL:순종 == 3
	TIMES B , 1.10
ELSEIF ABL:순종 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:봉사정신 == 0
	TIMES B , 0.50
ELSEIF ABL:봉사정신 == 1
	TIMES B , 0.80
ELSEIF ABL:봉사정신 == 2
	TIMES B , 1.20
ELSEIF ABL:봉사정신 == 3
	TIMES B , 1.50
ELSEIF ABL:봉사정신 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;TALENT:혀놀림、고양이혀をみる
SIF TALENT:혀놀림 || TALENT:54
	TIMES B , 2.00

;플레이어のABL:Ｃ감각をみる
IF ABL:PLAYER:Ｃ감각 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:Ｃ감각 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:Ｃ감각 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:Ｃ감각 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:Ｃ감각 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:PLAYER:사정게이지재장전 > 0
	B /= 20


SIF HAVE_PENIS(PLAYER)
	BASE:PLAYER:사정 += B

;조교자←조교대상
TFLAG:28 = 1

;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 150
DOWNBASE:TARGET:기력 += 200

SOURCE:TARGET:성행위 = 500
SOURCE:TARGET:아픔 = 200
SOURCE:TARGET:굴종 = 1500
SOURCE:TARGET:일탈 = 700
SOURCE:TARGET:반감 = 700

;上のほうで計算した불결데이터
SOURCE:TARGET:불결 = Y*40 + 100

;조교자に쾌Ｃ
SOURCE:PLAYER:쾌Ｃ = 160
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:TARGET:울굴 += 700

;-------------------------------------------------
;사정체크
;-------------------------------------------------
;사정先타입の설정
TCVAR:PLAYER:페니스사용대상 = TARGET
TCVAR:PLAYER:사정개소 = 4
;사정전선택の可・
TFLAG:704 = 1

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;奴隷の입⇔조교자のＰの불결が移動
CALL MOVE_STAIN("입", TARGET, "페니스", PLAYER)

;봉사정신ＬＶ２이상、기교ＬＶ２이상なら불결をなめ取る（볼재갈사용時は除く）
IF ABL:봉사정신 >= 2 && ABL:기교 >= 2 && TEQUIP:20 == 0
	;STAIN:PLAYER:2 = 2
	;タイミングをずらす
	TFLAG:청소펠라발생 = 1
	TFLAG:8 = 1
ENDIF

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:8+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:8+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;펠라경험
;PRINTFORML %EXPNAME:22%＋１
CALL ADDS_EXP("펠라경험", 1, TARGET)

;레즈경험
A = 7
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;애정경험(마조끼5이상限定）
IF (TALENT:연모 == 1 || TALENT:친애 == 1) && ASSIPLAY == 0 && ABL:마조끼 >= 5
	CALL ADDS_EXP("애정경험", 1, TARGET)
ENDIF

;의존도ベクトル
TFLAG:33 = 3

;의존도보정値(主人かつ연모、음란、복종で1。친애、창부、예속で2）
IF (TALENT:연모 || TALENT:음란 || TALENT:복종) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:친애 || TALENT:창부 || TALENT:예속) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

TFLAG:100 = 1
RETURN 1
;-------------------------------------------------
;実行판정処理
;밧줄で強制出来るの以外はフェラとおなじ
;-------------------------------------------------
@COM_ORDER_140, ARG, ARG:1
IF TEQUIP:ARG:밧줄
	CALL ADD_COM_ORDER, "판정무시", "결박"
	RETURN 1
ENDIF
CALL COM_ORDER_81, ARG, ARG:1
;-------------------------------------------------
;実行値
;-------------------------------------------------
@COM_ORDER_NUM_140
RETURN 36
;===================================================
;조교타입
;===================================================
@COMTYPE_140
RETURN COMTYPE("하드계")



