;-------------------------------------------------
;가슴맞대기
;조수によるコマンド、及び레즈プレイ、레벨1
;-------------------------------------------------
@ACT_COM126
PRINTL 가슴맞대기
TSTR:0 = 가슴맞대기
;CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;절정ゲージ체크（후타나리の場合）
;-------------------------------------------------
B = 1000

;ABL:조교자の기교をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;플레이어のABL:Ｂ감각をみる
IF ABL:PLAYER:6 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:6 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:6 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:6 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:6 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF


;再装填処理中は사정ゲージは20分の1に
IF CFLAG:PLAYER:9 > 0
	B /= 20
ENDIF

BASE:PLAYER:99 += B

;조교자←→조교대상
TFLAG:28 = 3
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 7
DOWNBASE:TARGET:기력 += 70

SOURCE:TARGET:쾌Ｂ = 200
SOURCE:TARGET:성행위 = 150
SOURCE:TARGET:노출 = 50
SOURCE:TARGET:불결 = 30
SOURCE:TARGET:일탈 = 100
SOURCE:TARGET:반감 = 200

;조교자にも同様の쾌Ｂ
SOURCE:PLAYER:쾌Ｂ = 200

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 100

;조교자が［유아퇴행］
IF TALENT:PLAYER:150
	TIMES SOURCE:TARGET:쾌Ｂ , 1.20
	TIMES SOURCE:TARGET:성행위 , 1.20
ENDIF

;조교자が［유치］
IF TALENT:PLAYER:151
	TIMES SOURCE:TARGET:쾌Ｂ , 1.20
	TIMES SOURCE:TARGET:성행위 , 1.20
ENDIF

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;奴隷のＢ⇔조교자の指の불결が移動
CALL MOVE_STAIN("가슴", TARGET, "손", PLAYER)
;奴隷の손⇔조교자のＢの불결が移動
CALL MOVE_STAIN("손", TARGET, "가슴", PLAYER)
;奴隷の가슴⇔조교자の가슴の불결が移動
CALL MOVE_STAIN("가슴", TARGET, "가슴", PLAYER)

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:3+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:3+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
A = 7
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;	PRINTFORML 봉사쾌락경험(조교자) + 1
	CALL ADDS_EXP("봉사쾌락경험", 1, PLAYER)
;의존도ベクトル
;P = PLAYER
IF GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:33 = 2
ELSEIF GET_RELATION(TARGET, PLAYER) < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;의존도보정値(相性100↑かつ음란で2、창부で4）
;P = PLAYER
IF TALENT:4 && GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:30 += 2
ELSEIF TALENT:7 && GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:30 += 4
ENDIF

RETURN 1

;===================================================
;조교타입
;===================================================
@COMTYPE_126
RETURN COMTYPE("조수・레즈계")



