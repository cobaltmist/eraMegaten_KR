;-------------------------------------------------
;가슴주무르기
;애무계コマンド、레벨1
;-------------------------------------------------
@JUMP_COM6
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;모유체질の場合착유に飛ぶ
SIF TALENT:모유체질
	RETURN 509
RETURN -1
@ACT_COM6


PRINTL 가슴주무르기
TSTR:0 = 가슴주무르기
;CALL KOJO_MESSAGE_COM

;조교者→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 5
DOWNBASE:TARGET:기력 += 30

SOURCE:TARGET:쾌Ｂ = 80
SOURCE:TARGET:정애 = 20
SOURCE:TARGET:반감 = 10


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 10

;조교者が［유치］か［유아퇴행］
IF TALENT:PLAYER:150 || TALENT:PLAYER:151
	TIMES SOURCE:TARGET:쾌Ｂ , 1.20
	TIMES SOURCE:TARGET:정애 , 1.20
ENDIF

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;奴隷のＢ⇔조교者の指の불결が移動
CALL MOVE_STAIN("가슴", TARGET, "손", PLAYER)

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:1+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:1+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
A = 5
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;의존도ベクトル
TFLAG:33 = 2


RETURN 1

;===================================================
;조교타입
;===================================================
@COMTYPE_6
RETURN COMTYPE("애무계")
