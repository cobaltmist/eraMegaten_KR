;-------------------------------------------------
;애널비즈
;道具사용コマンド、레벨3
;-------------------------------------------------
@COMNAME46
IF TEQUIP:애널비즈
	RESULTS = 애널비즈해제
ELSE
	RESULTS = 애널비즈
ENDIF
@ACT_COM46
PRINTL 애널비즈
TSTR:0 = 애널비즈
;CALL KOJO_MESSAGE_COM

;着脱の処理
IF TEQUIP:15 == 0
	TEQUIP:15 = 1
ELSE
	TEQUIP:15 = 0
ENDIF

;조교者→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 60
DOWNBASE:TARGET:기력 += 150

SOURCE:TARGET:도구쾌Ａ = 150
SOURCE:TARGET:아픔 = 200
SOURCE:TARGET:노출 = 50
SOURCE:TARGET:굴종 = 50
SOURCE:TARGET:일탈 = 60
SOURCE:TARGET:반감 = 80

;装着時は쾌Aは下がる
SIF TEQUIP:15 == 1
	TIMES SOURCE:TARGET:쾌Ａ , 0.80

;外す時は쾌Aは上がる
SIF TEQUIP:15 == 0
	TIMES SOURCE:TARGET:쾌Ａ , 3.00

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 70

;Aへの고통は先に処理しておく
;EXP:Ａ경험をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:TARGET:아픔 , 3.00
	TIMES SOURCE:TARGET:반감 , 3.00
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:TARGET:아픔 , 1.20
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:TARGET:아픔 , 0.80
	TIMES SOURCE:TARGET:반감 , 1.50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:TARGET:아픔 , 0.60
	TIMES SOURCE:TARGET:반감 , 1.20
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 0.40
ELSEIF EXP:1 >= EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 0.20
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:TARGET:아픔 , 1.20
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:TARGET:아픔 , 1.00
	TIMES SOURCE:TARGET:반감 , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:TARGET:아픔 , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 0.20
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
IF TEQUIP:19 == 0
	CALL ADDS_EXP("Ａ경험", 1, TARGET)
	;PRINTL Ａ경험＋１
ELSEIF TEQUIP:19 == 1
	CALL ADDS_EXP("Ａ경험", 2, TARGET)
	;PRINTL Ａ경험＋２
ENDIF

A = 1
B = NO:PLAYER + 899
IF IS_LESBIAN(TARGET,PLAYER)
	PRINTFORML %EXPNAME:40%+{A}
	CALL ADDS_EXP("레즈경험", A, TARGET)
ELSEIF IS_GAY(TARGET,PLAYER)
	PRINTFORML %EXPNAME:41%+{A}
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
ENDIF

;의존도ベクトル
TFLAG:33 = 3


RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM46
;애널비즈挿入中

PRINTL ＜애널비즈삽입중＞

DOWNBASE:TARGET:체력 += 60
DOWNBASE:TARGET:기력 += 150

SOURCE:TARGET:도구쾌Ａ += 75
SOURCE:TARGET:노출 += 25
SOURCE:TARGET:굴종 += 25
SOURCE:TARGET:일탈 += 25

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 35

;Aへの고통は先に処理しておく
;EXP:Ａ경험をみる
A = 100
B = 25
IF EXP:1 < EXPLV:1
	TIMES A , 3.00
	TIMES B , 3.00
ELSEIF EXP:1 < EXPLV:2
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF EXP:1 < EXPLV:3
	TIMES A , 0.80
	TIMES B , 1.50
ELSEIF EXP:1 < EXPLV:4
	TIMES A , 0.60
	TIMES B , 1.20
ELSEIF EXP:1 < EXPLV:5
	TIMES A , 0.40
ELSEIF EXP:1 >= EXPLV:5
	TIMES A , 0.20
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES A , 1.00
	TIMES B , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES A , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES A , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES A , 0.20
ENDIF
SOURCE:TARGET:아픔 += A
SOURCE:TARGET:반감 += B

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
CALL ADDS_EXP("Ａ경험", 1, TARGET)
;PRINTL Ａ경험＋１


RETURN 1

;===================================================
;조교타입
;===================================================
@COMTYPE_46
RETURN COMTYPE("도구계")


