;-------------------------------------------------
;파이즈리
;봉사계コマンド、레벨3
;-------------------------------------------------
@JUMP_COM82
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;전회の조교が커닐링구스か,펠라한다か、식스나인か６９파이즈리だと６９파이즈리に
IF GROUPMATCH(PREVCOM, 1, 2, 501, 508)
	CALL COM_ABLE_COMMON, 508
	SIF RESULT == 1
		RETURN 508
ENDIF
RETURN -1
@ACT_COM82
PRINTL 파이즈리

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 파이즈리
;CALL KOJO_MESSAGE_COM

;実行者が奴隷である플래그
TFLAG:28 = 1

;-------------------------------------------------
;사정ゲージ체크
;-------------------------------------------------
B = 2000

;ABL:조교자の기교をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;플레이어のABL:Ｃ감각をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20


SIF HAVE_PENIS(PLAYER)
	BASE:PLAYER:2 += B

;조교대상→조교자
TFLAG:28 = 1
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 10
DOWNBASE:TARGET:기력 += 100

SOURCE:TARGET:쾌Ｂ = 150
SOURCE:TARGET:정애 = 150
SOURCE:TARGET:성행위 = 400
SOURCE:TARGET:달성 = 200
SOURCE:TARGET:굴종 = 1000
SOURCE:TARGET:일탈 = 400
SOURCE:TARGET:반감 = 400

;上のほうで計算した불결데이터
SOURCE:TARGET:불결 = Y*10 + 60

;조교자に쾌Ｃ
SOURCE:PLAYER:0 = 150
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 600

;-------------------------------------------------
;사정체크
;-------------------------------------------------
;사정先타입の설정
TCVAR:PLAYER:페니스사용대상 = TARGET
TCVAR:PLAYER:사정개소 = 5
;사정전선택の可・
TFLAG:704 = 1

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;奴隷のＢ⇔조교자のＰの불결が移動
CALL MOVE_STAIN("가슴", TARGET, "페니스", PLAYER)

;봉사정신ＬＶ２이상、기교ＬＶ２이상なら불결をなめ取る（볼재갈사용時は除く）
IF ABL:7 >= 2 && ABL:2 >= 2 && TEQUIP:20 == 0
	;STAIN:PLAYER:2 = 2
	;タイミングをずらす
	TFLAG:청소펠라발생 = 1
	TFLAG:8 = 1
ENDIF

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:7+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:7+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;레즈경험
A = 7
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;애정경험
IF (TALENT:3 == 1 || TALENT:6 == 1) && ASSIPLAY == 0
;	PRINTS EXPNAME:23
;	PRINTL +1
	CALL ADDS_EXP("애정경험", 1, TARGET)
;	PRINTFORML 애정경험(주인)＋１
	CALL ADDS_EXP("애정경험", 1, MASTER)
ENDIF

;의존도ベクトル
TFLAG:33 = 1

;의존도보정値(主人かつ연모、음란、복종で1。친애、창부、예속で2）
IF (TALENT:3 || TALENT:4 || TALENT:5) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:6 || TALENT:7 || TALENT:8) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

TFLAG:100 = 1


RETURN 1
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------
@COM_ORDER_82, ARG, ARG:1
CALL ADD_COM_ORDER, "ABL", "욕망", 1, ARG, ARG:1
CALL ADD_COM_ORDER, "ABL", "봉사정신", 4, ARG, ARG:1
CALL ADD_COM_ORDER, "ABL", "정액중독", 3, ARG, ARG:1
CALL ADD_COM_ORDER, "MARK", "쾌락각인", 1, ARG, ARG:1
CALL ADD_COM_ORDER, "PALAM", "욕정", 1, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "부끄럼쟁이", -3, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "악취둔감", 1, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "악취민감", -3, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "헌신적", 6, ARG, ARG:1
SIF !TALENT:ARG:음란 && !TALENT:ARG:창부
	CALL ADD_COM_ORDER, "TALENT", "쾌감을부정", -3, ARG, ARG:1
;남성혐오（연모、친애で무효）
SIF HATE(ARG, ARG:1)
	CALL ADD_COM_ORDER, "TALENT", \@IS_MALE(PLAYER) ? 남성혐오 # 여성혐오 \@, -12, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "연모", 5, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "친애", 10, ARG, ARG:1
;조교자が후타나리
CALL ADD_COM_ORDER, "TALENT", "후타나리", 8, ARG:1, ARG
CALL ADD_COM_ORDER, "STAIN", "페니스", 100, ARG, ARG:1
CALL ADD_COM_ORDER, "RELATION", , , ARG, ARG:1

;-------------------------------------------------
;実行値
;-------------------------------------------------
@COM_ORDER_NUM_82
RETURN 30

;===================================================
;조교타입
;===================================================
@COMTYPE_82
RETURN COMTYPE("봉사계")
