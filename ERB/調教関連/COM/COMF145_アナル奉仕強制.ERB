;-------------------------------------------------
;애널봉사강제
;ハードな조교コマンド、레벨3
;-------------------------------------------------
@ACT_COM145
PRINTL 애널봉사강제

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 애널봉사강제
;CALL KOJO_MESSAGE_COM

;実行者が奴隷であるフラグ
TFLAG:28 = 1

;-------------------------------------------------
;사정ゲージ체크
;-------------------------------------------------
B = 250

;ABL:조교자の기교をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;TALENT:혀놀림、고양이혀をみる
SIF TALENT:52 || TALENT:54
	TIMES B , 2.00

;조수のABL:A감각をみる
IF ABL:PLAYER:5 == 0
	TIMES B , 0.50
ELSEIF ABL:PLAYER:5 == 1
	TIMES B , 0.80
ELSEIF ABL:PLAYER:5 == 2
	TIMES B , 1.00
ELSEIF ABL:PLAYER:5 == 3
	TIMES B , 1.20
ELSEIF ABL:PLAYER:5 == 4
	TIMES B , 1.50
ELSE
	TIMES B , 2.00
ENDIF

;조수のEXP:Ａ경험をみる
IF EXP:PLAYER:1 < EXPLV:1
	TIMES B , 0.20
ELSEIF EXP:PLAYER:1 < EXPLV:2
	TIMES B , 0.50
ELSEIF EXP:PLAYER:1 < EXPLV:3
	TIMES B , 0.80
ELSEIF EXP:PLAYER:1 < EXPLV:4
	TIMES B , 1.00
ELSEIF EXP:PLAYER:1 < EXPLV:5
	TIMES B , 1.20
ELSE
	TIMES B , 1.40
ENDIF
	LOCAL:99 = B
;플레이어のABL:Ｃ감각をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は사정ゲージは20分の1に
IF CFLAG:PLAYER:9 > 0
	B /= 20
	LOCAL:99 /= 20
ENDIF

SIF HAVE_PENIS(PLAYER)
	BASE:PLAYER:2 += B
SIF !IS_MALE(PLAYER)
	BASE:PLAYER:99 += LOCAL:99

;조교자←조교대상
TFLAG:28 = 1

;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 10
DOWNBASE:TARGET:기력 += 100

SOURCE:TARGET:성행위 = 300
SOURCE:TARGET:달성 = 300
SOURCE:TARGET:굴종 = 2500
SOURCE:TARGET:일탈 = 1200
SOURCE:TARGET:반감 = 1200

;上のほうで計算した불결데이터
SOURCE:TARGET:불결 = Y*80 + 50

;조교자に쾌Ａ
SOURCE:PLAYER:2 = 50
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 1200

;-------------------------------------------------
;사정체크
;-------------------------------------------------
;사정先타입の설정
TCVAR:PLAYER:페니스사용대상 = TARGET
TCVAR:PLAYER:사정개소 = 14
;사정전선택の可・
TFLAG:704 = 1

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;奴隷の입⇔조교자のＡの불결が移動
CALL MOVE_STAIN("입", TARGET, "애널", PLAYER)
;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:7+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:7+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
CALL ADDS_EXP("Ａ경험", 1, PLAYER)
A = 6
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;의존도ベクトル
TFLAG:33 = 5

;奴隷が고양이혀持ちの場合は、조교자に가학쾌락경험を추가
IF TALENT:TARGET:54
	CALL ADDS_EXP("가학쾌락경험", 1, TARGET)
;	PRINTFORML %EXPNAME:32%＋１
ENDIF

;의존도보정値(복종で1、예속で2）
IF TALENT:5 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:8 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

;봉사쾌락フラグ
TFLAG:100 = 1

RETURN 1
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------
@COM_ORDER_145, ARG, ARG:1
CALL ADD_COM_ORDER, "ABL", "욕망", 1, ARG, ARG:1
CALL ADD_COM_ORDER, "ABL", "봉사정신", 3, ARG, ARG:1
CALL ADD_COM_ORDER, "MARK", "쾌락각인", 1, ARG, ARG:1
;SIF !복종(ARG, ARG:1) && !예속(ARG, ARG:1)
;	CALL ADD_COM_ORDER, "TALENT", "반항적", -5, ARG, ARG:1
;CALL ADD_COM_ORDER, "TALENT", "프라이드높음", -12, ARG, ARG:1
;CALL ADD_COM_ORDER, "TALENT", "프라이드낮음", 2, ARG, ARG:1
;CALL ADD_COM_ORDER, "TALENT", "저항", -20, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "부끄럼쟁이", -1, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "악취둔감", 2, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "악취민감", -5, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "헌신적", 4, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "불결무시", 3, ARG, ARG:1
SIF !TALENT:ARG:음란 && !TALENT:ARG:창부
	CALL ADD_COM_ORDER, "TALENT", "쾌감을부정", -1, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "도착적", 3, ARG, ARG:1
;플레이어の새드
;CALL ADD_COM_ORDER, "TALENT", "새드", 3, ARG:1, ARG
CALL ADD_COM_ORDER, "TALENT", "연모", 5, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "친애", 10, ARG, ARG:1
CALL ADD_COM_ORDER, "STAIN", "애널", 100, ARG, ARG:1
CALL ADD_COM_ORDER, "RELATION", , , ARG, ARG:1
;-------------------------------------------------
;実行値
;-------------------------------------------------
@COM_ORDER_NUM_145
RETURN 23
;===================================================
;조교타입
;===================================================
@COMTYPE_145
RETURN COMTYPE("하드계")




