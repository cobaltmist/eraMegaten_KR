	;EXP:20 += 3
	PRINT 촉수대량사정
	IF TALENT:PLAYER:촉수 == 1 && PLAYER == MASTER
		PRINT (주인)
	ELSEIF TALENT:PLAYER:촉수 == 1 && PLAYER == ASSI
		PRINT (조수)
	ENDIF
	;PRINTL 
	;PRINTL 정액경험＋３
	IF TEQUIP:바이브
		IF CFLAG:자궁내피임결계
			PRINTL <피임결계>
		ELSE
			CALL ADDS_EXP("질사경험", 3, TARGET)
			IF TALENT:PLAYER:촉수 == 1 && PLAYER == MASTER
				CFLAG:주인의질내사정에의한임신판정 += 3
	;			PRINTL 질사경험＋３(주인)
			ELSEIF TALENT:PLAYER:촉수 == 1 && PLAYER == ASSI
				CFLAG:조수의질내사정에의한임신판정 += 3
	;			PRINTL 질사경험＋３(조수)
			ELSE
				CFLAG:촉수의질내사정에의한임신판정 += 3
	;			PRINTL 질사경험＋３(촉수)
			ENDIF
		ENDIF
		CALL ADDS_EXP("정액경험", 3, TARGET)
	ENDIF
	IF TEQUIP:PLAYER:바이브
		IF CFLAG:PLAYER:자궁내피임결계
			PRINTL <피임결계>
		ELSE
			CALL ADDS_EXP("질사경험", 3, PLAYER)
			IF TALENT:PLAYER:촉수 == 1 && PLAYER == MASTER
				CFLAG:PLAYER:주인의질내사정에의한임신판정 += 3
				;PRINTL 조교자질사경험＋３(주인)
			ELSEIF TALENT:PLAYER:촉수 == 1 && PLAYER == ASSI
				CFLAG:PLAYER:조수의질내사정에의한임신판정 += 3
				;PRINTL 조교자질사경험＋３(조수)
			ELSE
				CFLAG:PLAYER:촉수의질내사정에의한임신판정 += 3
				;PRINTL 조교자질사경험＋３(촉수)
			ENDIF
		ENDIF
		CALL ADDS_EXP("정액경험", 3, PLAYER)
	ENDIF
	;T += 1
	BASE:MASTER:사정 -= EJAC*2
	SIF BASE:MASTER:사정 >= EJAC
