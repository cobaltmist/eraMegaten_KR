;通常사정時
SIF E == 1
	A = 200
;大量사정時
SIF E == 2
	A = 500

IF E > 0
	;정액중독を見る
	IF ABL:정액중독 == 1
		TIMES A , 0.00
	ELSEIF ABL:정액중독 == 2
		TIMES A , 0.30
	ELSEIF ABL:정액중독 == 3
		TIMES A , 0.60
