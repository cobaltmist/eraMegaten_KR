		IF CFLAG:자궁내피임결계
			PRINTL <피임결계>
		ELSE
			;EXP:5 += 1
			CALL ADDS_EXP("질사경험", 1, TARGET)
			IF TALENT:PLAYER:촉수 == 1 && PLAYER == MASTER
				CFLAG:주인의질내사정에의한임신판정 += 1
				;PRINTL 질사경험＋１(주인)
			ELSEIF TALENT:PLAYER:촉수 == 1 && PLAYER == ASSI
				CFLAG:조수의질내사정에의한임신판정 += 1
				;PRINTL 질사경험＋１(조수)
			ELSE
				CFLAG:촉수의질내사정에의한임신판정 += 1
				;PRINTL 질사경험＋１(촉수)
			ENDIF
		ENDIF
		CALL ADDS_EXP("정액경험", 1, TARGET)
	ENDIF
	IF TEQUIP:PLAYER:바이브
		IF CFLAG:PLAYER:자궁내피임결계
			PRINTL <피임결계>
		ELSE
			CALL ADDS_EXP("질사경험", 1, PLAYER)
			;EXP:PLAYER:5 += 3
			IF TALENT:PLAYER:촉수 == 1 && PLAYER == MASTER
				CFLAG:PLAYER:주인의질내사정에의한임신판정 += 1
