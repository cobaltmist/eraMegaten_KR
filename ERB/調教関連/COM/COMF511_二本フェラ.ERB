;-------------------------------------------------
;두개펠라(奴隷に二本舐めさせる)
;봉사、레즈系派生コマンド
;-------------------------------------------------
@ACT_COM511
PRINTL 더블펠라

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 두개펠라
;SELECTCOM = 511
;CALL KOJO_MESSAGE_COM

;実行者が奴隷であるフラグ
TFLAG:28 = 1

;-------------------------------------------------
;사정ゲージ체크（主人）
;-------------------------------------------------
B = 750

;ABL:조교者の기교をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;TALENT:혀놀림、고양이혀をみる
SIF TALENT:52 || TALENT:54
	TIMES B , 2.00

;플레이어のABL:Ｃ감각をみる
IF ABL:MASTER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:MASTER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:MASTER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:MASTER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:MASTER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF HAVE_PENIS(MASTER)
	BASE:MASTER:2 += B


;-------------------------------------------------
;사정ゲージ체크（조수）
;-------------------------------------------------
B = 750

;ABL:조교者の기교をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;TALENT:혀놀림、고양이혀をみる
SIF TALENT:52 || TALENT:54
	TIMES B , 2.00

;플레이어のABL:Ｃ감각をみる
IF ABL:ASSI:3 == 0
	TIMES B , 1.00
ELSEIF ABL:ASSI:3 == 1
	TIMES B , 1.50
ELSEIF ABL:ASSI:3 == 2
	TIMES B , 2.00
ELSEIF ABL:ASSI:3 == 3
	TIMES B , 2.50
ELSEIF ABL:ASSI:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF HAVE_PENIS(ASSI)
	BASE:ASSI:2 += B


;조교者←조교대상
TFLAG:28 = 1
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 80
DOWNBASE:TARGET:기력 += 250

SOURCE:TARGET:정애 = 150
SOURCE:TARGET:성행위 = 400
SOURCE:TARGET:달성 = 300
SOURCE:TARGET:굴종 = 1000
SOURCE:TARGET:일탈 = 500
SOURCE:TARGET:반감 = 500


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 700

;-------------------------------------------------
;사정체크（主人）
;-------------------------------------------------
;사정先타입の설정
TCVAR:MASTER:페니스사용대상 = TARGET
TCVAR:MASTER:사정개소 = 4
;사정先選択の可・
TFLAG:704 = 0

;-------------------------------------------------
;사정체크（조수）
;-------------------------------------------------
;사정先타입の설정
TCVAR:ASSI:페니스사용대상 = TARGET
TCVAR:ASSI:사정개소 = 4
;사정先選択の可・
TFLAG:704 = 0



;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;奴隷の입⇔主人のＰの불결が移動
CALL MOVE_STAIN("입", TARGET, "페니스", PLAYER)
;奴隷の입⇔조수のＰの불결が移動
CALL MOVE_STAIN("입", TARGET, "페니스", ASSI)
;조교者のＰ⇔조수のＰの불결が移動
CALL MOVE_STAIN("페니스", ASSI, "페니스", PLAYER)

;봉사정신ＬＶ２이상、기교ＬＶ２이상なら불결をなめ取る（볼재갈사용時は除く）
IF ABL:7 >= 2 && ABL:2 >= 2 && TEQUIP:20 == 0
	;STAIN:MASTER:2 = 2
	;STAIN:ASSI:2 = 2
	TFLAG:청소펠라발생 = (4 | 8)
	TFLAG:8 = 1
ENDIF

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:MASTER:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:10+1) * BASE:LV
	CALL DRAIN_MAG,MASTER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:10+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,MASTER
ENDIF
IF CFLAG:ASSI:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:10+1) * BASE:LV
	CALL DRAIN_MAG,ASSI,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:10+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,ASSI
ENDIF



;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;펠라경험
CALL ADDS_EXP("펠라경험",2,TARGET)
;EXP:22 += 2


;레즈경험
IF IS_LESBIAN(TARGET,MASTER) && IS_LESBIAN(TARGET,ASSI) && IS_LESBIAN(MASTER,ASSI)
	A = 10
;	PRINTFORML %EXPNAME:40%+{A}
;	PRINTFORML %EXPNAME:40%(주인)+{A}
;	PRINTFORML %EXPNAME:40%(조수)+{A}
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, MASTER)
	CALL ADDS_EXP("레즈경험", A, ASSI)
ELSEIF IS_LESBIAN(TARGET,MASTER)
	A = 5
;	PRINTFORML %EXPNAME:40%+{A}
;	PRINTFORML %EXPNAME:40%(주인)+{A}
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, MASTER)
ELSEIF IS_LESBIAN(TARGET,ASSI)
	A = 5
;	PRINTFORML %EXPNAME:40%+{A}
;	PRINTFORML %EXPNAME:40%(조수)+{A}
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, ASSI)
ELSEIF IS_LESBIAN(MASTER,ASSI)
	A = 5
;	PRINTFORML %EXPNAME:40%(주인)+{A}
;	PRINTFORML %EXPNAME:40%(조수)+{A}
	CALL ADDS_EXP("레즈경험", A, MASTER)
	CALL ADDS_EXP("레즈경험", A, ASSI)
ENDIF

;ＢＬ경험
IF IS_GAY(TARGET,MASTER) && IS_GAY(TARGET,ASSI) && IS_GAY(MASTER,ASSI)
	A = 10
;	PRINTFORML %EXPNAME:41%+{A}
;	PRINTFORML %EXPNAME:41%(주인)+{A}
;	PRINTFORML %EXPNAME:41%(조수)+{A}
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, MASTER)
	CALL ADDS_EXP("ＢＬ경험", A, ASSI)
ELSEIF IS_GAY(TARGET,MASTER)
	A = 5
;	PRINTFORML %EXPNAME:41%+{A}
;	PRINTFORML %EXPNAME:41%(주인)+{A}
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, MASTER)
ELSEIF IS_GAY(TARGET,ASSI)
	A = 5
;	PRINTFORML %EXPNAME:41%+{A}
;	PRINTFORML %EXPNAME:41%(조수)+{A}
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, ASSI)
ELSEIF IS_GAY(MASTER,ASSI)
	A = 5
;	PRINTFORML %EXPNAME:41%(주인)+{A}
;	PRINTFORML %EXPNAME:41%(조수)+{A}
	CALL ADDS_EXP("ＢＬ경험", A, MASTER)
	CALL ADDS_EXP("ＢＬ경험", A, ASSI)
ENDIF


;奴隷が고양이혀持ちの場合は、조교者に가학쾌락경험を추가
IF TALENT:TARGET:54
	CALL ADDS_EXP("가학쾌락경험", 1, TARGET)
;	PRINTFORML %EXPNAME:32%＋１
ENDIF

;의존도ベクトル
;P = PLAYER
IF GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:33 = 2
ELSEIF GET_RELATION(TARGET, PLAYER) < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;의존도보정値(相性100↑かつ음란で2、창부で4）
;P = PLAYER
IF TALENT:4 && GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:30 += 2
ELSEIF TALENT:7 && GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:30 += 4
ENDIF

;봉사쾌락フラグ
TFLAG:100 = 1


RETURN 1
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------
@COM_ORDER_511, ARG, ARG:1
IF TEQUIP:ARG:음문발동
	CALL ADD_COM_ORDER, "판정無視", "음문 발동중"
	RETURN 0
ENDIF
CALL ADD_COM_ORDER, "ABL", "욕망", 1, ARG, ARG:1
CALL ADD_COM_ORDER, "ABL", "봉사정신", 4, ARG, ARG:1
CALL ADD_COM_ORDER, "ABL", "정액중독", 3, ARG, ARG:1
CALL ADD_COM_ORDER, "MARK", "쾌락각인", 1, ARG, ARG:1
CALL ADD_COM_ORDER, "PALAM", "욕정", 1, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "부끄럼쟁이", -1, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "악취둔감", 1, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "악취민감", -3, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "헌신적", 6, ARG, ARG:1
SIF !TALENT:ARG:음란 && !TALENT:ARG:창부
	CALL ADD_COM_ORDER, "TALENT", "쾌감을부정", -1, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "음란", 5, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "창부", 10, ARG, ARG:1
;조교者が후타나리
CALL ADD_COM_ORDER, "TALENT", "후타나리", 8, ARG:1, ARG
CALL ADD_COM_ORDER, "STAIN", "페니스", 100, ARG, ARG:1
CALL ADD_COM_ORDER, "STAIN", "페니스", 100, ARG, SUBPLAYER()
CALL ADD_COM_ORDER, "RELATION", , , ARG, ARG:1
CALL ADD_COM_ORDER, "RELATION", , , ARG, SUBPLAYER()
;-------------------------------------------------
;実行値
;-------------------------------------------------
@COM_ORDER_NUM_511
RETURN 32
;===================================================
;조교타입
;===================================================
@COMTYPE_511
RETURN COMTYPE("派生系")


