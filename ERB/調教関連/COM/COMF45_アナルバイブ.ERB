;-------------------------------------------------
;애널바이브
;道具사용コマンド、레벨2
;-------------------------------------------------
@COMNAME45
IF TEQUIP:애널바이브
	RESULTS = 애널바이브해제
ELSE
	RESULTS = 애널바이브
ENDIF
@ACT_COM45
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
IF TEQUIP:25
	PRINTL 애널촉수
	TSTR:0 = 애널촉수
	SELECTCOM = 162
ELSE
	PRINTL 애널바이브
	TSTR:0 = 애널바이브
ENDIF
;CALL KOJO_MESSAGE_COM

;着脱の処理
IF TEQUIP:14 == 0
	TEQUIP:14 = 1
	;애널촉수は촉수の불결を추가
	IF TEQUIP:25
		CALL SET_STAIN("애널", "점액", TARGET)
	ENDIF
ELSE
	TEQUIP:14 = 0
ENDIF
;조교자→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 60
DOWNBASE:TARGET:기력 += 150

SOURCE:TARGET:도구쾌Ａ = 250
SOURCE:TARGET:노출 = 50
SOURCE:TARGET:굴종 = 50
SOURCE:TARGET:일탈 = 50

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 70

;Aへの고통は先に処理しておく
;EXP:Ａ경험をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:TARGET:아픔 , 3.00
	TIMES SOURCE:TARGET:반감 , 3.00
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:TARGET:아픔 , 1.20
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:TARGET:아픔 , 0.80
	TIMES SOURCE:TARGET:반감 , 1.50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:TARGET:아픔 , 0.60
	TIMES SOURCE:TARGET:반감 , 1.20
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 0.40
ELSEIF EXP:1 >= EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 0.20
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:TARGET:아픔 , 1.20
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:TARGET:아픔 , 1.00
	TIMES SOURCE:TARGET:반감 , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:TARGET:아픔 , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 0.20
ENDIF

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;애널촉수なら、촉수体液が애널に付く
SIF TEQUIP:25
	CALL SET_STAIN("애널", "점액", TARGET)

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;A경험
CALL ADDS_EXP("Ａ경험", 2, TARGET)
;PRINTL Ａ경험＋２

;레즈경험
A = 1
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;촉수소환時、촉수경험の리셋をする
;SIF TEQUIP:25 == 1
;	T = 0

;의존도ベクトル
TFLAG:33 = 3


RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM45
;애널바이브挿入中
IF TEQUIP:25
	PRINTL ＜애널촉수삽입중＞
ELSE
	PRINTL ＜애널바이브삽입중＞
ENDIF

DOWNBASE:TARGET:체력 += 30
DOWNBASE:TARGET:기력 += 80

SOURCE:TARGET:도구쾌Ａ += 125
SOURCE:TARGET:아픔 += 100
SOURCE:TARGET:노출 += 25
SOURCE:TARGET:일탈 += 25
SOURCE:TARGET:반감 += 40

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 35

;Aへの고통は先に処理しておく
;EXP:Ａ경험をみる
A = 100
B = 40
IF EXP:1 < EXPLV:1
	TIMES A , 3.00
	TIMES B , 3.00
ELSEIF EXP:1 < EXPLV:2
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF EXP:1 < EXPLV:3
	TIMES A , 0.80
	TIMES B , 1.50
ELSEIF EXP:1 < EXPLV:4
	TIMES A , 0.60
	TIMES B , 1.20
ELSEIF EXP:1 < EXPLV:5
	TIMES A , 0.40
ELSEIF EXP:1 >= EXPLV:5
	TIMES A , 0.20
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES A , 1.00
	TIMES B , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES A , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES A , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES A , 0.20
ENDIF
SOURCE:TARGET:아픔 += A
SOURCE:TARGET:반감 += B

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;자위実行時は경험＋１
IF SELECTCOM == 11
	CALL ADDS_EXP("Ａ경험", 2, TARGET)
	;PRINTL Ａ경험＋２
ELSE
	CALL ADDS_EXP("Ａ경험", 1, TARGET)
	;PRINTL Ａ경험＋１
ENDIF

;촉수경험加算
SIF TEQUIP:25 == 1
	CALL ADDS_EXP("촉수경험", 1, TARGET)
;	T += 1


RETURN 1

;===================================================
;조교타입
;===================================================
@COMTYPE_45
RETURN COMTYPE("도구계")


