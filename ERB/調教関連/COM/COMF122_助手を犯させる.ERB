;-------------------------------------------------
;조수를범하게한다
;조수によるコマンド、及び레즈プレイ、레벨4
;-------------------------------------------------
@ACT_COM122
PRINTL 조수를범하게한다

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 조수를범하게한다
;CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;사정ゲージ체크（후타나리の場合）
;-------------------------------------------------
B = 2000

;ABL:조교자の기교をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;조수のABL:V감각をみる
IF ABL:PLAYER:4 == 0
	TIMES B , 0.50
ELSEIF ABL:PLAYER:4 == 1
	TIMES B , 0.80
ELSEIF ABL:PLAYER:4 == 2
	TIMES B , 1.00
ELSEIF ABL:PLAYER:4 == 3
	TIMES B , 1.20
ELSEIF ABL:PLAYER:4 == 4
	TIMES B , 1.50
ELSE
	TIMES B , 2.00
ENDIF

;조수のEXP:Ｖ경험をみる
IF EXP:PLAYER:0 < EXPLV:1
	TIMES B , 0.20
ELSEIF EXP:PLAYER:0 < EXPLV:2
	TIMES B , 0.50
ELSEIF EXP:PLAYER:0 < EXPLV:3
	TIMES B , 0.80
ELSEIF EXP:PLAYER:0 < EXPLV:4
	TIMES B , 1.00
ELSEIF EXP:PLAYER:0 < EXPLV:5
	TIMES B , 1.20
ELSE
	TIMES B , 1.40
ENDIF

	LOCAL:99 = B
;플레이어のABL:Ｃ감각をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は사정ゲージは20分の1に
IF CFLAG:PLAYER:9 > 0
	B /= 20
	LOCAL:99 /= 20
ENDIF

IF HAVE_PENIS(PLAYER)
	BASE:PLAYER:2 += B
	TEQUIP:PLAYER:Ｖ삽입자 = TARGET
	TFLAG:삽입속행 = 1
ENDIF
SIF TALENT:PLAYER:남자 == 0
	BASE:PLAYER:99 += LOCAL:99

;사정先타입の설정
TCVAR:PLAYER:페니스사용대상 = TARGET
TCVAR:PLAYER:사정개소 = 14
;사정전선택の可・
TFLAG:704 = 0

;조교자←→조교대상
TFLAG:28 = 3
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 50
DOWNBASE:TARGET:기력 += 150

SOURCE:TARGET:쾌Ｃ = 400
SOURCE:TARGET:성행위 = 500
SOURCE:TARGET:달성 = 400
SOURCE:TARGET:굴종 = 700
SOURCE:TARGET:일탈 = 500
SOURCE:TARGET:반감 = 500


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 500

;조수が처녀の場合、굴복일탈が倍化し、순종が2になる。ただし一度きり
IF TALENT:PLAYER:0 == 1 && CFLAG:99 == 0
	TIMES SOURCE:TARGET:굴종 , 5.00
	TIMES SOURCE:TARGET:일탈 , 3.00
	;TALENT:ASSI:0 = 0
	;이상경험추가（一回きり）
	;EXP:50 += 1
	;PRINTFORML %EXPNAME:50%＋１
	SIF !IS_MALE(TARGET)
		CALL ADDS_ABNORMAL_EXP("처녀탈취（동성）", TARGET)
	SIF !IS_MALE(TARGET)
		CALL ADDS_ABNORMAL_EXP("처녀상실（동성）", PLAYER)
	IF ABL:0 < 2
		PRINTL ＜조수가 처녀＞
		PRINTFORML 그리고 %ABLNAME:0%이 2가 되었다
		ABL:0 = 2
	ENDIF
	CFLAG:99 = 1
ENDIF
CALL ADDS_EXP("Ｖ경험", 1, PLAYER)
;EXP:ASSI:0 += 1

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;사정に関係なく行われる処理はここから
IF HAVE_PENIS(TARGET)
	;奴隷のＰ⇔조교자のＶの불결が移動
	CALL MOVE_STAIN("페니스", TARGET, "바기나", PLAYER)
ENDIF

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:10+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:10+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;레즈경험
A = 10
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;근친상간の場合이상경험부여
IF 근친체크(TARGET , PLAYER)
	CALL ADDS_ABNORMAL_EXP( @"근친상간（%RESULTS:0%）", TARGET)
	CALL ADDS_ABNORMAL_EXP( @"근친상간（%RESULTS:1%）", PLAYER)
ENDIF

;의존도ベクトル
;P = PLAYER
IF GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:33 = 2
ELSEIF GET_RELATION(TARGET, PLAYER) < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;의존도보정値(相性100↑かつ음란で2、창부で4）
;P = PLAYER
IF TALENT:4 && GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:30 += 2
ELSEIF TALENT:7 && GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:30 += 4
ENDIF



RETURN 1

;-------------------------------------------------
;実行판정処理
;-------------------------------------------------
@COM_ORDER_122, ARG, ARG:1
CALL ADD_COM_ORDER, "ABL", "욕망", 2, ARG, ARG:1
CALL ADD_COM_ORDER, "ABL", "봉사정신", 4, ARG, ARG:1

CALL ADD_COM_ORDER, "MARK", "쾌락각인", 2, ARG, ARG:1

CALL ADD_COM_ORDER, "PALAM", "욕정", 2, ARG, ARG:1

CALL ADD_COM_ORDER, "TALENT", "부끄럼쟁이", -1, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "헌신적", 6, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "쾌감에솔직", 2, ARG, ARG:1
SIF !TALENT:ARG:음란 && !TALENT:ARG:창부
	CALL ADD_COM_ORDER, "TALENT", "쾌감을부정", -2, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "음란", 5, ARG, ARG:1
CALL ADD_COM_ORDER, "TALENT", "창부", 10, ARG, ARG:1

;助手が処女
SIF TALENT:PLAYER:처녀 == 1
	CALL ADD_COM_ORDER, "EXTRA", "조수가처녀", -15, ARG, ARG:1

CALL ADD_COM_ORDER, "RELATION", , , ARG, ARG:1
;-------------------------------------------------
;実行値
;-------------------------------------------------
@COM_ORDER_NUM_122
RETURN 40

;===================================================
;조교타입
;===================================================
@COMTYPE_122
RETURN COMTYPE("조수・레즈계")



