;-------------------------------------------------
;커닐링구스
;애무계コマンド　레벨1
;-------------------------------------------------
@JUMP_COM1
;전회の조교が펠라치오か커닐링구스강제か식스나인だと식스나인に
IF GROUPMATCH(PREVCOM, 81 , 120 , 501)
	CALL COM_ABLE_COMMON, 501
	SIF RESULT == 1
		RETURN 501
ENDIF
;전회の조교が파이즈리か파이즈리펠라か６９파이즈리だと６９파이즈리に
IF GROUPMATCH(PREVCOM, 82 , 88, 508)
	CALL COM_ABLE_COMMON, 508
	SIF RESULT == 1
		RETURN 508
ENDIF
;수치플레이中は이와시미즈に
SIF TEQUIP:30 == 1
	RETURN 502
RETURN -1
@ACT_COM1
PRINTL 커닐링구스
TSTR:0 = 커닐링구스
;CALL KOJO_MESSAGE_COM

;조교자→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 5
DOWNBASE:TARGET:기력 += 50

SOURCE:TARGET:쾌Ｃ = 100
SOURCE:TARGET:액체 = 60
SOURCE:TARGET:노출 = 10
SOURCE:TARGET:일탈 = 15
SOURCE:TARGET:반감 = 15

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:TARGET:울굴 += 20

;舌を使う조교では、조교자の혀놀림によって、쾌感のソース증가
SIF TALENT:PLAYER:혀놀림
	TIMES SOURCE:TARGET:쾌Ｃ , 1.50

;고양이혀では減少
SIF TALENT:PLAYER:54
	TIMES SOURCE:TARGET:쾌Ｃ , 0.75

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;奴隷のＶ⇔조교자の입の불결が移動
CALL MOVE_STAIN("바기나", TARGET, "입", PLAYER)

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:4+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:4+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
A = 3
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;조교자が고양이혀持ちの場合は、조교자に가학쾌락경험を추가
IF TALENT:PLAYER:54
	CALL ADDS_EXP("가학쾌락경험", 1, PLAYER)
ENDIF

;의존도ベクトル
TFLAG:33 = 1

RETURN 1


;===================================================
;조교타입
;===================================================
@COMTYPE_1
RETURN COMTYPE("애무계")