;-------------------------------------------------
;펠라한다
;애무계コマンド、레벨1
;-------------------------------------------------
@JUMP_COM2
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;전회の조교が펠라치오か커닐링구스강제か식스나인だと식스나인に
IF GROUPMATCH(PREVCOM, 81, 120, 501)
	CALL COM_ABLE_COMMON, 501
	SIF RESULT == 1
		RETURN 501
ENDIF
;전회の조교が파이즈리か파이즈리펠라か６９파이즈리だと６９파이즈리に
IF GROUPMATCH(PREVCOM, 82, 88, 508)
	CALL COM_ABLE_COMMON, 508
	SIF RESULT == 1
		RETURN 508
ENDIF
RETURN -1
@ACT_COM2
PRINTL 펠라한다
TSTR:0 = 펠라한다
;CALL KOJO_MESSAGE_COM

;조교者→조교대상
TFLAG:28 = 2
TEQUIP:PLAYER:입삽입자 = TARGET
TFLAG:삽입속행 = 1
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
;소비체력、기력
DOWNBASE:TARGET:체력 += 10
DOWNBASE:TARGET:기력 += 80

SOURCE:TARGET:쾌Ｃ = 120
SOURCE:TARGET:노출 = 20
SOURCE:TARGET:일탈 = 20
SOURCE:TARGET:반감 = 20


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 20

;조교者のTALENT:혀놀림をみる
SIF TALENT:PLAYER:52
	TIMES SOURCE:TARGET:쾌Ｃ , 1.50

;조교者のTALENT:고양이혀をみる
SIF TALENT:PLAYER:54
	TIMES SOURCE:TARGET:쾌Ｃ , 0.75


;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;奴隷のＰ⇔조교者の입の불결が移動
CALL MOVE_STAIN("페니스", TARGET, "입", PLAYER)

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:5+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:5+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
A = 3
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;PRINTFORML 펠라경험(조교者) + 1
CALL ADDS_EXP("펠라경험", 1, PLAYER)

;조교者が고양이혀持ちの場合は、조교者に가학쾌락경험を추가
IF TALENT:PLAYER:54
	CALL ADDS_EXP("가학쾌락경험", 1, PLAYER)
ENDIF

;의존도ベクトル
IF TALENT:PLAYER:140 == 1
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 2
ENDIF

RETURN 1


