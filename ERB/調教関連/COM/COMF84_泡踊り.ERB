;-------------------------------------------------
;거품춤
;봉사계コマンド、레벨3
;-------------------------------------------------
@ACT_COM84
PRINTL 거품춤

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 거품춤
;CALL KOJO_MESSAGE_COM

;조교대상→조교者
TFLAG:28 = 1

;-------------------------------------------------
;사정ゲージ체크
;-------------------------------------------------
B = 2500

;ABL:조교者の기교をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;플레이어のABL:Ｃ감각をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20


SIF HAVE_PENIS(PLAYER)
	BASE:PLAYER:2 += B

;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 50
DOWNBASE:TARGET:기력 += 180

SOURCE:TARGET:쾌Ｃ = 200
SOURCE:TARGET:쾌Ｂ = 200
SOURCE:TARGET:정애 = 150
SOURCE:TARGET:성행위 = 450
SOURCE:TARGET:달성 = 250
SOURCE:TARGET:굴종 = 1200
SOURCE:TARGET:일탈 = 600
SOURCE:TARGET:반감 = 600

;조교者に쾌Ｃ、쾌Ｂ
SOURCE:PLAYER:0 = 200
SOURCE:PLAYER:3 = 200
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 1000

;-------------------------------------------------
;사정체크
;-------------------------------------------------

;사정先타입の설정
TCVAR:PLAYER:페니스사용대상 = TARGET
TCVAR:PLAYER:사정개소 = 14
;사정先選択の可・
TFLAG:704 = 1

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;조교대상의 불결을 씻겨내기
RESET_STAIN TARGET
;조교자와 조수(만약 있다면)의 불결을 씻겨내기
RESET_STAIN PLAYER
SIF ASSI >= 0
	RESET_STAIN ASSI
	
;IF ASSIPLAY == 0
;	STAIN:MASTER:0 = 0
;	STAIN:MASTER:1 = 0
;	STAIN:MASTER:2 = 2
;	STAIN:MASTER:3 = 1
;	STAIN:MASTER:4 = 8
;	STAIN:MASTER:5 = 0
;	STAIN:MASTER:6 = 0
;ELSE
;	STAIN:ASSI:0 = 0
;	STAIN:ASSI:1 = 0
;	STAIN:ASSI:2 = 2
;	STAIN:ASSI:3 = 1
;	STAIN:ASSI:4 = 8
;	STAIN:ASSI:6 = 0
;ENDIF

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:7+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:7+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;레즈경험
A = 7
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;애정경험
IF (TALENT:3 == 1 || TALENT:6 == 1) && ASSIPLAY == 0
;	PRINTS EXPNAME:23
;	PRINTL +1
	CALL ADDS_EXP("애정경험", 1, TARGET)
;	PRINTFORML 애정경험(주인)＋１
	CALL ADDS_EXP("애정경험", 1, MASTER)
ENDIF

;의존도ベクトル
TFLAG:33 = 1

;의존도보정値(主人かつ연모、음란、복종で1。친애、창부、예속で2）
IF (TALENT:3 || TALENT:4 || TALENT:5) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:6 || TALENT:7 || TALENT:8) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

TFLAG:100 = 1


RETURN 1
;-------------------------------------------------
;実行판정処理
;파이즈리とおなじ
;-------------------------------------------------
@COM_ORDER_84, ARG, ARG:1
IF TEQUIP:ARG:음문발동
	CALL ADD_COM_ORDER, "판정無視", "음문 발동중"
	RETURN 0
ENDIF
CALL COM_ORDER_82, ARG, ARG:1
;-------------------------------------------------
;実行値
;-------------------------------------------------
@COM_ORDER_NUM_84

RETURN 30
;===================================================
;조교타입
;===================================================
@COMTYPE_84
RETURN COMTYPE("봉사계")
