;-------------------------------------------------
;확장벌룬
;ハードな조교コマンド、레벨4
;-------------------------------------------------
@COMNAME148
RESULTS = 확장벌룬
SIF TEQUIP:22
	RESULTS = 확장벌룬해제
@ACT_COM148
;---- EDIT 002 촉수系コマンドを수정 MOD START ----
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
IF TEQUIP:25
	PRINTL 애널극태촉수
	TSTR:0 = 애널극태촉수
	SELECTCOM = 176
ELSE
	PRINTL 확장벌룬
	TSTR:0 = 확장벌룬
ENDIF
;---- EDIT 002 촉수系コマンドを수정 MOD END   ----
;CALL KOJO_MESSAGE_COM

;着脱の処理
IF TEQUIP:22 == 0
	TEQUIP:22 = 1
ELSE
	TEQUIP:22 = 0
ENDIF

;조교者→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 150
DOWNBASE:TARGET:기력 += 200

SOURCE:TARGET:도구쾌Ａ = 200
SOURCE:TARGET:아픔 = 800
SOURCE:TARGET:노출 = 300
SOURCE:TARGET:굴종 = 400
SOURCE:TARGET:일탈 = 700
SOURCE:TARGET:반감 = 700


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 700

;Aへの고통は先に処理しておく
;EXP:Ａ경험をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:TARGET:아픔 , 3.00
	TIMES SOURCE:TARGET:반감 , 3.00
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:TARGET:아픔 , 1.20
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:TARGET:아픔 , 0.80
	TIMES SOURCE:TARGET:반감 , 1.50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:TARGET:아픔 , 0.60
	TIMES SOURCE:TARGET:반감 , 1.20
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 0.40
ELSEIF EXP:1 >= EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 0.20
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:TARGET:아픔 , 1.20
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:TARGET:아픔 , 1.00
	TIMES SOURCE:TARGET:반감 , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:TARGET:아픔 , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 0.20
ENDIF


;-------------------------------------------------
;경험の処理
;-------------------------------------------------
CALL ADDS_EXP("Ａ경험", 25, TARGET)
;PRINTL Ａ경험＋５

;IF EXP:52 == 0 && EXP:53 == 0 
;	EXP:50 += 1
;	PRINTL 이상경험＋１
;	CALL ADDS_ABNORMAL_EXP("확장첫경험", TARGET)
;ENDIF

CALL ADDS_EXP("Ａ확장경험", 1, TARGET)
;PRINTL Ａ확장경험＋1

A = 1
IF IS_LESBIAN(TARGET,PLAYER)
	PRINTFORML %EXPNAME:40%+{A}
	CALL ADDS_EXP("레즈경험", A, TARGET)
ELSEIF IS_GAY(TARGET,PLAYER)
	PRINTFORML %EXPNAME:41%+{A}
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
ENDIF

;의존도ベクトル
TFLAG:33 = 5

;의존도보정値(복종で1、예속で2）
IF TALENT:5 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:8 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM148
;확장벌룬

;---- EDIT 002 촉수系コマンドを수정 MOD START ----
IF TEQUIP:25
	PRINTL ＜애널극태촉수중＞
ELSE
	PRINTL ＜확장벌룬삽입중＞
ENDIF
;---- EDIT 002 촉수系コマンドを수정 MOD END   ----

DOWNBASE:TARGET:체력 += 75
DOWNBASE:TARGET:기력 += 100

SOURCE:TARGET:도구쾌Ａ += 100
SOURCE:TARGET:노출 += 150
SOURCE:TARGET:굴종 += 200
SOURCE:TARGET:일탈 += 350

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 350

;Aへの고통は先に処理しておく
;EXP:Ａ경험をみる
A = 400
B = 350
IF EXP:1 < EXPLV:1
	TIMES A , 3.00
	TIMES B , 3.00
ELSEIF EXP:1 < EXPLV:2
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF EXP:1 < EXPLV:3
	TIMES A , 0.80
	TIMES B , 1.50
ELSEIF EXP:1 < EXPLV:4
	TIMES A , 0.60
	TIMES B , 1.20
ELSEIF EXP:1 < EXPLV:5
	TIMES A , 0.40
ELSEIF EXP:1 >= EXPLV:5
	TIMES A , 0.20
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES A , 1.00
	TIMES B , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES A , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES A , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES A , 0.20
ENDIF
SOURCE:TARGET:아픔 += A
SOURCE:TARGET:반감 += B

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
CALL ADDS_EXP("Ａ경험", 3, TARGET)
;PRINTL Ａ경험＋３

CALL ADDS_EXP("Ａ확장경험", 1, TARGET)
;PRINTL Ａ확장경험＋1

;===================================================
;조교타입
;===================================================
@COMTYPE_148
RETURN COMTYPE("하드계")




