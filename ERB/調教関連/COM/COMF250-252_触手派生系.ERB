;-------------------------------------------------

@COMNAME250
;触手Ｖピストン

IF TEQUIP:13 == 1
	RESULTS = 촉수Ｖ피스톤
ENDIF

@ACT_COM250
PRINTL 촉수Ｖ피스톤
TSTR:0 = 촉수Ｖ피스톤
SELECTCOM = 250
;CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:TARGET:체력 += 60
DOWNBASE:TARGET:기력 += 120

SOURCE:TARGET:쾌Ｖ = 800
SOURCE:TARGET:정애 = 2000
SOURCE:TARGET:아픔 = 400
SOURCE:TARGET:굴종 = 7500
SOURCE:TARGET:중독충족 = 2000
SOURCE:TARGET:노출 = 1200
SOURCE:TARGET:액체 = 4000
SOURCE:TARGET:욕정 = 12000

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;自慰実行時は経験＋１
IF SELECTCOM == 11
	CALL ADDS_EXP("Ｖ경험", 2, TARGET)
ELSE
	CALL ADDS_EXP("Ｖ경험", 1, TARGET)
ENDIF

;触手経験加算
SIF TEQUIP:촉수 == 1
	CALL ADDS_EXP("촉수경험", 1, TARGET)
;	T += 1
CALL ADDS_EXP("Ｖ확장경험", 1, TARGET)
;PRINTL Ｖ拡張経験＋1


RETURN 1


;===================================================
;調教タイプ
;===================================================
@COMTYPE_250
RETURN COMTYPE("촉수계")

RETURN 1

