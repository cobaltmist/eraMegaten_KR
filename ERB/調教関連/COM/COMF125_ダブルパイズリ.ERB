;-------------------------------------------------
;더블파이즈리
;조수によるコマンド、及び레즈プレイ、레벨3
;-------------------------------------------------
@ACT_COM125
PRINTL 더블파이즈리

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 더블파이즈리
;CALL KOJO_MESSAGE_COM

;実行者が奴隷であるフラグ
TFLAG:28 = 1

;-------------------------------------------------
;사정ゲージ체크
;-------------------------------------------------
B = 2500

;ABL:조교자の기교をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;TALENT:혀놀림、고양이혀をみる
SIF TALENT:52 || TALENT:54
	TIMES B , 2.00

;조수のABL:기교をみる
IF ABL:ASSI:2 >= 2
	TIMES B , 1.00
ELSEIF ABL:ASSI:2 == 3
	TIMES B , 1.20
ELSEIF ABL:ASSI:2 == 4
	TIMES B , 1.50
ELSE
	TIMES B , 1.80
ENDIF

;플레이어のABL:Ｃ감각をみる
IF ABL:MASTER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:MASTER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:MASTER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:MASTER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:MASTER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF


SIF HAVE_PENIS(MASTER)
	BASE:MASTER:2 += B

;조교자→조교대상
TFLAG:28 = 1
;조교자に쾌Ｃ
SOURCE:PLAYER:0 = 250
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 10
DOWNBASE:TARGET:기력 += 100

SOURCE:TARGET:쾌Ｂ = 150
SOURCE:TARGET:정애 = 200
SOURCE:TARGET:성행위 = 500
SOURCE:TARGET:달성 = 250
SOURCE:TARGET:굴종 = 900
SOURCE:TARGET:일탈 = 400
SOURCE:TARGET:반감 = 400

;上のほうで計算した불결데이터
SOURCE:TARGET:불결 = Y*40 + 100


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 600

;-------------------------------------------------
;사정체크
;-------------------------------------------------
;사정先타입の설정
TCVAR:MASTER:페니스사용대상 = TARGET + (ASSI+1)*10000
TCVAR:MASTER:사정개소 = 4
;사정전선택の可・
TFLAG:704 = 1

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;奴隷のＢ⇔조교자のＰの불결が移動
CALL MOVE_STAIN("가슴", TARGET, "페니스", PLAYER)
;조수のＢ⇔조교자のＰの불결が移動
CALL MOVE_STAIN("가슴", ASSI, "페니스", PLAYER)
;奴隷の가슴⇔조수の가슴の불결が移動
CALL MOVE_STAIN("가슴", TARGET, "가슴", ASSI)

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
A = 7

;マスターと奴隷の레즈
IF IS_LESBIAN(TARGET,MASTER)
;	PRINTFORML %EXPNAME:40%+{A}
;	PRINTFORML %EXPNAME:40%(주인)+{A}
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, MASTER)
ELSEIF IS_GAY(TARGET,MASTER)
;	PRINTFORML %EXPNAME:41%+{A}
;	PRINTFORML %EXPNAME:41%(주인)+{A}
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, MASTER)
ENDIF


;조수と奴隷の레즈
IF IS_LESBIAN(TARGET,ASSI)
;	PRINTFORML %EXPNAME:40%+{A}
;	PRINTFORML %EXPNAME:40%(조수)+{A}
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, ASSI)
ELSEIF IS_GAY(TARGET,ASSI)
;	PRINTFORML %EXPNAME:41%+{A}
;	PRINTFORML %EXPNAME:41%(조수)+{A}
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, ASSI)
ENDIF

;의존도ベクトル
;P = PLAYER
IF GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:33 = 2
ELSEIF GET_RELATION(TARGET, PLAYER) < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;의존도보정値(相性100↑かつ음란で2、창부で4）
;P = PLAYER
IF TALENT:4 && GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:30 += 2
ELSEIF TALENT:7 && GET_RELATION(TARGET, PLAYER) > 100
	TFLAG:30 += 4
ENDIF


TFLAG:100 = 1



RETURN 1
;-------------------------------------------------
;実行판정処理
;더블펠라と同じ
;-------------------------------------------------
@COM_ORDER_125, ARG, ARG:1
CALL COM_ORDER_124, ARG, ARG:1

;-------------------------------------------------
;実行値
;-------------------------------------------------
@COM_ORDER_NUM_125
RETURN 38
;===================================================
;조교타입
;===================================================
@COMTYPE_125
RETURN COMTYPE("조수・레즈계")



