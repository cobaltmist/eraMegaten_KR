@STAIN_INFO
#DIM CHARA , 1
#DIM 表示 , 3
#DIM LCOUNT
#LOCALSIZE 2
REDRAW 0
LOCAL:1 = LINECOUNT
DRAWLINE

FOR LOCAL , 0 , 2 + (ASSI>0)
	IF LOCAL == 0
		CHARA = MASTER
	ELSEIF LOCAL == 1
		CHARA = TARGET
	ELSE
		CHARA = ASSI
	ENDIF
	IF 表示:LOCAL == 1
		PRINTFORML 【%CALLNAME:CHARA%의 불결】[{LOCAL}]비표시
		FOR LCOUNT, 0, 22
			SIF LCOUNT == 2 && !HAVE_PENIS(CHARA)
				CONTINUE
			SIF LCOUNT == 3 && !HAVE_VAGINA(CHARA)
				CONTINUE
			SIF LCOUNT == 5 && !HAVE_TIT(CHARA)
				CONTINUE
			SIF LCOUNT == 6 && !HAVE_VAGINA(CHARA)
				CONTINUE
			SIF LCOUNT == 8
				LCOUNT = 10
			SIF RANGE(LCOUNT , 10 , 21) && !CFLAG:CHARA:GET_CLOTHESNAME(LCOUNT - 10)
				CONTINUE
			SELECTCASE LCOUNT
				CASE 2
					PRINT 　Ｐ：
				CASE 3
					PRINT 　Ｖ：
				CASE 4
					PRINT 　Ａ：
				CASE 5
					PRINT 　Ｂ：
				CASE 6
					PRINT 　질：
				CASE 10 TO 21
					CALLFORM CLOTHES_NAME_{CFLAG:CHARA:GET_CLOTHESNAME(LCOUNT - 10)}
					PRINTFORM 　%RESULTS%：
				CASEELSE
					PRINTFORM 　%STAINNAME:LCOUNT%：
			ENDSELECT
			SIF STAIN:CHARA:LCOUNT & 1
				PRINT <애액>
			SIF STAIN:CHARA:LCOUNT & 2
				PRINT <페니스>
			SIF STAIN:CHARA:LCOUNT & 4
				PRINT <정액>
			SIF STAIN:CHARA:LCOUNT & 8
				PRINT <애널>
			SIF STAIN:CHARA:LCOUNT & 16
				PRINT <모유>
			SIF STAIN:CHARA:LCOUNT & 32
				PRINT <점액>
			SIF STAIN:CHARA:LCOUNT & 64
				PRINT <파과혈>
			PRINTL 
		NEXT
	ELSE
		PRINTFORML 【%CALLNAME:CHARA%의 불결】[{LOCAL}]표시
	ENDIF
	PRINTL
NEXT
PRINTL [100] 돌아간다
CALL INPUTINT , 0 , 1 , 2*(ASSI>0) , 100
IF RESULT == 100
;돌아간다
	REDRAW 1
	RETURN 0
ELSE
	CLEARLINE LINECOUNT - LOCAL:1
	INVERTBIT 表示:RESULT , 0
	RESTART
ENDIF

