;====================================================================
;ＮＴＲ요바이
;====================================================================

@EVENT_NTR_SELF
#LOCALSIZE 1
#DIM LCOUNT
CUSTOMDRAWLINE =
LOCAL = 0
;キャラ登録数が1以下の時は引き返す
SIF CHARANUM <= 1
	RETURN 1
FOR LCOUNT, 0, CHARANUM
	;主人ならスキップ
	SIF LCOUNT == MASTER
		CONTINUE
	;대상がその場にいないなら除外
	SIF CFLAG:LCOUNT:이자리에없는플래그 == 1
		CONTINUE
	;렌탈中は除外
	SIF CFLAG:LCOUNT:노역플래그 == 3
		CONTINUE
	;체력が500以下なら除外
	SIF BASE:LCOUNT:0 <= 500
		CONTINUE
	;最も호감도が高いキャラがＮＰＣでなければ除外
	SIF CFLAG:LCOUNT:함락캐릭터 > -1
		CONTINUE
	;トロは別이벤트
	SIF CFLAG:LCOUNT:함락캐릭터 == -16
		CONTINUE
	;上司は別이벤트
	SIF CFLAG:LCOUNT:함락캐릭터 == -20
		CONTINUE
	
	;ＮＴＲれてないかつ바람心になってないキャラは除外
	IF TALENT:LCOUNT:ＮＴＲ == 0
		;음란도が最も高い호감도より高い場合はダメ
		SIF CFLAG:LCOUNT:음란도 >= ABS(FAVORITE(LCOUNT, (FAVORITE_1(LCOUNT))))
			CONTINUE
		;
		SIF !(FAVORITE_1_ID(LCOUNT) != CFLAG:LCOUNT:함락캐릭터 && ABS(FAVORITE(LCOUNT, (FAVORITE_1(LCOUNT)))) > ABS(FAVORITE_ID(LCOUNT , CFLAG:LCOUNT:함락캐릭터)) + 500)
			CONTINUE
	ELSE
		;淫ＮＴＲ以外は、함락대상が最も호감도の高いキャラじゃないと行かない
		SIF TALENT:LCOUNT:ＮＴＲ != 2 && FAVORITE_1_ID(LCOUNT) != CFLAG:LCOUNT:함락캐릭터
			CONTINUE
	ENDIF
	CALL ＮＴＲ요바이결정,LCOUNT
	LOCAL = 1
NEXT
SIF LOCAL > 0
	CUSTOMDRAWLINE =

@ＮＴＲ요바이결정,ARG
;どの요바이に進むか決定
IF TALENT:ARG:ＮＴＲ == 2 && CFLAG:ARG:음란도 >= ABS(FAVORITE(ARG, (FAVORITE_1(ARG))))
	;淫ＮＴＲ상태
	IF (EXP:ARG:변기경험 > 50) && RAND:2 == 0
		;공중변소へ
		CFLAG:ARG:ＮＴＲ요바이 = 3
	ELSEIF (CFLAG:ARG:현ＮＴＲ진행도 >= 10 || CFLAG:ARG:음란도 >= 5000) && RAND:3 == 0
		;乱交パーティへ
		CFLAG:ARG:ＮＴＲ요바이 = 1
	ELSEIF (CFLAG:ARG:현ＮＴＲ진행도 >= 5 || CFLAG:ARG:음란도 >= 4000) && RAND:2 == 0
		;公園へ
		CFLAG:ARG:ＮＴＲ요바이 = 2
	ELSE
		;夜の街へ
		CFLAG:ARG:ＮＴＲ요바이 = 4
	ENDIF
ELSEIF (FAVORITE_1_ID(ARG) != CFLAG:ARG:함락캐릭터) && ABS(FAVORITE(ARG, (FAVORITE_1(ARG)))) > ABS(FAVORITE_ID(ARG , CFLAG:ARG:함락캐릭터)) + 500
	;最も호감도の高いキャラが함락대상で無い場合、ＮＴＲ前逢引へ
	CFLAG:ARG:ＮＴＲ요바이 = 5
ELSEIF CFLAG:ARG:현ＮＴＲ진행도 == 1
	;ＮＴＲ後初の요바이
	CFLAG:ARG:ＮＴＲ요바이 = 6
ELSE
	$LOOP_RAND
	LOCAL = RAND:6
	SELECTCASE LOCAL
		CASE 0
		;野外조교(散歩他) 12
			SIF (TALENT:ARG:ＮＴＲ != 3 && TALENT:ARG:ＮＴＲ != 6) || CFLAG:ARG:현ＮＴＲ진행도 < 6
				GOTO LOOP_RAND
			CFLAG:ARG:ＮＴＲ요바이 = 12
		CASE 1
		;デート 7
			CFLAG:ARG:ＮＴＲ요바이 = 7

		CASE 2
		;本番 8
			CFLAG:ARG:ＮＴＲ요바이 = 8
		CASE 3
		;オナニー見せ合い 9
			SIF (TALENT:ARG:ＮＴＲ != 3 && TALENT:ARG:ＮＴＲ != 6) && ABL:ARG:노출증 < 3 && ABL:ARG:자위중독 < 1
				GOTO LOOP_RAND
			CFLAG:ARG:ＮＴＲ요바이 = 9
		CASE 4
		;애널섹스 10
			SIF (TALENT:ARG:ＮＴＲ != 3 && TALENT:ARG:ＮＴＲ != 6)
				GOTO LOOP_RAND
			CFLAG:ARG:ＮＴＲ요바이 = 10
		CASE 5
		;야외플레이 11
			SIF (TALENT:ARG:ＮＴＲ != 3 && TALENT:ARG:ＮＴＲ != 6) && ABL:ARG:노출증 < 4
				GOTO LOOP_RAND
			CFLAG:ARG:ＮＴＲ요바이 = 11

	ENDSELECT

ENDIF
CALLFORM NTR_SELF_{CFLAG:ARG:ＮＴＲ요바이},ARG
CALL 기절처리,ARG

RETURN 1

;淫系
;乱交パーティ
@NTR_SELF_1,ARG
#LOCALSIZE 1
#DIM 횟수
#DIM 정액량
#DIM Ｃ횟수
#DIM Ｖ횟수
#DIM Ｂ횟수
#DIM 절정횟수
#DIM 정액횟수
#DIM 정음횟수
#DIM 성교횟수
#DIM 질내사정횟수
#DIM 키스횟수
#DIM 펠라횟수
#DIM Ａ횟수
#DIM Ａ성교횟수
#DIM 절정포인트
#DIM 정액잔량

;NTRペナルティONの場合
IF FLAG:ＮＴＲ하드모드
	;횟수・人数の算出
	;기교人
	CFLAG:ARG:유혹인수 = ABL:ARG:기교 + RAND:(ABL:ARG:기교 + 1) + TALENT:ARG:매력 + TALENT:ARG:수수께끼의매력
	;各횟수の算出
	IF TALENT:ARG:남자 == 0 || ABL:ARG:정액중독 || ABL:ARG:ＢＬ끼
		CFLAG:ARG:펠라횟수 = (ABL:ARG:욕망 + 6)/4 + (ABL:ARG:정액중독 + 1)/2 + ABL:ARG:봉사정신 / 3
	ELSE
		CFLAG:ARG:펠라횟수 = 0
	ENDIF
	IF TALENT:ARG:남자 == 0
		CFLAG:ARG:본방횟수 = (ABL:ARG:욕망 + 6)/4 + (ABL:ARG:섹스중독 + 1)/2 + ABL:ARG:Ｖ감각 / 3
		SIF HAVE_PENIS(ARG)
			CFLAG:ARG:삽입횟수 = (ABL:ARG:욕망 + 6)/4 + (ABL:ARG:섹스중독 + 1)/2 + ABL:ARG:Ｃ감각 / 3
	ELSE
		CFLAG:ARG:본방횟수 = 0
		CFLAG:ARG:삽입횟수 = (ABL:ARG:욕망 + 6)/4 + (ABL:ARG:섹스중독 + 1)/2 + ABL:ARG:Ｃ감각 / 3
	ENDIF
	CFLAG:ARG:애널횟수 = (ABL:ARG:욕망 + 6)/4 + (ABL:ARG:섹스중독 + 1)/2 + ABL:ARG:Ａ감각 / 3
	SIF TALENT:ARG:Ｖ민감 && TALENT:ARG:남자 == 0
		CFLAG:ARG:본방횟수 += 1
	SIF TALENT:ARG:Ａ민감
		CFLAG:ARG:애널횟수 += 1

	CFLAG:ARG:펠라횟수 *= CFLAG:ARG:유혹인수
	CFLAG:ARG:본방횟수 *= CFLAG:ARG:유혹인수
	CFLAG:ARG:애널횟수 *= CFLAG:ARG:유혹인수
	;구상호출
	DRAWLINE
	SWAP TARGET , ARG
	TFLAG:13 = 51
	CALL KOJO_MESSAGE_EVENT
	SWAP TARGET , ARG

	VARSET TCVAR:ARG:0 , 0

	SIF TALENT:ARG:처녀
		CALL ADDS_ABNORMAL_EXP, "처녀상실（윤간）", ARG
	TALENT:ARG:처녀 = 0
	TCVAR:ARG:획득Ｖ경험 = CFLAG:ARG:본방횟수
	TCVAR:ARG:획득Ａ경험 = CFLAG:ARG:애널횟수
	TCVAR:ARG:획득키스경험 = (1+RAND:3)*CFLAG:ARG:유혹인수
	SIF HAVE_PENIS(ARG)
		TCVAR:ARG:획득사정경험 = CFLAG:ARG:삽입횟수 * 2 + CFLAG:ARG:애널횟수/2 + CFLAG:ARG:본방횟수/2
	TCVAR:ARG:획득성교경험 = CFLAG:ARG:삽입횟수 + CFLAG:ARG:애널횟수 + CFLAG:ARG:본방횟수
	TCVAR:ARG:획득펠라경험 = CFLAG:ARG:펠라횟수
	TCVAR:ARG:획득정액경험 = CFLAG:ARG:펠라횟수 * 2 + CFLAG:ARG:애널횟수 * 2 + CFLAG:ARG:본방횟수 * 2
	TCVAR:ARG:획득절정경험 = CFLAG:ARG:본방횟수 + CFLAG:ARG:애널횟수 + CFLAG:ARG:삽입횟수
	TCVAR:ARG:획득질사경험 = CFLAG:ARG:본방횟수 * 2
	TCVAR:ARG:획득봉사쾌락경험 = (CFLAG:ARG:본방횟수 + CFLAG:ARG:애널횟수 + CFLAG:ARG:펠라횟수)/3
	TCVAR:ARG:획득바람경험 = 1
	;체력の消耗
	LOCAL = CFLAG:ARG:펠라횟수 * 5 + CFLAG:ARG:애널횟수 * 15 + CFLAG:ARG:본방횟수 * 10
	PRINTFORML 체력 - {LOCAL}
	BASE:ARG:체력 -= LOCAL
	CALL SHOW_ADD_EXP, ARG
	CFLAG:ARG:현ＮＴＲ진행도 += 1
	;바람癖がある場合は음란도倍化
	IF TALENT:ARG:바람기
		CFLAG:ARG:음란도 += (LOCAL/10)*2
	ELSE
		CFLAG:ARG:음란도 += LOCAL/10
	ENDIF

ELSE
	;횟수・人数の算出
	;3〜5人
	;횟수설정
	횟수 = 3+RAND:3
	정액잔량 = 횟수*3

	Ｖ횟수 = 횟수 + RAND:(횟수*3+1)
	펠라횟수 = 횟수 + RAND:(횟수*3+1)
	Ａ횟수 = 횟수 + RAND:(횟수*3+1)
	;フェラ횟수が1以上の場合
	IF 펠라횟수
		;必ず정액량を半分は残す
		정음횟수 = LIMIT(1+RAND:펠라횟수, 0, 정액잔량/2)
		정액잔량 -= 정음횟수
	ENDIF
	
	성교횟수 = Ｖ횟수
	;성교횟수が1以上の場合
	SIF 성교횟수
		질내사정횟수 = LIMIT(1+RAND:성교횟수, 0, 정액잔량)

	;フェラ횟수か성교횟수が１以上の場合は정액횟수がカウントされる
	SIF (성교횟수+펠라횟수) > 0
		정액횟수 = 정음횟수 + 질내사정횟수

	키스횟수 = RAND:(횟수*3+1)

	;各횟수の算出(1人3回)
	IF TALENT:ARG:남자 == 0 || ABL:ARG:정액중독 || ABL:ARG:ＢＬ끼
		CFLAG:ARG:펠라횟수 = 펠라횟수
	ELSE
		CFLAG:ARG:펠라횟수 = 0
	ENDIF
	IF TALENT:ARG:남자 == 0
		CFLAG:ARG:본방횟수 = Ｖ횟수
		SIF HAVE_PENIS(ARG)
			CFLAG:ARG:삽입횟수 = Ｖ횟수
	ELSE
		CFLAG:ARG:본방횟수 = 0
		CFLAG:ARG:삽입횟수 = Ｖ횟수
	ENDIF
	CFLAG:ARG:애널횟수 = Ａ횟수
	;절정횟수計算
	절정횟수 = 0
	IF Ｖ횟수
		절정횟수 += RAND:Ｖ횟수
		SIF TALENT:ARG:Ｖ민감
			절정횟수 += 횟수
		SIF TALENT:ARG:Ｖ둔감
			절정횟수 = LIMIT(절정횟수-RAND:횟수, 0, 절정횟수)
	ENDIF
	IF Ａ횟수
		절정횟수 += RAND:Ａ횟수
		SIF TALENT:ARG:Ａ민감
			절정횟수 += 횟수
		SIF TALENT:ARG:Ａ둔감
			절정횟수 = LIMIT(절정횟수-RAND:횟수, 0, 절정횟수)
	ENDIF

	;구상호출
	DRAWLINE
	SWAP TARGET , ARG
	TFLAG:13 = 51
	CALL KOJO_MESSAGE_EVENT
	SWAP TARGET , ARG

	VARSET TCVAR:ARG:0 , 0

	SIF TALENT:ARG:처녀
		CALL ADDS_ABNORMAL_EXP, "처녀상실（윤간）", ARG
	TALENT:ARG:처녀 = 0
	TCVAR:ARG:획득Ｖ경험 = CFLAG:ARG:본방횟수
	TCVAR:ARG:획득Ａ경험 = CFLAG:ARG:애널횟수
	TCVAR:ARG:획득키스경험 = 키스횟수
	SIF HAVE_PENIS(ARG)
		TCVAR:ARG:획득사정경험 = CFLAG:ARG:삽입횟수 * 2 + CFLAG:ARG:애널횟수/2 + CFLAG:ARG:본방횟수/2
	TCVAR:ARG:획득성교경험 = CFLAG:ARG:삽입횟수 + CFLAG:ARG:애널횟수 + CFLAG:ARG:본방횟수
	TCVAR:ARG:획득펠라경험 = CFLAG:ARG:펠라횟수
	TCVAR:ARG:획득정액경험 = 정액횟수
	TCVAR:ARG:획득절정경험 = 절정횟수
	TCVAR:ARG:획득질사경험 = 질내사정횟수
	TCVAR:ARG:획득봉사쾌락경험 = (CFLAG:ARG:본방횟수 + CFLAG:ARG:애널횟수 + CFLAG:ARG:펠라횟수)/3
	TCVAR:ARG:획득바람경험 = 1

	LOCAL = CFLAG:ARG:펠라횟수 * 5 + CFLAG:ARG:애널횟수 * 15 + CFLAG:ARG:본방횟수 * 10
	PRINTFORML 체력 - {LOCAL}
	BASE:ARG:체력 -= LOCAL
	CALL SHOW_ADD_EXP, ARG
	CFLAG:ARG:현ＮＴＲ진행도 += 1
	;바람癖がある場合は음란도倍化
	IF TALENT:ARG:바람기
		CFLAG:ARG:음란도 += (LOCAL/10)*2
	ELSE
		CFLAG:ARG:음란도 += LOCAL/10
	ENDIF

ENDIF

