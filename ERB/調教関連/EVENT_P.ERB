				CONTINUE
			LOCAL = GETNUM(CFLAG, "奴隷の中出氏による임신판정")
		CASE 4
			LOCAL = GETNUM(CFLAG, "顧客の中出氏による임신판정")
		CASE 5
			LOCAL = GETNUM(CFLAG, "촉수の中出氏による임신판정")
		CASE 6
			LOCAL = GETNUM(CFLAG, "信者の中出氏による임신판정")
	ENDSELECT

	;확률処理(ここを抜けることで임신確定)
	;排卵剤の有無による定数설정
	IF FLAG:ふたごちゃんフラグ > 0
		;おまじない：さすがに精液が一滴も入ってなければMURI
		IF CFLAG:ARG:子宮現在容量 < 1
			CONTINUE
		ENDIF
		;ふたごちゃんフラグが3：胎内合体魔人だった場合、かつ十分に膣出ししている、かつ危険日or誘発剤使用時は確実に妊娠する
		;あとは何匹孕むかです
		SIF FLAG:ふたごちゃんフラグ > 2 && ((CFLAG:ARG:子宮최대容量 * 9 / 10) < CFLAG:ARG:子宮現在容量) && (CFLAG:ARG:배란유발제 || (위험일(ARG)))
			GOTO PREGNANCY_PROCESS
		;디버그용
		DEBUGPRINTFORML %CFLAGNAME:LOCAL%{CFLAG:ARG:LOCAL}
		;排卵誘発剤がなく、危険日でない場合、最大でも50％
		IF CFLAG:ARG:배란유발제 == 0 && (위험일(ARG) == 0) 
			;디버그용
			DEBUGPRINTFORML RAND値{CFLAG:ARG:LOCAL*受精率補正}
			DEBUGPRINTFORM 排卵誘発剤もなく危険日でもない。
			IF RAND:100 >= MIN(50, (CFLAG:ARG:LOCAL*受精率補正))
				DEBUGPRINTFORML RANDで弾かれた
				CONTINUE
			ENDIF
			DEBUGPRINTFORML 妊娠確定
			SIF FLAG:DEBUG
				FORCEWAIT
		ELSE
			DEBUGPRINTFORML RAND値{CFLAG:ARG:LOCAL*受精率補正 * ((위험일(ARG) > 0) + 1)}
			DEBUGPRINTFORM 排卵誘発剤もしくは危険日である。
			IF RAND:100 >= CFLAG:ARG:LOCAL*受精率補正 * ((위험일(ARG) > 0) + 1)
				DEBUGPRINTFORML RANDで弾かれた
				CONTINUE
			ENDIF
			DEBUGPRINTFORML 妊娠確定
			SIF FLAG:DEBUG
				FORCEWAIT
		ENDIF
	;多胎妊娠オプションオフの場合（通常）
	ELSE
		;排卵剤の有無による定数設定
		受精率補正 = 1 + (CFLAG:ARG:배란유발제 > 0)
		IF CFLAG:ARG:배란유발제 == 0 && (위험일(ARG) == 0) 
			SIF RAND:100 >= MIN(50, CFLAG:ARG:LOCAL)
				CONTINUE
		ELSE
			SIF RAND:100 >= CFLAG:ARG:LOCAL*受精率補正 * ((위험일(ARG) > 0) + 1)
				CONTINUE
		ENDIF
	ENDIF


$PREGNANCY_PROCESS

	;임신確定後の処理
	SIF 위험일(ARG) > 1
		CFLAG:ARG:발정임신 = 1
	CFLAG:ARG:출산予定日 = DAY + 6
	CFLAG:ARG:임신時の父親の판정 = LOCAL:1

	;임신確定したのでループを抜ける
	CFLAG:ARG:今回受胎人数 = 1

	;===================
	;●双子判定開始
	;===================
	;多胎妊娠オプションオンの場合
	IF FLAG:ふたごちゃんフラグ > 0

		;===================
		;●多胎率計算
		;===================
		;実際に双子以上になるかの判定
		;上限が高いほどなりやすい
		;ただしオプションによっては双子率には限度アリ
		;基本値は1(%)
		多胎率 = 1

		;排卵誘発剤があると+補正
		SIF CFLAG:ARG:배란유발제
			多胎率 += 3
		;危険日なら+補正
		SIF (위험일(ARG))
			多胎率 += 2
		;ケモノ系なら+補正
		IF TALENT:ARG:여우 || TALENT:ARG:요호 || TALENT:ARG:동물귀 || TALENT:ARG:발정가능 || TALENT:ARG:짐승 || TALENT:ARG:새 || TALENT:ARG:파충류 || TALENT:ARG:부정형 || TALENT:ARG:물고기 || TALENT:ARG:이형
			多胎率 += 1
			;かつ、危険日ならさらに+補正
			SIF (위험일(ARG))
				多胎率 += 2
		ENDIF
		;母性なら+補正
		SIF TALENT:ARG:모성
			多胎率 += 1
		;オトコの場合（男の娘の場合）孕む場所が子袋ではないのでマイナス補正
		SIF TALENT:ARG:남자 || TALENT:ARG:오토코노코
			多胎率 -= 3
		;出産経験に応じて多胎率上昇
		IF 0
		ELSEIF EXP:ARG:출산경험 >= 250
			多胎率 += 12
		ELSEIF EXP:ARG:출산경험 >= 175
			多胎率 += 10
		ELSEIF EXP:ARG:출산경험 >= 125
			多胎率 += 8
		ELSEIF EXP:ARG:출산경험 >= 75
			多胎率 += 6
		ELSEIF EXP:ARG:출산경험 >= 25
			多胎率 += 4
		ELSE
			;出産経験25未満なら補正ナシ
		ENDIF
		;ここまでの最大
		;誘発3+危険日2+獣危険日3+母性1+出産経験12＝21％
		;標準想定は誘発3+危険日2+母性1+出産経験0＝6%

		;たっぷり注がれていたら*補正
		SIF CFLAG:ARG:子宮現在容量 > (CFLAG:ARG:子宮최대容量 / 2)
			多胎率 *= (CFLAG:ARG:子宮現在容量 / (CFLAG:ARG:子宮최대容量 + 1)) * 2
		;妊娠しやすいなら*補正
		SIF TALENT:ARG:임신하기쉬움
			多胎率 *= 150
			多胎率 /= 10
		;妊娠しにくいなら/補正
		SIF TALENT:ARG:임신하기어려움
			多胎率 *= 8
			多胎率 /= 10
		;孕みたがりなら強めの*補正
		;妊娠しやすいと合わせて3.75倍
		SIF TALENT:ARG:잉태희망
			多胎率 *= 25
			多胎率 /= 10

		;ここまでの最大
		;21%*2*1.5*2.5＝157.5％
		;標準想定は6%*2＝12％

		;オプションによって確率補正
		;フラグ1の場合
		IF FLAG:ふたごちゃんフラグ == 1
			;最大でも10%
			;ちなみに現実では1%程度。自然妊娠に限れば、0.6%程度らしい
			;ただし北欧では1.5%程度と、人種か環境かによって、多少差異があるようだ。
			SIF 多胎率 >= 10
				多胎率 = 10
		;フラグ2の場合
		ELSEIF FLAG:ふたごちゃんフラグ == 2
			;双子率2倍
			多胎率 *= 2
			;ただし最大50%
			SIF 多胎率 >= 50
				多胎率 = 50
		;フラグ3以上の場合
		ELSE
			;双子率3倍だけして確率は補正しない
			多胎率 *= 3
		ENDIF
		;ここまでの最大
		;21%*2*1.5*2.5＝157.5％でフラグ3で472.5％、ここまで来ると双子～多胎上限までがだいたい等確率で出てくる
		;標準想定は6%*2＝12％から、10%、24%、36%

		;1%は最低保証
		SIF 多胎率 <= 0
			多胎率 = 1

		;===================
		;●受胎数決定
		;===================
		;「多胎率」％で双子ちゃんになる
		IF RAND:100 < 多胎率

			;===================
			;●受胎上限設定
			;===================
			;双子以上になる場合、2-上限の間の数字を取る
			多胎上限 = 2
			;体格による最大値補正
			;小人体型は上限-補正
			IF TALENT:ARG:소인체형
				多胎上限 -= 1
			ELSEIF TALENT:ARG:유아체형
				多胎上限 += 0
			ELSEIF TALENT:ARG:큰체형
				多胎上限 += 2
			ELSEIF TALENT:ARG:거체
				多胎上限 += 3
			;普通体格
			ELSE
				多胎上限 += 1
			ENDIF
			;ケツがデカい安産型なら+補正
			SIF TALENT:ARG:巨尻 || TALENT:ARG:爆尻
				多胎上限 += 1
			SIF TALENT:ARG:魔尻
				多胎上限 += 2
			;妊娠しやすいなら+補正
			SIF TALENT:ARG:임신하기쉬움
				多胎上限 += 1
			;妊娠しにくいならマイナス補正
			SIF TALENT:ARG:임신하기어려움
				多胎上限 -= 1
			;孕みたがりなら強めの+補正
			SIF TALENT:ARG:잉태희망
				多胎上限 += 3

			;出産経験による補正
			;出産経験1000で打ち止め……にしようかと思ったが1万までは元気よく増える、頑張れ
			IF 0
			ELSEIF EXP:ARG:출산경험 >= 10000
				;このあたりでYURUSITE　つまりイザナミママンは最低でも20-150人は産む
				多胎上限 += 150
			ELSEIF EXP:ARG:출산경험 >= 1100
				;ここからは100刻みで増やす
				;1100で100/100+20＝21
				;1200で200/100+20＝22
				;9900で8900/100+20＝109
				多胎上限 += (EXP:ARG:출산경험 - 1000) / 100 +20
			;1000以下はまあだいたい50刻みで増えていく
			ELSEIF EXP:ARG:출산경험 >= 1000
				多胎上限 += 20
			ELSEIF EXP:ARG:출산경험 >= 950
				多胎上限 += 19
			ELSEIF EXP:ARG:출산경험 >= 900
				多胎上限 += 18
			ELSEIF EXP:ARG:출산경험 >= 850
				多胎上限 += 17
			ELSEIF EXP:ARG:출산경험 >= 800
				多胎上限 += 16
			ELSEIF EXP:ARG:출산경험 >= 750
				多胎上限 += 15
			ELSEIF EXP:ARG:출산경험 >= 700
				多胎上限 += 14
			ELSEIF EXP:ARG:출산경험 >= 650
				多胎上限 += 13
			ELSEIF EXP:ARG:출산경험 >= 600
				多胎上限 += 12
			ELSEIF EXP:ARG:출산경험 >= 550
				多胎上限 += 11
			ELSEIF EXP:ARG:출산경험 >= 500
				多胎上限 += 10
			ELSEIF EXP:ARG:출산경험 >= 450
				多胎上限 += 9
			ELSEIF EXP:ARG:출산경험 >= 400
				多胎上限 += 8
			ELSEIF EXP:ARG:출산경험 >= 350
				多胎上限 += 7
			ELSEIF EXP:ARG:출산경험 >= 300
				多胎上限 += 6
			ELSEIF EXP:ARG:출산경험 >= 200
				多胎上限 += 5
			ELSEIF EXP:ARG:출산경험 >= 150
				多胎上限 += 4
			ELSEIF EXP:ARG:출산경험 >= 100
				多胎上限 += 3
			ELSEIF EXP:ARG:출산경험 >= 50
				多胎上限 += 2
			ELSEIF EXP:ARG:출산경험 >= 10
				多胎上限 += 1
			ELSE
				;出産経験9以下なら補正ナシ
			ENDIF

			;オプションによって最大値補正
			;体がデカければ数を抱えられるが……という感じ
			;フラグ3以下の場合
			IF FLAG:ふたごちゃんフラグ < 3
				;体格による限界
				;小人:2　小柄:3　普通:5　큰체형:6　거체:8
				;ただし魔尻とケモノ系があればそれぞれ+1
				多胎限界 = 5
				IF TALENT:ARG:소인체형
					多胎限界 = 2
				ELSEIF TALENT:ARG:유아체형
					多胎限界 = 3
				ELSEIF TALENT:ARG:큰체형
					多胎限界 = 6
				ELSEIF TALENT:ARG:거체
					多胎限界 = 8
				;普通体格
				ELSE
					多胎限界 = 5
				ENDIF

				;獣系は多産
				SIF TALENT:ARG:여우 || TALENT:ARG:요호 || TALENT:ARG:동물귀 || TALENT:ARG:발정가능 || TALENT:ARG:짐승 || TALENT:ARG:새 || TALENT:ARG:파충류 || TALENT:ARG:부정형 || TALENT:ARG:물고기 || TALENT:ARG:이형
					多胎限界 += 1
				;ケツがデカい安産型、特に魔の領域に達した尻なら+補正
				SIF TALENT:ARG:魔尻
					多胎限界 += 1
				;フラグが1ならどんな巨体でも最大値は2、「ふたごちゃん」しか生まれない
				SIF FLAG:ふたごちゃんフラグ == 1 && 多胎上限 > 2
					多胎上限 = 2
				;多胎上限が多胎限界以上なら多胎限界まで補正
				SIF 多胎上限 > 多胎限界
					多胎上限 = 多胎限界

			ELSE
				;フラグ3の場合？　私は何も補正しない　自由に羽ばたいてほしい
			ENDIF


			;===================
			;●受胎数計算
			;===================
			;何人受胎するかは下記の計算で行う
			;　Nつ子を孕む率の重みは「基本値100/受胎数N+多胎率」
			;　上記計算を、双子の場合をN2、三つ子の場合をN3……上限つ子の場合をNEとし、任意の受胎数をNXと置いた場合、
			;　NXを孕む確率は「NX/(N2+N3+N4……NE)」となる
			;　ex1.多胎率20%で三つ子までの場合、「100/2+20＝70」「100/3+20＝53」
			;　　　70/（70+53）≒56%で双子、53/（70+53）≒43%で三つ子、となる
			;　ex2.多胎率50%で三つ子までの場合、「100/2+50＝100」「100/3+50＝83」
			;　　　100/（100+83）≒54%で双子、83/（100+83）≒45%で三つ子、となる
			;　N2から順番に、「判定し、重みを抜いて、次の判定を行い、重みを抜いて……」と繰り返すことで確率計算ができる
			;　多胎率が高くなればなるほど、相対的に補正の割合が増えるため、より多くの子を妊娠しやすくなる
			;　（まあ100人以上孕むとほとんど全部多胎率になって2～多胎上限がほとんど等確率になるんだけど）
			;　（500/60000で33つ子が産まれるよ！！！みたいな）
			;　なおそもそも双子以上か否かの判定があるので、実際に三つ子以上が生まれる確率はここでの計算より低い
			;　また、ふたごちゃんフラグが1の場合は双子までしか産まれない

			;受胎率に重みをつける
			FOR 受胎数,2,多胎上限
				;多胎上限が受胎数より高い場合、重みづけを行う
				IF 受胎数 <= 多胎上限
					多胎率合計 += 100/受胎数 + 多胎率
				;念のため：受胎数が上限より高くなった場合抜ける
				ELSE
					BREAK
				ENDIF
			NEXT

			;受胎数決定
			;念のため：最大数で初期化
			CFLAG:ARG:今回受胎人数 = 多胎上限
			FOR 受胎数,2,多胎上限
				;受胎数判定
				IF RAND:(多胎率合計) < (100/受胎数 + 多胎率)
					CFLAG:ARG:今回受胎人数 = 受胎数
					BREAK
				ENDIF
				;重みを抜く
				多胎率合計 -= (100/受胎数 + 多胎率)
				;念のため：多胎率が1以下になったら抜ける
				SIF 多胎率合計 < 1
					BREAK
			NEXT

			;===================
			;●下限補正
			;===================
			;99%念のため（ただしイザナミママン）
			;下限はここに来てる時点で通常２
			多胎下限 = 2
			;ふたごちゃんフラグが3以上の時のみ、ヤオヨロズ・マザーは下限からして限界突破
			IF FLAG:ふたごちゃんフラグ > 2
				;800万以上で+20
				SIF EXP:ARG:출산경험 >= 8000000
					多胎下限 += 20
			ENDIF

			;下限より今回受胎人数が小さければ補正
			SIF CFLAG:ARG:今回受胎人数 < 多胎下限
				CFLAG:ARG:今回受胎人数 = 多胎下限

			;===================
			;●妊娠期間補正
			;===================
			;受胎数-1か6の小さな方をさらに予定日に加算（最大妊娠期間は12日）
			;双子であれば7日、4つ子であれば9日
			CFLAG:ARG:출산予定日 += MIN(6, (CFLAG:ARG:今回受胎人数-1))

		ENDIF
	ENDIF

	;最大記録より今回人数の方が大きければ記録を行う
	SIF CFLAG:ARG:最大妊娠人数 < CFLAG:ARG:今回受胎人数
		CFLAG:ARG:最大妊娠人数 = CFLAG:ARG:今回受胎人数

	;妊娠確定したのでループを抜ける
		BREAK
NEXT

;フラグ초기化
CALL CLEAR_PREGNANCY_DATERMINATION, ARG
;こっちは임신してない時だけ초기化
SIF LOCAL:1 == 0
	CFLAG:ARG:노역中の中出氏による暫定父親 = -1

RETURN CFLAG:ARG:임신時の父親の판정

;==========================================================
;@CLEAR_PREGNANCY_FLAG
;임신판정用のフラグをクリアする。
;<引数>
;	ARG = -1(생략時):全員
;	      -2		:오토코노코
;         0以上		:대상キャラの登録番号
;<戻り値>
;	なし
;==========================================================
@CLEAR_PREGNANCY_DATERMINATION, ARG = -1
#DIM LCOUNT
;全員のフラグをクリア
IF ARG == -1
	CVARSET CFLAG,"主人の中出氏による임신판정",0
	CVARSET CFLAG,"조수の中出氏による임신판정",0
	CVARSET CFLAG,"奴隷の中出氏による임신판정",0
	CVARSET CFLAG,"顧客の中出氏による임신판정",0
	CVARSET CFLAG,"촉수の中出氏による임신판정",0
	CVARSET CFLAG,"信者の中出氏による임신판정",0
;오토코노코のフラグをクリア
ELSEIF ARG == -2
	FOR LCOUNT, 0, CHARANUM
		;女で오토코노코はないと思うけど念のため
		IF TALENT:LCOUNT:남자 && TALENT:LCOUNT:오토코노코
			CFLAG:LCOUNT:主人の中出氏による임신판정 = 0
			CFLAG:LCOUNT:조수の中出氏による임신판정 = 0
			CFLAG:LCOUNT:奴隷の中出氏による임신판정 = 0
			CFLAG:LCOUNT:顧客の中出氏による임신판정 = 0
			CFLAG:LCOUNT:촉수の中出氏による임신판정 = 0
			CFLAG:LCOUNT:信者の中出氏による임신판정 = 0
		ENDIF
	NEXT
;대상キャラのフラグをクリア
ELSEIF ARG >= 0 && ARG < CHARANUM
	CFLAG:ARG:主人の中出氏による임신판정 = 0
	CFLAG:ARG:조수の中出氏による임신판정 = 0
	CFLAG:ARG:奴隷の中出氏による임신판정 = 0
	CFLAG:ARG:顧客の中出氏による임신판정 = 0
	CFLAG:ARG:촉수の中出氏による임신판정 = 0
	CFLAG:ARG:信者の中出氏による임신판정 = 0
ENDIF

;==========================================================
;@CLEAR_PREGNANCY_MULTI_BIRTHS
;妊娠判定用のフラグをクリアする。
;<引数>
;	ARG = -1(省略時):全員
;         0以上		:対象キャラの登録番号
;<戻り値>
;	なし
;該当キャラが妊娠している場合、その受胎人数を1にする処理
;また、出産予定日も再設定する
;==========================================================
@CLEAR_PREGNANCY_MULTI_BIRTHS, ARG = -1
#DIM LCOUNT
;全員のフラグをクリア
IF ARG == -1
	FOR LCOUNT, 0, CHARANUM
		IF TALENT:LCOUNT:임신
			;多胎によって増えている出産予定日を再設定する
			SIF CFLAG:LCOUNT:今回受胎人数 > 1
				CFLAG:LCOUNT:출산予定日 -= MIN(6, CFLAG:ARG:今回受胎人数 - 1)

			;受胎人数をひとりに
			CFLAG:LCOUNT:今回受胎人数 = 1

			;精液ボテ状態の解消
			;ただし増えすぎた容積はひとまず戻さない（次の調教で補正されるはず）
			SIF CFLAG:LCOUNT:子宮現在容量 > CFLAG:LCOUNT:子宮최대容量
				CFLAG:LCOUNT:子宮現在容量 = CFLAG:LCOUNT:子宮최대容量

		ENDIF
	NEXT
;対象キャラのフラグをクリア
ELSEIF ARG >= 0 && ARG < CHARANUM
	IF TALENT:ARG:임신
		;多胎によって増えている出産予定日を再設定する
		SIF CFLAG:ARG:今回受胎人数 > 1
			CFLAG:ARG:출산予定日 -= MIN(6, CFLAG:ARG:今回受胎人数 - 1)

		;受胎人数をひとりに
		CFLAG:ARG:今回受胎人数 = 1

		;精液ボテ状態の解消
		SIF CFLAG:ARG:子宮現在容量 > CFLAG:ARG:子宮최대容量
			CFLAG:ARG:子宮現在容量 = CFLAG:ARG:子宮최대容量

	ENDIF
ENDIF





;==========================================================
;@SET_FATHERINFO
;임신したキャラに아이の父親に関する情報を설정する。
;<引数>
;	ARG   = 임신したキャラの登録番号
;	ARG:1 = 父親のCSV番号
;	ARG:2 = 父親のキャラ固有番号
;	ARGS  = 父親の명칭
;<戻り値>
;	なし
;==========================================================
@SET_FATHERINFO, ARG, ARG:1, ARG:2 = -1, ARGS = ""
;ARGの値がキャラ登録番号の範囲外なら何もせずに戻る
SIF ARG < 0 || ARG >= CHARANUM
	RETURN

;父親情報の설정
CFLAG:ARG:임신確定後の父親の판정 = ARG:1
CFLAG:ARG:父親の固有번호親 = ARG:2
CSTR:ARG:父親명칭 = %ARGS%

