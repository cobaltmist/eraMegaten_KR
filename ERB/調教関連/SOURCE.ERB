	;B *= LOCAL
	;B /= 500

	;相性による수정
	IF CHARA == TARGET
		LOCAL:1 *= LIMIT(100 + (GET_RELATION(CHARA, CHARA:1) - 100)/4 , 75, 125)
		LOCAL:1 /= 100
	ENDIF


	;마조끼
	SELECTCASE ABL:CHARA:마조끼
		CASE 0
			TIMES LOCAL:1 , 0.00
		CASE 1
			TIMES LOCAL:1 , 0.10
		CASE 2
