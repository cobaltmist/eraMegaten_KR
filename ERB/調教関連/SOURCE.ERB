		SIF EXPNAME:LOCAL == "사정경험" && (!IS_MALE(ARG) || EXP:ARG:ＴＳ경험 % 2 == 1)
			CALL ADDS_ABNORMAL_EXP, "사정" , ARG
		EXP:ARG:LOCAL += TCVAR:ARG:LOCAL
		TCVAR:ARG:LOCAL = 0
	NEXT
ENDIF

