	ENDIF

	CUP:CHARA:윤활 += SOURCE:CHARA:액체

	;-------------------------------------------------
	;욕정のソース
	;-------------------------------------------------

	;충성도による수정
	;LOCAL = (BASE:ARG:충성도 >= 500 ? LIMIT(BASE:충성도*3/4,500,750) # LIMIT(BASE:충성도/2,1,500))

	;SOURCE:CHARA:욕정 *= LOCAL
	;SOURCE:CHARA:욕정 /= 500

	LOCAL = SOURCE:CHARA:욕정

	;욕망を見る
	SELECTCASE ABL:CHARA:욕망
		CASE 0
			TIMES LOCAL , 0.00
		CASE 1
			TIMES LOCAL , 0.40
		CASE 2
