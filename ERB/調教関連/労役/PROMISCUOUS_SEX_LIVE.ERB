@MODECHANGE_SEX_LIVE, ARG
SELECTCASE ARG
	CASE -1
		;FLAG:포장마차차금を使ってない様子なので再利用　ダメなら新たにFLAGを설정してください
		SIF FLAG:포장마차차금 == 0
			PRINTL [999] 피임결계 사용을 선택
		SIF FLAG:포장마차차금 == 1
			PRINTL [999] 피임결계를 사용하지 않는다
		SIF FLAG:포장마차차금 == 2
			PRINTL [999] 피임결계를 사용
	CASE 999
		RETURN ARG , 0
ENDSELECT
RETURN ARG , 1

