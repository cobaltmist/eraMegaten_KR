;==========================================================
;@NTR_PROSTITUTION
; 매춘노역でのNTR処理
;<引数>
;	ARG   = ＮＴＲキャラの등록번호
;	ARG:1 = 섹스系プ레이횟수
;	ARG:2 = SM系プ레이횟수
;	ARG:3 = 相손をした人数
;<戻り値>
;	なし
;==========================================================
@NTR_PROSTITUTION, ARG, ARG:1, ARG:2, ARG:3
#LOCALSIZE 5
;LOCAL:1 = 호감도변화量
;LOCAL:2 = NTR好き客からの声掛けられ확률
;LOCAL:3 = NTR引っかかり확률마이ナス보정
;LOCAL:4 = NTR客判別

;LOCAL配列초기化
VARSET LOCAL,0

;창관ＮＴＲ이벤트がOFFの場合は何もせず戻る
;専有奴隷の개목걸이を장비している場合は何もせず戻る
;남자は弾く
SIF !GETBIT(FLAG:ＮＴＲ기능 , 2) || (CFLAG:ARG:그외 == 1116 || CFLAG:ARG:그외2 == 1116 || CFLAG:ARG:그외3 == 1116 || CFLAG:ARG:그외4 == 1116 || CFLAG:ARG:그외5 == 1116 || CFLAG:ARG:그외6 == 1116) || TALENT:ARG:남자 == 1
	RETURN

;NTR好き客からの声掛けられ확률산출
LOCAL:2 = LIMIT(CFLAG:ARG:放置日数 * 2, 0, 30) + TALENT:ARG:인기 * 10

;NTR引っかかり확률마이ナス보정の計算
LOCAL:3 = CFLAG:ARG:放置日数 * 2 + TALENT:ARG:즉각함락 * 50
SIF 위험일(ARG) > 1
	LOCAL:3 += 위험일(ARG) * 10

;ＮＴＲ客の설정処理
SELECTCASE FLAG:ＮＴＲ客の설정
	CASE  0
		TIMES LOCAL:2 , 1.00
		TIMES LOCAL:3 , 1.00
	CASE  1
		TIMES LOCAL:2 , 1.50
		TIMES LOCAL:3 , 2.00
	CASE  2
		TIMES LOCAL:2 , 3.00
		TIMES LOCAL:3 , 4.00
