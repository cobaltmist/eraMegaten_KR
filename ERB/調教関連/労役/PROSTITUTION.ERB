;창관で働かせる
;奴隷を一日だけ창관に貸して働かせます。客の数だけ収益が上がるため、욕망や레즈が重要視されます

;==========================================================
;@PROSTITUTION
; 宿屋(창관)で働かせる奴隷を選択する。
;<引数>
;	なし
;<戻り値>
;	なし
;==========================================================
@PROSTITUTION
#LOCALSIZE 5
$PROSTITUTION_TOP
;사용する変数の초기化
A = 0
B = 0
C = 0
D = 0
;LOCAL   = 選択したキャラ
;LOCAL:1 = 가운価
;LOCAL:2 = 매춘가능人数
;LOCAL:3 = 一括매춘用화조ンタ
;LOCAL:4 = 노역확인결과
;LOCAL配列초기化
VARSET LOCAL,0
$PRINT_LIST
CALL INPUT_CHARA_LIST("매춘시킬 노예를 선택하세요", "CASTING_PROSTITUTION", "", "", "MODECHANGE_PROSTITUTION")
IF RESULT == 1000
	RETURN 0
ENDIF

IF RESULT == 999
	;一括選択できない상태のためINPUT_LOOP_1に戻る
	SIF FLAG:노역실행확인생략설정 == 0 || A < 2
		GOTO PRINT_LIST
	;一応확인
	PRINTL 현재 매춘 가능한 모든 노예를 매춘시킵니다.
	PRINTL 좋습니까?
	CALL INPUT_YN
	SIF RESULT == 1
		;キャラ一覧に戻る。
		GOTO PRINT_LIST
	;一括노역플래그ON
	LOCAL:3 = 1
	LOCAL = Q:(LOCAL:3-1)
	LOCAL:2 = A
ELSE
	LOCAL = RESULT
ENDIF

$SELECT_LOOP

;가운価を計算。
DRAWLINE

CALL SALARY_PROSTITUTION,LOCAL,1
LOCAL:1 = RESULT

IF LOCAL:1 > 0
	;노역실행확인생략설정が有効な場合、표시後WAITしないようにする。
	IF FLAG:노역실행확인생략설정 == 0
		PRINTFORMW 매춘 하루 동안 {LOCAL:1}마카 정도 전망됩니다
	ELSE
		PRINTFORML 매춘 하루 동안 {LOCAL:1}마카 정도 전망됩니다
	ENDIF
ELSE
	PRINTFORMW 매춘 가격이 0마카를 밑돌았으므로 중지합니다
	IF FLAG:노역실행확인생략설정 == 1
		IF LOCAL:3 > 0 && LOCAL:3 < LOCAL:2
			;LOCAL:3が0より大きく(一括選択)、LOCAL:3がLOCAL:2미만(まだ全員の노역従事が終わってない)場合
			;다음 캐릭터の노역従事
			LOCAL:3 += 1
			LOCAL = Q:(LOCAL:3-1)

			GOTO SELECT_LOOP
		ELSE
			;노역실행확인생략설정が有効な場合、関数の머리に戻る
			GOTO PROSTITUTION_TOP
		ENDIF
	ENDIF
	RETURN 0
ENDIF

;창관コマンドを実行するか？
IF FLAG:노역실행확인생략설정 == 0
	PRINTFORML 오늘 하루, %CALLNAME:LOCAL%에게 매춘시키겠습니까?
	CALL INPUT_YN,"매춘시킨다","그만둔다"
	LOCAL:4 = RESULT
ELSE
	;노역실행확인생략설정が有効な場合、実行확인は無조건で実行する(0)を설정
	LOCAL:4 = 0
ENDIF

IF LOCAL:4 == 0
	PRINTFORML 다음 날까지 %CALLNAME:LOCAL%에게 손님을 받게 합니다
	;이벤트구상
	T = TARGET
	TARGET = LOCAL
	TFLAG:13 = 16
	;未조교함락フラグを折る
	CFLAG:被조교횟수 += 1
	CALL KOJO_MESSAGE_EVENT
	TARGET = T
	;SIF LOCAL == TARGET
	;	TARGET = -1
	;SIF LOCAL == ASSI
	;	ASSI = -1
	;SIF CPOS(LOCAL) > 0
	;	CALL REMOVE_POSITION,CPOS(LOCAL)
	CFLAG:LOCAL:노역플래그 = 2
	CFLAG:LOCAL:노역단가 = LOCAL:1
	PRINTL 
	IF FLAG:노역실행확인생략설정 == 1
		IF LOCAL:3 > 0 && LOCAL:3 < LOCAL:2
			;LOCAL:3が0より大きく(一括選択)、LOCAL:3がLOCAL:2미만(まだ全員の노역従事が終わってない)場合
			;다음 캐릭터の노역従事
			LOCAL:3 += 1
			LOCAL = Q:(LOCAL:3-1)

			GOTO SELECT_LOOP
		ELSE
			;노역실행확인생략설정が有効な場合、関数の머리に戻る
			GOTO PROSTITUTION_TOP
		ENDIF
	ENDIF
ELSE
	RESTART
ENDIF

@CASTING_PROSTITUTION(ARG)
SIF CFLAG:ARG:노역플래그 > 0
	RETURN 0
SIF ARG != MASTER && (ABL:ARG:욕망 + ABL:ARG:기교 + ABL:ARG:노출증 + TALENT:ARG:음란*3 + TALENT:ARG:창부*3) < 7
	RETURN 0
SIF BASE:ARG:0 <= 500
	RETURN 0
SIF CFLAG:ARG:この場に居ないフラグ == 1
	RETURN 0
RETURN 1

@MODECHANGE_PROSTITUTION, ARG
SELECTCASE ARG
	CASE -1
		;노역実行확인생략옵션が有効な場合にのみ選択肢を표시する。
		SIF FLAG:노역실행확인생략설정 == 1 && A >= 2
			PRINTL [999] 매춘할 수 있는 모든 노예를 매춘시킨다
	CASE 999
		RETURN ARG , 0
ENDSELECT
RETURN ARG , 1

;==========================================================
;@END_PROSTITUTION
; 宿屋(창관)での노역日報処理
;<引数>
;	ARG = 노역を行うキャラの등록번호
;<戻り値>
;	なし
;==========================================================
@END_PROSTITUTION,ARG
#LOCALSIZE 25
#DIM LCOUNT
#DIM 客入り
#DIM Ｖプレイ回数
#DIM Ａプレイ回数
;LOCAL:1  = 作業用領域
;LOCAL:10 = 客入り
;LOCAL:9  = 計算用
;LOCAL:8  = 顧客만족도
;LOCAL:7  = 評価
;LOCAL:6  = 합계プ레이횟수
;LOCAL:11 = 섹스系プ레이頻度(횟수)
;LOCAL:12 = 봉사계プ레이頻度(횟수)
;LOCAL:13 = ＳＭ계プ레이頻度(횟수)
;LOCAL:14 = 동성愛(레즈、ＢＬ)系プ레이頻度(횟수)
;LOCAL:16 = プ레이頻度합계(割합계算用)
;LOCAL:20 = Ｖ／Ａプ레이횟수差計算用
;Ｖプレイ回数 = Ｖプ레이횟수
;Ａプレイ回数 = Ａプ레이횟수
;LOCAL:23 = 처녀상실による珠加算보정

;LOCAL配列초기化
VARSET LOCAL,0

;最初に노역단가を再計算
CALL SALARY_PROSTITUTION,ARG,0
CFLAG:ARG:노역단가 = RESULT

CUSTOMDRAWLINE ･


;창부の인기に応じて客が来ます
;初めての場合は店の인기に依存
IF EXP:ARG:매춘경험 == 0
	LOCAL:8 = 100
	SIF ITEM:여관 || ITEM:창관
		LOCAL:8 = FLAG:매춘인기 + 40
	LOCAL = 0
	LOCAL:9 = 0
	WHILE LOCAL:8 > 100 * (POWER(4 , LOCAL)-1) / 3
		LOCAL:9 += (MIN(LOCAL:8 , 100 * (POWER(4 , LOCAL+1) - 1) / 3) - 100 * (POWER(4 , LOCAL) -1) / 3) / (10 * (POWER(4,LOCAL)))
		LOCAL ++
	WEND
	IF LOCAL:9
		FOR LCOUNT, 0, LOCAL:9
			客入り += RAND:2
		NEXT
	ENDIF
ELSE
	LOCAL = 0
	LOCAL:9 = 0
	WHILE CFLAG:ARG:매춘인기 > 100 * (POWER(2 , LOCAL)-1) / 3
		LOCAL:9 += (MIN(CFLAG:ARG:매춘인기 , 100 * (POWER(2 , LOCAL+1) - 1) / 3) - 100 * (POWER(2 , LOCAL) -1) / 3) / (10 * (POWER(2,LOCAL)))
		LOCAL ++
	WEND
	IF LOCAL:9
		FOR LCOUNT, 0, LOCAL:9
			客入り += RAND:2
		NEXT
	ENDIF
ENDIF
;店に인기に応じて客が来ます
LOCAL:8 = 100
SIF ITEM:여관 || ITEM:창관
	LOCAL:8 = FLAG:매춘인기
LOCAL = 0
LOCAL:9 = 0
WHILE LOCAL:8 > 100 * (POWER(4 , LOCAL)-1) / 3
	LOCAL:9 += (MIN(LOCAL:8 , 100 * (POWER(4 , LOCAL+1) - 1) / 3) - 100 * (POWER(4 , LOCAL) -1) / 3) / (10 * (POWER(4,LOCAL)))
	LOCAL ++
WEND
IF 客入り
	FOR LCOUNT, 0, LOCAL:9
		客入り += RAND:2
	NEXT
ENDIF


客入り = TABLE_CALC2(客入り, 5, 95)
客入り = TABLE_CALC2(客入り, 10, 90)
客入り = TABLE_CALC2(客入り, 25, 80)
客入り = TABLE_CALC2(客入り, 30, 10)


;최저一人は来る
SIF 客入り < 1
	客入り = 1

;만족도판정
;랜덤で40を추가します。매춘경험に応じて期待値は上昇します。
IF EXP:ARG:매춘경험 > 30000
	LOCAL:8 = 60
ELSEIF EXP:ARG:매춘경험 > 10000
	LOCAL:8 = RAND:31 + 30
ELSEIF EXP:ARG:매춘경험 > 5000
	LOCAL:8 = RAND:36 + 25
ELSEIF EXP:ARG:매춘경험 > 3000
	LOCAL:8 = RAND:41 + 20
ELSEIF EXP:ARG:매춘경험 > 1000
	LOCAL:8 = RAND:46 + 15
ELSEIF EXP:ARG:매춘경험 > 500
	LOCAL:8 = RAND:51 + 10
ELSEIF EXP:ARG:매춘경험 > 100
	LOCAL:8 = RAND:56 + 5
ELSE
	LOCAL:8 = RAND:61
ENDIF

;욕망を체크する。욕망が相손の욕망を上回るかどうか。성공なら욕망레벨の２倍、실패なら１倍入ります。
;CBVA매춘では、相손の욕망の최대値が4です。
LOCAL:9 = RAND:9
IF LOCAL:9 <= ABL:ARG:욕망
	LOCAL:8 += ABL:ARG:욕망
	LOCAL:8 += ABL:ARG:욕망
ELSE
	LOCAL:8 += ABL:ARG:욕망
ENDIF

;相손からの行動が発生します。相손の기교がこちらの욕망以下の場合、不満そうな表情が出る가능性があります。
;相손の기교＋こちらの순종の２倍＞こちらの욕망＋２なら、問題なく行為を노출증と봉사정신を加える事が出来ます。
;발りない場合は加えられません。
LOCAL:9 = RAND:5
LOCAL:9 += ABL:ARG:순종
LOCAL:9 += ABL:ARG:순종
LOCAL:9 -= 2

IF LOCAL:9 >= ABL:ARG:기교
	LOCAL:8 += ABL:ARG:봉사정신
	LOCAL:8 += ABL:ARG:노출증
ENDIF

;ここまでで최대120％

;순종が3LVないと、著しく微妙になります
IF ABL:ARG:순종 < 3 && ARG != MASTER
	LOCAL:8 /= 2
ENDIF

;この時点でのLOCAL:8が만족도

;総プ레이횟수を計算
LOCAL:6 = 0
FOR LCOUNT, 0, 客入り
	;客一人当たり、１＋０～만족도/10回プ레이していく
	LOCAL:6 += 1 + (RAND:(LOCAL:8/10+1))
NEXT

;LOCAL:11 ~ 15 섹스系　～동성愛系の割合、16 = 割合합계

;소질、능력に応じて行為の比率の決定(各소질は持ってれば1が入っている）
;섹스系、기본的に大きい。AV감각、처녀、재생처녀、섹스중독、섹스광、애널광
;남자が無い場合のみＶでの횟수を加算する
LOCAL:11 = 0
SIF TALENT:ARG:남자 == 0
	LOCAL:11 = ABL:ARG:Ｖ감각+TALENT:ARG:처녀*7+TALENT:ARG:재생처녀*3+TALENT:ARG:섹스광*5
LOCAL:11 += ABL:ARG:Ａ감각+ABL:ARG:섹스중독+TALENT:ARG:애널광*5

;봉사계、봉사정신と정액중독が高く、헌신적
LOCAL:12 = ABL:ARG:봉사정신+ABL:ARG:정액중독+TALENT:ARG:헌신적*5

;SM系、|새드끼-마조끼|の数値が大きい、도착적、새드、마조、체력大눈
;마조傾向
IF ABL:ARG:마조끼 >= ABL:ARG:새드끼
	LOCAL:13 = ABL:ARG:마조끼-ABL:ARG:새드끼+TALENT:ARG:마조*10+TALENT:ARG:도착적*5
;サド傾向
ELSEIF ABL:ARG:새드끼 > ABL:ARG:마조끼
	LOCAL:13 = ABL:ARG:새드끼-ABL:ARG:마조끼+TALENT:ARG:새드*10+TALENT:ARG:도착적*5
;同値
ELSE
	LOCAL:13 = ABL:ARG:마조끼+TALENT:ARG:도착적*5
ENDIF
;5미만は0にする
SIF LOCAL:13 < 5
	LOCAL:13 = 0

;레즈系、레즈끼、레즈중독、바이、男性の場合はＢＬ끼とＢＬ중독
LOCAL:14 = ABL:ARG:레즈끼+ABL:ARG:레즈중독*2+TALENT:ARG:바이*5+ABL:ARG:ＢＬ끼+ABL:ARG:ＢＬ중독*2
;5미만は0にする
SIF LOCAL:14 < 5
	LOCAL:14 = 0

;高頻度プ레이(매춘구상分岐)판정用の値を설정
;1:섹스系、2:봉사계、3:ＳＭ계、4:동성愛(레즈、ＢＬ)系
CFLAG:ARG:창관でのプ레이頻度판정 = 1
SIF LOCAL:(10+CFLAG:ARG:창관でのプ레이頻度판정) < LOCAL:12
	CFLAG:ARG:창관でのプ레이頻度판정 = 2
SIF LOCAL:(10+CFLAG:ARG:창관でのプ레이頻度판정) < LOCAL:13
	CFLAG:ARG:창관でのプ레이頻度판정 = 3
SIF LOCAL:(10+CFLAG:ARG:창관でのプ레이頻度판정) < LOCAL:14
	CFLAG:ARG:창관でのプ레이頻度판정 = 4

LOCAL:16 = MAX(1,LOCAL:11 + LOCAL:12 + LOCAL:13 + LOCAL:14)

;それぞれの系統のプ레이횟수を산출してLOCAL:11 ～ 14に入れる
LOCAL:11 = LOCAL:6 * LOCAL:11 / LOCAL:16
LOCAL:12 = LOCAL:6 * LOCAL:12 / LOCAL:16
LOCAL:13 = LOCAL:6 * LOCAL:13 / LOCAL:16
LOCAL:14 = LOCAL:6 * LOCAL:14 / LOCAL:16

SIF LOCAL:11 + LOCAL:12 + LOCAL:13 + LOCAL:14 == 0
	LOCAL:(11+RAND:4) = LOCAL:6

;매춘時の구상표시
LOCAL:1 = TARGET
TARGET = ARG
TFLAG:13 = 63
CALL KOJO_MESSAGE_EVENT
TARGET = LOCAL:1
PRINTFORML %CALLNAME:ARG%의 매춘결과：

;체력の減少기본치
LOSEBASE:체력 = LOCAL:6*25
;스트레스치の付加
SIF ARG != MASTER
	CFLAG:ARG:10 += 2

LOCAL:23 = 0
IF LOCAL:11 > 0
	;VとAのプ레이差
	;남자はAのみ
	IF TALENT:ARG:남자
		LOCAL:20 = ABL:ARG:Ａ감각
		Ｖプレイ回数 = 0
		Ａプレイ回数 = LOCAL:11
	ELSEIF ABL:ARG:Ｖ감각 > ABL:ARG:Ａ감각
		LOCAL:20 = ABL:ARG:Ｖ감각-ABL:ARG:Ａ감각
		Ｖプレイ回数 = LOCAL:11/2+LOCAL:20
		Ａプレイ回数 = LOCAL:11-Ｖプレイ回数
	ELSE
		LOCAL:20 = ABL:ARG:Ａ감각-ABL:ARG:Ｖ감각
		Ｖプレイ回数 = LOCAL:11/2-LOCAL:20
		Ａプレイ回数 = LOCAL:11-Ｖプレイ回数
	ENDIF
	;奴隷が処女の場合
	IF TALENT:ARG:처녀
		Ｖプレイ回数 += LOCAL:20/2
		Ａプレイ回数 -= LOCAL:20/2
		SIF Ｖプレイ回数 > 0
			LOCAL:23 = 20
	ENDIF
	SIF Ｖプレイ回数 < 0
		Ｖプレイ回数 = 0
	SIF Ａプレイ回数 < 0
		Ａプレイ回数 = 0
	EXP:ARG:Ｖ경험 += Ｖプレイ回数
	EXP:ARG:Ａ경험 += Ａプレイ回数
	EXP:ARG:절정경험 += LOCAL:11/3
	;ふたなりかオトコ
	SIF TALENT:ARG:후타나리 || TALENT:ARG:남자
		EXP:ARG:사정경험 += LOCAL:11/3
	;男の娘はケツマンコで妊娠
	SIF TALENT:ARG:오토코노코 && CFLAG:ARG:자궁내피임결계 == 0
		CFLAG:ARG:顧客の中出氏による임신판정 += Ａプレイ回数
	EXP:ARG:성교경험 += Ｖプレイ回数 + Ａプレイ回数
	;질사경험
	IF CFLAG:ARG:자궁내피임결계 == 0
		EXP:ARG:질사경험 += Ｖプレイ回数
		CFLAG:ARG:顧客の中出氏による임신판정 += Ｖプレイ回数
	ENDIF
	JUEL:ARG:쾌Ｖ += Ｖプレイ回数*50
	JUEL:ARG:쾌Ａ += Ａプレイ回数*50
	JUEL:ARG:욕정 += LOCAL:11*30
	JUEL:ARG:습득 += LOCAL:11*50
	SIF ARG != MASTER
		JUEL:ARG:부정 += (LOCAL:23+LOCAL:11)*100/MAX(1,ABL:ARG:순종)
ENDIF
;봉사계
IF LOCAL:12 > 0
	EXP:ARG:정액경험 += LOCAL:12
	EXP:ARG:봉사쾌락경험 += LOCAL:12
	EXP:ARG:펠라경험 += LOCAL:12
	EXP:ARG:키스경험 += LOCAL:12
	JUEL:ARG:복종 += LOCAL:12*10
	JUEL:ARG:습득 += LOCAL:12*60
	SIF ARG != MASTER
		JUEL:ARG:부정 += LOCAL:12*100/MAX(1,ABL:ARG:순종)
ENDIF
;ＳＭ系
IF LOCAL:13 > 0
