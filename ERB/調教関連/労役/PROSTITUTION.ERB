							PRINTFORMW %CALLNAME:ARG%의 거절과 혐오의 기분과는 반대로、딱딱해진 페니스는 자궁을 계속 자극하여、몸이 반응해 버리고 있다……
							PRINTFORML 
							PRINTFORMW 몇 번이나 억지로 다루어진 %조사처리(CALLNAME:ARG,"는")% 애액을 흘려보내、남자의 기분이 좋아질 때까지 교배를 허락해 버렸다……
						ELSEIF RAND:6 == 0
							PRINTFORMW 약으로 휘청거리는 %조사처리(CALLNAME:ARG,"를")% 남자는 손대기 쉽게 묶어、로터와 e맛서지와 애널바이브를 꺼냈다……
							PRINTFORMW 남자는 세 가지 귀중한 보물과 높은 기교로 %조사처리(CALLNAME:ARG,"를")% 흐늘흐늘하게 하여 반발을 없앤 후、충분히 교배를 했다……
						ELSEIF RAND:5 == 0
							PRINTFORMW 남자는 %조사처리(CALLNAME:ARG,"를")% 배트에 던지며、방에 있던 살찐 중년 남자로부터 돈을 받고, 방을 나와 버렸다……
							PRINTFORMW 살찐 중년남자는 %CALLNAME:ARG%의 속옷을 질질 내려 침을 비순에 발라 자지를 찔러 넣었다……
							PRINTFORML 
							PRINTFORMW %조사처리(CALLNAME:ARG,"는")% 제대로 된 저항도 하지 못하고、중년 남자의 농후한 정액을 몇 번이나 흘려 넣어졌다……
						ELSEIF RAND:4 == 0
							PRINTFORMW %CALLNAME:ARG%의 저항은 허무하게、남자의 욕망덩어리를 보지 안쪽에 몇 번이나 쏟아부어져 버렸다…… 
						ELSEIF RAND:3 == 0
							PRINTFORML %조사처리(CALLNAME:ARG,"는")% 양다리가 크게 벌어진 자세로 침대의 머리 부분의 로프로 구속되어 있다……
							PRINTFORMW 그러자、남자는 노출된 보지에 자지를 꽂으며 몹시 거칠게 허리를 흔들었다……
							PRINTFORML 
							PRINTFORMW 그리고、눈물을 흘리는 %CALLNAME:ARG%의 머리카락을 잡아 결합부를 과시하며、질내사정했다……
						ELSEIF RAND:2 == 0
							PRINTFORMW 남자는 몽롱해진 %조사처리(CALLNAME:ARG,"를")% 덮쳐、허리를 흔들고 있다……
							PRINTFORMW %조사처리(CALLNAME:ARG,"는")% 혼탁한 의식 속에서 %CALLNAME:MASTER%의 이름을 중얼거리며、정액을 흘려 넣어지고 있다……
						ELSE
							PRINTFORMW %조사처리(CALLNAME:ARG,"는")% 는 방에 들어온 똘마니들에게 윤간 당했다……
							PRINTFORMW 움직일 수 없는 %CALLNAME:ARG%의 몸을 똘마니들의 페니스가 용서 없이 유린하고 있다……
							CFLAG:ARG:레이프フラグ = 1
						ENDIF
					ENDIF
				ENDIF
				RESETCOLOR
			ENDIF
			;試験用
			;레벨업판정
			PRINTFORML 
			CALL CHECK_LEVEL_UP,ARG
		ELSE
			PRINTFORMW %조사처리(CALLNAME:ARG,"는")% 손님과 식사를 하게 됬다……
			;放置日数によって変化
			PRINTL 
			IF 함락(ARG) * 50 - LOCAL:2 > 80
				PRINTFORMW %조사처리(CALLNAME:ARG,"는")% 빨리 끝맺은 것 같다
			ELSEIF 함락(ARG) * 50 - LOCAL:2 > 60
				PRINTFORMW %조사처리(CALLNAME:ARG,"는")% 식사를 즐기고 온 것 같다
				CFLAG:ARG:放置日数 += 1 + RAND:3
			ELSEIF 함락(ARG) * 50 - LOCAL:2 <= 60
				PRINTFORMW %조사처리(CALLNAME:ARG,"는")% 다음에 만날 약속을 해 온 것 같다…
				CFLAG:ARG:放置日数 += 1 + RAND:3
			ENDIF
		ENDIF
	ENDIF
ENDIF
;==========================================================
;@NTR_PROSTITUTION_SEX
; ＮＴＲ섹스計算
;<引数>
;	ARG   = ＮＴＲキャラの등록번호
;	ARG:1 = NTR客の判別
;<사용変数>
;	LOCAL:1 = 相손の사정횟수
;	LOCAL:2 = 相손の성교횟수
;	LOCAL:3 = 호감도
;	LOCAL:4 = 음란度
;	LOCAL:5 = 호감도減少処理用
;==========================================================
@NTR_PROSTITUTION_SEX, ARG, ARG:1
#LOCALSIZE 6
#DIM LCOUNT

;LOCAL配列초기化
VARSET LOCAL,0
CFLAG:ARG:펠라횟수 = 0
CFLAG:ARG:本番횟수 = 0
CFLAG:ARG:애널횟수 = 0
CFLAG:ARG:中出し횟수 = 0
CFLAG:ARG:손コキ횟수 = 0
;優しい창관顧客）
IF ARG:1 == -4
	CFLAG:ARG:中出し횟수 = 1
	CFLAG:ARG:本番횟수 = 1
	LOCAL:1 = 3 + RAND:5
	LOCAL:2 = LOCAL:1 + RAND:(LOCAL:1)
	TCVAR:ARG:획득키스경험 = 3
	TCVAR:ARG:획득바람경험 = 1

;鬼畜な창관顧客
ELSEIF ARG:1 == -5
	CFLAG:ARG:中出し횟수 = 2
	CFLAG:ARG:本番횟수 = 2
	LOCAL:1 = 6 + RAND:10
	LOCAL:2 = LOCAL:1 + RAND:(LOCAL:1)
	TCVAR:ARG:획득윤간경험 = 1
ENDIF


FOR LCOUNT , 0 , LOCAL:2
	IF RAND:100 < 60
		IF HAVE_PENIS(ARG) && HAVE_VAGINA(ARG)
			CFLAG:ARG:本番횟수 += 1
			CFLAG:ARG:손コキ횟수 += 1
		ELSEIF HAVE_PENIS(ARG)
			CFLAG:ARG:애널횟수 += 1
			CFLAG:ARG:손コキ횟수 += 1
		ELSE
			CFLAG:ARG:本番횟수 += 1
		ENDIF
	ELSEIF RAND:100 < 50
		IF HAVE_PENIS(ARG)
			CFLAG:ARG:손コキ횟수 += 1
		ELSE
			CFLAG:ARG:펠라횟수 += 1
		ENDIF
	ELSE
		IF HAVE_PENIS(ARG) && HAVE_VAGINA(ARG)
			CFLAG:ARG:애널횟수 += 1
			CFLAG:ARG:손コキ횟수 += 1
		ELSEIF HAVE_PENIS(ARG)
			CFLAG:ARG:애널횟수 += 1
			CFLAG:ARG:손コキ횟수 += 1
		ELSE
			CFLAG:ARG:애널횟수 += 1
		ENDIF
	ENDIF
	;키스경험処理：適当に랜덤で
	SIF RAND:2 == 0
		TCVAR:ARG:획득키스경험 += 1

NEXT
CFLAG:ARG:中出し횟수 +=LIMIT(RAND:(1+CFLAG:ARG:本番횟수) , 0, LOCAL:1)

;획득경험
TCVAR:ARG:획득펠라경험 = CFLAG:ARG:펠라횟수
TCVAR:ARG:획득봉사쾌락경험 = CFLAG:ARG:펠라횟수
TCVAR:ARG:획득Ｖ경험 = CFLAG:ARG:本番횟수
TCVAR:ARG:획득Ａ경험 = CFLAG:ARG:애널횟수
TCVAR:ARG:획득성교경험 = CFLAG:ARG:本番횟수 + CFLAG:ARG:애널횟수
TCVAR:ARG:획득질사경험 = CFLAG:ARG:中出し횟수
TCVAR:ARG:획득정액경험 = LOCAL:1
SIF CFLAG:ARG:손コキ횟수 > 0
	TCVAR:ARG:획득사정경험 = RAND:(CFLAG:ARG:손コキ횟수)
SIF TCVAR:ARG:획득성교경험 + TCVAR:ARG:획득사정경험 > 0
	TCVAR:ARG:획득절정경험 = RAND:(TCVAR:ARG:획득성교경험 + TCVAR:ARG:획득사정경험)

;처녀상실処理
IF TALENT:ARG:처녀
	CALL ADDS_ABNORMAL_EXP, "처녀상실（윤간）", ARG
	SETCOLOR COLOR("red")
	IF ARG:1 == -5
		PRINTFORML 【＋２】   처녀상실（윤간）
	ELSE
		PRINTFORML 【＋２】   처녀상실(ＮＴＲ)
	ENDIF
	RESETCOLOR
	TALENT:ARG:처녀 = 0
ELSEIF TALENT:ARG:재생처녀
	PRINTFORML 재생처녀 상실
	TALENT:ARG:재생처녀 = 0
ENDIF

LOCAL:3 = TCVAR:ARG:획득정액경험 * 5 + (TCVAR:ARG:획득절정경험 + TCVAR:ARG:획득질사경험) * 10 + CFLAG:ARG:放置日数
LOCAL:4 = CFLAG:ARG:本番횟수 + CFLAG:ARG:애널횟수 + TCVAR:ARG:획득절정경험 * 10
LOCAL:5 = CFLAG:ARG:中出し횟수 * 5 + TCVAR:ARG:획득절정경험 * 7

;ＮＴＲ客の설정処理
SELECTCASE FLAG:ＮＴＲ客の설정
	CASE  0
		TIMES LOCAL:3 , 1.00
		TIMES LOCAL:4 , 1.00
		TIMES LOCAL:5 , 1.00
	CASE  1
		TIMES LOCAL:3 , 2.00
		TIMES LOCAL:4 , 2.00
		TIMES LOCAL:5 , 2.00
	CASE  2
		TIMES LOCAL:3 , 5.00
		TIMES LOCAL:4 , 5.00
		TIMES LOCAL:5 , 5.00
ENDSELECT




;호감도
IF ARG:1 == -4
	CDFLAG:ARG:キャラ間好感度:(96) += LOCAL:3
ELSEIF ARG:1 == -5
	CDFLAG:ARG:キャラ間好感度:(95) -= LOCAL:3
ENDIF

;음란度上昇
;바람기がある場合は음란度倍化
IF TALENT:ARG:바람기
	CFLAG:ARG:음란度 += LOCAL:4*2
ELSE
	CFLAG:ARG:음란度 += LOCAL:4
ENDIF

;子宮現在容量加算
IF TALENT:ARG:오토코노코
	CFLAG:(ARG):子宮現在容量 += LOCAL:5 * 2
ELSEIF !IS_MALE(ARG)
	CFLAG:(ARG):子宮現在容量 += LOCAL:5 * 2
ENDIF

;호감도減少処理
CALL NTR_SHOOT , ARG , ARG:1 , LOCAL:5

;임신用
SIF CFLAG:ARG:자궁내피임결계 == 0
	CFLAG:ARG:顧客の中出氏による임신판정 += TCVAR:ARG:획득질사경험*2

CALL SHOW_ADD_EXP, ARG

