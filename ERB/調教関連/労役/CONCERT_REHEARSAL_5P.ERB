		PRINTFORM [6]%TEXT_LJ("바이올린: ", 15)%
	ELSE
		PRINTFORM [6]%TEXT_LJ("키보드: ", 15)%
	ENDIF
	IF KEYBORAD > -1
		PRINTFORML %NAME:KEYBORAD%
	ELSE
		PRINTFORML 미지정
	ENDIF
ELSEIF BAND_TYPE == 2
;보컬 + 기타 + 베이스 + 드럼 + 키보드
	PRINTFORM [2]%TEXT_LJ("보컬: ", 15)%
	IF PURE_VOCAL > -1
		PRINTFORML %NAME:PURE_VOCAL%
	ELSE
		PRINTFORML 미지정
	ENDIF
	PRINTFORM [3]%TEXT_LJ("기타: ", 15)%
	IF PURE_GUITAR > -1
		PRINTFORML %NAME:PURE_GUITAR%
	ELSE
		PRINTFORML 미지정
	ENDIF
	PRINTFORM [4]%TEXT_LJ("베이스: ", 15)%
	IF BASS > -1
		PRINTFORML %NAME:BASS%
	ELSE
		PRINTFORML 미지정
	ENDIF
	PRINTFORM [5]%TEXT_LJ("드럼: ", 15)%
	IF DRUM > -1
		PRINTFORML %NAME:DRUM%
	ELSE
		PRINTFORML 미지정
	ENDIF
	;야시오 루이일 경우 바이올린이다
	IF KEYBORAD > -1 && NO:KEYBORAD == 7929
		PRINTFORM [6]%TEXT_LJ("바이올린: ", 15)%
	ELSE
		PRINTFORM [6]%TEXT_LJ("키보드: ", 15)%
	ENDIF
	IF KEYBORAD > -1
		PRINTFORML %NAME:KEYBORAD%
	ELSE
		PRINTFORML 미지정
	ENDIF
ENDIF
DRAWLINE
	PRINTFORM [7]%TEXT_LJ("DJ서포터: ", 15)%
	IF DJ_SUPPORTER > -1
		PRINTFORML %NAME:DJ_SUPPORTER%
	ELSE
		PRINTFORML 미지정
	ENDIF
DRAWLINE
IF BAND_TYPE == 1
	IF (VOCAL_GUITAR > -1) && (PURE_GUITAR > -1) && (BASS > -1) && (DRUM > -1) && (KEYBORAD > -1)
		PRINTL [99] 실행
	ELSE
		SETCOLOR 0x1c1c1c
		PRINTL [x] 실행
		RESETCOLOR
	ENDIF
ELSEIF BAND_TYPE == 2
	IF (PURE_VOCAL > -1) && (PURE_GUITAR > -1) && (BASS > -1) && (DRUM > -1) && (KEYBORAD > -1)
		PRINTL [99] 실행
	ELSE
		SETCOLOR 0x1c1c1c
		PRINTL [x] 실행
		RESETCOLOR
	ENDIF
ENDIF

PRINTL [100] 그만둔다
$INPUT_5P_NOMINATE_LOOP
INPUT
IF RESULT == 0
;밴드유형1로
	CLEARLINE LINECOUNT - LOCAL
	BAND_TYPE = 1
	;역할변수 초기화
	VOCAL_GUITAR = -1
	PURE_VOCAL = -1
	PURE_GUITAR = -1
	BASS = -1
	DRUM = -1
	KEYBORAD = -1
	DJ_SUPPORTER = -1
	RESTART
ELSEIF RESULT == 1
;밴드유형2로
	CLEARLINE LINECOUNT - LOCAL
	BAND_TYPE = 2
	;역할변수 초기화
	VOCAL_GUITAR = -1
	PURE_VOCAL = -1
	PURE_GUITAR = -1
	BASS = -1
	DRUM = -1
	KEYBORAD = -1
	DJ_SUPPORTER = -1
	RESTART
ELSEIF RESULT == 2
;보컬&기타 or 순수 보컬
	IF BAND_TYPE == 1
		CALL INPUT_CONCERT_CHARA_LIST("보컬&기타를 맡을 노예를 선택하십시오", "VOCAL_GUITAR_5P_CASTING_REHEARSAL")
		SIF RESULT != 1000
			VOCAL_GUITAR = RESULT
	ELSE
		CALL INPUT_CONCERT_CHARA_LIST("보컬을 맡을 노예를 선택하십시오", "PURE_VOCAL_5P_CASTING_REHEARSAL")
		SIF RESULT != 1000
			PURE_VOCAL = RESULT
	ENDIF
	CLEARLINE LINECOUNT - LOCAL
	RESTART
ELSEIF RESULT == 3
;순수 기타
	CALL INPUT_CONCERT_CHARA_LIST("기타를 맡을 노예를 선택하십시오", "PURE_GUITAR_5P_CASTING_REHEARSAL")
	SIF RESULT != 1000
		PURE_GUITAR = RESULT
	CLEARLINE LINECOUNT - LOCAL
