	SIF RESULT != 1000
		BASS = RESULT
	CLEARLINE LINECOUNT - LOCAL
	RESTART
ELSEIF RESULT == 5
;드럼
	CALL INPUT_CONCERT_CHARA_LIST("드럼을 맡을 노예를 선택하십시오", "DRUM_5P_CASTING_REHEARSAL")
	SIF RESULT != 1000
		DRUM = RESULT
	CLEARLINE LINECOUNT - LOCAL
	RESTART
