;==============================
;담당 악기역할 서치(5P 밴드)
;ARG - 캐릭터 번호
;==============================
@SEARCH_INSTRUMENT_5P, ARG
#LOCALSIZE 2
#LOCALSSIZE 2

SELECTCASE ARG
	CASE VOCAL_GUITAR
	;연주자가 기타&보컬일 경우
		RETURN 0
	CASE PURE_VOCAL
	;연주자가 순수 보컬일 경우
		RETURN 1
	CASE PURE_GUITAR
	;연주자가 순수 기타일 경우
		RETURN 2
	CASE BASS
	;연주자가 베이스일 경우
		RETURN 3
	CASE DRUM
	;연주자가 드럼일 경우
		RETURN 4
	CASE KEYBORAD
	;연주자가 키보드일 경우
		RETURN 5
ENDSELECT


