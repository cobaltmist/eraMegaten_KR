	CALL INPUT_CONCERT_CHARA_LIST("DJ 및 서포터를 맡을 노예를 선택하십시오", "DJ_SUPPORTER_5P_CASTING_REHEARSAL")
	SIF RESULT != 1000
		DJ_SUPPORTER = RESULT
	CLEARLINE LINECOUNT - LOCAL
	RESTART
ELSEIF RESULT == 99
;밴드 리허설 시작
	IF BAND_TYPE == 1 && ((VOCAL_GUITAR > -1) && (PURE_GUITAR > -1) && (BASS > -1) && (DRUM > -1) && (KEYBORAD > -1))
	;보컬&기타 + 기타 + 베이스 + 드럼 + 키보드
		CALL BAND_5P_REHEARSAL, VOCAL_GUITAR, PURE_GUITAR, BASS, DRUM, KEYBORAD
