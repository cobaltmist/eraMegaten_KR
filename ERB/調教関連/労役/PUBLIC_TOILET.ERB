			ENDIF
		ELSEIF HAVE_PENIS(ARG)
			IF CFLAG:ARG:개별성적서비스 & 32 || (TALENT:ARG:ＮＴＲ == 2 || TALENT:ARG:ＮＴＲ == 5)
				CFLAG:ARG:애널횟수 += 1
				CFLAG:ARG:손コキ횟수 += 1
			ELSEIF CFLAG:ARG:개별성적서비스 & 8
				CFLAG:ARG:펠라횟수 += 1
				CFLAG:ARG:손コキ횟수 += 1
			ELSE
				CFLAG:ARG:손コキ횟수 += 1
			ENDIF
		ELSE
			IF CFLAG:ARG:개별성적서비스 & 32 || (TALENT:ARG:ＮＴＲ == 2 || TALENT:ARG:ＮＴＲ == 5)
				CFLAG:ARG:애널횟수 += 1
			ELSEIF CFLAG:ARG:개별성적서비스 & 16
				CFLAG:ARG:本番횟수 += 1
			ELSEIF CFLAG:ARG:개별성적서비스 & 8
				CFLAG:ARG:펠라횟수 += 1
			ELSE

			ENDIF
		ENDIF
	ENDIF
	;키스경험処理：適当に랜덤で
	SIF RAND:2 == 0
		TCVAR:ARG:획득키스경험 += 1
NEXT
SIF CFLAG:ARG:개별성적서비스 & 64 || (TALENT:ARG:ＮＴＲ == 2 || TALENT:ARG:ＮＴＲ == 5)
	CFLAG:ARG:中出し횟수 +=LIMIT(RAND:(1+CFLAG:ARG:本番횟수) , 0, LOCAL:1)

;획득경험
TCVAR:ARG:획득펠라경험 = CFLAG:ARG:펠라횟수
TCVAR:ARG:획득봉사쾌락경험 = CFLAG:ARG:펠라횟수
TCVAR:ARG:획득Ｖ경험 = CFLAG:ARG:本番횟수
TCVAR:ARG:획득Ａ경험 = CFLAG:ARG:애널횟수
TCVAR:ARG:획득성교경험 = CFLAG:ARG:本番횟수 + CFLAG:ARG:애널횟수
TCVAR:ARG:획득질사경험 = CFLAG:ARG:中出し횟수
TCVAR:ARG:획득정액경험 = LOCAL:1
SIF CFLAG:ARG:손コキ횟수 > 0
	TCVAR:ARG:획득사정경험 = RAND:(CFLAG:ARG:손コキ횟수)
SIF TCVAR:ARG:획득성교경험 + TCVAR:ARG:획득사정경험 > 0
	TCVAR:ARG:획득절정경험 = RAND:(TCVAR:ARG:획득성교경험 + TCVAR:ARG:획득사정경험)
IF TCVAR:ARG:획득Ｖ확장경험
	TCVAR:ARG:획득Ｖ확장경험 = CFLAG:ARG:本番횟수
	TCVAR:ARG:획득Ａ확장경험 = CFLAG:ARG:애널횟수
ENDIF
;처녀/재생처녀상실時の메시지표시および이상경험加算
IF CFLAG:ARG:처녀상실フラグ
	CFLAG:ARG:처녀상실フラグ = 0
	TALENT:ARG:처녀 = 0
	;SETCOLOR COLOR("red")
	;PRINTFORML 이상경험획득　【＋２】　처녀상실（ＮＴＲ）
	;RESETCOLOR
	;EXP:ARG:이상경험 += 2
	;SETBIT CFLAG:ARG:이상경험記録 , 35
	CALL ADDS_ABNORMAL_EXP, "처녀상실（변기）" , ARG
	CALL SHOW_ABNORMAL_EXP, ARG
ELSEIF TALENT:ARG:재생처녀
	PRINTFORML 재생처녀 상실
	TALENT:ARG:재생처녀 = 0
ENDIF
;확장첫경험時の메시지표시および이상경험加算
IF TCVAR:ARG:획득Ｖ확장경험 && (EXP:ARG:Ｖ확장경험 == 0 || EXP:ARG:Ａ확장경험 == 0)
	;SETCOLOR COLOR("red")
	;PRINTFORML 이상경험획득　【＋１】　확장첫경험
	;RESETCOLOR
	;EXP:ARG:이상경험 += 1
	;SETBIT CFLAG:ARG:이상경험記録 , 13
	CALL ADDS_ABNORMAL_EXP, "확장첫경험" , ARG
	CALL SHOW_ABNORMAL_EXP, ARG
ENDIF


LOCAL:3 = TCVAR:ARG:획득정액경험 * 5 + (TCVAR:ARG:획득절정경험 + TCVAR:ARG:획득질사경험) * 10 + CFLAG:ARG:放置日数
LOCAL:4 = CFLAG:ARG:本番횟수 + CFLAG:ARG:애널횟수 + TCVAR:ARG:획득절정경험 * 10
LOCAL:5 = CFLAG:ARG:中出し횟수 * 5 + TCVAR:ARG:획득절정경험 * 7

;ＮＴＲ客の설정処理
SELECTCASE FLAG:ＮＴＲ客の설정
	CASE  0
		TIMES LOCAL:3 , 1.00
		TIMES LOCAL:4 , 1.00
		TIMES LOCAL:5 , 1.00
	CASE  1
		TIMES LOCAL:3 , 2.00
		TIMES LOCAL:4 , 2.00
		TIMES LOCAL:5 , 2.00
	CASE  2
		TIMES LOCAL:3 , 5.00
		TIMES LOCAL:4 , 5.00
		TIMES LOCAL:5 , 5.00
ENDSELECT
;耐え切った場合はそれぞれ半分に
IF ARG:1 == 0
	LOCAL:3 /= 2
	LOCAL:4 /= 2
	LOCAL:5 /= 2
ENDIF

;호감도
IF ARG:1 == -8
	CDFLAG:ARG:キャラ間好感度:(96) += LOCAL:3
;不特定多数に설정
ELSE
	ARG:1 = -1
	LOCAL:5 += LOCAL:3/2
	;음란度上昇
	;바람기がある場合は음란度倍化
	IF TALENT:ARG:바람기
		CFLAG:ARG:음란度 += LOCAL:3
	ELSE
		CFLAG:ARG:음란度 += LOCAL:3/2
	ENDIF
ENDIF
;음란度上昇
;바람기がある場合は음란度倍化
IF TALENT:ARG:바람기
	CFLAG:ARG:음란度 += LOCAL:4*2
ELSE
	CFLAG:ARG:음란度 += LOCAL:4
ENDIF

;子宮現在容量加算
IF TALENT:ARG:오토코노코
	CFLAG:(ARG):子宮現在容量 += LOCAL:5 * 3
ELSEIF !IS_MALE(ARG)
	CFLAG:(ARG):子宮現在容量 += LOCAL:5 * 3
ENDIF

;호감도減少処理
CALL NTR_SHOOT , ARG , ARG:1 , LOCAL:5
SIF ARG:1 == -1
	CDFLAG:ARG:キャラ間好感度:(99) -= LOCAL:5


;임신用
SIF CFLAG:ARG:자궁내피임결계 == 0
	CFLAG:ARG:顧客の中出氏による임신판정 += TCVAR:ARG:획득질사경험*2

CALL SHOW_ADD_EXP, ARG

