	LOCAL:4 = BASE:(ARG:2):GET_TYPE(LOCAL:2) == 999 ? -100 # BASE:(ARG:2):GET_TYPE(LOCAL:2)
	BASE:GET_TYPE(LOCAL:2) = (LOCAL:3+LOCAL:4) / 2
NEXT

