	IF BASE:GET_TYPE(LOCAL:2) > 100
		LOCAL:3++
	ELSEIF BASE:GET_TYPE(LOCAL:2) < 1
		LOCAL:4++
	ENDIF
	SELECTCASE BASE:GET_TYPE(LOCAL:2)
	CASE IS > 999
		BASE:GET_TYPE(LOCAL:2) = 995
	CASE IS < -99
		BASE:GET_TYPE(LOCAL:2) = 999
	CASE 101 TO 104
