FOR LOCAL:2, 0, 5
	SELECTCASE RAND:100
		CASE IS < 4
			BASE:GET_TYPE(RAND:(FLAG:상성수)) -= 50
		CASE IS < 19
			BASE:GET_TYPE(RAND:(FLAG:상성수)) -= 25
		CASE IS < 39
			BASE:GET_TYPE(RAND:(FLAG:상성수)) += 25
		CASE IS < 49
			BASE:GET_TYPE(RAND:(FLAG:상성수)) += 50
	ENDSELECT
NEXT

