;조수 대화구상 실행
;==============================
;==============================
@PRINT_KOJO_OF_ASSISTANT
#LOCALSIZE 2
#LOCALSSIZE 2

IF CFLAG:ASSI:199 >= 50
;범용구상
	TRYCCALLFORM KOJO_MESSAGE_COM_PUB{ABL:ASSI:회화타입}
	CATCH
		IF CFLAG:ASSI:199 > 0
		;전용 별도구상
			TRYCCALLFORM KOJO_MESSAGE_COM_K{NO:ASSI}_{CFLAG:ASSI:199}
			CATCH
			ENDCATCH
		ELSE
		;통상 전용구상
			TRYCCALLFORM KOJO_MESSAGE_COM_K{NO:ASSI}
			CATCH
			ENDCATCH
		ENDIF
	ENDCATCH
ELSE
;전용구상
	IF CFLAG:ASSI:199 > 0
	;전용 별도구상
		TRYCCALLFORM KOJO_MESSAGE_COM_K{NO:ASSI}_{CFLAG:ASSI:199}
		CATCH
			TRYCCALLFORM KOJO_MESSAGE_COM_PUB{ABL:ASSI:회화타입}
			CATCH
			ENDCATCH
		ENDCATCH
	ELSE
	;통상 전용구상
		TRYCCALLFORM KOJO_MESSAGE_COM_K{NO:ASSI}
		CATCH
			TRYCCALLFORM KOJO_MESSAGE_COM_PUB{ABL:ASSI:회화타입}
			CATCH
			ENDCATCH
		ENDCATCH
	ENDIF
ENDIF