;PRINTFORML 조수캐릭번호: {NO:ASSI} --> 디버그용
;==============================
;밀어 넘어뜨리기中 노예가 기승위時 조수가 하는 자동 행동처리
;ARG - 매개변수로 받은 선택된 커맨드 번호
;==============================
@ASSI_3P_DOING, ARG = 1000
#DIM DYNAMIC TEMPSELECTCOM ;커맨드번호 임시저장 변수
#DIM DYNAMIC TEMP_TARGET ;조교대상번호 임시저장 변수

SELECTCASE ARG
	CASE 1
	;안면기승위(커널링쿠스) --> ★ 조수 3P 커맨드(노예가 기승위 파생커맨드로 '키스하기'를 실행하지 않을 시 조수가 실행 가능)
		TSTR:3 = 안면기승위
		PRINTFORML ★조수의 행동: %TSTR:3%★
		TEMPSELECTCOM = SELECTCOM ;현 커맨드번호 임시저장
		TEMP_TARGET = TARGET ;현 조교대상번호 임시저장
		SELECTCOM = 1 ;조수의 자동선택 커맨드번호 대입
		TARGET = ASSI ;조수를 일시적으로 조교대상으로 변경
		;조수의 대사 출력
		CALL PRINT_KOJO_OF_ASSISTANT
		SELECTCOM = TEMPSELECTCOM ;현 커맨드번호 복구
		TARGET = TEMP_TARGET ;현 조교대상번호 복구
		PRINTL
		;-------------------------------------------------
		;ソース추가処理
		;-------------------------------------------------
		;소비체력、기력
		BASE:ASSI:체력 -= 5
		IF BASE:ASSI:기력 <= 0
			BASE:ASSI:기력 = 0
		ELSE
			BASE:ASSI:기력 -= 50
		ENDIF

		SOURCE:ASSI:쾌Ｃ = 100
		SOURCE:ASSI:액체 = 60
		SOURCE:ASSI:노출 = 10
		SOURCE:ASSI:일탈 = 15
		
		CUP:ASSI:쾌Ｃ += (200 + RAND:50)
		CALL ASSI_PALAM_UP, "쾌Ｃ"
		CUP:ASSI:욕정 += (220 + RAND:50)
		CALL ASSI_PALAM_UP, "욕정"
		CUP:ASSI:치정 += (210 + RAND:40)
		CALL ASSI_PALAM_UP, "치정"
		CUP:ASSI:윤활 += (250 + RAND:30)
		CALL ASSI_PALAM_UP, "윤활"
		CUP:ASSI:굴복 += (200 + RAND:50)
		CALL ASSI_PALAM_UP, "굴복"
		
		;-------------------------------------------------
		;特殊ソース処理
		;-------------------------------------------------
		;억압、저항持ちには、억울ソース추가
		SIF TALENT:ASSI:32 || TALENT:ASSI:34
			SOURCE:ASSI:울굴 += 20

		;舌を使う조교では、조교자の혀놀림によって、쾌感のソース증가
		SIF TALENT:PLAYER:52
			TIMES SOURCE:ASSI:쾌Ｃ , 1.50

		;고양이혀では減少
		SIF TALENT:PLAYER:54
			TIMES SOURCE:ASSI:쾌Ｃ , 0.75

		;-------------------------------------------------
		;불결の処理
		;-------------------------------------------------
		;奴隷のＶ⇔조교자の입の불결が移動
		CALL MOVE_STAIN("바기나", ASSI, "입", PLAYER)

		;-------------------------------------------------
		;吸精処理
		;-------------------------------------------------
		IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
			LOCAL = (RAND:4+1) * BASE:ASSI:LV
			CALL DRAIN_MAG,PLAYER,LOCAL,ASSI
		ELSEIF CFLAG:ASSI:ＭＡＧ비피흡수 == 0
			LOCAL = (RAND:4+1) * BASE:ASSI:LV
			CALL DRAIN_MAG,ASSI,LOCAL,PLAYER
		ENDIF

		;-------------------------------------------------
		;경험の処理
		;-------------------------------------------------
		A = 3
		IF IS_LESBIAN(ASSI,PLAYER)
			CALL ADDS_EXP("레즈경험", A, ASSI)
			CALL ADDS_EXP("레즈경험", A, PLAYER)
		ELSEIF IS_GAY(ASSI,PLAYER)
			CALL ADDS_EXP("ＢＬ경험", A, ASSI)
			CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
		ENDIF

		;조교자が고양이혀持ちの場合は、조교자に가학쾌락경험を추가
		IF TALENT:PLAYER:54
			CALL ADDS_EXP("가학쾌락경험", 1, PLAYER)
		ENDIF
		
	CASE 20
	;키스한다 --> ★ 조수 3P 커맨드(노예가 기승위 파생커맨드로 '키스하기'를 실행하지 않을 시 조수가 실행 가능)
		TSTR:3 = 키스한다
		PRINTFORML ★조수의 행동: %TSTR:3%★
		TEMPSELECTCOM = SELECTCOM ;현 커맨드번호 임시저장
		TEMP_TARGET = TARGET ;현 조교대상번호 임시저장
		SELECTCOM = 20 ;조수의 자동선택 커맨드번호 대입
		TARGET = ASSI ;조수를 일시적으로 조교대상으로 변경
		;조수의 대사 출력
		CALL PRINT_KOJO_OF_ASSISTANT
		SELECTCOM = TEMPSELECTCOM ;현 커맨드번호 복구
		TARGET = TEMP_TARGET ;현 조교대상번호 복구
		PRINTL
		;-------------------------------------------------
		;사정ゲージ체크（후타나리・남자の場合）
		;-------------------------------------------------
		B = 300

		;ABL:조교자の기교をみる
		IF ABL:ASSI:2 == 0
			TIMES B , 1.00
		ELSEIF ABL:ASSI:2 == 1
			TIMES B , 1.20
		ELSEIF ABL:ASSI:2 == 2
			TIMES B , 1.40
		ELSEIF ABL:ASSI:2 == 3
			TIMES B , 1.60
		ELSEIF ABL:ASSI:2 == 4
			TIMES B , 1.80
		ELSE
			TIMES B , 2.00
		ENDIF

		;ABL:순종をみる
		IF ABL:ASSI:0 == 0
			TIMES B , 0.80
		ELSEIF ABL:ASSI:0 == 1
			TIMES B , 0.90
		ELSEIF ABL:ASSI:0 == 2
			TIMES B , 1.00
		ELSEIF ABL:ASSI:0 == 3
			TIMES B , 1.10
		ELSEIF ABL:ASSI:0 == 4
			TIMES B , 1.20
		ELSE
			TIMES B , 1.30
		ENDIF

		;ABL:봉사정신をみる
		IF ABL:ASSI:7 == 0
			TIMES B , 0.50
		ELSEIF ABL:ASSI:7 == 1
			TIMES B , 0.80
		ELSEIF ABL:ASSI:7 == 2
			TIMES B , 1.20
		ELSEIF ABL:ASSI:7 == 3
			TIMES B , 1.50
		ELSEIF ABL:ASSI:7 == 4
			TIMES B , 1.80
		ELSE
			TIMES B , 2.40
		ENDIF

		;TALENT:혀놀림、고양이혀をみる
		SIF TALENT:ASSI:52 || TALENT:ASSI:54
			TIMES B , 2.00

			LOCAL:99 = B
		;플레이어のABL:Ｃ감각をみる
		IF ABL:PLAYER:3 == 0
			TIMES B , 1.00
		ELSEIF ABL:PLAYER:3 == 1
			TIMES B , 1.50
		ELSEIF ABL:PLAYER:3 == 2
			TIMES B , 2.00
		ELSEIF ABL:PLAYER:3 == 3
			TIMES B , 2.50
		ELSEIF ABL:PLAYER:3 == 4
			TIMES B , 3.50
		ELSE
			TIMES B , 5.00
		ENDIF

		;再装填処理中は사정ゲージは20分の1に
		IF CFLAG:PLAYER:9 > 0
			B /= 20
			LOCAL:99 /= 20
		ENDIF

		SIF HAVE_PENIS(PLAYER)
			BASE:PLAYER:2 += B
		SIF !IS_MALE(PLAYER)
			BASE:PLAYER:99 += LOCAL:99


		;-------------------------------------------------
		;ソース추가処理
		;-------------------------------------------------
		;소비체력、기력
		BASE:ASSI:체력 -= 0
		IF BASE:ASSI:기력 <= 0
			BASE:ASSI:기력 = 0
		ELSE
			BASE:ASSI:기력 -= 20
		ENDIF

		SOURCE:ASSI:정애 = 50
		SOURCE:ASSI:성행위 = 10
		SOURCE:ASSI:굴종 = 10
		SOURCE:ASSI:일탈 = 10
		IF ABL:ASSI:키스중독
			SOURCE:ASSI:정애 += ABL:ASSI:키스중독*10 + ABL:PLAYER:키스중독*10
			SOURCE:ASSI:성행위 += ABL:ASSI:키스중독*20 + ABL:PLAYER:키스중독*10
		ENDIF
		;上のほうで計算した불결데이터
		SOURCE:ASSI:불결 = Y*20 + 10
		
		CUP:ASSI:욕정 += (280 + RAND:80)
		CALL ASSI_PALAM_UP, "욕정"
		CUP:ASSI:치정 += (190 + RAND:50)
		CALL ASSI_PALAM_UP, "치정"
		CUP:ASSI:굴복 += (250 + RAND:50)
		CALL ASSI_PALAM_UP, "굴복"
		CUP:ASSI:습득 += (250 + RAND:100)
		CALL ASSI_PALAM_UP, "습득"
		CUP:ASSI:복종 += (250 + RAND:80)
		CALL ASSI_PALAM_UP, "복종"

		;-------------------------------------------------
		;特殊ソース処理
		;-------------------------------------------------
		;억압、저항持ちには、억울ソース추가
		SIF TALENT:ASSI:32 || TALENT:ASSI:34
			SOURCE:ASSI:울굴 += 10

		;舌を使う조교では、조교자の혀놀림によって、쾌感のソース증가
		IF TALENT:PLAYER:52
			TIMES SOURCE:ASSI:정애 , 1.50
		ENDIF

		;고양이혀では減少
		IF TALENT:PLAYER:54
			TIMES SOURCE:ASSI:정애 , 0.75
		ENDIF

		;-------------------------------------------------
		;불결の処理
		;-------------------------------------------------
		;奴隷の입⇔조교자の입の불결が移動
		CALL MOVE_STAIN("입", ASSI, "입", PLAYER)

		;-------------------------------------------------
		;吸精処理
		;-------------------------------------------------
		IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
			LOCAL = (RAND:7+1) * (BASE:ASSI:LV + ABL:ASSI:키스중독 + ABL:PLAYER:키스중독)
			CALL DRAIN_MAG,PLAYER,LOCAL,ASSI
		ELSEIF CFLAG:ASSI:ＭＡＧ비피흡수 == 0
			LOCAL = (RAND:7+1) * (BASE:ASSI:LV + ABL:ASSI:키스중독 + ABL:PLAYER:키스중독)
			CALL DRAIN_MAG,ASSI,LOCAL,PLAYER
		ENDIF

		;-------------------------------------------------
		;経験の処理
		;-------------------------------------------------
		A = 3
		IF IS_LESBIAN(ASSI,PLAYER)
			CALL ADDS_EXP("레즈경험", A, ASSI)
			CALL ADDS_EXP("레즈경험", A, PLAYER)
		ELSEIF IS_GAY(ASSI,PLAYER)
			CALL ADDS_EXP("ＢＬ경험", A, ASSI)
			CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
		ENDIF

		;키스경험
		CALL ADDS_EXP("키스경험", 1, ASSI)
		CALL ADDS_EXP("키스경험", 1, PLAYER)

		;애정경험
		IF TALENT:ASSI:연모 || TALENT:ASSI:친애
			CALL ADDS_EXP("애정경험", 1, ASSI)
			CALL ADDS_EXP("애정경험", 1, MASTER)
		ENDIF

		;奴隷が고양이혀持ちの場合は、조교자に가학쾌락경험を추가
		IF TALENT:ASSI:54
			CALL ADDS_EXP("가학쾌락경험", 1, ASSI)
		ENDIF
		
	CASE 30
	;파후파후 --> ★ 조수 3P 커맨드(노예가 기승위 파생커맨드로 '키스하기'를 실행하지 않을 시 조수가 실행 가능)
		TSTR:3 = 파후파후
		PRINTFORML ★조수의 행동: %TSTR:3%★
		TEMPSELECTCOM = SELECTCOM ;현 커맨드번호 임시저장
		TEMP_TARGET = TARGET ;현 조교대상번호 임시저장
		SELECTCOM = 30 ;조수의 자동선택 커맨드번호 대입
		TARGET = ASSI ;조수를 일시적으로 조교대상으로 변경
		;조수의 대사 출력
		CALL PRINT_KOJO_OF_ASSISTANT
		SELECTCOM = TEMPSELECTCOM ;현 커맨드번호 복구
		TARGET = TEMP_TARGET ;현 조교대상번호 복구
		PRINTL
		;-------------------------------------------------
		;ソース추가処理
		;-------------------------------------------------
		;소비체력、기력
		BASE:ASSI:체력 -= 5
		IF BASE:ASSI:기력 <= 0
			BASE:ASSI:기력 = 0
		ELSE
			BASE:ASSI:기력 -= 20
		ENDIF

		SOURCE:ASSI:복종 = 50
		SOURCE:ASSI:3 = 100
		
		CUP:ASSI:쾌Ｂ += (250 + RAND:60)
		CALL ASSI_PALAM_UP, "쾌Ｂ"
		CUP:ASSI:윤활 += (250 + RAND:90)
		CALL ASSI_PALAM_UP, "윤활"
		CUP:ASSI:복종 += (230 + RAND:80)
		CALL ASSI_PALAM_UP, "복종"

		;-------------------------------------------------
		;경험の処理
		;-------------------------------------------------
		;애정경험
		IF (TALENT:ASSI:3 == 1 || TALENT:ASSI:6 == 1) && ASSIPLAY == 0
			CALL ADDS_EXP("애정경험", 1, ASSI)
			CALL ADDS_EXP("애정경험", 1, MASTER)
		ENDIF
		
	CASE 31
	;손가락츄파 --> ★ 조수 3P 커맨드
		TSTR:3 = 손가락츄파
		PRINTFORML ★조수의 행동: %TSTR:3%★
		TEMPSELECTCOM = SELECTCOM ;현 커맨드번호 임시저장
		TEMP_TARGET = TARGET ;현 조교대상번호 임시저장
		SELECTCOM = 31 ;조수의 자동선택 커맨드번호 대입
		TARGET = ASSI ;조수를 일시적으로 조교대상으로 변경
		;조수의 대사 출력
		CALL PRINT_KOJO_OF_ASSISTANT
		SELECTCOM = TEMPSELECTCOM ;현 커맨드번호 복구
		TARGET = TEMP_TARGET ;현 조교대상번호 복구
		PRINTL
		;-------------------------------------------------
		;사정ゲージ체크（후타나리・남자の場合）
		;-------------------------------------------------
		B = 300

		;ABL:조교자の기교をみる
		IF ABL:ASSI:2 == 0
			TIMES B , 1.00
		ELSEIF ABL:ASSI:2 == 1
			TIMES B , 1.20
		ELSEIF ABL:ASSI:2 == 2
			TIMES B , 1.40
		ELSEIF ABL:ASSI:2 == 3
			TIMES B , 1.60
		ELSEIF ABL:ASSI:2 == 4
			TIMES B , 1.80
		ELSE
			TIMES B , 2.00
		ENDIF

		;ABL:순종をみる
		IF ABL:ASSI:0 == 0
			TIMES B , 0.80
		ELSEIF ABL:ASSI:0 == 1
			TIMES B , 0.90
		ELSEIF ABL:ASSI:0 == 2
			TIMES B , 1.00
		ELSEIF ABL:ASSI:0 == 3
			TIMES B , 1.10
		ELSEIF ABL:ASSI:0 == 4
			TIMES B , 1.20
		ELSE
			TIMES B , 1.30
		ENDIF

		;ABL:봉사정신をみる
		IF ABL:ASSI:7 == 0
			TIMES B , 0.50
		ELSEIF ABL:ASSI:7 == 1
			TIMES B , 0.80
		ELSEIF ABL:ASSI:7 == 2
			TIMES B , 1.20
		ELSEIF ABL:ASSI:7 == 3
			TIMES B , 1.50
		ELSEIF ABL:ASSI:7 == 4
			TIMES B , 1.80
		ELSE
			TIMES B , 2.40
		ENDIF

		;TALENT:혀놀림、고양이혀をみる
		SIF TALENT:ASSI:52 || TALENT:ASSI:54
			TIMES B , 2.00

			LOCAL:99 = B
		;플레이어のABL:Ｃ감각をみる
		IF ABL:PLAYER:3 == 0
			TIMES B , 1.00
		ELSEIF ABL:PLAYER:3 == 1
			TIMES B , 1.50
		ELSEIF ABL:PLAYER:3 == 2
			TIMES B , 2.00
		ELSEIF ABL:PLAYER:3 == 3
			TIMES B , 2.50
		ELSEIF ABL:PLAYER:3 == 4
			TIMES B , 3.50
		ELSE
			TIMES B , 5.00
		ENDIF

		;再装填処理中は사정ゲージは20分の1に
		IF CFLAG:PLAYER:9 > 0
			B /= 20
			LOCAL:99 /= 20
		ENDIF

		SIF HAVE_PENIS(PLAYER)
			BASE:PLAYER:2 += B
		SIF !IS_MALE(PLAYER)
			BASE:PLAYER:99 += LOCAL:99


		;-------------------------------------------------
		;ソース추가処理
