		ENDIF

		SOURCE:ASSI:정애 = 50
		SOURCE:ASSI:성행위 = 10
		SOURCE:ASSI:굴종 = 10
		SOURCE:ASSI:일탈 = 10

		;上のほうで計算した불결데이터
		SOURCE:ASSI:불결 = Y*20 + 10
		
		CUP:ASSI:욕정 += (140 + RAND:30)
		CALL ASSI_PALAM_UP, "욕정"
		CUP:ASSI:굴복 += (120 + RAND:20)
		CALL ASSI_PALAM_UP, "굴복"
		CUP:ASSI:습득 += (130 + RAND:20)
		CALL ASSI_PALAM_UP, "습득"
		CUP:ASSI:복종 += (140 + RAND:30)
		CALL ASSI_PALAM_UP, "복종"
		
		;-------------------------------------------------
		;特殊ソース処理
