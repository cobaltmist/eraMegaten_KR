		B = 100000
		C = 30000
		D = 350
	ELSEIF ABL:18 == 6
		A = 40000
		B = 120000
		C = 35000
		D = 500
	ELSEIF ABL:18 == 7
		A = 45000
		B = 140000
		C = 40000
		D = 600
