;섹스중독LVUP판정
@DECIDE_ABLUP18

;조건別にＯＫかダメかを記録する
I = 0
J = 0
;능력アップ가능なら1にする
Z = 0

IF TALENT:남자
	;Iを0以外にしておく
	I |= 8
	IF ABL:18 == 0
		A = 2500
		B = 6000
		C = 1000
		D = 10
	ELSEIF ABL:18 == 1
		A = 5000
		B = 14000
		C = 2500
		D = 25
	ELSEIF ABL:18 == 2
		A = 9000
		B = 27000
		C = 4800
		D = 80
	ELSEIF ABL:18 == 3
		A = 17000
		B = 65000
		C = 10000
		D = 150
	ELSEIF ABL:18 == 4
		A = 30000
		B = 80000
		C = 25000
		D = 250
	ELSEIF ABL:18 == 5
		A = 35000
		B = 100000
		C = 30000
		D = 350
	ELSEIF ABL:18 == 6
		A = 40000
		B = 120000
		C = 35000
		D = 500
	ELSEIF ABL:18 == 7
		A = 45000
		B = 140000
		C = 40000
		D = 600
	ELSEIF ABL:18 == 8
		A = 50000
		B = 160000
		C = 45000
		D = 700
	ELSEIF ABL:18 == 9
		A = 60000
		B = 200000
		C = 50000
		D = 800
	ENDIF

	;일선을넘지않음
	IF TALENT:27 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && 함락() < 2)
		TIMES A , 2.00
		TIMES B , 2.00
		TIMES C , 2.00
		TIMES D , 2.00
	ENDIF
	;중독되기쉬움
	IF TALENT:72
		TIMES A , 0.50
		TIMES B , 0.50
		TIMES C , 0.50
		TIMES D , 0.50
	ENDIF

	;ＬＶ２から３に上げるときは이상경험필요（소질：[중독되기쉬움][애널광]なら無視できる）
	IF ABL:18 == 2 && TALENT:72 == 0 && TALENT:76 == 0
		IF EXP:50 == 0
			;이상경험が부족
			J |= 2
		ENDIF
	;ＬＶ３から４に上げるときは이상경험2이상필요（소질：[중독되기쉬움][애널광]なら無視できる）
	ELSEIF ABL:18 == 3 && TALENT:72 == 0 && TALENT:76 == 0
		IF EXP:50 < 2
			;이상경험が부족
			J |= 2
		ENDIF
	;ＬＶ４から５に上げるときは이상경험3이상필요（소질：[중독되기쉬움][애널광]なら無視できる）
	ELSEIF ABL:18 == 4 && TALENT:72 == 0 && TALENT:76 == 0
		IF EXP:50 < 3
			;이상경험が부족
			J |= 2
		ENDIF
	;ＬＶ８から９に上げるときは이상경험7이상필요（소질：[중독되기쉬움][애널광]なら無視できる）
	ELSEIF ABL:18 == 8 && TALENT:72 == 0 && TALENT:76 == 0
		IF EXP:50 < 7
			;이상경험が부족
			J |= 2
		ENDIF
	ENDIF

	;A감각が필요
	IF ABL:5 < ABL:18 + 1
		J |= 4
	ENDIF

	;욕정의 구슬で上げる
	SIF JUEL:6 < A
		J |= 1
	;쾌A의 구슬で上げる
	SIF JUEL:2 < B
		J |= 1
	;굴복의 구슬で上げる
	SIF JUEL:7 < C
		J |= 1
	;성교경험が필요
	SIF EXP:4 < D
		J |= 2
	IF J == 0
		Z = 1
	ENDIF

	RETURN 1
ELSE
	;Jを0以外にしておく
	J |= 8
	IF ABL:18 == 0
