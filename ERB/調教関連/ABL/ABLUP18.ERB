	ELSEIF ABL:18 == 4 && TALENT:72 == 0 && TALENT:75 == 0
		IF EXP:50 < 3
			;이상경험が부족
			I |= 2
		ENDIF
	;ＬＶ８から９に上げるときは이상경험7이상필요（소질：[중독되기쉬움][섹스광]なら無視できる）
	ELSEIF ABL:18 == 8 && TALENT:72 == 0 && TALENT:75 == 0
		IF EXP:50 < 7
			;이상경험が부족
			I |= 2
		ENDIF
	ENDIF

	;V감각が필요
	IF ABL:4 < ABL:18 + 1
		I |= 4
	ENDIF

	;욕정의 구슬で上げる
	SIF JUEL:6 < A
		I |= 1
	;쾌V의 구슬で上げる
	SIF JUEL:1 < B
		I |= 1
	;굴복의 구슬で上げる
	SIF JUEL:7 < C
		I |= 1
	;성교경험が필요
	SIF EXP:4 < D
		I |= 2
	IF I == 0
