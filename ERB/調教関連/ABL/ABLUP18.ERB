		A = 60000
		B = 200000
		C = 50000
		D = 800
	ENDIF

	;일선을넘지않음
	IF TALENT:27 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && 함락() < 2)
		TIMES A , 2.00
		TIMES B , 2.00
		TIMES C , 2.00
