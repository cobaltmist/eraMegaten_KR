;섹스중독可否
@ISABLUP18
;연모、친애、음란、창부はMAXLV10
IF ABL:18 >= 10
	RETURN 0
ELSEIF ABL:18 >= 5 && !함락(TARGET) && TARGET != MASTER
	RETURN 0
ENDIF

CALL DECIDE_ABLUP18

IF Z == 0
	RETURN 0
ELSE
	RETURN 1
ENDIF

