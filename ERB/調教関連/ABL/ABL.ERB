@ABL_MANUAL_MENU
VARSET LOCAL
;타겟退避(TFLAG:75)
LOCAL:3 = TARGET

$INPUT_LIST
;キャラリストを표시・選択
CALL INPUT_CHARA_LIST("누구의 능력을 상승시킵니까？", "CASTING_ABL_MANUAL")
