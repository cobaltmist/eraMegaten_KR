;ARGは生成キャラの등록番号
;小学生は서머너(デビチル枠)か이능자
;레어キャラは女限定で마법소녀(っぽいもの)
;스킬はもう面倒なので공격系、カジャ系、魔脈/コンセ、만능の一律세트にして
;各自お好みの스킬카드で整えてもらうようにする
@RANDOMCHARA_4903, ARGS, ARG, ARG:1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "일인칭"
		IF TALENT:ARG:남자 == 1
			RESULTS = 나
		ELSE
			RESULTS = 나
		ENDIF
	CASE "이인칭"
		RESULTS = %CALLNAME:MASTER%
	CASE "체력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2500)
		ELSE
			RETURN RAND(800, 1500)
		ENDIF
	CASE "기력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2500)
		ELSE
			RETURN RAND(800, 1500)
		ENDIF
	CASE "속성"
		RETURN RAND(100, 200), RAND(100, 200)
	CASE "종족"
		RETURN 0
	CASE "전투소질"
		;1/2で非戦闘員
		IF RAND:2 > 0
			RESULTS = 비전투원
			RETURN 1
		ENDIF
		;女の場合、1/10で레어枠の이능자シフター(마법소녀)
		IF TALENT:ARG:남자 == 0 && RAND:10 == 0
			RESULTS = 이능자
			RESULTS:1 = 데빌시프터
			;강화フラグがある場合、1/2で도구지식3
			IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
				RESULTS:2 = 도구지식
				RETURN 1, 1, 3
			ENDIF
			RETURN 1, 1
		ENDIF
		;それ이외は서머너か이능자
		;서머너が多くてもアレなので、少し이능자を出やすくする
		SELECTCASE RAND:100
			CASE IS < 35
				;純サマ
				RESULTS = 서머너
				;ガンスリと석세서はどちらかのみ
				IF RAND:2 > 0
					RESULTS:1 = 석세서
				ELSE
					RESULTS:1 = 건슬링거
				ENDIF
				RESULTS:2 = 도구지식
				RETURN 5, 1, 3
			CASEELSE
				RESULTS = 이능자
				;純이능자(도구지식持ち)か이능자서머너
				IF RAND:2 > 0
					RESULTS:1 = 서머너
						;強化フラグがある場合、1/2で強化。サクセサーも付ける。
						IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
						IF EXTRA強化(3)
								RESULTS:2 = 석세서
								RETURN 1, 2, 1
							ELSE
								RESULTS:2 = 달인
								RESULTS:3 = 석세서
								RESULTS:4 = 천사
								RETURN  0, 1, 1, 1, 1
							ENDIF
					ENDIF
				ELSE
					RESULTS:1 = 도구지식
					;강화フラグがある場合、1/2で강화。도구지식を3に
					IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
						RETURN 1, 3
					ENDIF
				ENDIF
				RETURN 1, 2
		ENDSELECT
	;戻り値は0:랜덤 それ이외は指定
	CASE "초기페르소나"
		RETURN 0
	;戻り値は0:랜덤 それ이외は指定
	CASE "링크악마"
		RETURN 0
	CASE "초기스테이터스比率"
		RETURN 7, 7, 8, 7, 8, 10
	CASE "스킬"
		IF TALENT:ARG:달인 == 1 && TALENT:ARG:이능자 == 0 && TALENT:ARG:천사 == 1
				RESULT = [[스킬:마하・라기온]]
				RESULT:1 = [[스킬:티없는위광]]
				RESULT:2 = [[스킬:리캄도라]]
				RESULT:3 = [[스킬:이악물기]]
				RESULT:4 = [[스킬:컨센트레이트]]
				RESULT:5 = [[스킬:침묵의기도]]
				RESULT:6 = [[스킬:마하・라기다인]]
				RESULT:6 = [[스킬:삼할의마맥]]
				RESULT:7 = [[스킬:델타포스]]
				RESULT:8 = [[스킬:불굴의투지]]
				RESULT:9 = [[스킬:후리후리월]]
				RESULT:10 = [[스킬:기공(대)]]
				RESULT:11 = [[스킬:프로미넌스]]
				RESULT:12 = [[스킬:자애의기도]]
				RESULT:13 = [[스킬:메기도라온]]
				RETURN RESULT
		ELSEIF 이벤트플래그:88:0 > 3 && TALENT:ARG:서머너 > 1 && TALENT:ARG:이능자 == 0
			;純サマ강화스킬
			;とりあえず自動스킬を山盛り
			RESULT = [[스킬:아이템숙련]]
			RESULT:1 = [[스킬:어드바이스]]
			RESULT:2 = [[스킬:이악물기]]
			RESULT:3 = [[스킬:추격의심득]]
			RESULT:4 = [[스킬:황천의회심]]
			RESULT:5 = [[스킬:정천의회심]]
			RESULT:6 = [[스킬:서드아이]]
			RESULT:7 = [[스킬:관통]]
			RETURN RESULT
		ENDIF
		IF TALENT:ARG:이능자 == 0
			RETURN 0
		ELSEIF TALENT:ARG:데빌시프터 == 1
			RETURN 0
		ENDIF
		
		;이능자스킬
		;공격
		SELECTCASE RAND:7
			CASE 0
				RESULT = [[스킬:아기]]
				RESULT:1 = [[스킬:아기라오]]
				RESULT:2 = [[스킬:아기다인]]
				RESULT:3 = [[스킬:데들리번]]
			CASE 1
				RESULT = [[스킬:마그나]]
				RESULT:1 = [[스킬:마그나스]]
				RESULT:2 = [[스킬:마그다인]]
				RESULT:3 = [[스킬:대지의분노]]
			CASE 2
				RESULT = [[스킬:아쿠아]]
				RESULT:1 = [[스킬:아쿠에스]]
				RESULT:2 = [[스킬:아쿠아다인]]
				RESULT:3 = [[스킬:아쿠아리타이드]]
			CASE 3
				RESULT = [[스킬:갈]]
				RESULT:1 = [[스킬:갈라]]
				RESULT:2 = [[스킬:갈다인]]
				RESULT:3 = [[스킬:선풍진]]
			CASE 4
				RESULT = [[스킬:코우하]]
				RESULT:1 = [[스킬:바이코우하]]
				RESULT:2 = [[스킬:마하코우하]]
				RESULT:3 = [[스킬:사마리캄]]
			CASE 5
				RESULT = [[스킬:베어내기]]
				RESULT:1 = [[스킬:히트웨이브]]
				RESULT:2 = [[스킬:무사차]]
				RESULT:3 = [[스킬:산산조각베기]]
			CASE 6
				RESULT = [[스킬:베어내기]]
				RESULT:1 = [[스킬:섬머솔트]]
				RESULT:2 = [[스킬:열진각]]
				RESULT:3 = [[스킬:맹돌진]]
		ENDSELECT
		SELECTCASE RAND:4
			CASE 0
				RESULT:4 = [[스킬:타루・카쟈]]
				RESULT:5 = [[스킬:다크브레스]]
			CASE 1
				RESULT:4 = [[스킬:스쿠・카쟈]]
				RESULT:5 = [[스킬:도발]]
			CASE 2
				RESULT:4 = [[스킬:라쿠・카쟈]]
				RESULT:5 = [[스킬:파워브레스]]
			CASE 3
				RESULT:4 = [[스킬:사마・카쟈]]
				RESULT:5 = [[스킬:위협]]
		ENDSELECT
		;魔脈/コンセ
		IF RAND:2 > 0
			RESULT:6 = [[스킬:일할의마맥]]
		ELSE
			RESULT:6 = [[스킬:컨센트레이트]]
		ENDIF
		;만능
		RESULT:7 = [[스킬:메기도]]
		RESULT:8 = [[스킬:메기도라온]]
		
		;강화
		IF 이벤트플래그:88:0 > 3
			RESULT:9 = [[스킬:데・카쟈]]
			RESULT:10 = [[스킬:리캄도라]]
				IF RESULT:6 == [[스킬:일할의마맥]]
					RESULT:11 = [[스킬:컨센트레이트]]
				ELSE
					RESULT:11 = [[스킬:일할의마맥]]
				ENDIF
		ENDIF
		RETURN RESULT
	CASE "상성"
		SELECTCASE GET_TYPE(ARG:1)
			CASE "파마"
				RETURN 0
			CASEELSE
				RETURN 100
		ENDSELECT
	CASE "소질"
		RETURN GETNUM(TALENT, "겁쟁이"), GETNUM(TALENT, "아픔에약함"), GETNUM(TALENT, "건방짐"), GETNUM(TALENT, "호기심"), GETNUM(TALENT, "젖기어려움"), GETNUM(TALENT, "솔직함")
	CASE "除外소질"
		RETURN GETNUM(TALENT, "도착적"), GETNUM(TALENT, "바늘다루기"), GETNUM(TALENT, "조합지식")
	;처녀は特別 0は랜덤(20%), 1なら처녀, 2なら非처녀
	CASE "처녀"
		IF  RAND:5 == 0
			RETURN 2
		ELSE
			RETURN 1
		ENDIF
	;戻り値は0:랜덤 1:소인체형 2:유아체형 4:큰체형 5:거체
	CASE "体型"
		RETURN 2
	;戻り値は0:랜덤 1:절벽　2:빈유　3:並　4:거유　5:폭유
	CASE "가슴"
		IF TALENT:ARG:남자 == 0
			SELECTCASE RAND:10
				CASE IS < 6
					RETURN 2
				CASE IS < 8
					RETURN 3
				CASE IS < 9
					RETURN 1
				CASE IS < 10
					IF RAND:10 == 0
						RETURN 5
					ELSE
						RETURN 4
					ENDIF
			ENDSELECT
		ELSEIF TALENT:ARG:오토코노코 == 1
			SELECTCASE RAND:4
				CASE IS < 2
					RETURN 2
				CASE IS < 4
					RETURN 1
			ENDSELECT
		ELSE
			RETURN 1
		ENDIF
	;추가対応。戻り値は0:랜덤 1:小尻　2:並尻　3:巨尻　4:爆尻
	CASE "尻"
		IF TALENT:ARG:남자 == 0 || TALENT:ARG:오토코노코 == 1
			SELECTCASE RAND:10
				CASE IS < 6
					RETURN 1
				CASE IS < 8
					RETURN 3
				CASE IS < 9
					RETURN 2
				CASE IS < 10
					IF RAND:10 == 0
						RETURN 0
					ELSE
						RETURN 4
					ENDIF
			ENDSELECT
		ELSE
			RETURN 2
		ENDIF
	;통상の인간と肉体部位が違う場合はここで直接代入してください
	CASE "肉体部位"
		RETURN 0
	CASE "ABL"
		SELECTCASE ABLNAME:(ARG:1)
			CASE "순종", "욕망", "기교"
				RETURN !RAND:3+!RAND:5
			CASE "Ｃ감각", "Ｖ감각", "Ｂ감각"
				RETURN !RAND:5
			CASE "마조끼", "노출증", "봉사정신"
				RETURN !RAND:10
			CASE "새드끼"
				RETURN !RAND:5 * !ABL:ARG:마조끼
			CASE "Ａ감각", "자위중독", "정액중독", "레즈끼", "ＢＬ끼", "섹스중독"
				RETURN !RAND:10
		ENDSELECT
	;EXPはABLが1이상ある場合、ある程度調整される(ただし、ABL0の時は調整が動かないので適当に초기値をきめてください)
	CASE "EXP"
		;全く손付かずの場合
		SIF !GROUPMATCH(1, !TALENT:ARG:처녀, ABL:ARG:Ｃ감각, ABL:ARG:Ｖ감각, ABL:ARG:Ａ감각, ABL:ARG:Ｂ감각, ABL:ARG:기교)
			RETURN 0
		SELECTCASE EXPNAME:(ARG:1)
			CASE "Ｖ경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "Ａ경험"
				RETURN RAND(0, 20) * !RAND:5
			CASE "절정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "사정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "성교경험"
				RETURN 0
			CASE "질사경험"
				RETURN EXP:ARG:사정경험/2*EXP:ARG:성교경험 > 0
			CASE "자위경험"
				RETURN RAND(0, 20)
			CASE "조교자위경험"
				RETURN 0
			CASE "정액경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "봉사쾌락경험"
				RETURN 0
			CASE "펠라경험"
				RETURN RAND(0, 10) * RAND:2
			CASE "애정경험"
				RETURN 0
			CASE "고통쾌락경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "방뇨경험"
				RETURN 0
			CASE "가학쾌락경험"
				RETURN RAND(0, 10) * !RAND:4
			CASE "야외노출경험"
				RETURN 0
			CASE "레즈경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "ＢＬ경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "이상경험"
				RETURN 0
			CASE "긴박경험"
				RETURN RAND(0, 10) * EXP:ARG:고통쾌락경험 > 0
		ENDSELECT
	CASE "모자"
		;마법소녀の場合は옷装もそれっぽくする
		IF TALENT:ARG:데빌시프터 == 1
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:화이트브림]]
				CASE 1
					RETURN [[衣装:너스캡]]
				CASE 2
					RETURN [[衣装:토끼귀밴드]]
				CASE 3
					RETURN [[衣装:리본]]
				CASE 4
					RETURN [[衣装:티아라]]
				CASE 5
					RETURN [[衣装:네코미미카츄샤]]
				CASE 6
					RETURN [[衣装:실크햇]]
				CASE 7
					RETURN [[衣装:깃털모자]]
				CASE 8
					RETURN [[衣装:베일]]
				CASE 9
					RETURN [[衣装:왕관]]
			ENDSELECT
		ELSEIF TALENT:ARG:남자 == 0
			SELECTCASE RAND:10
				CASE IS < 1
					RETURN [[衣装:프로스트두건]]
				CASE IS < 8
					RETURN [[衣装:리본]]
				CASE IS < 10
					RETURN [[衣装:헤어밴드]]
			ENDSELECT
		ENDIF
	CASE "옷"
		;마법소녀の場合は옷装もそれっぽくする
		;(변신してない場合は一般的な옷装になるんじゃ…というツッコミは認める。
		;でも변신姿じゃない囚われの마법소녀とか浪漫がないやんけ！という욕망のためにあえてね？あえて…)
		IF TALENT:ARG:데빌시프터 == 1
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:세일러복]]
				CASE 1
					RETURN [[衣装:간호사복]]
				CASE 2
					RETURN [[衣装:기모노]]
				CASE 3
					RETURN [[衣装:유카타]]
				CASE 4
					RETURN [[衣装:고스로리옷（흑）]]
				CASE 5
					RETURN [[衣装:고스로리옷（백）]]
				CASEELSE
					;전신복を설정するケースを想定してここでは설정なし
					RETURN 0
			ENDSELECT
		ELSEIF IS_MALE(ARG)
			RETURN [[衣装:런닝셔츠]]
		ELSE
			IF RAND:10 == 0
				RETURN [[衣装:체육복]]
			ELSE
				RETURN [[衣装:블라우스]]
			ENDIF
		ENDIF
	CASE "하의"
		IF TALENT:ARG:데빌시프터 == 1
			;옷がなければ하의もなし(後で전신복を설정)
			SIF CFLAG:ARG:옷 == 0
				RETURN 0
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:스커트]]
				CASE 1
					RETURN [[衣装:롱스커트]]
				CASE 2
					RETURN [[衣装:플리츠스커트]]
				CASE 3
					RETURN [[衣装:플레어스커트]]
				CASE 4
					RETURN [[衣装:미니스커트]]
			ENDSELECT
		ELSEIF IS_MALE(ARG)
			RETURN [[衣装:바지]]
		ELSEIF CFLAG:ARG:옷 == [[衣装:체육복]]
			RETURN [[衣装:블루머]]
		ELSE
			SELECTCASE RAND:3
				CASE 0
					RETURN [[衣装:점퍼스커트]]
				CASE 1
					RETURN [[衣装:숏팬츠]]
				CASE 2
					RETURN [[衣装:스커트]]
			ENDSELECT
		ENDIF
	CASE "전신복"
		IF CFLAG:ARG:옷 == 0 && CFLAG:ARG:하의 == 0
			SELECTCASE RAND:15
				CASE 0
					RETURN [[衣装:로브]]
				CASE 1
					RETURN [[衣装:원피스]]
				CASE 2
					RETURN [[衣装:드레스]]
				CASE 3
					RETURN [[衣装:에이프런드레스]]
				CASE 4
					RETURN [[衣装:메이드복]]
				CASE 5
					RETURN [[衣装:프렌치메이드복]]
				CASE 6
					RETURN [[衣装:인형탈]]
				CASE 7
					RETURN [[衣装:민족의상]]
				CASE 8
					RETURN [[衣装:네코마타수트(의상)]]
				CASE 9
					RETURN [[衣装:도사복]]
				CASE 10
					RETURN [[衣装:광대의옷]]
				CASE 11
					RETURN [[衣装:고딕드레스(V넥)]]
				CASE 12
					RETURN [[衣装:미니원피스]]
				CASEELSE
					; 전신속옷を설정するケースを想定してここでは설정なし
					RETURN 0
			ENDSELECT
		ELSE
			RETURN 0
		ENDIF
	CASE "손"
		SELECTCASE RAND:5
			CASE 0
				RETURN [[衣装:반지]]
			CASE 1
				RETURN [[衣装:팔찌]]
			CASE 2
				RETURN [[衣装:장갑]]
			CASEELSE
				RETURN 0
		ENDSELECT
	CASE "속옷（상）"
		IF IS_MALE(ARG)
			RETURN 0
		ELSEIF CFLAG:ARG:옷 == 0 && CFLAG:ARG:하의 == 0 && CFLAG:ARG:전신복 == 0
			; 전신속옷を설정するためなし
			RETURN 0
		ELSE
			IF TALENT:ARG:거유 == 1 || TALENT:ARG:폭유 == 1 || TALENT:ARG:마유 == 1
				RETURN [[衣装:브래지어]]
			ELSEIF RAND:2 == 0
				RETURN [[衣装:스포츠브라]]
			ELSE
				RETURN 0
			ENDIF
		ENDIF
	CASE "속옷（하）"
		IF IS_MALE(ARG)
			IF RAND:5
				RETURN [[衣装:트렁크스]]
			ELSE
				RETURN [[衣装:부메랑팬츠]]
			ENDIF
		ELSEIF CFLAG:ARG:옷 == 0 && CFLAG:ARG:하의 == 0 && CFLAG:ARG:전신복 == 0
			; 전신속옷を설정するためなし
			RETURN 0
		ELSE
			SELECTCASE RAND:5
				CASE 0
