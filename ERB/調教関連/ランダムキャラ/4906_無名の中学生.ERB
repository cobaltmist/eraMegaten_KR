;ARGは生成キャラの등록番号
;中学生は서머너か이능자か페르소나구사자と幅広い
;레어キャラも마법소녀、변신ヒーロー、아이돌とこちらも幅広いが、発現率は他より低め
;이능자스킬のコンセプトは小学生を参照
@RANDOMCHARA_4906, ARGS, ARG, ARG:1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "일인칭"
		IF TALENT:ARG:남자 == 1
			RESULTS = 나
		ELSE
			RESULTS = 나
		ENDIF
	CASE "이인칭"
		RESULTS = %CALLNAME:MASTER%
	CASE "체력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2700)
		ELSE
			RETURN RAND(800, 1700)
		ENDIF
	CASE "기력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2500)
		ELSE
			RETURN RAND(800, 1500)
		ENDIF
	CASE "속성"
		RETURN RAND(100, 200), RAND(100, 200)
	CASE "종족"
		RETURN 0
	CASE "전투소질"
		;1/3で비전투원
		IF RAND:3 == 0
			RESULTS = 비전투원
			;1/10で아이돌(영매체질)
			IF RAND:10 == 0
				RESULTS:1 = 영매체질
				SELECTCASE RAND:5
					CASE 0
						;1/5でフレーバーとして천사を부여
						RESULTS:2 = 천사
						RETURN 1, 1, 1
					CASE 1
						;1/5で半フレーバーとして기계의처녀を부여
						RESULTS:2 = 기계의처녀
						RETURN 1, 1, 1
					CASEELSE
						;3/5で何もなし
						RETURN 1, 1
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		;1/15で레어枠
		IF RAND:15 == 0
			IF TALENT:ARG:남자 == 0
				;女の場合、이능자シフター(마법소녀)
				RESULTS = 이능자
				RESULTS:1 = 데빌시프터
			ELSE
				;男の場合、식노(변신ヒーロー)
				RESULTS = 식노
				RESULTS:1 = 건슬링거
			ENDIF
			;강화フラグがある場合、1/2で도구지식3
			IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
				RESULTS:2 = 도구지식
				RETURN 1, 1, 3
			ENDIF
			RETURN 1, 1
		ENDIF
		;それ이외は서머너か이능자か페르소나
		;서머너が多くてもアレなので、少し이능자・페르소나を出やすくする
		SELECTCASE RAND:100
			CASE IS < 20
				;純サマ
				RESULTS = 서머너
				;ガンスリと석세서はどちらかのみ
				IF RAND:2 > 0
					RESULTS:1 = 석세서
				ELSE
					RESULTS:1 = 건슬링거
				ENDIF
				RESULTS:2 = 도구지식
				RETURN 5, 1, 3
			CASE IS < 60
				;페르소나구사자
				RESULTS = 페르소나구사자
				;강화フラグがある場合、1/2で도구지식1を추가
				IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
					RETURN 1, 1
				ENDIF
				RETURN 1
			CASEELSE
				RESULTS = 이능자
				;純이능자(도구지식持ち)か이능자서머너
				IF RAND:2 > 0
					RESULTS:1 = 서머너
						;強化フラグがある場合、1/2で強化。サクセサーも付ける。
						IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
							IF EXTRA강화(3)
								RESULTS:2 = 석세서
								RETURN 1, 2, 1
							ELSE
								RESULTS:2 = 달인
								RESULTS:3 = 석세서
								RETURN  0, 1, 1, 1
							ENDIF
						ENDIF
				ELSE
					RESULTS:1 = 도구지식
					;강화フラグがある場合、1/2で강화。도구지식を3に
					IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
						RETURN 1, 3
					ENDIF
				ENDIF
				RETURN 1, 2
		ENDSELECT
	;戻り値は0:랜덤 それ이외は指定
	CASE "초기페르소나"
		RETURN 0
	;戻り値は0:랜덤 それ이외は指定
	CASE "초기스테이터스비율"
		RETURN 7, 7, 8, 7, 8, 10
	CASE "스킬"
		IF TALENT:ARG:달인 == 1 && TALENT:ARG:이능자 == 0 && TALENT:ARG:석세서 == 1;サクセサー達人サマナー
				RESULT = [[스킬:회전베기]]
				RESULT:1 = [[스킬:기합]]
				RESULT:2 = [[스킬:양팔찍기]]
				RESULT:3 = [[스킬:이악물기]]
				RESULT:4 = [[스킬:라쿤・다]]
				RESULT:5 = [[스킬:간파]]
				RESULT:6 = [[스킬:타루・카쟈]]
				RESULT:7 = [[스킬:기공(대)]]
				RESULT:8 = [[스킬:불굴의투지]]
				RESULT:9 = [[스킬:삼할의활천]]
				RESULT:10 = [[스킬:관통]]
				RESULT:11 = [[스킬:오의일섬]]
				RESULT:12 = [[스킬:삼할의마맥]]
				RESULT:13 = [[스킬:아공양단]]
				RETURN RESULT
		ELSEIF 이벤트플래그:88:0 > 3 && TALENT:ARG:서머너 > 1 && TALENT:ARG:이능자 == 0
			;純サマ강화스킬
			;とりあえず自動스킬を山盛り
			RESULT = [[스킬:아이템숙련]]
			RESULT:1 = [[스킬:어드바이스]]
			RESULT:2 = [[스킬:이악물기]]
			RESULT:3 = [[스킬:추격의심득]]
			RESULT:4 = [[스킬:황천의회심]]
			RESULT:5 = [[스킬:정천의회심]]
			RESULT:6 = [[스킬:서드아이]]
			RESULT:7 = [[스킬:관통]]
			RETURN RESULT
		ELSEIF TALENT:ARG:영매체질 == 1
			;아이돌用스킬
			;(使うには함락させた上に전투소질を付けないといけないので、ぶっちゃけ使う人は物好きと思われる)
			;それっぽい스킬2つ
			SELECTCASE RAND:4
				CASE 0
					RESULT = [[스킬:해피스텝]]
				CASE 1
					RESULT = [[스킬:고양의노래]]
				CASE 2
					RESULT = [[스킬:던지는키스]]
				CASE 3
					RESULT = [[스킬:자장가]]
			ENDSELECT
			SELECTCASE RAND:4
				CASE 0
					RESULT:1 = [[스킬:용분의춤]]
				CASE 1
					RESULT:1 = [[스킬:천상의춤]]
				CASE 2
					RESULT:1 = [[스킬:마음의외침]]
				CASE 3
					RESULT:1 = [[스킬:마계의조사]]
			ENDSELECT
			;持ち歌(가창스킬)を2曲ずつ
			SELECTCASE RAND:4
				CASE 0
					RESULT:2 = [[스킬:Go-Round]]
				CASE 1
					RESULT:2 = [[스킬:Breakin' Through]]
				CASE 2
					RESULT:2 = [[스킬:Light the Fire up in the Night]]
				CASE 3
					RESULT:2 = [[스킬:Time to Make History]]
			ENDSELECT
			SELECTCASE RAND:5
				CASE 0
					RESULT:3 = [[스킬:Burn my Dread]]
				CASE 1
					RESULT:3 = [[스킬:『너의눈은일만볼트』]]
				CASE 2
					RESULT:3 = [[스킬:『얼려버리는라인』]]
				CASE 3
					RESULT:3 = [[스킬:『봄버걸』]]
				CASE 4
					RESULT:4 = [[스킬:『염의러너』]]
			ENDSELECT
			IF GETBIT(FLAG:EXTRA이벤트설정, 0) == 0
				;最近の아이돌は마법소녀も兼ねていたりするため、마법소녀스킬もオマケで1つ
				;패턴を増やし、幻想郷스킬も추가
				SELECTCASE RAND:6
					CASE 0
						RESULT:4 = [[스킬:침식하는검은날개]]
					CASE 1
						RESULT:4 = [[스킬:티로・피날레]]
					CASE 2
						RESULT:4 = [[스킬:나의세계]]
					CASE 3
						RESULT:4 = [[스킬:피니투라・프레차]]
					CASE 4
						RESULT:4 = [[스킬:스쿠왈타토레]]
					CASE 5
						RESULT:4 = [[스킬:롯소・판타즈마]]
				ENDSELECT
			ENDIF
			RETURN RESULT
		ENDIF 
		IF TALENT:ARG:이능자 == 0
			RETURN 0
		ELSEIF TALENT:ARG:데빌시프터 == 1
			RETURN 0
		ENDIF
		
		;이능자스킬
		;공격
		IF TALENT:ARG:이능자 == 1
		SELECTCASE RAND:7
			CASE 0
				RESULT = [[스킬:아기]]
				RESULT:1 = [[스킬:아기라오]]
				RESULT:2 = [[스킬:아기다인]]
				RESULT:3 = [[스킬:데들리번]]
			CASE 1
				RESULT = [[스킬:마그나]]
				RESULT:1 = [[스킬:마그나스]]
				RESULT:2 = [[스킬:마그다인]]
				RESULT:3 = [[스킬:대지의분노]]
			CASE 2
				RESULT = [[스킬:아쿠아]]
				RESULT:1 = [[스킬:아쿠에스]]
				RESULT:2 = [[스킬:아쿠아다인]]
				RESULT:3 = [[스킬:아쿠아리타이드]]
			CASE 3
				RESULT = [[스킬:갈]]
				RESULT:1 = [[스킬:갈라]]
				RESULT:2 = [[스킬:갈다인]]
				RESULT:3 = [[스킬:선풍진]]
			CASE 4
				RESULT = [[스킬:코우하]]
				RESULT:1 = [[스킬:바이코우하]]
				RESULT:2 = [[스킬:마하코우하]]
				RESULT:3 = [[스킬:사마리캄]]
			CASE 5
				RESULT = [[스킬:베어내기]]
				RESULT:1 = [[스킬:히트웨이브]]
				RESULT:2 = [[스킬:무사차]]
				RESULT:3 = [[스킬:산산조각베기]]
			CASE 6
				RESULT = [[스킬:베어내기]]
				RESULT:1 = [[스킬:섬머솔트]]
				RESULT:2 = [[스킬:열진각]]
				RESULT:3 = [[스킬:맹돌진]]
		ENDSELECT
		;カジャ系
		SELECTCASE RAND:4
			CASE 0
				RESULT:4 = [[스킬:타루・카쟈]]
				RESULT:5 = [[스킬:다크브레스]]
			CASE 1
				RESULT:4 = [[스킬:스쿠・카쟈]]
				RESULT:5 = [[스킬:도발]]
			CASE 2
				RESULT:4 = [[스킬:라쿠・카쟈]]
				RESULT:5 = [[스킬:파워브레스]]
			CASE 3
				RESULT:4 = [[스킬:사마・카쟈]]
				RESULT:5 = [[스킬:위협]]
		ENDSELECT
		;魔脈/コンセ
		IF RAND:2 > 0
			RESULT:6 = [[스킬:일할의마맥]]
		ELSE
			RESULT:6 = [[스킬:컨센트레이트]]
		ENDIF
		;만능
		RESULT:7 = [[스킬:메기도]]
		RESULT:8 = [[스킬:메기도라온]]
		
		;강화
		IF 이벤트플래그:88:0 > 3
			RESULT:9 = [[스킬:데・카쟈]]
			RESULT:10 = [[스킬:리캄도라]]
			IF TALENT:ARG:서머너 == 0
				;純이능자なら気功
				RESULT:11 = [[스킬:기공(소)]]
			ELSE
				;異能サマならコンセと魔脈を両方
				IF RESULT:6 == [[스킬:일할의마맥]]
					RESULT:11 = [[스킬:컨센트레이트]]
				ELSE
					RESULT:11 = [[스킬:일할의마맥]]
				ENDIF
			ENDIF
		ENDIF
		ENDIF
		RETURN RESULT
	CASE "상성"
		SELECTCASE GET_TYPE(ARG:1)
			CASE "파마"
				RETURN 0
			CASEELSE
				RETURN 100
		ENDSELECT
	CASE "소질"
		RESULT = GETNUM(TALENT, "겁쟁이")
		RESULT:1 = GETNUM(TALENT, "아픔에약함")
		RESULT:2 = GETNUM(TALENT, "건방짐")
		RESULT:3 = GETNUM(TALENT, "호기심")
		RESULT:4 = GETNUM(TALENT, "솔직함")
		IF TALENT:ARG:영매체질 == 1
			RESULT:5 = GETNUM(TALENT, "인기")
		ENDIF
		IF TALENT:ARG:천사 == 1
			;천사の場合、날개を부여
			RESULT:6 = GETNUM(TALENT, "날개")
		ELSEIF TALENT:ARG:기계의처녀 == 1 && RAND:3 == 0
			;기계의처녀の場合、1/3で동물귀を부여(헤드パーツとして付いている扱い)
			RESULT:6 = GETNUM(TALENT, "동물귀")
		ENDIF
		RETURN RESULT
	CASE "제외소질"
		RETURN GETNUM(TALENT, "바늘다루기"), GETNUM(TALENT, "조합지식")
	;처녀は特別 0は랜덤(20%), 1なら처녀, 2なら非처녀
	CASE "처녀"
		IF  RAND:5 == 0
			RETURN 2
		ELSE
			RETURN 1
		ENDIF
	;戻り値は0:랜덤 1:소인체형 2:유아체형 4:큰체형 5:거체
	CASE "체형"
		;기계의처녀の場合、소인체형の分岐を추가
		IF TALENT:ARG:기계의처녀 == 1 && RAND:4 == 0
			RETURN 1
		ENDIF
		SELECTCASE RAND:10
			CASE IS < 5
				RETURN 2
			CASE IS < 8
				RETURN 3
			CASE IS < 9
				RETURN 4
		ENDSELECT
	;戻り値は0:랜덤 1:절벽　2:빈유　3:並　4:거유　5:폭유
	CASE "가슴"	
		IF TALENT:ARG:남자 == 0
			SELECTCASE RAND:10
				CASE IS < 6
					RETURN 2
				CASE IS < 8
					RETURN 3
				CASE IS < 9
					RETURN 1
				CASE IS < 10
					IF RAND:10 == 0
						RETURN 5
					ELSE
						RETURN 4
					ENDIF
			ENDSELECT
		ELSEIF TALENT:ARG:오토코노코 == 1
			SELECTCASE RAND:4
				CASE IS < 2
					RETURN 2
				CASE IS < 4
					RETURN 1
			ENDSELECT
		ELSE
			RETURN 1
		ENDIF
	;추가対応。戻り値は0:랜덤 1:小尻　2:並尻　3:巨尻　4:爆尻
	CASE "尻"
		RETURN 0
	;통상の인간と肉体部位が違う場合はここで直接代入してください
	CASE "육체부위"
		RETURN 0
	CASE "ABL"
		SELECTCASE ABLNAME:(ARG:1)
			CASE "순종", "욕망", "기교"
				RETURN !RAND:3+!RAND:5
			CASE "Ｃ감각", "Ｖ감각", "Ｂ감각"
				RETURN !RAND:5
			CASE "마조끼", "노출증", "봉사정신"
				RETURN !RAND:10
			CASE "새드끼"
				RETURN !RAND:5 * !ABL:ARG:마조끼
			CASE "Ａ감각", "자위중독", "정액중독", "레즈끼", "ＢＬ끼", "섹스중독"
				RETURN !RAND:10
			CASE "가창기능"
				IF TALENT:ARG:영매체질 == 1
					;아이돌の場合、가창기능を1이상保証
					RETURN RAND:5 + 1
				ELSE
					RETURN 0
				ENDIF
		ENDSELECT
	;EXPはABLが1이상ある場合、ある程度調整される(ただし、ABL0の時は調整が動かないので適当に초기値をきめてください)
	CASE "EXP"
		;全く손付かずの場合
		SIF !GROUPMATCH(1, !TALENT:ARG:처녀, ABL:ARG:Ｃ감각, ABL:ARG:Ｖ감각, ABL:ARG:Ａ감각, ABL:ARG:Ｂ감각, ABL:ARG:기교)
			RETURN 0
		SELECTCASE EXPNAME:(ARG:1)
			CASE "Ｖ경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "Ａ경험"
				RETURN RAND(0, 20) * !RAND:5
			CASE "절정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "사정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "성교경험"
				RETURN 0
			CASE "질사경험"
				RETURN EXP:ARG:사정경험/2*EXP:ARG:성교경험 > 0
			CASE "자위경험"
				RETURN RAND(0, 20)
			CASE "조교자위경험"
				RETURN 0
			CASE "정액경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "봉사쾌락경험"
				RETURN 0
			CASE "펠라경험"
				RETURN RAND(0, 10) * RAND:2
			CASE "애정경험"
				RETURN 0
			CASE "고통쾌락경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "방뇨경험"
				RETURN 0
			CASE "가학쾌락경험"
				RETURN RAND(0, 10) * !RAND:4
			CASE "야외노출경험"
				RETURN 0
			CASE "레즈경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "ＢＬ경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "이상경험"
				RETURN 0
			CASE "긴박경험"
				RETURN RAND(0, 10) * EXP:ARG:고통쾌락경험 > 0
			CASE "가창경험"
				IF TALENT:ARG:영매체질 == 1
					;아이돌の場合はそれなりに가창경험を付ける
					RETURN (RAND(10, 20) * ABL:ARG:가창기능) + RAND:5
				ELSE
					RETURN 0
				ENDIF
		ENDSELECT
	CASE "모자"
		;동물귀がある場合はなし
		SIF TALENT:ARG:동물귀 == 0 
			RETURN 0
		;마법소녀の場合は옷装もそれっぽくする
		IF TALENT:ARG:데빌시프터 == 1
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:화이트브림]]
				CASE 1
					RETURN [[衣装:너스캡]]
				CASE 2
					RETURN [[衣装:토끼귀밴드]]
				CASE 3
					RETURN [[衣装:리본]]
				CASE 4
					RETURN [[衣装:티아라]]
				CASE 5
					RETURN [[衣装:네코미미카츄샤]]
				CASE 6
					RETURN [[衣装:실크햇]]
				CASE 7
					RETURN [[衣装:깃털모자]]
				CASE 8
					RETURN [[衣装:베일]]
				CASE 9
					RETURN [[衣装:왕관]]
			ENDSELECT
		ELSEIF IS_MALE(ARG)
			SELECTCASE RAND:3
				CASE 0
					RETURN [[衣装:모자]]
				CASE 1
