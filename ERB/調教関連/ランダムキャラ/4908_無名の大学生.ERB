;ARGは生成キャラの등록번호
;大学生は아이템係、ガンスリシフター、식노석세서、機械乙女페르소나など変則キャラオンリー
;레어キャラはなし。というか全部레어みたいなもん(強いかどうかは別として)
;이능자스킬のコンセプトは小学生を参照
@RANDOMCHARA_4908, ARGS, ARG, ARG:1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "일인칭"
		IF TALENT:ARG:남자 == 1
			SELECTCASE RAND:5
				CASE 0
					RESULTS = 나
				CASE 1
					RESULTS = 나
				CASE 2
					RESULTS = 나
				CASE 3
					RESULTS = 나
				CASE 4
					RESULTS = 나
			ENDSELECT
		ELSE
			SELECTCASE RAND:9
				CASE 0
					RESULTS = 나
				CASE 1
					RESULTS = 저
				CASE 2
					RESULTS = 나
				CASE 3
					RESULTS = 저
				CASE 4
					RESULTS = 저
				CASE 5
					RESULTS = 저
				CASE 6
					RESULTS = 본녀
				CASE 7
					RESULTS = 본녀
				CASE 8
					RESULTS = 나
			ENDSELECT
		ENDIF
	CASE "이인칭"
		RESULTS = %CALLNAME:MASTER%
	CASE "체력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2800)
		ELSE
			RETURN RAND(800, 1800)
		ENDIF
	CASE "기력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2500)
		ELSE
			RETURN RAND(800, 1500)
		ENDIF
	CASE "속성"
		RETURN RAND(100, 200), RAND(100, 200)
	CASE "종족"
		RETURN 0
	CASE "전투소질"
		;1/3で비전투원
		IF RAND:3 == 0
			RESULTS = 비전투원
			SELECTCASE RAND:10
				CASE 0
					;1/10で비전투원だがガンスリ
					RESULTS:2 = 건슬링거
					RETURN 1, 1
				CASE 1
					;1/10で비전투원だが석세서
					RESULTS:2 = 석세서
					RETURN 1, 1
				CASE 2
					;1/10で비전투원だが도구지식2
					RESULTS:2 = 도구지식
					RETURN 1, 2
				CASEELSE
					;7/10で何もなし
					RETURN 1
			ENDSELECT
		ENDIF
		
		SELECTCASE RAND:100
			CASE IS < 20
				;서머너
				RESULTS = 서머너
				RESULTS:1 = 도구지식
				RESULTS:2 = 아이템숙련
					;강화フラグがある場合、1/2で達인간화。
					IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
						RESULTS:3 = 달인
						RETURN  5, 3, 1, 1
					ENDIF
				RETURN 5, 3, 1
			CASE IS < 40
				;데빌시프터
				RESULTS = 데빌시프터
				RESULTS:1 = 건슬링거
				RETURN 1, 1
			CASE IS < 60
				;식노
				RESULTS = 식노
				RESULTS:1 = 석세서
				RETURN 1, 1
			CASE IS < 80
				;페르소나구사자
				RESULTS = 페르소나구사자
				RESULTS:1 = 기계의처녀
				;1/2でP1・2仕様かP3以降仕様に別れる
				SIF RAND:2 == 0
					RETURN 2, 1
				RETURN 1, 1
			CASEELSE
				;이능자
				IF EXTRA강화(3)
					RESULTS = 이능자
					RESULTS:1 = 도구지식
					RESULTS:2 = 아이템숙련
					;강화フラグがある場合、1/2で도구지식Lv2
					IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
						RETURN  1, 2, 1
					ENDIF
					RETURN 1, 1, 1
				ELSE
					RESULTS = 달인
					RESULTS:1 = 도구지식
					RESULTS:2 = 아이템숙련
					RESULTS:3 = 건슬링거
					RETURN  1, 2, 1, 1
				ENDIF
		ENDSELECT
	;戻り値は0:랜덤 それ이외は指定
	CASE "초기페르소나"
		RETURN 0
	;戻り値は0:랜덤 それ이외は指定
	CASE "링크악마"
		RETURN 0
	;戻り値は0:랜덤 それ이외は指定
	CASE "초기스테이터스비율"
		RETURN 7, 7, 8, 7, 8, 10
	CASE "스킬"
		IF 이벤트플래그:88:0 > 3 && TALENT:ARG:서머너 >= 1 && TALENT:ARG:달인 == 0;達人なしの通常強化の場合はこっち
			RESULT = [[스킬:아이템숙련]]
			RESULT:1 = [[스킬:어드바이스]]
			RETURN RESULT
		ELSEIF TALENT:ARG:서머너 >= 1 && TALENT:ARG:달인 == 1 && TALENT:ARG:이능자 == 0;달인ありの추가강화の場合はこっち
			RESULT = [[스킬:아이템숙련]]
			RESULT:1 = [[스킬:서드아이]]
		SELECTCASE RAND:3
			CASE 0
				RESULT:2 = [[스킬:화염부스터]]
			CASE 1
				RESULT:2 = [[스킬:빙결부스터]]
			CASE 2
				RESULT:2 = [[스킬:전격부스터]]
			ENDSELECT
			RESULT:3 = [[스킬:삼할의활천]]
			RETURN RESULT
		ELSEIF TALENT:ARG:달인 == 1 && TALENT:ARG:이능자 == 0 && TALENT:ARG:건슬링거 == 1;달인ありの추가강화の場合はこっち
			RESULT = [[스킬:트라프리]]
			RESULT:1 = [[스킬:싱글샷]]
			RESULT:2 = [[스킬:백열찌르기]]
			RESULT:3 = [[스킬:아이템숙련]]
			RESULT:4 = [[스킬:불굴의투지]]
			RESULT:5 = [[스킬:저격]]
			RESULT:6 = [[스킬:결계부・치유]]
			RESULT:7 = [[스킬:다크브레스]]
			RESULT:8 = [[스킬:결계부・물리]]
			RESULT:9 = [[스킬:파워브레스]]
			RESULT:10 = [[스킬:기공(중)]]
			RESULT:11 = [[스킬:결계부・마법]]
			RESULT:12 = [[스킬:천열찌르기]]
			RESULT:13 = [[스킬:정의의방패]]
			RETURN RESULT
		ELSEIF TALENT:ARG:이능자 == 1
		;이능자스킬
		;공격
		SELECTCASE RAND:7
			CASE 0
				RESULT = [[스킬:아기]]
				RESULT:1 = [[스킬:아기라오]]
				RESULT:2 = [[스킬:아기다인]]
				RESULT:3 = [[스킬:데들리번]]
			CASE 1
				RESULT = [[스킬:마그나]]
				RESULT:1 = [[스킬:마그나스]]
				RESULT:2 = [[스킬:마그다인]]
				RESULT:3 = [[스킬:대지의분노]]
			CASE 2
				RESULT = [[스킬:아쿠아]]
				RESULT:1 = [[스킬:아쿠에스]]
				RESULT:2 = [[스킬:아쿠아다인]]
				RESULT:3 = [[스킬:아쿠아리타이드]]
			CASE 3
				RESULT = [[스킬:갈]]
				RESULT:1 = [[스킬:갈라]]
				RESULT:2 = [[스킬:갈다인]]
				RESULT:3 = [[스킬:선풍진]]
			CASE 4
				RESULT = [[스킬:코우하]]
				RESULT:1 = [[스킬:바이코우하]]
				RESULT:2 = [[스킬:마하코우하]]
				RESULT:3 = [[스킬:사마리캄]]
			CASE 5
				RESULT = [[스킬:베어내기]]
				RESULT:1 = [[스킬:히트웨이브]]
				RESULT:2 = [[스킬:무사차]]
				RESULT:3 = [[스킬:산산조각베기]]
			CASE 6
				RESULT = [[스킬:베어내기]]
				RESULT:1 = [[스킬:섬머솔트]]
				RESULT:2 = [[스킬:열진각]]
				RESULT:3 = [[스킬:맹돌진]]
		ENDSELECT
		;カジャ系
		SELECTCASE RAND:4
			CASE 0
				RESULT:4 = [[스킬:타루・카쟈]]
				RESULT:5 = [[스킬:다크브레스]]
			CASE 1
				RESULT:4 = [[스킬:스쿠・카쟈]]
				RESULT:5 = [[스킬:도발]]
			CASE 2
				RESULT:4 = [[스킬:라쿠・카쟈]]
				RESULT:5 = [[스킬:파워브레스]]
			CASE 3
				RESULT:4 = [[스킬:사마・카쟈]]
				RESULT:5 = [[스킬:위협]]
		ENDSELECT
		;魔脈/コンセ
		IF RAND:2 > 0
			RESULT:6 = [[스킬:일할의마맥]]
		ELSE
			RESULT:6 = [[스킬:컨센트레이트]]
		ENDIF
		;만능
		RESULT:7 = [[스킬:메기도]]
		RESULT:8 = [[스킬:메기도라온]]
		
		;강화
		IF 이벤트플래그:88:0 > 3
		RESULT:9 = [[스킬:일할의활천]]
		RESULT:10 = [[스킬:데・카쟈]]
		RESULT:11 = [[스킬:리캄도라]]
		RESULT:12 = [[스킬:아이템숙련]]
		ENDIF
		RETURN RESULT
		ENDIF	
	CASE "상성"
		SELECTCASE GET_TYPE(ARG:1)
			CASE "파마"
				RETURN 0
			CASEELSE
				RETURN 100
		ENDSELECT
	CASE "소질"
		RETURN GETNUM(TALENT, "아픔에약함")
	CASE "제외소질"
		RETURN GETNUM(TALENT, "바늘다루기"), GETNUM(TALENT, "조합지식")
	;처녀は特別 0は랜덤(20%), 1なら처녀, 2なら非처녀
	CASE "처녀"
		IF  RAND:5 == 0
			RETURN 2
		ELSE
			RETURN 1
		ENDIF
	;戻り値は0:랜덤 1:소인체형 2:유아체형 4:큰체형 5:거체
	CASE "체형"
		;기계의처녀の場合、소인체형の分岐を추가
		IF TALENT:ARG:기계의처녀 == 1 && RAND:4 == 0
			RETURN 1
		ENDIF
		SELECTCASE RAND:10
			CASE IS < 3
				RETURN 2
			CASE IS < 8
				RETURN 3
			CASE IS < 9
				RETURN 4
		ENDSELECT
	;戻り値は0:랜덤 1:절벽　2:빈유　3:並　4:거유　5:폭유
	CASE "가슴"	
		IF TALENT:ARG:남자 == 0
			SELECTCASE RAND:10
				CASE IS < 6
					RETURN 2
				CASE IS < 8
					RETURN 3
				CASE IS < 9
					RETURN 1
				CASE IS < 10
					IF RAND:10 == 0
						RETURN 5
					ELSE
						RETURN 4
					ENDIF
			ENDSELECT
		ELSEIF TALENT:ARG:오토코노코 == 1
			SELECTCASE RAND:4
				CASE IS < 2
					RETURN 2
				CASE IS < 4
					RETURN 1
			ENDSELECT
		ELSE
			RETURN 1
		ENDIF
	;추가対応。戻り値は0:랜덤 1:小尻　2:並尻　3:巨尻　4:爆尻
	CASE "尻"
		RETURN 0
	;통상の인간と肉体部位が違う場合はここで直接代入してください
	CASE "육체부위"
		RETURN 0
	CASE "ABL"
		SELECTCASE ABLNAME:(ARG:1)
			CASE "순종", "욕망", "기교"
				RETURN !RAND:3+!RAND:5
			CASE "Ｃ감각", "Ｖ감각", "Ｂ감각"
				RETURN !RAND:5
			CASE "마조끼", "노출증", "봉사정신"
				RETURN !RAND:10
			CASE "새드끼"
				RETURN !RAND:5 * !ABL:ARG:마조끼
			CASE "Ａ감각", "자위중독", "정액중독", "레즈끼", "ＢＬ끼", "섹스중독"
				RETURN !RAND:10
		ENDSELECT
	;EXPはABLが1이상ある場合、ある程度調整される(ただし、ABL0の時は調整が動かないので適当に초기値をきめてください)
	CASE "EXP"
		;全く손付かずの場合
		SIF !GROUPMATCH(1, !TALENT:ARG:처녀, ABL:ARG:Ｃ감각, ABL:ARG:Ｖ감각, ABL:ARG:Ａ감각, ABL:ARG:Ｂ감각, ABL:ARG:기교)
			RETURN 0
		SELECTCASE EXPNAME:(ARG:1)
			CASE "Ｖ경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "Ａ경험"
				RETURN RAND(0, 20) * !RAND:5
			CASE "절정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "사정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "성교경험"
				RETURN 0
			CASE "질사경험"
				RETURN EXP:ARG:사정경험/2*EXP:ARG:성교경험 > 0
			CASE "자위경험"
				RETURN RAND(0, 20)
			CASE "조교자위경험"
				RETURN 0
			CASE "정액경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "봉사쾌락경험"
				RETURN 0
			CASE "펠라경험"
				RETURN RAND(0, 10) * RAND:2
			CASE "애정경험"
				RETURN 0
			CASE "고통쾌락경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "방뇨경험"
				RETURN 0
			CASE "가학쾌락경험"
				RETURN RAND(0, 10) * !RAND:4
			CASE "야외노출경험"
				RETURN 0
			CASE "레즈경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "ＢＬ경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "이상경험"
				RETURN 0
			CASE "긴박경험"
				RETURN RAND(0, 10) * EXP:ARG:고통쾌락경험 > 0
		ENDSELECT
	CASE "모자"
		IF IS_MALE(ARG)
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:모자]]
				CASE 1
					RETURN [[衣装:반다나]]
				CASE 2
					RETURN [[衣装:헬멧]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ELSE
			SELECTCASE RAND:6
				CASE 0
					RETURN [[衣装:모자]]
				CASE 1
					RETURN [[衣装:리본]]
				CASE 2
					RETURN [[衣装:헤어밴드]]
				CASE 3
					RETURN [[衣装:카츄샤]]
				CASE 4
					RETURN [[衣装:프로스트두건]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ENDIF
	CASE "옷"
		IF CFLAG:ARG:모자 == [[衣装:헬멧]]
			;전신복を설정するため、ここでは설정なし
			RETURN 0
		ELSEIF IS_MALE(ARG)
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:겉옷]]
				CASE 1
					RETURN [[衣装:와이셔츠]]
				CASE 2
					RETURN [[衣装:T셔츠]]
				CASE 3
					RETURN [[衣装:탱크탑]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ELSE
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:겉옷]]
				CASE 1
					RETURN [[衣装:블라우스]]
				CASE 2
					RETURN [[衣装:T셔츠]]
				CASE 3
					RETURN [[衣装:튜브탑]]
				CASE 4
					RETURN [[衣装:터틀넥]]
				CASE 5
					RETURN [[衣装:캐미솔]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ENDIF
	CASE "하의"
		IF CFLAG:ARG:옷 == 0
			;전신복を설정するため、ここでは설정なし
			RETURN 0
		ELSEIF IS_MALE(ARG)
			RETURN [[衣装:바지]]
		ELSE
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:스커트]]
				CASE 1
					RETURN [[衣装:롱스커트]]
				CASE 2
					RETURN [[衣装:미니스커트]]
				CASE 3
					RETURN [[衣装:타이트스커트]]
				CASE 4
					RETURN [[衣装:숏팬츠]]
				CASE 5
					RETURN [[衣装:플리츠스커트]]
				CASE 6
					RETURN [[衣装:플레어스커트]]
				CASE 7
					RETURN [[衣装:타이트미니스커트]]
				CASEELSE
					RETURN [[衣装:바지]]
			ENDSELECT
		ENDIF
	CASE "전신복"
		IF CFLAG:ARG:모자 == [[衣装:헬멧]]
			SELECTCASE RAND:2
				CASE 0
					RETURN [[衣装:라이더수트]]
				CASE 1
					RETURN [[衣装:트라이브수트]]
			ENDSELECT
		ELSEIF CFLAG:ARG:옷 == 0
			IF IS_MALE(ARG)
				RETURN [[衣装:사무에]]
			ELSE
				SELECTCASE RAND:5
					CASE 0
						RETURN [[衣装:차이나드레스]]
					CASE 1
						RETURN [[衣装:원피스]]
					CASE 2
						RETURN [[衣装:미니원피스]]
					CASE 3
						RETURN [[衣装:고딕원피스]]
					CASE 4
						RETURN [[衣装:메이드복]]
				ENDSELECT
			ENDIF
		ELSE
			RETURN 0
		ENDIF
	CASE "손"
		RETURN 0
	CASE "속옷（상）"
		IF IS_MALE(ARG)
			RETURN 0
		ELSE
			SELECTCASE RAND:5
				CASE 0
					RETURN 0
				CASE 1
					RETURN [[衣装:브래지어(흑)]]
				CASE 2
					RETURN [[衣装:브래지어(적)]]
				CASE 3
					RETURN [[衣装:레이스달린브래지어]]
				CASEELSE
					IF TALENT:ARG:거유 == 1 || TALENT:ARG:폭유 == 1 || TALENT:ARG:마유 == 1
						RETURN [[衣装:브래지어]]
					ELSE
						RETURN [[衣装:스포츠브라]]
					ENDIF
			ENDSELECT
		ENDIF
	CASE "속옷（하）"
		IF IS_MALE(ARG)
			IF RAND:5
				RETURN [[衣装:트렁크스]]
			ELSE
				RETURN [[衣装:부메랑팬츠]]
			ENDIF
		ELSE
			SELECTCASE RAND:7
				CASE 0
					RETURN [[衣装:로우라이즈]]
				CASE 1
					RETURN [[衣装:팬티(흑)]]
				CASE 2
					RETURN [[衣装:팬티(적)]]
				CASE 3
					RETURN [[衣装:레이스달린팬티]]
				CASE 4
					RETURN [[衣装:줄무늬팬티]]
				CASE 5
					RETURN [[衣装:스패츠]]
				CASEELSE
					RETURN [[衣装:팬티]]
			ENDSELECT
		ENDIF
	CASE "전신속옷"
		RETURN 0
	CASE "양말"
		IF IS_MALE(ARG)
			RETURN [[衣装:양말]]
		ELSE
			SELECTCASE RAND:7
				CASE 0
					RETURN [[衣装:하이삭스]]
				CASE 1
