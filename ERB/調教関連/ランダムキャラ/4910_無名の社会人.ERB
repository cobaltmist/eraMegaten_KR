;ARGは生成キャラの등록番号
;社会人はフリーターと同様にほぼ비전투원
;레어キャラで달인
;스킬はぶっちゃけ強そーなのをテキトーに詰め込んだだけでバランスとれてるかﾜｶﾝﾈ。誰か加筆調整ﾀﾉﾑ…
;ブー스타問題회피のため、カジャオートや자동효과스킬を代わりに詰め込む
@RANDOMCHARA_4910, ARGS, ARG, ARG:1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "一人称"
		IF TALENT:ARG:남자 == 1
			SELECTCASE RAND:5
				CASE 0
					RESULTS = 나
				CASE 1
					RESULTS = 나
				CASE 2
					RESULTS = 나
				CASE 3
					RESULTS = 나
				CASE 4
					RESULTS = 나
			ENDSELECT
		ELSE
			SELECTCASE RAND:9
				CASE 0
					RESULTS = 나
				CASE 1
					RESULTS = 저
				CASE 2
					RESULTS = 나
				CASE 3
					RESULTS = 저
				CASE 4
					RESULTS = 저
				CASE 5
					RESULTS = 저
				CASE 6
					RESULTS = 본녀
				CASE 7
					RESULTS = 본녀
				CASE 8
					RESULTS = 나
			ENDSELECT
		ENDIF
	CASE "二人称"
		RESULTS = %CALLNAME:MASTER%
	CASE "체력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2800)
		ELSE
			RETURN RAND(800, 1800)
		ENDIF
	CASE "기력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2800)
		ELSE
			RETURN RAND(800, 1800)
		ENDIF
	CASE "속성"
		RETURN RAND(100, 200), RAND(100, 200)
	CASE "종족"
		RETURN 0
	CASE "전투소질"
		SELECTCASE RAND:10
			CASE IS < 1
				;1/10で달인
				RESULTS = 달인
				RETURN 1
			CASEELSE
				RESULTS = 비전투원
				RETURN 1
		ENDSELECT
	;戻り値は0:랜덤 それ이외は指定
	CASE "초기페르소나"
		RETURN 0
	;戻り値は0:랜덤 それ이외は指定
	CASE "링크악마"
		RETURN 0
	;戻り値は0:랜덤 それ이외は指定
	CASE "초기스테이터스比率"
		RETURN 7, 7, 8, 7, 8, 10
	CASE "스킬"
		SIF TALENT:ARG:달인 == 0
			RETURN 0
		;달인は前半세트と後半세트でそれぞれ설정
		;前半세트は물리系
		SELECTCASE RAND:3
			CASE 0
				;검の달인
				RESULT = [[스킬:기합]]
				RESULT:1 = [[스킬:무도의마음가짐]]
				RESULT:2 = [[스킬:추격의심득]]
				RESULT:3 = [[스킬:서드아이]]
				RESULT:4 = [[스킬:맹반격]]
				RESULT:5 = [[스킬:검격가드킬]]
				RESULT:6 = [[스킬:안개베기]]
				RESULT:7 = [[스킬:갑옷관통]]
				RESULT:8 = [[스킬:오의일섬]]
				RESULT:9 = [[스킬:블레이드댄스]]
			CASE 1
				;拳の달인
				RESULT = [[스킬:기합]]
				RESULT:1 = [[스킬:무도의마음가짐]]
				RESULT:2 = [[스킬:추격의심득]]
				RESULT:3 = [[스킬:서드아이]]
				RESULT:4 = [[스킬:불굴의투지]]
				RESULT:5 = [[스킬:타격가드킬]]
				RESULT:6 = [[스킬:지옥찌르기]]
				RESULT:7 = [[스킬:약자필멸권]]
				RESULT:8 = [[스킬:아카샤아츠]]
				RESULT:9 = [[스킬:예광구리가라축]]
			CASE 2
				;총の달인
				RESULT = [[스킬:기합]]
				RESULT:1 = [[스킬:하이퍼카운터]]
				RESULT:2 = [[스킬:백발백중]]
				RESULT:3 = [[스킬:서드아이]]
				RESULT:4 = [[스킬:어드바이스]]
				RESULT:5 = [[스킬:비구가드킬]]
				RESULT:6 = [[스킬:해피트리거]]
				RESULT:7 = [[스킬:원샷킬]]
				RESULT:8 = [[스킬:퀵드로]]
				RESULT:9 = [[스킬:트리플다운]]
		ENDSELECT
		;後半세트は마법
		SELECTCASE RAND:9
			CASE 0
				;炎の달인
				RESULT:10 = [[스킬:트리스아기온]]
				RESULT:11 = [[스킬:아스테로이드봄]]
				RESULT:12 = [[스킬:마카카쟈오토]]
				RESULT:13 = [[스킬:사마카쟈오토]]
				RESULT:14 = [[스킬:화염가드브레이크]]
				RESULT:15 = [[스킬:핵열가드브레이크]]
				RESULT:16 = [[스킬:노바사이저]]
				RESULT:17 = [[스킬:사신소환・주작]]
				RESULT:18 = [[스킬:충의의화염]]
				RESULT:19 = [[스킬:기공(중)]]
			CASE 1
				;氷の달인
				RESULT:10 = [[스킬:아이스에이지]]
				RESULT:11 = [[스킬:베이퍼봄]]
				RESULT:12 = [[스킬:마카카쟈오토]]
				RESULT:13 = [[스킬:사마카쟈오토]]
				RESULT:14 = [[스킬:빙결가드브레이크]]
				RESULT:15 = [[스킬:수격가드브레이크]]
				RESULT:16 = [[스킬:피의허니문]]
				RESULT:17 = [[스킬:대냉계]]
				RESULT:18 = [[스킬:충의의빙결]]
				RESULT:19 = [[스킬:기공(중)]]
			CASE 2
				;風の달인
				RESULT:10 = [[스킬:진공절단]]
				RESULT:11 = [[스킬:살풍격]]
				RESULT:12 = [[스킬:마카카쟈오토]]
				RESULT:13 = [[스킬:사마카쟈오토]]
				RESULT:14 = [[스킬:질풍가드브레이크]]
				RESULT:15 = [[스킬:충격가드브레이크]]
				RESULT:16 = [[스킬:트윙클네뷸라]]
				RESULT:17 = [[스킬:사신소환・백호]]
				RESULT:18 = [[스킬:충의의충격]]
				RESULT:19 = [[스킬:기공(중)]]
			CASE 3
				;地の달인
				RESULT:10 = [[스킬:기간트마키아]]
				RESULT:11 = [[스킬:이클립스]]
				RESULT:12 = [[스킬:마카카쟈오토]]
				RESULT:13 = [[스킬:사마카쟈오토]]
				RESULT:14 = [[스킬:지변가드브레이크]]
				RESULT:15 = [[스킬:중력가드브레이크]]
				RESULT:16 = [[스킬:어둠의포미러버]]
				RESULT:17 = [[스킬:사신소환・현무]]
				RESULT:18 = [[스킬:다무도라온]]
				RESULT:19 = [[스킬:기공(중)]]
			CASE 4
				;光闇の달인
				RESULT:10 = [[스킬:에이가온]]
				RESULT:11 = [[스킬:코우가온]]
				RESULT:12 = [[스킬:마카카쟈오토]]
				RESULT:13 = [[스킬:사마카쟈오토]]
				RESULT:14 = [[스킬:파마가드브레이크]]
				RESULT:15 = [[스킬:주살가드브레이크]]
				RESULT:16 = [[스킬:암흑광염난무]]
				RESULT:17 = [[스킬:포미러버]]
				RESULT:18 = [[스킬:저지먼트]]
				RESULT:19 = [[스킬:기공(중)]]
			CASE 5
				;雷の달인(穴埋めに전술)
				RESULT:10 = [[스킬:팔색뇌공]]
				RESULT:11 = [[스킬:데몬레이브]]
				RESULT:12 = [[스킬:마카카쟈오토]]
				RESULT:13 = [[스킬:사마카쟈오토]]
				RESULT:14 = [[스킬:전격가드브레이크]]
				RESULT:15 = [[스킬:천장뇌담진]]
				RESULT:16 = [[스킬:전광석화]]
				RESULT:17 = [[스킬:사신소환・청룡]]
				RESULT:18 = [[스킬:충의의전격]]
				RESULT:19 = [[스킬:기공(중)]]
			CASE 6
				;신경・정신の달인
				RESULT:10 = [[스킬:수해의실]]
				RESULT:11 = [[스킬:스카프런지]]
				RESULT:12 = [[스킬:마카카쟈오토]]
				RESULT:13 = [[스킬:사마카쟈오토]]
				RESULT:14 = [[스킬:정신가드브레이크]]
				RESULT:15 = [[스킬:신경가드브레이크]]
				RESULT:16 = [[스킬:마계의조사]]
				RESULT:17 = [[스킬:브레인버스트]]
				RESULT:18 = [[스킬:메가스트라이크]]
				RESULT:19 = [[스킬:기공(중)]]
			CASE 8
				;지원の달인
				RESULT:10 = [[스킬:사마리캄]]
				RESULT:11 = [[스킬:리캄도라]]
				RESULT:12 = [[스킬:자애의기도]]
				RESULT:13 = [[스킬:데・카쟈]]
				RESULT:14 = [[스킬:파워브레스]]
				RESULT:15 = [[스킬:다크브레스]]
				RESULT:16 = [[스킬:마하애널라이즈]]
				RESULT:17 = [[스킬:삼할의마맥]]
				RESULT:18 = [[스킬:천사의혈배]]
				RESULT:19 = [[스킬:기공(대)]]
		ENDSELECT
		RETURN RESULT
	CASE "相性"
		SELECTCASE GET_TYPE(ARG:1)
			CASE "파마"
				RETURN 0
			CASEELSE
				RETURN 100
		ENDSELECT
	CASE "소질"
		RETURN GETNUM(TALENT, "아픔에약함")
	CASE "除外소질"
		RETURN GETNUM(TALENT, "바늘다루기"), GETNUM(TALENT, "조합지식")
	;처녀は特別 0は랜덤(20%), 1なら처녀, 2なら非처녀
	CASE "처녀"
		IF  RAND:5 == 0
			RETURN 2
		ELSE
			RETURN 1
		ENDIF
	;戻り値は0:랜덤 1:소인체형 2:유아체형 4:큰체형 5:거체
	CASE "体型"
		SELECTCASE RAND:10
			CASE IS < 2
				RETURN 2
			CASE IS < 8
				RETURN 3
			CASE IS < 9
				RETURN 4
		ENDSELECT
	;戻り値は0:랜덤 1:절벽　2:빈유　3:並　4:거유　5:폭유
	CASE "가슴"	
		IF TALENT:ARG:남자 == 0
			SELECTCASE RAND:10
				CASE IS < 6
					RETURN 2
				CASE IS < 8
					RETURN 3
				CASE IS < 9
					RETURN 1
				CASE IS < 10
					IF RAND:10 == 0
						RETURN 5
					ELSE
						RETURN 4
					ENDIF
			ENDSELECT
		ELSEIF TALENT:ARG:오토코노코 == 1
			SELECTCASE RAND:4
				CASE IS < 2
					RETURN 2
				CASE IS < 4
					RETURN 1
			ENDSELECT
		ELSE
			RETURN 1
		ENDIF
	;추가対応。戻り値は0:랜덤 1:小尻　2:並尻　3:巨尻　4:爆尻
	CASE "尻"
		RETURN 0
	;통상の인간と肉体部位が違う場合はここで直接代入してください
	CASE "肉体部位"
		RETURN 0
	CASE "ABL"
		SELECTCASE ABLNAME:(ARG:1)
			CASE "순종", "욕망", "기교"
				RETURN !RAND:3+!RAND:5
			CASE "Ｃ감각", "Ｖ감각", "Ｂ감각"
				RETURN !RAND:5
			CASE "마조끼", "노출증", "봉사정신"
				RETURN !RAND:10
			CASE "새드끼"
				RETURN !RAND:5 * !ABL:ARG:마조끼
			CASE "Ａ감각", "자위중독", "정액중독", "레즈끼", "ＢＬ끼", "섹스중독"
				RETURN !RAND:10
			CASE "가창기능", "요리기능", "촬영기능"
				RETURN !RAND:3+!RAND:5
		ENDSELECT
	;EXPはABLが1이상ある場合、ある程度調整される(ただし、ABL0の時は調整が動かないので適当に초기値をきめてください)
	CASE "EXP"
		;全く손付かずの場合
		SIF !GROUPMATCH(1, !TALENT:ARG:처녀, ABL:ARG:Ｃ감각, ABL:ARG:Ｖ감각, ABL:ARG:Ａ감각, ABL:ARG:Ｂ감각, ABL:ARG:기교)
			RETURN 0
		SELECTCASE EXPNAME:(ARG:1)
			CASE "Ｖ경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "Ａ경험"
				RETURN RAND(0, 20) * !RAND:5
			CASE "절정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "사정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "성교경험"
				RETURN 0
			CASE "질사경험"
				RETURN EXP:ARG:사정경험/2*EXP:ARG:성교경험 > 0
			CASE "자위경험"
				RETURN RAND(0, 20)
			CASE "조교자위경험"
				RETURN 0
			CASE "정액경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "봉사쾌락경험"
				RETURN 0
			CASE "펠라경험"
				RETURN RAND(0, 10) * RAND:2
			CASE "애정경험"
				RETURN 0
			CASE "고통쾌락경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "방뇨경험"
				RETURN 0
			CASE "가학쾌락경험"
				RETURN RAND(0, 10) * !RAND:4
			CASE "야외노출경험"
				RETURN 0
			CASE "레즈경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "ＢＬ경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "이상경험"
				RETURN 0
			CASE "긴박경험"
				RETURN RAND(0, 10) * EXP:ARG:고통쾌락경험 > 0
			CASE "가창경험"
				RETURN (RAND(5, 20) * RAND(0, 3)) + RAND:5
			CASE "가사경험"
				RETURN (RAND(5, 20) * RAND(0, 3)) + RAND:5
			CASE "촬영경험"
				RETURN (RAND(5, 20) * RAND(0, 3)) + RAND:5
		ENDSELECT
	CASE "모자"
		IF IS_MALE(ARG)
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:경관모]]
				CASE 1
					RETURN [[衣装:군모]]
				CASE 2
					RETURN [[衣装:헬멧]]
				CASE 3
					RETURN [[衣装:모자]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ELSE
			SELECTCASE RAND:10
				CASE 0
					RETURN [[衣装:화이트브림]]
				CASE 1
					RETURN [[衣装:너스캡]]
				CASE 2
					RETURN [[衣装:경관모]]
				CASE 3
					RETURN [[衣装:군모]]
				CASE 4
					RETURN [[衣装:토끼귀밴드]]
				CASE 5
					RETURN [[衣装:리본]]
				CASE 6
					RETURN [[衣装:헤어밴드]]
				CASE 7
					RETURN [[衣装:모자]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ENDIF
	CASE "옷"
		IF CFLAG:ARG:모자 == [[衣装:경관모]]
			RETURN [[衣装:경관복]]
		ELSEIF CFLAG:ARG:모자 == [[衣装:너스캡]]
			RETURN [[衣装:간호사복]]
		ELSEIF CFLAG:ARG:모자 == [[衣装:군모]] || CFLAG:ARG:모자 == [[衣装:헬멧]] || CFLAG:ARG:모자 == [[衣装:화이트브림]] || CFLAG:ARG:모자 == [[衣装:토끼귀밴드]]
			;전신복を설정するため、ここでは설정なし
			RETURN 0
		ELSEIF IS_MALE(ARG)
			IF CFLAG:ARG:모자 == [[衣装:모자]] || RAND:2 == 0
				RETURN [[衣装:와이셔츠]]
			ELSE
				;전신복を설정するため、ここでは설정なし
				RETURN 0
			ENDIF
		ELSE
			IF CFLAG:ARG:모자 == [[衣装:모자]] || CFLAG:ARG:모자 == [[衣装:리본]] || CFLAG:ARG:모자 == [[衣装:헤어밴드]] || RAND:2 == 0
				RETURN [[衣装:블라우스]]
			ELSE
				;전신복を설정するため、ここでは설정なし
				RETURN 0
			ENDIF
		ENDIF
	CASE "하의"
		IF CFLAG:ARG:옷 == 0
			;전신복を설정するため、ここでは설정なし
			RETURN 0
		ELSEIF IS_MALE(ARG)
			RETURN [[衣装:바지]]
		ELSE
			IF RAND:5 > 2
				RETURN [[衣装:스커트]]
			ELSEIF RAND:2
				RETURN [[衣装:롱스커트]]
			ELSE
				RETURN [[衣装:바지]]
			ENDIF
		ENDIF
	CASE "전신복"
		IF CFLAG:ARG:옷 == 0 && CFLAG:ARG:하의 == 0
			IF CFLAG:ARG:모자 == [[衣装:군모]]
				RETURN [[衣装:군복]]
			ELSEIF CFLAG:ARG:모자 == [[衣装:토끼귀밴드]]
				RETURN [[衣装:바니수트]]
			ELSEIF CFLAG:ARG:모자 == [[衣装:화이트브림]]
				SELECTCASE RAND:3
					CASE 0
						RETURN [[衣装:에이프런드레스]]
					CASE 1
						RETURN [[衣装:메이드복]]
					CASE 2
						RETURN [[衣装:프렌치메이드복]]
				ENDSELECT
			ELSEIF CFLAG:ARG:모자 == [[衣装:헬멧]]
				SELECTCASE RAND:3
					CASE 0
						RETURN [[衣装:길리수트]]
					CASE 1
						RETURN [[衣装:라이더수트]]
					CASE 2
						RETURN [[衣装:트라이브수트]]
				ENDSELECT
			ELSEIF IS_MALE(ARG)
				SELECTCASE RAND:4
					CASE 0
						RETURN [[衣装:인형탈]]
					CASE 1
						RETURN [[衣装:민족의상]]
					CASE 2
						RETURN [[衣装:도사복]]
					CASE 3
						RETURN [[衣装:죄수복]]
				ENDSELECT
			ELSE
				SELECTCASE RAND:5
					CASE 0
						RETURN [[衣装:차이나드레스]]
					CASE 1
						RETURN [[衣装:인형탈]]
					CASE 2
