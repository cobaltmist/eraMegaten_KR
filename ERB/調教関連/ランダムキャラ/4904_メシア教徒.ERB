;ARGは生成キャラの登録番号
;2020/03/20　メシアンはあんまり変更なし。
;異能者スキルのコンセプトは小学生を参照
@RANDOMCHARA_4904, ARGS, ARG, ARG:1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "일인칭"
		RESULTS = \@IS_MALE(ARG) ? 저#저\@
	CASE "이인칭"
		RESULTS = %CALLNAME:MASTER%
	CASE "체력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2800)
		ELSE
			RETURN RAND(800, 1800)
		ENDIF
	CASE "기력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1700, 3500)
		ELSE
			RETURN RAND(1200, 2500)
		ENDIF
	CASE "속성"
		RETURN RAND(100, 180), RAND(150, 255)
	CASE "종족"
		RETURN 0
	CASE "전투소질"
;メシア教徒はほとんどが이능자 2割が템플나이트 1%がアデプト 1割が비전투원
		SELECTCASE RAND:100
			CASE IS < 5
				RESULTS = 영매체질
				RESULTS:1 = 이능자
				RESULTS:2 = 도구지식
					;강화フラグがある場合、1/2で강화。도구지식を2に
					IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
						RETURN 1, 1, 2
					ENDIF
				RETURN 1, 1, 1
			CASE IS < 15
				RESULTS = 비전투원
				RETURN 1
			CASE IS < 35
				RESULTS = 이능자
				RESULTS:1 = 도구지식
				RESULTS:2 = 검술
					;강화フラグがある場合、1/2で강화。도구지식を2に
					IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
						RETURN 1, 2, 1
					ENDIF
				RETURN 1, 1, 1
			CASE IS < 55
				RESULTS = 석세서
				RESULTS:1 = 건슬링거
				RESULTS:2 = 도구지식
					;강화フラグがある場合、1/2で강화。도구지식を2に
					IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
						RETURN 1, 1, 2
					ENDIF
				RETURN 1, 1, 1
			CASEELSE
				RESULTS = 이능자
				RESULTS:1 = 도구지식
				RESULTS:2 = 제사
					;강화フラグがある場合、1/2で강화。도구지식を2に
					IF 이벤트플래그:88:0 > 3 && RAND:2 > 0
						RETURN 1, 2, 1
					ENDIF
				RETURN  1, 1, 1
		ENDSELECT
	CASE "초기페르소나"
		RETURN 0
	CASE "링크악마"
		RETURN 0
	CASE "초기스테이터스비율"
		RETURN 7, 12, 10, 7, 7, 5
	CASE "스킬"
		IF TALENT:ARG:건슬링거 == 1 && TALENT:ARG:이능자 == 0 && TALENT:ARG:석세서 == 1;エグゼクター
			RESULT = [[스킬:치유촉진(소)]]
			RESULT:1 = [[스킬:일할의활천]]
			RESULT:2 = [[스킬:코칭]]
			RESULT:3 = [[스킬:이할의활천]]
			RESULT:4 = [[스킬:아이템숙련]]
			RESULT:5 = [[스킬:치유촉진(중)]]
			RESULT:6 = [[스킬:불굴의투지]]
			RESULT:7 = [[스킬:아이템숙련]]
			RESULT:8 = [[스킬:장비지식Lv5]]
			RESULT:9 = [[스킬:삼할의활천]]
			RESULT:10 = [[스킬:서드아이]]
			RESULT:11 = [[스킬:치유촉진(대)]]
			RESULT:12 = [[스킬:관통]]
			RESULT:13 = [[스킬:무도의마음가짐]]
			RETURN RESULT

		ELSEIF TALENT:ARG:이능자 == 1 && TALENT:ARG:건슬링거 == 0 && TALENT:ARG:석세서 == 0
		
		;メシアン스킬
		RESULT = [[스킬:한마]]
		RESULT:1 = [[스킬:마한마]]
		RESULT:2 = [[스킬:파마의뇌광]]
		
		;이능자스킬
		;공격
		SELECTCASE RAND:3
			CASE 0
				RESULT:3 = [[스킬:잔]]
				RESULT:4 = [[스킬:잔마]]
				RESULT:5 = [[스킬:마하・잔]]
				RESULT:6 = [[스킬:림도라]]
			CASE 1
				RESULT:3 = [[스킬:마구날뛰기]]
				RESULT:4 = [[스킬:회전베기]]
				RESULT:5 = [[스킬:돌려차기]]
				RESULT:6 = [[스킬:자폭]]
			CASE 2
				RESULT:3 = [[스킬:메・디아라마]]
				RESULT:4 = [[스킬:리캄]]
				RESULT:5 = [[스킬:사마리캄]]
				RESULT:6 = [[스킬:리캄도라]]
		ENDSELECT
		;カジャ系
		SELECTCASE RAND:4
			CASE 0
				RESULT:7 = [[스킬:타루・카쟈]]
				RESULT:8 = [[스킬:다크브레스]]
			CASE 1
				RESULT:7 = [[스킬:스쿠・카쟈]]
				RESULT:8 = [[스킬:도발]]
			CASE 2
				RESULT:7 = [[스킬:라쿠・카쟈]]
				RESULT:8 = [[스킬:파워브레스]]
			CASE 3
				RESULT:7 = [[스킬:사마・카쟈]]
				RESULT:8 = [[스킬:위협]]
		ENDSELECT
		;魔脈/コンセ
		IF RAND:2 > 0
			RESULT:9 = [[스킬:일할의마맥]]
		ELSE
			RESULT:9 = [[스킬:컨센트레이트]]
		ENDIF
		;만능
		RESULT:10 = [[스킬:메기도]]
		RESULT:11 = [[스킬:메기도라온]]
		;強化フラグが立っていると奥義的ななにかを覚えるように
			IF 이벤트플래그:88:0 > 3 && TALENT:ARG:검술 == 1
				RESULT:12 = [[스킬:천군의검]]
			ELSEIF 이벤트플래그:88:0 > 3 && TALENT:ARG:제사 == 1
				RESULT:12 = [[스킬:릴리즈제일]]
			ENDIF
		RETURN RESULT
		ENDIF
	CASE "상성"
		SELECTCASE GET_TYPE(ARG:1)
			CASE "파마"
				RETURN 0
			CASEELSE
				RETURN 100
		ENDSELECT
	CASE "소질"
		RETURN GETNUM(TALENT, "저항"), GETNUM(TALENT, "억압"), GETNUM(TALENT, "헌신적"), GETNUM(TALENT, "자제심"), GETNUM(TALENT, "쾌감을부정"), GETNUM(TALENT, "일선을넘지않음"), GETNUM(TALENT, "정조관념"),GETNUM(TALENT, "악취민감")
	CASE "제외소질"
		RETURN GETNUM(TALENT, "솔직함"), GETNUM(TALENT, "프라이드낮음"), GETNUM(TALENT, "정조관결여"), GETNUM(TALENT, "낙관적"), GETNUM(TALENT, "소악마")
	;처녀は特別 0は랜덤(20%), 1なら처녀, 2なら非처녀
	CASE "처녀"
		IF RAND:10 == 0
			RETURN 2
		ELSE
			RETURN 1
		ENDIF
	;戻り値は0:랜덤 1:소인체형 2:유아체형 4:큰체형 5:거체
	CASE "체형"
		RETURN 0
	;戻り値は0:랜덤 1:절벽　2:빈유　3:並　4:거유　5:폭유
	CASE "가슴"
		RETURN 0
	;추가対応。戻り値は0:랜덤 1:小尻　2:並尻　3:巨尻　4:爆尻
	CASE "尻"
		RETURN 0
	;통상の인간と肉体部位が違う場合はここで直接代入してください
	CASE "육체부위"
		RETURN 0
	CASE "ABL"
		SELECTCASE ABLNAME:(ARG:1)
			CASE "순종", "욕망", "기교"
				RETURN !RAND:5+!RAND:10
			CASE "Ｃ감각", "Ｖ감각", "Ｂ감각"
				RETURN !RAND:4
			CASE "마조끼", "노출증", "봉사정신"
				RETURN !RAND:10
			CASE "새드끼"
				RETURN !RAND:10 * !ABL:ARG:마조끼
			CASE "Ａ감각", "자위중독", "정액중독", "레즈끼", "ＢＬ끼", "섹스중독"
				RETURN !RAND:20
		ENDSELECT
	;EXPはABLが1이상ある場合、ある程度調整される(ただし、ABL0の時は調整が動かないので適当に초기値をきめてください)
	CASE "EXP"
		;全く손付かずの場合
		SIF !GROUPMATCH(1, !TALENT:ARG:처녀, ABL:ARG:Ｃ감각, ABL:ARG:Ｖ감각, ABL:ARG:Ａ감각, ABL:ARG:Ｂ감각, ABL:ARG:기교)
			RETURN 0
		SELECTCASE EXPNAME:(ARG:1)
			CASE "Ｖ경험"
				RETURN RAND(0, 20) * !RAND:3
			CASE "Ａ경험"
				RETURN RAND(0, 20) * !RAND:10
			CASE "절정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "사정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "성교경험"
				RETURN 0
			CASE "질사경험"
				RETURN EXP:ARG:사정경험/2*EXP:ARG:성교경험 > 0
			CASE "자위경험"
				RETURN RAND(0, 20)
			CASE "조교자위경험"
				RETURN 0
			CASE "정액경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "봉사쾌락경험"
				RETURN 0
			CASE "펠라경험"
				RETURN RAND(0, 10) * !RAND:4
			CASE "애정경험"
				RETURN 0
			CASE "고통쾌락경험"
				RETURN RAND(0, 20) * !RAND:8
			CASE "방뇨경험"
				RETURN 0
			CASE "가학쾌락경험"
				RETURN RAND(0, 10) * !RAND:8
			CASE "야외노출경험"
				RETURN 0
			CASE "레즈경험"
				RETURN RAND(0, 20) * !RAND:8
			CASE "ＢＬ경험"
				RETURN RAND(0, 20) * !RAND:8
			CASE "이상경험"
				RETURN 0
			CASE "긴박경험"
				RETURN RAND(0, 10) * EXP:ARG:고통쾌락경험 > 0
		ENDSELECT
	CASE "모자"
		IF TALENT:ARG:남자 == 0 && TALENT:ARG:검술 == 0
			SELECTCASE RAND:8
				CASE 0
					RETURN [[衣装:베일]]
				CASE 1
					RETURN [[衣装:화관]]
				CASE 2
					RETURN [[衣装:머리장식]]
				CASE 3
					RETURN [[衣装:꽃송이]]
				CASE 4
					RETURN [[衣装:리본]]
				CASE 5
					RETURN [[衣装:후드]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:깃털모자]]
				CASE 1
					RETURN [[衣装:반다나]]
				CASE 2
					RETURN [[衣装:모자]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ENDIF
	CASE "옷"
		IF TALENT:ARG:남자 == 0 && TALENT:ARG:검술 == 0
			IF TALENT:ARG:영매체질 == 1
				;5割で법의이외の옷装
				SELECTCASE RAND:4
					CASE 0
						RETURN [[衣装:고스로리옷（백）]]
					CASE 1
						RETURN [[衣装:블라우스]]
					CASEELSE
						RETURN 0
				ENDSELECT
			ELSEIF TALENT:ARG:비전투원 == 1
				;5割で법의이외の옷装
				SELECTCASE RAND:4
					CASE 0
						RETURN [[衣装:세일러복]]
					CASE 1
						RETURN [[衣装:블라우스]]
					CASEELSE
						RETURN 0
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 0
	CASE "하의"
		IF CFLAG:ARG:옷 != 0
			;옷がある場合は下位옷も설정
			SELECTCASE RAND:5
				CASE 0
					RETURN [[衣装:스커트]]
				CASE 1
					RETURN [[衣装:롱스커트]]
				CASE 2
					RETURN [[衣装:플리츠스커트]]
				CASE 3
					RETURN [[衣装:플레어스커트]]
				CASE 4
					RETURN [[衣装:미니스커트]]
			ENDSELECT
		ELSEIF TALENT:ARG:검술
			RETURN [[衣装:바지]]
		ELSE
			RETURN 0
		ENDIF
	CASE "전신복"
		IF TALENT:ARG:검술
			RETURN [[衣装:전신갑주]]
		ELSEIF CFLAG:ARG:옷 == 0
			;옷が未설정の場合は법의を설정
			RETURN [[衣装:법의]]
		ENDIF
	CASE "손"
		RETURN 0
	CASE "속옷（상）"
		IF IS_MALE(ARG)
			RETURN 0
		ELSE
			RETURN [[衣装:브래지어]]
		ENDIF
	CASE "속옷（하）"
		IF IS_MALE(ARG)
			IF RAND:5
				RETURN [[衣装:훈도시]]
			ELSE
				RETURN [[衣装:트렁크스]]
			ENDIF
		ELSE
			SELECTCASE RAND:6
				CASE 0
					RETURN [[衣装:팬티]]
				CASE 1
					RETURN [[衣装:팬티(흑)]]
				CASE 2
					RETURN [[衣装:팬티(적)]]
				CASE 3
					RETURN [[衣装:레이스달린팬티]]
				CASE 4
					RETURN [[衣装:줄무늬팬티]]
				CASE 5
					RETURN [[衣装:드로워즈]]
			ENDSELECT
		ENDIF
	CASE "전신속옷"
		RETURN 0
	CASE "양말"
		IF IS_MALE(ARG)
			RETURN [[衣装:하이삭스]]
		ELSE
			SELECTCASE RAND:7
				CASE 0
					RETURN [[衣装:하이삭스]]
				CASE 1
					RETURN [[衣装:양말]]
				CASE 2
					RETURN [[衣装:니삭스]]
				CASE 3
					RETURN [[衣装:사이하이삭스]]
				CASE 4
					RETURN [[衣装:가터스타킹]]
				CASE 5
					RETURN [[衣装:팬티스타킹]]
				CASE 6
					RETURN [[衣装:검은색타이즈]]
			ENDSELECT
		ENDIF
	CASE "신발"
		IF IS_MALE(ARG)
			RETURN [[衣装:부츠]]
		ELSE
			SELECTCASE RAND:7
				CASE 0
					RETURN [[衣装:하이부츠]]
				CASE 1
					RETURN [[衣装:핀힐]]
				CASE 2
					RETURN [[衣装:하이힐]]
				CASE 3
					RETURN [[衣装:뮬]]
				CASE 4
					RETURN [[衣装:펌프스]]
				CASE 5
					RETURN [[衣装:숏부츠]]
				CASE 6
					RETURN [[衣装:부츠]]
			ENDSELECT
		ENDIF
	CASE "겉옷"
		IF TALENT:ARG:이능자
			RETURN [[衣装:망토]]
		ELSE
			RETURN 0
		ENDIF
	CASE "그외"
		SELECTCASE RAND:10
			CASE 0
				RETURN [[衣装:벨트]]
			CASE 1
				RETURN [[衣装:쵸커]]
			CASE 2
				RETURN [[衣装:안대]]
			CASE 3
				RETURN [[衣装:귀걸이]]
			CASE 4
				RETURN [[衣装:앵클릿]]
			CASE 5
				RETURN [[衣装:넥클리스]]
			CASE 6
				RETURN [[衣装:안경]]
			CASE 7
				RETURN [[衣装:회중시계]]
			CASE 8
				RETURN [[衣装:카메오브로치]]
			CASE 9
				RETURN [[衣装:경전]]
		ENDSELECT
	;악마汎用사용しています0:랜덤 1:女_아이　2:女_소녀　3:女_청소년　4:女_성인　5:女_여성　6:女_고풍　7:男_청소년　8:男_성인　9:짐승_짐승
	CASE "회화타입"
		IF TALENT:ARG:남자 == 0
			SIF RAND:5 > 2
				RETURN 5
		ELSE
			RETURN 0
		ENDIF
	CASE "이름"
		$SET_NAME
		IF (TALENT:ARG:마유 == 1 || TALENT:ARG:폭유 == 1) && TALENT:ARG:처녀 == 1
			RESULTS = 성유(聖乳) 
		ELSEIF ABL:ARG:Ｖ감각 >= 1 && EXP:ARG:정액경험 > 10
			RESULTS = 배덕의 
		ELSEIF ABL:ARG:Ａ감각 >= 1 && EXP:ARG:Ａ경험 >= 15
			RESULTS = 애널광 
		ELSE
			RESULTS = 원(元) 
		ENDIF
		IF TALENT:ARG:광기 == 1
			RESULTS += "광신자"
		ELSEIF TALENT:ARG:영매체질 == 1
			IF CFLAG:ARG:옷 != 0 && TALENT:ARG:남자 == 0
				;법의이외の옷装の自由がある扱い
				RESULTS += "성녀 후보"
			ELSE
				RESULTS += "달인"
			ENDIF
		ELSEIF TALENT:ARG:검술
			RESULTS += "템플나이트"
		ELSEIF TALENT:ARG:건슬링거
			RESULTS += "이그젝터"
		ELSEIF TALENT:ARG:이능자
			IF CFLAG:ARG:옷 == 0 && CFLAG:ARG:모자 != 0 && TALENT:ARG:남자 == 0
				;법의の着用が義務付け+추가옷飾が認められてない扱い
				RESULTS += "수녀"
			ELSE
				RESULTS += "메시아교도"
			ENDIF
		ELSE
			RESULTS += "메시아교신자"
		ENDIF
	CASE "애칭"
		;이름と同じため생략
		GOTO SET_NAME
	CASE "해설"
		RESULTS = TOKYO를 중심으로 급속히 세력을 늘리고 있는 신흥 종교 메시아교의 신자。「믿는 자 모두 구해지리라」
		IF TALENT:ARG:영매체질
			RESULTS:1 = 신자중에서도 특별히 메시아에 가깝다고 여겨지는 상층계급、「아뎁트」에 속해 있었다
		ELSEIF TALENT:ARG:검술
			RESULTS:1 = 교단내의 엘리트 집단「템플나이트」의 전 구성원。특수한 무기의 취급이 허가되고 있다
		ELSEIF TALENT:ARG:이능자
			RESULTS:1 = 신의 기적과도 같은 힘을 가진 구세주에 가까워지기 위해、이능의 힘을 몸에 익힌 신자도 많다
		ELSEIF TALENT:ARG:건슬링거
			RESULTS:1 = 교단내의 킬러 집단 「이그젝터」의 전 구성원. 템플나이트에는 맡길 수 없는 더러운 일을 하고 있었다.
		ELSE
			RESULTS:1 = 메시아교내에는 엄격한 격차가 존재해、낮은 쪽에 속하는 일반 신자의 생활은 극히 어렵다
