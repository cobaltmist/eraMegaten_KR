;ARGは生成キャラの등록番号
;高校生は서머너、이능자、페르소나구사자、シフター、식노と幅広く発現し、랜덤キャラとしては一番オーソドックスな扱い
;레어キャラは아이돌のみ
;이능자스킬のコンセプトは小学生を参照
@RANDOMCHARA_4907, ARGS, ARG, ARG:1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "일인칭"
		IF TALENT:ARG:남자 == 1
			SELECTCASE RAND:5
				CASE 0
					RESULTS = 나
				CASE 1
					RESULTS = 나
				CASE 2
					RESULTS = 나
				CASE 3
					RESULTS = 나
				CASE 4
					RESULTS = 나
			ENDSELECT
		ELSE
			SELECTCASE RAND:9
				CASE 0
					RESULTS = 나
				CASE 1
					RESULTS = 저
				CASE 2
					RESULTS = 나
				CASE 3
					RESULTS = 저
				CASE 4
					RESULTS = 저
				CASE 5
					RESULTS = 저
				CASE 6
					RESULTS = 본녀
				CASE 7
					RESULTS = 본녀
				CASE 8
					RESULTS = 나
			ENDSELECT
		ENDIF
	CASE "이인칭"
		RESULTS = %CALLNAME:MASTER%
	CASE "체력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2800)
		ELSE
			RETURN RAND(800, 1800)
		ENDIF
	CASE "기력"
		IF 이벤트플래그:88:0 > 1
			RETURN RAND(1300, 2500)
		ELSE
			RETURN RAND(800, 1500)
		ENDIF
	CASE "속성"
		RETURN RAND(100, 200), RAND(100, 200)
	CASE "종족"
		RETURN 0
	CASE "전투소질"
		;1/3で비전투원
		IF RAND:3 == 0
			RESULTS = 비전투원
			;1/6で아이돌(영매체질)
			IF RAND:6 == 0
				RESULTS:1 = 영매체질
				SELECTCASE RAND:5
					CASE 0
						;1/5でフレーバーとして천사を부여
						RESULTS:2 = 천사
						RETURN 1, 1, 1
					CASE 1
						;1/5で半フレーバーとして기계의처녀を부여
						RESULTS:2 = 기계의처녀
						RETURN 1, 1, 1
					CASEELSE
						;3/5で何もなし
						RETURN 1, 1
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		;それ이외は各種기본職
		SELECTCASE RAND:100
			CASE IS < 10
				;서머너
				RESULTS = 서머너
				RESULTS:1 = 석세서
				RESULTS:2 = 건슬링거
				RESULTS:3 = 도구지식
				RETURN 3, 1, 1, 3
			CASE IS < 30
				;페르소나구사자
				RESULTS = 페르소나구사자
				;1/2でP1・2仕様かP3以降仕様に別れる
				SIF RAND:2 == 0
					RETURN 2
				RETURN 1
			CASE IS < 50
				;シフター
				RESULTS = 데빌시프터
				RESULTS:1 = 도구지식
				RETURN 1, 3
			CASE IS < 70
				;식노
				RESULTS = 식노
				RESULTS:1 = 건슬링거
				RETURN 1, 1
			CASE IS < 90
				;純이능자
				RESULTS = 이능자
				RETURN 1
			CASEELSE
				;이능자서머너
				RESULTS = 이능자
				RESULTS:1 = 서머너
				RETURN 1, 1
		ENDSELECT
	;戻り値は0:랜덤 それ이외は指定
	CASE "초기페르소나"
		RETURN 0
	;戻り値は0:랜덤 それ이외は指定
	CASE "링크악마"
		RETURN 0
	;戻り値は0:랜덤 それ이외は指定
	CASE "초기스테이터스비율"
		RETURN 7, 7, 8, 7, 8, 10
	CASE "스킬"
		IF 이벤트플래그:88:0 > 3 && TALENT:ARG:서머너 > 1 && TALENT:ARG:이능자 == 0
			;純サマ강화스킬
			;とりあえず自動스킬を山盛り
			RESULT = [[스킬:아이템숙련]]
			RESULT:1 = [[스킬:어드바이스]]
			RESULT:2 = [[스킬:이악물기]]
			RESULT:3 = [[스킬:추격의심득]]
			RESULT:4 = [[스킬:황천의회심]]
			RESULT:5 = [[스킬:정천의회심]]
			RESULT:6 = [[스킬:서드아이]]
			RESULT:7 = [[스킬:관통]]
			RETURN RESULT
		ELSEIF TALENT:ARG:영매체질 == 1
			;아이돌用스킬
			;(使うには함락させた上に전투소질を付けないといけないので、ぶっちゃけ使う人は物好きと思われる)
			;それっぽい스킬2つ
			SELECTCASE RAND:4
				CASE 0
					RESULT = [[스킬:해피스텝]]
				CASE 1
					RESULT = [[스킬:고양의노래]]
				CASE 2
					RESULT = [[스킬:던지는키스]]
				CASE 3
					RESULT = [[스킬:자장가]]
			ENDSELECT
			SELECTCASE RAND:4
				CASE 0
					RESULT:1 = [[스킬:용분의춤]]
				CASE 1
					RESULT:1 = [[스킬:천상의춤]]
				CASE 2
					RESULT:1 = [[스킬:마음의외침]]
				CASE 3
					RESULT:1 = [[스킬:마계의조사]]
			ENDSELECT
			;持ち歌(가창스킬)を2曲ずつ
			SELECTCASE RAND:4
				CASE 0
					RESULT:2 = [[스킬:Go-Round]]
				CASE 1
					RESULT:2 = [[스킬:Breakin' Through]]
				CASE 2
					RESULT:2 = [[스킬:Light the Fire up in the Night]]
				CASE 3
					RESULT:2 = [[스킬:Time to Make History]]
			ENDSELECT
			SELECTCASE RAND:5
				CASE 0
					RESULT:3 = [[스킬:Burn my Dread]]
				CASE 1
					RESULT:3 = [[스킬:『너의눈은일만볼트』]]
				CASE 2
					RESULT:3 = [[스킬:『얼려버리는라인』]]
				CASE 3
					RESULT:3 = [[스킬:『봄버걸』]]
				CASE 4
					RESULT:4 = [[스킬:『불의전차』]]
			ENDSELECT
			IF GETBIT(FLAG:EXTRA이벤트설정, 0) == 0
				;最近の아이돌は마법소녀も兼ねていたりするため、마법소녀스킬もオマケで1つ
				;패턴を増やし、幻想郷스킬も추가
				SELECTCASE RAND:6
					CASE 0
						RESULT:4 = [[스킬:침식하는검은날개]]
					CASE 1
						RESULT:4 = [[스킬:티로・피날레]]
					CASE 2
						RESULT:4 = [[스킬:나의세계]]
					CASE 3
						RESULT:4 = [[스킬:피니투라・프레차]]
					CASE 4
						RESULT:4 = [[스킬:스쿠왈타토레]]
					CASE 5
						RESULT:4 = [[스킬:롯소・판타즈마]]
				ENDSELECT
			ENDIF
			RETURN RESULT
		ENDIF 
		IF TALENT:ARG:이능자 == 0
			RETURN 0
		ENDIF
		
		;이능자스킬
		;공격
		SELECTCASE RAND:5
			CASE 0
				RESULT = [[스킬:아기]]
				RESULT:1 = [[스킬:아기라오]]
				RESULT:2 = [[스킬:아기다인]]
				RESULT:3 = [[스킬:데들리번]]
			CASE 1
				RESULT = [[스킬:마그나]]
				RESULT:1 = [[스킬:마그나스]]
				RESULT:2 = [[스킬:마그다인]]
				RESULT:3 = [[스킬:대지의분노]]
			CASE 2
				RESULT = [[스킬:아쿠아]]
				RESULT:1 = [[스킬:아쿠에스]]
				RESULT:2 = [[스킬:아쿠아다인]]
				RESULT:3 = [[스킬:아쿠아리타이드]]
			CASE 3
				RESULT = [[스킬:갈]]
				RESULT:1 = [[스킬:갈라]]
				RESULT:2 = [[스킬:갈다인]]
				RESULT:3 = [[스킬:선풍진]]
			CASE 4
				RESULT = [[스킬:코우하]]
				RESULT:1 = [[스킬:바이코우하]]
				RESULT:2 = [[스킬:마하코우하]]
				RESULT:3 = [[스킬:사마리캄]]
		ENDSELECT
		;カジャ系
		SELECTCASE RAND:4
			CASE 0
				RESULT:4 = [[스킬:타루・카쟈]]
				RESULT:5 = [[스킬:다크브레스]]
			CASE 1
				RESULT:4 = [[스킬:스쿠・카쟈]]
				RESULT:5 = [[스킬:도발]]
			CASE 2
				RESULT:4 = [[스킬:라쿠・카쟈]]
				RESULT:5 = [[스킬:파워브레스]]
			CASE 3
				RESULT:4 = [[스킬:사마・카쟈]]
				RESULT:5 = [[스킬:위협]]
		ENDSELECT
		;魔脈/コンセ
		IF RAND:2 > 0
			RESULT:6 = [[스킬:일할의마맥]]
		ELSE
			RESULT:6 = [[스킬:컨센트레이트]]
		ENDIF
		;만능
		RESULT:7 = [[스킬:메기도]]
		RESULT:8 = [[스킬:메기도라온]]
		
		;강화
		IF 이벤트플래그:88:0 > 3
			RESULT:9 = [[스킬:데・카쟈]]
			RESULT:10 = [[스킬:리캄도라]]
			IF TALENT:ARG:서머너 == 0
				;純이능자なら気功
				RESULT:11 = [[스킬:기공(소)]]
			ELSE
				;異能サマならコンセと魔脈を両方
				IF RESULT:6 == [[스킬:일할의마맥]]
					RESULT:11 = [[스킬:컨센트레이트]]
				ELSE
					RESULT:11 = [[스킬:일할의마맥]]
				ENDIF
			ENDIF
		ENDIF
		RETURN RESULT
	CASE "상성"
		SELECTCASE GET_TYPE(ARG:1)
			CASE "파마"
				RETURN 0
			CASEELSE
				RETURN 100
		ENDSELECT
	CASE "소질"
		RESULT = GETNUM(TALENT, "아픔에약함")
		RESULT:1 = GETNUM(TALENT, "호기심")
		IF TALENT:ARG:영매체질 == 1
			RESULT:2 = GETNUM(TALENT, "인기")
		ENDIF
		IF TALENT:ARG:천사 == 1
			;천사の場合、날개を부여
			RESULT:3 = GETNUM(TALENT, "날개")
		ELSEIF TALENT:ARG:기계의처녀 == 1 && RAND:3 == 0
			;기계의처녀の場合、1/3で동물귀を부여(헤드パーツとして付いている扱い)
			RESULT:3 = GETNUM(TALENT, "동물귀")
		ENDIF
		RETURN RESULT
	CASE "제외소질"
		RETURN GETNUM(TALENT, "바늘다루기"), GETNUM(TALENT, "조합지식")
	;처녀は特別 0は랜덤(20%), 1なら처녀, 2なら非처녀
	CASE "처녀"
		IF  RAND:5 == 0
			RETURN 2
		ELSE
			RETURN 1
		ENDIF
	;戻り値は0:랜덤 1:소인체형 2:유아체형 4:큰체형 5:거체
	CASE "체형"
		;기계의처녀の場合、소인체형の分岐を추가
		IF TALENT:ARG:기계의처녀 == 1 && RAND:4 == 0
			RETURN 1
		ENDIF
		SELECTCASE RAND:10
			CASE IS < 2
				RETURN 2
			CASE IS < 8
				RETURN 3
			CASE IS < 9
				RETURN 4
		ENDSELECT
	;戻り値は0:랜덤 1:절벽　2:빈유　3:並　4:거유　5:폭유
	CASE "가슴"	
		IF TALENT:ARG:남자 == 0
			SELECTCASE RAND:10
				CASE IS < 6
					RETURN 2
				CASE IS < 8
					RETURN 3
				CASE IS < 9
					RETURN 1
				CASE IS < 10
					IF RAND:10 == 0
						RETURN 5
					ELSE
						RETURN 4
					ENDIF
			ENDSELECT
		ELSEIF TALENT:ARG:오토코노코 == 1
			SELECTCASE RAND:4
				CASE IS < 2
					RETURN 2
				CASE IS < 4
					RETURN 1
			ENDSELECT
		ELSE
			RETURN 1
		ENDIF
	;추가対応。戻り値は0:랜덤 1:小尻　2:並尻　3:巨尻　4:爆尻
	CASE "尻"
		RETURN 0
	;통상の인간と肉体部位が違う場合はここで直接代入してください
	CASE "육체부위"
		RETURN 0
	CASE "ABL"
		SELECTCASE ABLNAME:(ARG:1)
			CASE "순종", "욕망", "기교"
				RETURN !RAND:3+!RAND:5
			CASE "Ｃ감각", "Ｖ감각", "Ｂ감각"
				RETURN !RAND:5
			CASE "마조끼", "노출증", "봉사정신"
				RETURN !RAND:10
			CASE "새드끼"
				RETURN !RAND:5 * !ABL:ARG:마조끼
			CASE "Ａ감각", "자위중독", "정액중독", "레즈끼", "ＢＬ끼", "섹스중독"
				RETURN !RAND:10
			CASE "가창기능"
				IF TALENT:ARG:영매체질 == 1
					;아이돌の場合、가창기능を1이상保証
					RETURN RAND:5 + 1
				ELSE
					RETURN 0
				ENDIF
		ENDSELECT
	;EXPはABLが1이상ある場合、ある程度調整される(ただし、ABL0の時は調整が動かないので適当に초기値をきめてください)
	CASE "EXP"
		;全く손付かずの場合
		SIF !GROUPMATCH(1, !TALENT:ARG:처녀, ABL:ARG:Ｃ감각, ABL:ARG:Ｖ감각, ABL:ARG:Ａ감각, ABL:ARG:Ｂ감각, ABL:ARG:기교)
			RETURN 0
		SELECTCASE EXPNAME:(ARG:1)
			CASE "Ｖ경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "Ａ경험"
				RETURN RAND(0, 20) * !RAND:5
			CASE "절정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "사정경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "성교경험"
				RETURN 0
			CASE "질사경험"
				RETURN EXP:ARG:사정경험/2*EXP:ARG:성교경험 > 0
			CASE "자위경험"
				RETURN RAND(0, 20)
			CASE "조교자위경험"
				RETURN 0
			CASE "정액경험"
				RETURN RAND(0, 20) * RAND:2
			CASE "봉사쾌락경험"
				RETURN 0
			CASE "펠라경험"
				RETURN RAND(0, 10) * RAND:2
			CASE "애정경험"
				RETURN 0
			CASE "고통쾌락경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "방뇨경험"
				RETURN 0
			CASE "가학쾌락경험"
				RETURN RAND(0, 10) * !RAND:4
			CASE "야외노출경험"
				RETURN 0
			CASE "레즈경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "ＢＬ경험"
				RETURN RAND(0, 20) * !RAND:4
			CASE "이상경험"
				RETURN 0
			CASE "긴박경험"
				RETURN RAND(0, 10) * EXP:ARG:고통쾌락경험 > 0
			CASE "가창경험"
				IF TALENT:ARG:영매체질 == 1
					;아이돌の場合はそれなりに가창경험を付ける
					RETURN (RAND(10, 20) * ABL:ARG:가창기능) + RAND:5
				ELSE
					RETURN 0
				ENDIF
		ENDSELECT
	CASE "모자"
		;동물귀がある場合はなし
		SIF TALENT:ARG:동물귀 == 0 
			RETURN 0
		IF IS_MALE(ARG)
			SELECTCASE RAND:3
				CASE 0
					RETURN [[衣装:모자]]
				CASE 1
					RETURN [[衣装:반다나]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ELSE
			SELECTCASE RAND:6
				CASE 0
					RETURN [[衣装:모자]]
				CASE 1
					RETURN [[衣装:리본]]
				CASE 2
					RETURN [[衣装:헤어밴드]]
				CASE 3
					RETURN [[衣装:카츄샤]]
				CASE 4
					RETURN [[衣装:머리끈]]
				CASEELSE
					RETURN 0
			ENDSELECT
		ENDIF
	CASE "옷"
		IF IS_MALE(ARG)
			SELECTCASE RAND:2
				CASE 0
					RETURN [[衣装:깃세운옷]]
				CASE 1
					RETURN [[衣装:와이셔츠]]
			ENDSELECT
		ELSE
			;女아이돌の場合はコスプレ中の場合を추가
			IF TALENT:ARG:영매체질 == 1 && RAND:3 == 0
				SELECTCASE RAND:6
					CASE 0
						RETURN [[衣装:기모노]]
					CASE 1
						RETURN [[衣装:유카타]]
					CASE 2
						RETURN [[衣装:간호사복]]
					CASE 3
						RETURN [[衣装:고스로리옷（흑）]]
					CASE 4
						RETURN [[衣装:고스로리옷（백）]]
					CASEELSE
						;전신복を설정する想定
						RETURN 0
				ENDSELECT
			ENDIF
			SELECTCASE RAND:3
				CASE 0
					RETURN [[衣装:체육복]]
				CASE 1
					RETURN [[衣装:세일러복]]
				CASE 2
					RETURN [[衣装:블라우스]]
			ENDSELECT
		ENDIF
	CASE "하의"
		IF CFLAG:ARG:옷 == [[衣装:체육복]]
			RETURN [[衣装:블루머]]
		ELSEIF CFLAG:ARG:옷 == [[衣装:깃세운옷]] || CFLAG:ARG:옷 == [[衣装:와이셔츠]]
			RETURN [[衣装:바지]]
		ELSEIF CFLAG:ARG:옷 == [[衣装:세일러복]] || CFLAG:ARG:옷 == [[衣装:블라우스]]
			RETURN [[衣装:스커트]]
		ELSEIF CFLAG:ARG:옷 == 0
			;전신복を설정する想定
			RETURN 0
		ELSE
			SELECTCASE RAND:8
				CASE 0
					RETURN [[衣装:스커트]]
				CASE 1
					RETURN [[衣装:롱스커트]]
				CASE 2
					RETURN [[衣装:미니스커트]]
				CASE 3
					RETURN [[衣装:타이트스커트]]
				CASE 4
					RETURN [[衣装:숏팬츠]]
				CASE 5
					RETURN [[衣装:플리츠스커트]]
				CASE 6
					RETURN [[衣装:플레어스커트]]
				CASE 7
					RETURN [[衣装:타이트미니스커트]]
			ENDSELECT
		ENDIF
	CASE "전신복"
		IF CFLAG:ARG:옷 == 0
			SELECTCASE RAND:15
				CASE 0
					RETURN [[衣装:군복]]
				CASE 1
					RETURN [[衣装:바니수트]]
				CASE 2
					RETURN [[衣装:에이프런드레스]]
				CASE 3
					RETURN [[衣装:메이드복]]
				CASE 4
					RETURN [[衣装:프렌치메이드복]]
				CASE 5
					RETURN [[衣装:민족의상]]
