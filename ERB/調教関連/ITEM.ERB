@SHOPCOMABLE_204
RESULT:1 = 2
RESULTS = 조교아이템구입
RETURN 1


@SHOP_COM_204
#DIM LINE, 2
LINE = LINECOUNT
REDRAW 0
DRAWLINE
PRINTL 구입할 아이템을 선택해주세요
PRINTFORML 소지금:￥{MONEY}
LOCAL:1 = 0
DRAWLINE
FOR LOCAL, 0, 128
	IF GROUPMATCH(LOCAL, 40, 60, 70)
		SIF LOCAL:1 % 2
			PRINTL 
		DRAWLINE
		LOCAL:1 = 0
	ENDIF
	SELECTCASE LOCAL
		CASE 0
			PRINTFORM 【조교도구】　　　
			PRINTBUTTON "[900]표시전환", 900
			PRINTL 
		CASE 40
			PRINTFORM 【소모품】　　　　
			PRINTBUTTON "[901]표시전환", 901
			PRINTL 
		CASE 60
			PRINTFORM 【소질아이템】　　
			PRINTBUTTON "[902]표시전환", 902
			PRINTL 
		CASE 70
			PRINTFORM 【약품류】　　　　
			PRINTBUTTON "[903]표시전환", 903
			PRINTL 
	ENDSELECT
	SIF LOCAL < 40 && !GETBIT(FLAG:조교도구구입화면설정,0)
		CONTINUE
	SIF LOCAL >= 40 && LOCAL < 60 && !GETBIT(FLAG:조교도구구입화면설정,1)
		CONTINUE
	SIF LOCAL >= 60 && LOCAL < 70 && !GETBIT(FLAG:조교도구구입화면설정,2)
		CONTINUE
	SIF LOCAL >= 70 && !GETBIT(FLAG:조교도구구입화면설정,3)
		CONTINUE
	SIF LOCAL >= 101 && LOCAL <= 109
		CONTINUE
	SIF LOCAL >= 900 && LOCAL <= 910
		CONTINUE
	SELECTCASE TRAINITEM_SALEABLE(LOCAL)
		CASE -1
			CONTINUE
		CASE 0
			SETCOLOR COLOR("gray")
			SELECTCASE LOCAL
				CASE IS < 40
					PRINTFORM [{LOCAL,2}] %ITEMNAME:LOCAL,24,LEFT%%"(구입완료)",10,RIGHT%　
				CASE IS < 60
					PRINTFORM [{LOCAL,2}] %ITEMNAME:LOCAL+"(소지:"+@"{ITEM:LOCAL}"+")",24,LEFT%%"(소지한계)",10, RIGHT%　
				CASE IS < 70
					PRINTFORM [{LOCAL,2}] %ITEMNAME:LOCAL,24,LEFT%%"(습득완료)",10,RIGHT%　
				CASEELSE
					PRINTFORM [{LOCAL,2}] %ITEMNAME:LOCAL,24,LEFT%%"(대상없음)",10,RIGHT%　
			ENDSELECT
		CASE 1
			SIF MONEY < ITEMPRICE:LOCAL
				SETCOLOR COLOR("gray")
			IF LOCAL >= 40 && LOCAL < 60
				PRINTFORM [{LOCAL,2}] %ITEMNAME:LOCAL+"(소지:"+@"{ITEM:LOCAL}"+")",24,LEFT%%"￥"+@"{ITEMPRICE:LOCAL}",10, RIGHT%　
			ELSE
				PRINTFORM [{LOCAL,2}] %ITEMNAME:LOCAL,24,LEFT%%"￥"+@"{ITEMPRICE:LOCAL}",10, RIGHT%　
			ENDIF
	ENDSELECT
	RESETCOLOR
	SIF LOCAL:1 % 2 == 1
		PRINTL 
	LOCAL:1++
NEXT
SIF LOCAL:1 % 2
	PRINTL 
DRAWLINE
