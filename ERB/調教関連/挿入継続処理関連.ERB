;挿入継続の処理
;TRACHECKとセットで機能しています
;----------------------------------------------------------------------
@EQUIP_P_INSERT_V_TARGET
;----------------------------------------------------------------------
;IF TEQUIP:TARGET:Ｖ섹스 > 0 && TEQUIP:TARGET:Ｖ섹스 < 2
;TEQUIP50が3と4でで経験処理、1(挿入なしの状態でV挿入）、2(V挿入中にもう一度V挿入コマンド実行時）は処理しない。1,2のときはELSEIFで3,4に変更し、次回以降のV挿入コマンド以外のときに経験処理する
IF TEQUIP:TARGET:Ｖ섹스 > 2
	;조교자←→조교대상
	TFLAG:28 = 3
	B += 750

	;再装填処理中は사정ゲージは20分の1に
	SIF CFLAG:PLAYER:사정게이지재장전 > 0
		B /= 20

	SIF HAVE_PENIS(PLAYER)
		BASE:PLAYER:사정 += B

	DOWNBASE:TARGET:체력 += 25
	DOWNBASE:TARGET:기력 += 50

	SOURCE:TARGET:쾌Ｖ += 200
	SOURCE:TARGET:정애 += 75
	SOURCE:TARGET:노출 += 25

	;조교자に쾌Ｃ
	SOURCE:PLAYER:쾌Ｃ += 100

	;억압、저항持ちには、억울소스추가
	SIF TALENT:억압 || TALENT:저항
		SOURCE:TARGET:울굴 += 150

	LOCAL = 250
	LOCAL:1 = 150
	LOCAL:2 = 100

	;정조관념持ちで연모、친애持ち이외には常に反発의 소스を추가する（섹스系のみ）
	SIF TALENT:정조관념 && !IS_LOVER(TARGET)
		SOURCE:TARGET:반감 += 500

	;Vへの고통は先に処理しておく
	;EXP:Ｖ경험をみる（처녀の반감は別途処理済みなので생략）
	IF EXP:Ｖ경험 < EXPLV:1
		TIMES LOCAL , 2.50
	ELSEIF EXP:Ｖ경험 < EXPLV:2
		TIMES LOCAL , 0.80
	ELSEIF EXP:Ｖ경험 < EXPLV:3
		TIMES LOCAL , 0.40
	ELSEIF EXP:Ｖ경험 < EXPLV:4
		TIMES LOCAL , 0.10
	ELSEIF EXP:Ｖ경험 < EXPLV:5
		TIMES LOCAL , 0.00
	ELSEIF EXP:Ｖ경험 >= EXPLV:5
		TIMES LOCAL , 0.00
	ENDIF

	;PALAM:윤활をみる
	IF PALAM:윤활 < PALAMLV:1
		TIMES LOCAL , 1.20
		TIMES LOCAL:1 , 1.50
	ELSEIF PALAM:윤활 < PALAMLV:2
		TIMES LOCAL , 0.70
		TIMES LOCAL:1 , 1.20
	ELSEIF PALAM:윤활 < PALAMLV:3
		TIMES LOCAL , 0.40
	ELSEIF PALAM:윤활 < PALAMLV:4
		TIMES LOCAL , 0.10
	ELSEIF PALAM:윤활 >= PALAMLV:4
		TIMES LOCAL , 0.10
	ENDIF

	;중독による중독충족의 소스は先に処理しておく
	;섹스중독を見る
	IF ABL:섹스중독 == 0
		TIMES LOCAL:2 , 0.00
	ELSEIF ABL:섹스중독 == 1
		TIMES LOCAL:2 , 0.30
	ELSEIF ABL:섹스중독 == 2
		TIMES LOCAL:2 , 0.60
	ELSEIF ABL:섹스중독 == 3
		TIMES LOCAL:2 , 0.90
	ELSEIF ABL:섹스중독 == 4
		TIMES LOCAL:2 , 1.20
	ELSEIF ABL:섹스중독 == 5
		TIMES LOCAL:2 , 1.50
	ELSEIF ABL:섹스중독 == 6
		TIMES LOCAL:2 , 1.80
	ELSEIF ABL:섹스중독 == 7
		TIMES LOCAL:2 , 2.10
	ELSEIF ABL:섹스중독 == 8
		TIMES LOCAL:2 , 2.40
	ELSEIF ABL:섹스중독 == 9
		TIMES LOCAL:2 , 3.00
	ELSEIF ABL:섹스중독 >= 10
		TIMES LOCAL:2 , 5.00
	ENDIF

	SOURCE:TARGET:아픔 += LOCAL
	SOURCE:TARGET:반감 += LOCAL:1
	SOURCE:TARGET:중독충족 += LOCAL:2

	;-------------------------------------------------
	;사정체크
	;-------------------------------------------------

	;사정先타입の설정
	TCVAR:PLAYER:페니스사용대상 = TARGET
	TCVAR:PLAYER:사정개소 = 1
	;사정전선택の可・불가
	TFLAG:704 = 1
	;-------------------------------------------------
	;汚れの処理
	;-------------------------------------------------
	;사정に関係なく行われる処理はここから
	;질내に파과혈의 불결を付加
	SIF TALENT:처녀 == 1 || TALENT:재생처녀 == 1
		CALL SET_STAIN("질내", "파과혈", TARGET)
	;奴隷の질내⇔조교자のＰ의 불결が移動
	CALL MOVE_STAIN("질내", TARGET, "페니스", PLAYER)

	;-------------------------------------------------
	;吸精処理
	;-------------------------------------------------
	IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
		LOCAL = (RAND:10+1) * BASE:LV
		CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
	ELSEIF CFLAG:TARGET:ＭＡＧ비피흡수 == 0
		LOCAL = (RAND:10+1) * BASE:LV
		CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
	ENDIF


	;경험の処理
	;-------------------------------------------------
	;V경험
	CALL ADDS_EXP("Ｖ경험", 1, TARGET)

	;성교경험
	CALL ADDS_EXP("성교경험", 1, TARGET)
	CALL ADDS_EXP("삽입경험", 1, PLAYER)
	;수간경험
	IF (TALENT:PLAYER:짐승 || TALENT:PLAYER:새) && !(TALENT:TARGET:짐승 || TALENT:TARGET:새)
		CALL ADDS_EXP("수간경험", 1, TARGET)
	ENDIF

	;처녀で同性の場合이상경험
	IF TALENT:TARGET:처녀
		SIF !IS_MALE(PLAYER) 
			CALL ADDS_ABNORMAL_EXP("처녀탈취（동성）", PLAYER)
		SIF !IS_MALE(PLAYER)
			CALL ADDS_ABNORMAL_EXP("처녀상실（동성）", TARGET)
	ENDIF

	;근친상간の場合이상경험부여
	IF 근친체크(TARGET , PLAYER)
		CALL ADDS_ABNORMAL_EXP( @"근친상간（%RESULTS:0%）", TARGET)
		CALL ADDS_ABNORMAL_EXP( @"근친상간（%RESULTS:1%）", PLAYER)
	ENDIF

	;레즈경험
	A = 4
	IF IS_LESBIAN(TARGET,PLAYER)
		CALL ADDS_EXP("레즈경험", A, TARGET)
		CALL ADDS_EXP("레즈경험", A, PLAYER)
	ELSEIF IS_GAY(TARGET,PLAYER)
		CALL ADDS_EXP("ＢＬ경험", A, TARGET)
		CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
	ENDIF

	;애정경험
	IF TALENT:연모 == 1 && ASSIPLAY == 0
		CALL ADDS_EXP("애정경험", 2, TARGET)
		CALL ADDS_EXP("애정경험", 1, MASTER)
		CALL ADDS_EXP("봉사쾌락경험", 1, PLAYER)
	ELSEIF TALENT:친애 == 1 && ASSIPLAY == 0
		CALL ADDS_EXP("애정경험", 4, TARGET)
		CALL ADDS_EXP("애정경험", 1, MASTER)
		CALL ADDS_EXP("봉사쾌락경험", 1, PLAYER)
	ENDIF

	;의존도ベクトル
	TFLAG:33 = 1

	;의존도보정値(연모で1、친애で2）
	IF TALENT:연모 && ASSIPLAY == 0
		TFLAG:30 += 1
	ELSEIF TALENT:친애 && ASSIPLAY == 0
		TFLAG:30 += 2
	ENDIF
	

;(V挿入コマンドの地の文で挿入なし→挿入の分岐用）
ELSEIF TEQUIP:TARGET:Ｖ섹스 == 1
	TEQUIP:TARGET:Ｖ섹스 = 3
;(V挿入コマンドの地の文で挿入継続時に挿入を続ける文の分岐用)
ELSEIF TEQUIP:TARGET:Ｖ섹스 == 2
	TEQUIP:TARGET:Ｖ섹스 = 4
ENDIF
PRINTFORML ＜%CALLNAME:TARGET%에게 Ｖ삽입중＞


;----------------------------------------------------------------------
@EQUIP_P_INSERT_A_TARGET
;----------------------------------------------------------------------
IF TEQUIP:TARGET:Ａ섹스 > 0 && TEQUIP:TARGET:Ａ섹스 < 2

	B += 750
	;再装填処理中は사정ゲージは20分の1に
	SIF CFLAG:PLAYER:사정게이지재장전 > 0
		B /= 20

	SIF HAVE_PENIS(PLAYER)
		BASE:PLAYER:사정 += B

	;조교자←→조교대상
	TFLAG:28 = 3

	;-------------------------------------------------
	;소스추가処理
	;-------------------------------------------------
	;소비체력、기력
	DOWNBASE:TARGET:체력 += 40
	DOWNBASE:TARGET:기력 += 60

	SOURCE:TARGET:쾌Ａ += 200
	SOURCE:TARGET:정애 += 50
	SOURCE:TARGET:노출 += 30
	SOURCE:TARGET:일탈 += 100

	;조교자に쾌Ｃ
	SOURCE:PLAYER:쾌Ｃ += 300
	;-------------------------------------------------
	;特殊소스処理
	;-------------------------------------------------
	;억압、저항持ちには、억울소스추가
	SIF TALENT:억압 || TALENT:저항
		SOURCE:TARGET:울굴 += 250

	LOCAL = 400
	LOCAL:1 = 250
	LOCAL:2 = 100

	;정조관념持ちで연모、친애持ち이외には常に反発의 소스を추가する（섹스系のみ）
	SIF TALENT:정조관념 && !IS_LOVER(TARGET)
		SOURCE:TARGET:반감 += 500
		
	;Aへの苦痛は先に処理しておく
	;EXP:Ａ경험をみる
	IF EXP:Ａ경험 < EXPLV:1
		TIMES LOCAL , 3.00
		TIMES LOCAL:1 , 3.00
	ELSEIF EXP:Ａ경험 < EXPLV:2
		TIMES LOCAL , 1.20
		TIMES LOCAL:1 , 2.00
	ELSEIF EXP:Ａ경험 < EXPLV:3
		TIMES LOCAL , 0.80
		TIMES LOCAL:1 , 1.50
	ELSEIF EXP:Ａ경험 < EXPLV:4
		TIMES LOCAL , 0.60
		TIMES LOCAL:1 , 1.20
	ELSEIF EXP:Ａ경험 < EXPLV:5
		TIMES LOCAL , 0.40
	ELSEIF EXP:Ａ경험 >= EXPLV:5
		TIMES LOCAL , 0.20
	ENDIF
	
	;PALAM:윤활をみる
	IF PALAM:윤활 < PALAMLV:1
		TIMES LOCAL , 1.20
		TIMES LOCAL:1 , 2.00
	ELSEIF PALAM:윤활 < PALAMLV:2
		TIMES LOCAL , 1.00
		TIMES LOCAL:1 , 1.50
	ELSEIF PALAM:윤활 < PALAMLV:3
		TIMES LOCAL , 0.60
	ELSEIF PALAM:윤활 < PALAMLV:4
		TIMES LOCAL , 0.30
	ELSEIF PALAM:윤활 >= PALAMLV:4
		TIMES LOCAL , 0.20
	ENDIF

	;중독による중독충족의 소스は先に処理しておく
	;섹스중독を見る
	IF ABL:섹스중독 == 0
		TIMES LOCAL:2 , 0.00
	ELSEIF ABL:섹스중독 == 1
		TIMES LOCAL:2 , 0.30
	ELSEIF ABL:섹스중독 == 2
		TIMES LOCAL:2 , 0.60
	ELSEIF ABL:섹스중독 == 3
		TIMES LOCAL:2 , 0.90
	ELSEIF ABL:섹스중독 == 4
		TIMES LOCAL:2 , 1.20
	ELSEIF ABL:섹스중독 == 5
		TIMES LOCAL:2 , 1.50
	ELSEIF ABL:섹스중독 == 6
		TIMES LOCAL:2 , 1.80
	ELSEIF ABL:섹스중독 == 7
		TIMES LOCAL:2 , 2.10
	ELSEIF ABL:섹스중독 == 8
		TIMES LOCAL:2 , 2.40
	ELSEIF ABL:섹스중독 == 9
		TIMES LOCAL:2 , 3.00
	ELSEIF ABL:섹스중독 >= 10
		TIMES LOCAL:2 , 5.00
	ENDIF

	SOURCE:TARGET:아픔 += LOCAL
	SOURCE:TARGET:반감 += LOCAL:1
	SOURCE:TARGET:중독충족 += LOCAL:2

	;-------------------------------------------------
	;사정체크
	;-------------------------------------------------
	;사정先타입の설정
	TCVAR:PLAYER:페니스사용대상 = TARGET
	TCVAR:PLAYER:사정개소 = 2
	;사정전선택の可・불가
	TFLAG:704 = 1

	;-------------------------------------------------
	;汚れの処理
	;-------------------------------------------------
	;사정に関係なく行われる処理はここから
	;奴隷のＡ⇔조교자のＰ의 불결が移動
	CALL MOVE_STAIN("애널", TARGET, "페니스", PLAYER)

	;-------------------------------------------------
	;吸精処理
	;-------------------------------------------------
	IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
		LOCAL = (RAND:10+1) * BASE:LV
		CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
	ELSEIF CFLAG:TARGET:ＭＡＧ비피흡수 == 0
		LOCAL = (RAND:10+1) * BASE:LV
		CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
	ENDIF

	;-------------------------------------------------
	;경험の処理
	;-------------------------------------------------
	;A경험
	CALL ADDS_EXP("Ａ경험", 2, TARGET)

	;성교경험
	CALL ADDS_EXP("성교경험", 1, TARGET)
	CALL ADDS_EXP("삽입경험", 1, PLAYER)
	;수간경험
	IF (TALENT:PLAYER:짐승 || TALENT:PLAYER:새) && !(TALENT:TARGET:짐승 || TALENT:TARGET:새)
		CALL ADDS_EXP("수간경험", 1, TARGET)
	ENDIF

	;레즈경험
	A = 4
	IF IS_LESBIAN(TARGET,PLAYER)
		CALL ADDS_EXP("레즈경험", A, TARGET)
		CALL ADDS_EXP("레즈경험", A, PLAYER)
	ELSEIF IS_GAY(TARGET,PLAYER)
		CALL ADDS_EXP("ＢＬ경험", A, TARGET)
		CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
	ENDIF

	;애정경험
	IF TALENT:연모 == 1 && ASSIPLAY == 0
		CALL ADDS_EXP("애정경험", 1, TARGET)
		CALL ADDS_EXP("애정경험", 1, MASTER)
		CALL ADDS_EXP("봉사쾌락경험", 1, PLAYER)
	ELSEIF TALENT:친애 == 1 && ASSIPLAY == 0
		CALL ADDS_EXP("애정경험", 2, TARGET)
		CALL ADDS_EXP("애정경험", 1, MASTER)
		CALL ADDS_EXP("봉사쾌락경험", 1, PLAYER)
	ENDIF

	;의존도ベクトル
	TFLAG:33 = 1

	;의존도보정値(복종で1、예속で2）
	IF TALENT:복종 && ASSIPLAY == 0
		TFLAG:30 += 1
	ELSEIF TALENT:예속 && ASSIPLAY == 0
		TFLAG:30 += 2
	ENDIF
ENDIF
PRINTFORML ＜%CALLNAME:TARGET%에게 Ａ삽입중＞


;----------------------------------------------------------------------
@EQUIP_F_INSERT_V_TARGET
;----------------------------------------------------------------------
IF TEQUIP:TARGET:Ｖ손가락삽입 > 0 && TEQUIP:TARGET:Ｖ손가락삽입 < 2
	;조교자→조교대상
	TFLAG:28 = 2

	;-------------------------------------------------
	;소스추가処理
	;-------------------------------------------------
	;소비체력、기력
	DOWNBASE:TARGET:체력 += 15
	DOWNBASE:TARGET:기력 += 40


	IF TALENT:처녀 == 0 && TALENT:재생처녀 == 0
		SOURCE:TARGET:쾌Ｖ += 60
		SOURCE:TARGET:노출 += 5
		SOURCE:TARGET:일탈 += 10
		LOCAL = 7
		LOCAL:1 = 10
	;V경험が0の時はV경험を伴わない物に派生
	;V의 구슬は入らないが、快Vが高いと慣らしてあるため처녀상실時に影響
	ELSE
		SIF PALAM:쾌Ｖ < 100
			SOURCE:TARGET:쾌Ｖ += 30
		SOURCE:TARGET:노출 += 15
		SOURCE:TARGET:일탈 += 15
		LOCAL = 5
		LOCAL:1 += 15
	ENDIF


	;-------------------------------------------------
	;特殊소스処理
	;-------------------------------------------------
	;억압、저항持ちには、억울소스추가
	IF TALENT:억압 || TALENT:저항
		SOURCE:TARGET:울굴 += 10
		;처녀
		SIF TALENT:처녀
			SOURCE:TARGET:울굴 += 5
	ENDIF

	;Vへの고통は先に処理しておく
	;EXP:Ｖ경험をみる（처녀の반감は別途処理済みなので생략）
	IF EXP:Ｖ경험 < EXPLV:1
		TIMES LOCAL , 2.50
	ELSEIF EXP:Ｖ경험 < EXPLV:2
		TIMES LOCAL , 0.80
	ELSEIF EXP:Ｖ경험 < EXPLV:3
		TIMES LOCAL , 0.40
	ELSEIF EXP:Ｖ경험 < EXPLV:4
		TIMES LOCAL , 0.10
	ELSEIF EXP:Ｖ경험 < EXPLV:5
		TIMES LOCAL , 0.00
	ELSEIF EXP:Ｖ경험 >= EXPLV:5
		TIMES LOCAL , 0.00
	ENDIF

	;PALAM:윤활をみる
	IF PALAM:윤활 < PALAMLV:1
		TIMES LOCAL , 1.20
		TIMES LOCAL:1 , 1.50
	ELSEIF PALAM:윤활 < PALAMLV:2
		TIMES LOCAL , 0.70
		TIMES LOCAL:1 , 1.20
	ELSEIF PALAM:윤활 < PALAMLV:3
		TIMES LOCAL , 0.40
	ELSEIF PALAM:윤활 < PALAMLV:4
		TIMES LOCAL , 0.10
	ELSEIF PALAM:윤활 >= PALAMLV:4
		TIMES LOCAL , 0.10
	ENDIF

	SOURCE:TARGET:아픔 += LOCAL
	SOURCE:TARGET:반감 += LOCAL:1

	;-------------------------------------------------
	;汚れの処理
	;-------------------------------------------------
	;奴隷の질내⇔조교자の指의 불결が移動
	CALL MOVE_STAIN("질내", TARGET, "손", PLAYER)

	;-------------------------------------------------
	;吸精処理
	;-------------------------------------------------
	IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
		LOCAL = (RAND:4+1) * BASE:LV
		CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
	ELSEIF CFLAG:TARGET:ＭＡＧ비피흡수 == 0
		LOCAL = (RAND:4+1) * BASE:LV
		CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
	ENDIF

	;-------------------------------------------------
	;경험の処理
	;-------------------------------------------------
	;V경험
	SIF TALENT:처녀 == 0 && TALENT:재생처녀 == 0
		CALL ADDS_EXP("Ｖ경험", 1, TARGET)

	;레즈경험
	A = 1
	IF IS_LESBIAN(TARGET,PLAYER)
		CALL ADDS_EXP("레즈경험", A, TARGET)
		CALL ADDS_EXP("레즈경험", A, PLAYER)
	ELSEIF IS_GAY(TARGET,PLAYER)
		CALL ADDS_EXP("ＢＬ경험", A, TARGET)
		CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
	ENDIF

	;의존도ベクトル
	IF TALENT:처녀
		TFLAG:33 = 3
	ELSE
		TFLAG:33 = 1
	ENDIF
ENDIF
PRINTFORML ＜ %CALLNAME:TARGET%에게 손가락Ｖ삽입중 ＞


;----------------------------------------------------------------------
@EQUIP_F_INSERT_A_TARGET
;----------------------------------------------------------------------
