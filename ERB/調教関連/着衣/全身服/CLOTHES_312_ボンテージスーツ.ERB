
@CLOTHES_NAME_312
RESULTS = 본디지수트

;노출부위
@CLOTHES_EXPOSE_312,ARG
;ARG = 노출되는부위
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = 배꼽노출 7 = 음순노출 8 = 둔부노출
SELECTCASE ARG
	CASE 0
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비부위
@CLOTHES_PART_312
RETURN 3

;옷의 앞쪽이 열리는가
;1 열 수 있다、2 열린 채
@CLOTHES_OPENFRONT_312
RETURN 0

;가랑이의구조
;1 = 바지、2 = 비키니타입、3 = 스커트、4 = 구멍뚫림、5 앞열림、6 개폐가능
@CLOTHES_CROTCH_312
RETURN 4

;장비시의 조교효과
@CLOTHES_SOURCE_312,ARG = -1
RETURN 0

;장비시의 경험추가
@CLOTHES_EXP_312,ARG = -1
IF FLAG:SHOP커맨드 == 0
	SIF ARG == -1
		RETURN 0
	PRINTFORML <본디지수트ckrdydwnd>\@ ARG == MASTER ? (wndls) #\@\@ ARG == ASSI ? (조수) #\@
	IF TCVAR:ARG:획득가학쾌락경험
		TCVAR:ARG:획득가학쾌락경험 += MAX(TCVAR:ARG:획득가학쾌락경험*140/100 , 1)
		SIF ARG == TARGET
			TIMES DOWNBASE:ARG:1 , 1.20
	ENDIF
	IF TCVAR:ARG:획득고통쾌락경험
		TCVAR:ARG:획득고통쾌락경험 += MAX(TCVAR:ARG:획득고통쾌락경험*140/100 , 1)
		SIF ARG == TARGET
			TIMES DOWNBASE:ARG:1 , 1.20
	ENDIF
ELSE
ENDIF


@CLOTHES_PRICE_312
RETURN 2000
