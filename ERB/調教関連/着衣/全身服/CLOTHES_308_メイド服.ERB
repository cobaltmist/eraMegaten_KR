@CLOTHES_PRICE_308
RETURN 1000

;촉각
;1 = スルー 2 = 上から伝わる 4 = 潜り込ませられる
;ARG 0 = Ｃ 1 = Ｖ 2 = Ａ 3 = 유두 4 = 유방
@CLOTHES_촉각_308,ARG
SELECTCASE ARG
	CASE 3,4
		RETURN 2
ENDSELECT
