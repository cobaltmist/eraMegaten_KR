
@CLOTHES_NAME_1034
RESULTS = 산타코트

;노출부위
@CLOTHES_EXPOSE_1034,ARG
;ARG = 노출되는부위
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = 배꼽노출 7 = 음순노출 8 = 둔부노출

SELECTCASE ARG
	CASE 1,3,4,5,6,7,8
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비부위
@CLOTHES_PART_1034
RETURN 10

;옷의 앞쪽이 열리는가
;1 열 수 있다、2 열린 채
@CLOTHES_OPENFRONT_1034
RETURN 1

;고간의구조
;1 = 바지、2 = 비키니타입、3 = 스커트、4 = 구멍뚫림、5 앞열림、6 개폐가능
@CLOTHES_CROTCH_1034
RETURN 5

;장비시의 조교효과
@CLOTHES_SOURCE_1034,ARG = -1
RETURN 0

;장비시의 경험추가
@CLOTHES_EXP_1034,ARG = -1
		RETURN 0

;装備するための条件
@CLOTHES_PREREQUISITE_1034,ARG
SIF TEQUIP:ARG:옷 == 1
	RETURN 0
RETURN 1



@CLOTHES_PRICE_1034
