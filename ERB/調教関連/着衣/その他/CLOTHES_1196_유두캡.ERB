
@CLOTHES_NAME_1196
RESULTS = 유두캡

;노출부위
@CLOTHES_EXPOSE_1196,ARG
;ARG = 노출되는부위
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = 배꼽노출 7 = 음순노출 8 = 둔부노출


SELECTCASE ARG
	CASE 0
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비부위
@CLOTHES_PART_1196
RETURN 11

;옷의 앞쪽이 열리는가
;1 열 수 있다、2 열린 채
@CLOTHES_OPENFRONT_1196
RETURN 0

;가랑이의구조
;1 = 바지、2 = 비키니타입、3 = 스커트、4 = 구멍뚫림、5 앞열림、6 개폐가능
@CLOTHES_CROTCH_1196
RETURN 0

;장비시의 조교효과
@CLOTHES_SOURCE_1196,ARG
IF FLAG:SHOP커맨드 == 0
	SOURCE:ARG:쾌Ｂ += 125
	SOURCE:ARG:노출 += 10
	SOURCE:ARG:일탈 += 20

	;억압、저항持ちには、억울ソース추가
	SIF TALENT:ARG:억압 || TALENT:ARG:저항
		SOURCE:ARG:울굴 += 20
		
ENDIF

;장비시의 경험추가
@CLOTHES_EXP_1196,ARG
IF FLAG:SHOP커맨드 == 0
	SIF ARG == -1
		RETURN 0
ELSE

ENDIF


;장비するための조건 - 몰래 뗄 수 있는거라 좀 쎄게 잡아둠
@CLOTHES_PREREQUISITE_1196,ARG
;니플피어스특성 이나 유두캡 장착시 유두캡 추가 장착 불가능
SIF TALENT:ARG:유두피어스
	RETURN 0
SIF (CFLAG:ARG:그외 == 1196 || CFLAG:ARG:그외2 == 1196 || CFLAG:ARG:그외3 == 1196 || CFLAG:ARG:그외4 == 1196 || CFLAG:ARG:그외5 == 1196 || CFLAG:ARG:그외6 == 1196)
	RETURN 0
SIF (CFLAG:ARG:그외 == 1200 || CFLAG:ARG:그외2 == 1200 || CFLAG:ARG:그외3 == 1200 || CFLAG:ARG:그외4 == 1200 || CFLAG:ARG:그외5 == 1200 || CFLAG:ARG:그외6 == 1200)
	RETURN 0
SIF ARG == MASTER
	RETURN 1
SIF ABL:ARG:순종 >= 5
	RETURN 1
SIF ABL:ARG:마조끼 >= 2
	RETURN 1
SIF MARK:ARG:굴복각인 >= 3
	RETURN 1
RETURN 0

@CLOTHES_PRICE_1196
RETURN 2000
