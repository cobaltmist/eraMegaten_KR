@CLOTHES_NAME_1176
RESULTS = 사용한콘돔

;노출부위
@CLOTHES_EXPOSE_1176,ARG
;ARG = 노출되는부위
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = 배꼽노출 7 = 음순노출 8 = 둔부노출


SELECTCASE ARG
	CASE 0
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비부위
@CLOTHES_PART_1176
RETURN 11

;옷의 앞쪽이 열리는가
;1 열 수 있다、2 열린 채
@CLOTHES_OPENFRONT_1176
RETURN 0

;고간의구조
;1 = 바지、2 = 비키니타입、3 = 스커트、4 = 구멍뚫림、5 앞열림、6 개폐가능
@CLOTHES_CROTCH_1176
RETURN 0


;장비시의 조교효과
@CLOTHES_SOURCE_1176,ARG = -1
IF FLAG:SHOP커맨드 == 0
	SIF ARG == TARGET
		TFLAG:19 = 1
	SOURCE:ARG:정애 += (ABL:ARG:정액중독 * 4)
	SOURCE:ARG:중독충족 += 10 + (ABL:ARG:정액중독 * 4)
	SOURCE:ARG:불결 += 10 - ABL:ARG:정액중독
	SOURCE:ARG:일탈 += 25
	SOURCE:ARG:반감 += 40

	;抑圧、抵抗持ちには、抑鬱ソース追加
	SIF TALENT:ARG:억압 || TALENT:ARG:저항
		SOURCE:ARG:울굴 += 10
ELSE
ENDIF

;장비시의 경험추가
@CLOTHES_EXP_1176,ARG = -1
IF FLAG:SHOP커맨드 == 0
	SIF ARG == -1
		RETURN 0
	PRINTFORML <사용한콘돔착용중>\@ ARG == MASTER ? (주인) #\@\@ ARG == ASSI ? (조수) #\@
		CALL ADDS_EXP("정액경험", 1, ARG)
	ELSE
	ENDIF

;装備するための条件
@CLOTHES_PREREQUISITE_1176,ARG
SIF ARG == MASTER
	RETURN 1
SIF ABL:ARG:정액중독 >= 1
	RETURN 1
RETURN 0

@CLOTHES_PRICE_1176
RETURN 100
