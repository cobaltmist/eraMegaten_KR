
@CLOTHES_NAME_1104
RESULTS = 구속구

;노출부위
@CLOTHES_EXPOSE_1104,ARG
;ARG = 노출되는부위
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = 배꼽노출 7 = 음순노출 8 = 둔부노출


SELECTCASE ARG
	CASE 5,7,8
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비부위
@CLOTHES_PART_1104
RETURN 11

;옷의 앞쪽이 열리는가
;1 열 수 있다、2 열린 채
@CLOTHES_OPENFRONT_1104
RETURN 0

;가랑이의구조
;1 = 바지、2 = 비키니타입、3 = 스커트、4 = 구멍뚫림、5 앞열림、6 개폐가능
@CLOTHES_CROTCH_1104
RETURN 2


;장비시의 조교효과
@CLOTHES_SOURCE_1104,ARG = -1
RETURN 0

;장비시의 경험추가
@CLOTHES_EXP_1104,ARG = -1
IF FLAG:SHOP커맨드 == 0
	SIF ARG == -1
		RETURN 0
	PRINTFORML <구속구착용중>\@ ARG == MASTER ? (주인) #\@\@ ARG == ASSI ? (조수) #\@
	IF TCVAR:ARG:획득고통쾌락경험
		TCVAR:ARG:획득고통쾌락경험 += MAX(TCVAR:ARG:획득고통쾌락경험*140/100 , 1)
		SIF ARG == TARGET
			TIMES DOWNBASE:ARG:1 , 1.10
	ENDIF
ELSE

ENDIF

@CLOTHES_PRICE_1104
RETURN 1000

;촉각
;1 = スルー 2 = 上から伝わる 4 = 潜り込ませられる
;ARG 0 = Ｃ 1 = Ｖ 2 = Ａ 3 = 유두 4 = 유방
@CLOTHES_촉각_1104,ARG
SELECTCASE ARG
	CASE 0 TO 4
		RETURN 4
ENDSELECT
