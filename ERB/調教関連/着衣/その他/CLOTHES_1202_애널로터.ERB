
@CLOTHES_NAME_1202
RESULTS = 애널로터삽입

;노출부위
@CLOTHES_EXPOSE_1202,ARG
;ARG = 노출되는부위
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = 배꼽노출 7 = 음순노출 8 = 둔부노출


SELECTCASE ARG
	CASE 8
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비부위
@CLOTHES_PART_1202
RETURN 11

;옷의 앞쪽이 열리는가
;1 열 수 있다、2 열린 채
@CLOTHES_OPENFRONT_1202
RETURN 0

;가랑이의구조
;1 = 바지、2 = 비키니타입、3 = 스커트、4 = 구멍뚫림、5 앞열림、6 개폐가능
@CLOTHES_CROTCH_1202
RETURN 2

;장비시의 조교효과
@CLOTHES_SOURCE_1202,ARG
IF FLAG:SHOP커맨드 == 0
	IF ARG == TARGET
		TFLAG:19 = 1
	ENDIF
	SOURCE:ARG:쾌Ａ += 50
	SOURCE:ARG:노출 += 10
	SOURCE:ARG:일탈 += 20

	;억압、저항持ちには、억울ソース추가
	SIF TALENT:ARG:억압 || TALENT:ARG:저항
		SOURCE:ARG:울굴 += 20
		
	;Vへの고통は先に処理しておく
	;EXP:ARG:Ａ경험をみる（처녀はありえない）
	A = 25
	B = 20
	IF EXP:ARG:0 < EXPLV:2
		TIMES A , 0.80
		TIMES B , 1.20
	ELSEIF EXP:ARG:0 < EXPLV:3
		TIMES A , 0.40
	ELSEIF EXP:ARG:0 < EXPLV:4
		TIMES A , 0.10
	ELSEIF EXP:ARG:0 < EXPLV:5
		TIMES A , 0.00
	ELSEIF EXP:ARG:0 >= EXPLV:5
		TIMES A , 0.00
	ENDIF

	;PALAM:ARG:윤활をみる
	IF PALAM:ARG:4 < PALAMLV:1
		TIMES A , 1.20
		TIMES B , 1.50
	ELSEIF PALAM:ARG:4 < PALAMLV:2
		TIMES A , 0.70
		TIMES B , 1.20
	ELSEIF PALAM:ARG:4 < PALAMLV:3
		TIMES A , 0.40
	ELSEIF PALAM:ARG:4 < PALAMLV:4
		TIMES A , 0.10
	ELSEIF PALAM:ARG:4 >= PALAMLV:4
		TIMES A , 0.10
	ENDIF
	SOURCE:ARG:7 += A
	SOURCE:ARG:18 += B
	IF ARG == TARGET
		DOWNBASE:1 += 50
		DOWNBASE:0 += 10
	ENDIF
	
ELSE
ENDIF

;장비시의 경험추가
@CLOTHES_EXP_1202,ARG
IF FLAG:SHOP커맨드 == 0
	SIF ARG == -1
		RETURN 0
	PRINTFORML <Ａ바이브 삽입중>\@ ARG == MASTER ? (주인) #\@\@ ARG == ASSI ? (조수) #\@
	IF SELECTCOM == 11
		CALL ADDS_EXP("Ａ경험", 2, ARG)
	ELSE
		CALL ADDS_EXP("Ａ경험", 1, ARG)
	ENDIF
ELSE

ENDIF


;장비するための조건 - 로터라 제한을 적게둠
@CLOTHES_PREREQUISITE_1202,ARG
SIF IS_MALE(ARG)
	RETURN 0
;바이브달린 정조대나 양구멍 정조대면 추가 삽입 불능
SIF (CFLAG:ARG:속옷（하） == 624 || CFLAG:ARG:속옷（하） == 626)
	RETURN 0
SIF (CFLAG:ARG:그외 == 1198 || CFLAG:ARG:그외2 == 1198 || CFLAG:ARG:그외3 == 1198 || CFLAG:ARG:그외4 == 1198 || CFLAG:ARG:그외5 == 1198 || CFLAG:ARG:그외6 == 1198)
	RETURN 0
SIF ARG == MASTER
	RETURN 1
SIF EXP:ARG:Ａ경험 < 30 && ABL:ARG:Ａ감각 < 2
	RETURN 0
SIF TALENT:ARG:소인체형 && EXP:ARG:Ａ확장경험 < 3 && TALENT:MASTER:금단의지식 == 0
	RETURN 0
SIF ABL:ARG:순종 >= 5
	RETURN 1
SIF ABL:ARG:마조끼 >= 3
	RETURN 1
SIF ABL:ARG:Ａ감각 >= 3
	RETURN 1
SIF MARK:ARG:굴복각인 >= 3
	RETURN 1
RETURN 0

@CLOTHES_PRICE_1202
RETURN 2000
