
@CLOTHES_NAME_624
RESULTS = 바이브달린정조대

;노출부위
@CLOTHES_EXPOSE_624,ARG
;ARG = 노출되는부위
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = 배꼽노출 7 = 음순노출 8 = 둔부노출


SELECTCASE ARG
	CASE 7,8
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비부위
@CLOTHES_PART_624
RETURN 6

;옷의 앞쪽이 열리는가
;1 열 수 있다、2 열린 채
@CLOTHES_OPENFRONT_624
RETURN 0

;고간의구조
;1 = 바지、2 = 비키니타입、3 = 스커트、4 = 구멍뚫림、5 앞열림、6 개폐가능
@CLOTHES_CROTCH_624
RETURN 2

;장비시의 조교효과
@CLOTHES_SOURCE_624,ARG
IF FLAG:SHOP커맨드 == 0
	IF ARG == TARGET
		TFLAG:19 = 1
	ENDIF
	SOURCE:ARG:쾌Ｖ += 125
	SOURCE:ARG:노출 += 10
	SOURCE:ARG:일탈 += 20

	;抑圧、抵抗持ちには、抑鬱ソース追加
	SIF TALENT:ARG:억압 || TALENT:ARG:저항
		SOURCE:ARG:울굴 += 20
		
	;Vへの苦痛は先に処理しておく
	;EXP:ARG:Ｖ경험をみる（処女はありえない）
	A = 25
	B = 20
	IF EXP:ARG:0 < EXPLV:2
		TIMES A , 0.80
		TIMES B , 1.20
	ELSEIF EXP:ARG:0 < EXPLV:3
		TIMES A , 0.40
	ELSEIF EXP:ARG:0 < EXPLV:4
		TIMES A , 0.10
	ELSEIF EXP:ARG:0 < EXPLV:5
		TIMES A , 0.00
	ELSEIF EXP:ARG:0 >= EXPLV:5
		TIMES A , 0.00
	ENDIF

	;PALAM:ARG:윤활をみる
	IF PALAM:ARG:4 < PALAMLV:1
		TIMES A , 1.20
		TIMES B , 1.50
	ELSEIF PALAM:ARG:4 < PALAMLV:2
		TIMES A , 0.70
		TIMES B , 1.20
	ELSEIF PALAM:ARG:4 < PALAMLV:3
		TIMES A , 0.40
	ELSEIF PALAM:ARG:4 < PALAMLV:4
		TIMES A , 0.10
	ELSEIF PALAM:ARG:4 >= PALAMLV:4
		TIMES A , 0.10
	ENDIF
	SOURCE:ARG:7 += A
	SOURCE:ARG:18 += B
	IF ARG == TARGET
		DOWNBASE:1 += 50
		DOWNBASE:0 += 10
	ENDIF
	
ELSE
ENDIF

;장비시의 경험추가
@CLOTHES_EXP_624,ARG
IF FLAG:SHOP커맨드 == 0
	SIF ARG == -1
		RETURN 0
	PRINTFORML <바이브삽입중>\@ ARG == MASTER ? (주인) #\@\@ ARG == ASSI ? (조수) #\@
	IF SELECTCOM == 11
		CALL ADDS_EXP("Ｖ경험", 2, ARG)
	ELSE
		CALL ADDS_EXP("Ｖ경험", 1, ARG)
	ENDIF
ELSE

ENDIF


;装備するための条件
@CLOTHES_PREREQUISITE_624,ARG
SIF IS_MALE(ARG)
	RETURN 0
SIF TALENT:ARG:1 || TALENT:ARG:2
	RETURN 0
SIF ARG == MASTER
	RETURN 1
SIF EXP:ARG:Ｖ경험 < 10
	RETURN 0
SIF TALENT:ARG:소인체형 && EXP:ARG:Ｖ확장경험 < 3 && TALENT:MASTER:금단의지식 == 0
	RETURN 0
SIF ABL:ARG:순종 >= 3
	RETURN 1
SIF ABL:ARG:마조끼 >= 2
	RETURN 1
SIF ABL:ARG:Ｖ감각 >= 2
	RETURN 1
RETURN 0

@CLOTHES_PRICE_624
RETURN 2000
