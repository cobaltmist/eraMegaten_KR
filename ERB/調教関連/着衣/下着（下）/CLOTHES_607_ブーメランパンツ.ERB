
@CLOTHES_NAME_607
RESULTS = 부메랑팬츠

;노출부위
@CLOTHES_EXPOSE_607,ARG
;ARG = 노출되는부위
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = 배꼽노출 7 = 음순노출 8 = 둔부노출


SELECTCASE ARG
	CASE 7,8
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비부위
@CLOTHES_PART_607
RETURN 6

;옷의 앞쪽이 열리는가
;1 열 수 있다、2 열린 채
@CLOTHES_OPENFRONT_607
RETURN 0

;고간의구조
;1 = 바지、2 = 비키니타입、3 = 스커트、4 = 구멍뚫림、5 앞열림、6 개폐가능
@CLOTHES_CROTCH_607
RETURN 2

;장비시의 조교효과
@CLOTHES_SOURCE_607,ARG = -1
RETURN 0

;장비시의 경험추가
@CLOTHES_EXP_607,ARG = -1
		RETURN 0

@CLOTHES_PRICE_607
RETURN 1000

;촉각
;1 = 통과  2 = 위에서부터 전해진다  4 = 집어넣을수 있다
;ARG 0 = Ｃ 1 = Ｖ 2 = Ａ 3 = 유두 4 = 유방
@CLOTHES_촉각_607,ARG
SELECTCASE ARG
	CASE 0 TO 2
		RETURN 6
ENDSELECT
