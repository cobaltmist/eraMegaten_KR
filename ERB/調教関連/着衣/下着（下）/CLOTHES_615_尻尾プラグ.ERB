
@CLOTHES_NAME_615
RESULTS = 꼬리플러그

;노출부위
@CLOTHES_EXPOSE_615,ARG
;ARG = 노출되는부위
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = 배꼽노출 7 = 음순노출 8 = 둔부노출


SELECTCASE ARG
	CASE 8
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비부위
@CLOTHES_PART_615
RETURN 6

;옷의 앞쪽이 열리는가
;1 열 수 있다、2 열린 채
@CLOTHES_OPENFRONT_615
RETURN 0

;고간의구조
;1 = 바지、2 = 비키니타입、3 = 스커트、4 = 구멍뚫림、5 앞열림、6 개폐가능
@CLOTHES_CROTCH_615
RETURN 4

;장비시의 조교효과
@CLOTHES_SOURCE_615,ARG
IF FLAG:SHOP커맨드 == 0
	SOURCE:ARG:쾌Ａ += 65
	SOURCE:ARG:아픔 += 100
	SOURCE:ARG:노출 += 50
	SOURCE:ARG:일탈 += 50
	SOURCE:ARG:반감 += 100

	;抑圧、抵抗持ちには、抑鬱ソース追加
	SIF TALENT:ARG:억압 || TALENT:ARG:저항
		SOURCE:ARG:울굴 += 50
		
	;Vへの苦痛は先に処理しておく
	;EXP:ARG:Ｖ경험をみる（処女はありえない）
	A = 100
	B = 40
	IF EXP:ARG:1 < EXPLV:1
		TIMES A , 3.00
		TIMES B , 3.00
	ELSEIF EXP:ARG:1 < EXPLV:2
		TIMES A , 1.20
		TIMES B , 2.00
	ELSEIF EXP:ARG:1 < EXPLV:3
		TIMES A , 0.80
		TIMES B , 1.50
	ELSEIF EXP:ARG:1 < EXPLV:4
		TIMES A , 0.60
		TIMES B , 1.20
	ELSEIF EXP:ARG:1 < EXPLV:5
		TIMES A , 0.40
	ELSEIF EXP:ARG:1 >= EXPLV:5
		TIMES A , 0.20
	ENDIF


	;PALAM:ARG:윤활をみる
	IF PALAM:ARG:4 < PALAMLV:1
		TIMES A , 1.20
		TIMES B , 2.00
	ELSEIF PALAM:ARG:4 < PALAMLV:2
		TIMES A , 1.00
		TIMES B , 1.50
	ELSEIF PALAM:ARG:4 < PALAMLV:3
		TIMES A , 0.60
	ELSEIF PALAM:ARG:4 < PALAMLV:4
		TIMES A , 0.30
	ELSEIF PALAM:ARG:4 >= PALAMLV:4
		TIMES A , 0.20
	ENDIF
	SOURCE:ARG:7 += A
	SOURCE:ARG:18 += B
	IF ARG == TARGET
		DOWNBASE:ARG:0 += 10
		DOWNBASE:ARG:1 += 90
	ENDIF
	
ELSE
ENDIF

;장비시의 경험추가
@CLOTHES_EXP_615,ARG
IF FLAG:SHOP커맨드 == 0
	SIF ARG == -1
		RETURN 0
	PRINTFORML <꼬리플러그삽입중>\@ ARG == MASTER ? (주인) #\@\@ ARG == ASSI ? (조수) #\@
	CALL ADDS_EXP("Ａ경험", 1, ARG)
ELSE

ENDIF



;装備するための条件
@CLOTHES_PREREQUISITE_615,ARG
SIF ARG == MASTER
	RETURN 1
SIF EXP:ARG:Ａ경험 < 15 && ABL:ARG:Ａ감각 < 1
	RETURN 0
SIF TALENT:ARG:소인체형 && EXP:ARG:Ａ확장경험 < 1
	RETURN 0
SIF ABL:ARG:순종 >= 3
	RETURN 1
SIF ABL:ARG:마조끼 >= 2
	RETURN 1
SIF ABL:ARG:Ａ감각 >= 2
	RETURN 1
SIF ABL:ARG:노출증 >= 2
	RETURN 1
RETURN 0

@CLOTHES_PRICE_615
RETURN 1500
