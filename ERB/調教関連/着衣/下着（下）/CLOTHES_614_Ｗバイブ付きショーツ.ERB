
@CLOTHES_NAME_614
RESULTS = Ｗ바이브달린쇼츠

;노출部位
@CLOTHES_EXPOSE_614,ARG
;ARG = 노출する部位
;1 = 팔노출 2 = 발노출 3 = 다리노출 4 = 유방노출 5 = 유두노출 6 = ヘソ노출 7 = 음순노출 8 = 둔부노출


SELECTCASE ARG
	CASE 7,8
		RETURN 0
	CASEELSE
		RETURN 1
ENDSELECT

;장비部位
@CLOTHES_PART_614
RETURN 6

;옷の前が開くか
;1 開けられる、2 開きっぱなし
@CLOTHES_OPENFRONT_614
RETURN 0

;가랑이の구조
;1 = 바지、2 = ビキニ타입、3 = 스커트、4 = 穴開き、5 앞열림、6 開閉가능
@CLOTHES_CROTCH_614
RETURN 2

;장비時の조교効果
@CLOTHES_SOURCE_614,ARG
IF FLAG:SHOPコマンド == 0
	IF ARG == TARGET
		TFLAG:19 = 1
	ENDIF
	SOURCE:ARG:쾌Ｖ += 125
	SOURCE:ARG:노출 += 10
	SOURCE:ARG:일탈 += 20

	;억압、저항持ちには、억울ソース추가
	SIF TALENT:ARG:억압 || TALENT:ARG:저항
		SOURCE:ARG:울굴 += 20
		
	;Vへの고통は先に処理しておく
	;EXP:ARG:Ｖ경험をみる（처녀はありえない）
	A = 25
	B = 20
	IF EXP:ARG:0 < EXPLV:2
		TIMES A , 0.80
		TIMES B , 1.20
	ELSEIF EXP:ARG:0 < EXPLV:3
		TIMES A , 0.40
	ELSEIF EXP:ARG:0 < EXPLV:4
		TIMES A , 0.10
	ELSEIF EXP:ARG:0 < EXPLV:5
		TIMES A , 0.00
	ELSEIF EXP:ARG:0 >= EXPLV:5
		TIMES A , 0.00
	ENDIF

	;PALAM:ARG:윤활をみる
	IF PALAM:ARG:4 < PALAMLV:1
		TIMES A , 1.20
		TIMES B , 1.50
	ELSEIF PALAM:ARG:4 < PALAMLV:2
		TIMES A , 0.70
		TIMES B , 1.20
	ELSEIF PALAM:ARG:4 < PALAMLV:3
		TIMES A , 0.40
	ELSEIF PALAM:ARG:4 < PALAMLV:4
		TIMES A , 0.10
	ELSEIF PALAM:ARG:4 >= PALAMLV:4
		TIMES A , 0.10
	ENDIF
	SOURCE:ARG:7 += A
	SOURCE:ARG:18 += B
	
	SOURCE:ARG:쾌Ａ += 125
	SOURCE:ARG:아픔 += 100
	SOURCE:ARG:노출 += 25
	SOURCE:ARG:일탈 += 25
	SOURCE:ARG:반감 += 40

	;억압、저항持ちには、억울ソース추가
	SIF TALENT:ARG:억압 || TALENT:ARG:저항
		SOURCE:ARG:울굴 += 20
		
	;Vへの고통は先に処理しておく
	;EXP:ARG:Ｖ경험をみる（처녀はありえない）
	A = 100
	B = 40
	IF EXP:ARG:1 < EXPLV:1
		TIMES A , 3.00
		TIMES B , 3.00
	ELSEIF EXP:ARG:1 < EXPLV:2
		TIMES A , 1.20
		TIMES B , 2.00
	ELSEIF EXP:ARG:1 < EXPLV:3
		TIMES A , 0.80
		TIMES B , 1.50
	ELSEIF EXP:ARG:1 < EXPLV:4
		TIMES A , 0.60
		TIMES B , 1.20
	ELSEIF EXP:ARG:1 < EXPLV:5
		TIMES A , 0.40
	ELSEIF EXP:ARG:1 >= EXPLV:5
		TIMES A , 0.20
	ENDIF


	;PALAM:ARG:윤활をみる
	IF PALAM:ARG:4 < PALAMLV:1
		TIMES A , 1.20
		TIMES B , 2.00
	ELSEIF PALAM:ARG:4 < PALAMLV:2
		TIMES A , 1.00
		TIMES B , 1.50
	ELSEIF PALAM:ARG:4 < PALAMLV:3
		TIMES A , 0.60
	ELSEIF PALAM:ARG:4 < PALAMLV:4
		TIMES A , 0.30
	ELSEIF PALAM:ARG:4 >= PALAMLV:4
		TIMES A , 0.20
	ENDIF
	SOURCE:ARG:7 += A
	SOURCE:ARG:18 += B
	IF ARG == TARGET
		DOWNBASE:ARG:0 += 30
		DOWNBASE:ARG:1 += 80
		DOWNBASE:ARG:1 += 50
		DOWNBASE:ARG:0 += 10
	ENDIF
	
ELSE
ENDIF


;장비時の경험추가
@CLOTHES_EXP_614,ARG
IF FLAG:SHOPコマンド == 0
	SIF ARG == -1
		RETURN 0
	PRINTFORML <Ｗ바이브삽입중>\@ ARG == MASTER ? (주인) #\@\@ ARG == ASSI ? (조수) #\@
	IF SELECTCOM == 11
		CALL ADDS_EXP("Ｖ경험", 2, ARG)
		CALL ADDS_EXP("Ａ경험", 2, ARG)
	ELSE
		CALL ADDS_EXP("Ｖ경험", 1, ARG)
		CALL ADDS_EXP("Ａ경험", 1, ARG)
	ENDIF
	
ELSE
ENDIF

;장비するための조건
@CLOTHES_PREREQUISITE_614,ARG
SIF IS_MALE(ARG)
	RETURN 0
SIF TALENT:ARG:1 || TALENT:ARG:2
	RETURN 0
SIF ARG == MASTER
	RETURN 1
SIF EXP:ARG:Ｖ경험 < 10
	RETURN 0
SIF EXP:ARG:Ａ경험 < 30 && ABL:ARG:Ａ감각 < 2
	RETURN 0
SIF TALENT:ARG:소인체형 && EXP:ARG:Ｖ확장경험 < 3 && EXP:ARG:Ａ확장경험 < 3 && TALENT:MASTER:금단의지식 == 0
	RETURN 0
SIF ABL:ARG:순종 >= 3
	RETURN 1
SIF ABL:ARG:마조끼 >= 3
	RETURN 1
SIF ABL:ARG:Ｖ감각 >= 2 && ABL:ARG:Ａ감각 >= 3
	RETURN 1
RETURN 0

@CLOTHES_PRICE_614
RETURN 4000
