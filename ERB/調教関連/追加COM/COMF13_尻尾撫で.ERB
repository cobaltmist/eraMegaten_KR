;-------------------------------------------------
;꼬리애무
;애무계コマンド　레벨2
;-------------------------------------------------
@CHECK_COM_ABLE13

SIF !USE_HAND(PLAYER)
	RETURN 0

SIF !USE_TAIL(TARGET)
	RETURN 0

	RETURN 1

@ACT_COM13

PRINTL 꼬리애무
TSTR:0 = 꼬리애무
;CALL KOJO_MESSAGE_COM

;조교者→조교대상
TFLAG:28 = 2

;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 5
DOWNBASE:TARGET:기력 += 30

SOURCE:TARGET:정애 = 75
SOURCE:TARGET:성행위 = 30
SOURCE:TARGET:노출 = 10
SOURCE:TARGET:반감 = 20

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 20

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:3+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:3+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF


;-------------------------------------------------
;경험の処理
;-------------------------------------------------
A = 3
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;의존도ベクトル
TFLAG:33 = 2

RETURN 1

@MESSAGE_COM13
IF TEQUIP:체위 == 2 || TEQUIP:체위 == 3 || TEQUIP:체위 == 4 || TEQUIP:체위 == 5 || TEQUIP:체위 == 6 || TEQUIP:체위 == 11 || TEQUIP:체위 == 12 || TEQUIP:체위 == 13 || TEQUIP:체위 == 14 || TEQUIP:체위 == 15
	PRINTFORM %조사선택(CALLNAME:PLAYER, "는")% %조사선택(CALLNAME:TARGET, "을")% 범하면서 허리의 움직임에 따라 흔들리는  %CALLNAME:TARGET%의 
	PRINTL
ELSEIF TEQUIP:체위 == 25
	PRINTFORM %조사선택(CALLNAME:PLAYER, "는")% %조사선택(CALLNAME:ASSI, "을")% 범하는데 빠져있는 %CALLNAME:TARGET%의 
ENDIF
PRINTFORMW 꼬리를 가볍게 깨물면서 정성스럽게 애무했다……
;===================================================
;조교타입
;===================================================
@COMTYPE_13
RETURN COMTYPE("애무계")

