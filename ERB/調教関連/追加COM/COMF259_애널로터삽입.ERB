;-------------------------------------------------
;애널로터
;道具사용コマンド、레벨2
;-------------------------------------------------
@CHECK_COM_ABLE259
;애널비즈実行판정
;촉수조교中はダメ
SIF TEQUIP:25
	RETURN 0

;삼각목마騎乗中はダメ
SIF TEQUIP:41
	RETURN 0

;해제はいつでも가능
SIF TEQUIP:애널로터
	RETURN 1
;소인체형持ちは、双方が조교자と조교대상ならOK
;それ以外の場合は主人が금단의지식持ちじゃないとダメ
SIF TALENT:142 && (TALENT:MASTER:144 == 0 && TALENT:PLAYER:142 == 0)
	RETURN 0

;아이템を持っているか、아이템なし설정でないとダメ
SIF !EXIST_ITEM(29)
	RETURN 0

SIF !USE_ANUS(TARGET)
	RETURN 0

;새댁플레이中はダメ
SIF TEQUIP:33
	RETURN 0

RETURN 1


@COMNAME259
IF TEQUIP:애널로터
	RESULTS = 애널로터해제
ELSE
	RESULTS = 애널로터삽입
ENDIF
@ACT_COM259
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
#LOCALSSIZE 1
CALL COMNAME259
LOCALS = %RESULTS%
PRINTFORML %LOCALS%

;着脱の処理
IF TEQUIP:애널로터 == 0
	TEQUIP:애널로터 = 1
ELSE
	TEQUIP:애널로터 = 0
	SIF TEQUIP:그외 == 1202
		TEQUIP:그외 = 0
	SIF TEQUIP:그외2 == 1202
		TEQUIP:그외2 = 0
	SIF TEQUIP:그외3 == 1202
		TEQUIP:그외3 = 0
	SIF TEQUIP:그외4 == 1202
		TEQUIP:그외4 = 0
	SIF TEQUIP:그외5 == 1202
		TEQUIP:그외5 = 0
	SIF TEQUIP:그외6 == 1202
		TEQUIP:그외6 = 0
ENDIF
;조교자→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 60
DOWNBASE:TARGET:기력 += 150

SOURCE:TARGET:도구쾌Ａ = 180
SOURCE:TARGET:노출 = 50
SOURCE:TARGET:굴종 = 50
SOURCE:TARGET:일탈 = 50

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 70

;Aへの고통は先に処理しておく
;EXP:Ａ경험をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:TARGET:아픔 , 3.00
	TIMES SOURCE:TARGET:반감 , 3.00
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:TARGET:아픔 , 1.20
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:TARGET:아픔 , 0.80
	TIMES SOURCE:TARGET:반감 , 1.50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:TARGET:아픔 , 0.60
	TIMES SOURCE:TARGET:반감 , 1.20
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 0.40
ELSEIF EXP:1 >= EXPLV:5
	TIMES SOURCE:TARGET:아픔 , 0.20
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:TARGET:아픔 , 1.20
	TIMES SOURCE:TARGET:반감 , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:TARGET:아픔 , 1.00
	TIMES SOURCE:TARGET:반감 , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:TARGET:아픔 , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:TARGET:아픔 , 0.20
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;A경험
CALL ADDS_EXP("Ａ경험", 2, TARGET)
;PRINTL Ａ경험＋２

;레즈경험
A = 1
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF


;의존도ベクトル
TFLAG:33 = 3


RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM259
;애널로터挿入中
PRINTL ＜애널에 로터 삽입중＞

DOWNBASE:TARGET:체력 += 30
DOWNBASE:TARGET:기력 += 80

SOURCE:TARGET:도구쾌Ａ += 80
SOURCE:TARGET:아픔 += 100
SOURCE:TARGET:노출 += 25
SOURCE:TARGET:일탈 += 25
SOURCE:TARGET:반감 += 40

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 35

;Aへの고통は先に処理しておく
;EXP:Ａ경험をみる
A = 100
B = 40
IF EXP:1 < EXPLV:1
	TIMES A , 3.00
	TIMES B , 3.00
ELSEIF EXP:1 < EXPLV:2
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF EXP:1 < EXPLV:3
	TIMES A , 0.80
	TIMES B , 1.50
ELSEIF EXP:1 < EXPLV:4
	TIMES A , 0.60
	TIMES B , 1.20
ELSEIF EXP:1 < EXPLV:5
	TIMES A , 0.40
ELSEIF EXP:1 >= EXPLV:5
	TIMES A , 0.20
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES A , 1.20
	TIMES B , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES A , 1.00
	TIMES B , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES A , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES A , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES A , 0.20
ENDIF
SOURCE:TARGET:아픔 += A
SOURCE:TARGET:반감 += B

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;자위実行時は경험＋１
IF SELECTCOM == 11
	CALL ADDS_EXP("Ａ경험", 2, TARGET)
	;PRINTL Ａ경험＋２
ELSE
	CALL ADDS_EXP("Ａ경험", 1, TARGET)
	;PRINTL Ａ경험＋１
ENDIF

RETURN 1

;===================================================
;조교타입
;===================================================
@COMTYPE_259
RETURN COMTYPE("도구계")

@MESSAGE_COM259
;装着時
IF TEQUIP:클리로터
	PRINTFORM %CALLNAME:TARGET%의 
	PRINT 고혈에 로터를 밀어 넣었다
	PRINTFORML 로터에서 늘어진 줄 끝에 달린 분홍색 스위치의 강도를 올린 뒤 
	IF TEQUIP:클리로터 || TEQUIP:로터
		PRINT 다른 스위치 옆에 나란히 고정시켰다
	ELSEIF TEQUIP:양말 == 803
		PRINT 허벅지까지 올라온 양말에 끼워 넣었다
	ELSEIF TEQUIP:양말 == [[衣装:가터스타킹]]
		PRINT 스타킹에 끼워 넣어 고정시켰다
	ELSE
		PRINT 허벅지에 벨트로 고정시켰다
	ENDIF
	PRINTW
ELSE
	PRINTFORM %CALLNAME:TARGET%의 허벅지에서 스위치를 뽑아 잡아당겨 
	PRINT 줄로 연결된 로터를 거칠게 뽑아냈다
	PRINTW 순간의 진공음과 함께 고혈에서 뽑혀나온 로터는 장액으로 젖은채 따스한 김을 내며 떨고있다.
	
ENDIF

