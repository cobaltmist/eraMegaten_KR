;-------------------------------------------------
;유선개발
;특수커맨드
;-------------------------------------------------
@ACT_COM195
PRINTL 유선개발
TSTR:0 = 유선개발
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------

;-------------------------------------------------
;実行決定
;-------------------------------------------------
IF ABL:PLAYER:기교 >= 5 || ABL:순종 >= 5
	IF TALENT:Ｂ민감
		TFLAG:유선개발실패 = RAND:2
	ELSEIF TALENT:Ｂ둔감
		TFLAG:유선개발실패 = RAND:6
	ELSE
		TFLAG:유선개발실패 = RAND:4
	ENDIF
	SIF TALENT:음유 && A > 0
		TFLAG:유선개발실패 = RAND:2
ELSE
	TFLAG:유선개발실패 = RAND:8
ENDIF
SIF ITEM:14 && (TALENT:PLAYER:새드 || ABL:PLAYER:새드끼 > 2) && TFLAG:유선개발실패 > 0
	TFLAG:유선개발실패 -= 1

;CALL KOJO_MESSAGE_COM
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
;소비체력、기력
LOSEBASE:0 += 500
LOSEBASE:1 += 3000

IF ABL:PLAYER:기교 == 4
	SOURCE:아픔 = 5000
ELSEIF ABL:PLAYER:기교 >= 5
	SOURCE:아픔 = 2000
ENDIF

;ABL:순종をみる
IF ABL:순종 == 0
	SOURCE:일탈 = 10000
ELSEIF ABL:순종 == 1
	SOURCE:일탈 = 8000
ELSEIF ABL:순종 == 2
	SOURCE:일탈 = 6000
ELSEIF ABL:순종 == 3
	SOURCE:일탈 = 4000
ELSEIF ABL:순종 == 4
	SOURCE:일탈 = 2000
ELSE
	SOURCE:일탈 = 500
ENDIF

;음유
SIF TALENT:음유
	SOURCE:욕정 = 300

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------

IF TFLAG:유선개발실패 == 0 && TALENT:절벽 == 0
	TALENT:모유체질 = 1
	MAXBASE:모유 = 10000
;		PRINTL 이상경험＋1
;		EXP:50 += 1
	CALL ADDS_ABNORMAL_EXP("유선개발", TARGET)
ENDIF

IF TALENT:남자 == 0 && TALENT:PLAYER:남자 == 0
	PRINTFORML %EXPNAME:40%＋2
	EXP:40 += 2
ELSEIF TALENT:남자 && TALENT:PLAYER:남자
	PRINTFORML %EXPNAME:41%＋2
	EXP:41 += 2
ENDIF


RETURN 1

