@CHECK_COM_ABLE96
;애널후배위시킨다実行판정

;조교자のアヌスが使えないとダメ
SIF !USE_ANUS(PLAYER)
	RETURN 0

;조교대상の페니스が使えないとダメ
SIF !USE_PENIS(TARGET) && !USE_PBAND(TARGET)
	RETURN 0

;촉수조교中はダメ
SIF TEQUIP:25
	RETURN 0

;밧줄・拘束中はダメ
SIF TEQUIP:19
	RETURN 0

;삼각목마騎乗中はダメ
SIF TEQUIP:41
	RETURN 0

;목욕탕플레이中は플레이매트が無いとダメ
IF TEQUIP:31
	SIF !EXIST_ITEM(20)
		RETURN 0
ENDIF

RETURN 1

@JUMP_COM96
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;前のプレイがアナル後背位させるかつ対象の技巧3以上の場合、挿入前立腺責めさせるへ派生
IF PREVCOM == 96 && (ABL:TARGET:2 > 2)
	CALL COM_ABLE_COMMON, 520
	SIF RESULT == 1
		RETURN 520
ENDIF
RETURN -1

@ACT_COM96
;애널후배위시킨다
PRINTL 애널후배위시킨다
TSTR:0 = 애널후배위시킨다
;-------------------------------------------------
;実行決定
;-------------------------------------------------
;CALL KOJO_MESSAGE_COM

;조교자←→조교대상
TFLAG:28 = 3

;-------------------------------------------------
;사정ゲージ체크　●付け발し
;-------------------------------------------------
B = 1500

;ABL:조교대상の기교をみる
IF ABL:TARGET:2 == 0
	TIMES B , 1.00
ELSEIF ABL:TARGET:2 == 1
	TIMES B , 1.20
ELSEIF ABL:TARGET:2 == 2
	TIMES B , 1.40
ELSEIF ABL:TARGET:2 == 3
	TIMES B , 1.60
ELSEIF ABL:TARGET:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:욕망をみる
IF ABL:1 == 0
	TIMES B , 1.00
ELSEIF ABL:1 == 1
	TIMES B , 1.10
ELSEIF ABL:1 == 2
	TIMES B , 1.20
ELSEIF ABL:1 == 3
	TIMES B , 1.30
ELSEIF ABL:1 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES B , 0.60
ELSEIF PALAM:4 < PALAMLV:2
	TIMES B , 0.80
ELSEIF PALAM:4 < PALAMLV:3
	TIMES B , 1.00
ELSEIF PALAM:4 < PALAMLV:4
	TIMES B , 1.20
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES B , 1.40
ENDIF

;조교대상のABL:Ｃ감각をみる
IF ABL:TARGET:3 == 0
	TIMES B , 1.00
ELSEIF ABL:TARGET:3 == 1
	TIMES B , 1.50
ELSEIF ABL:TARGET:3 == 2
	TIMES B , 2.00
ELSEIF ABL:TARGET:3 == 3
	TIMES B , 2.50
ELSEIF ABL:TARGET:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20


SIF HAVE_PENIS(PLAYER)
	BASE:PLAYER:2 += B

;조교대상→조교자
TFLAG:28 = 1
TEQUIP:PLAYER:Ａ삽입자 = TARGET
TFLAG:삽입속행 = 1


;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 40
LOSEBASE:1 += 160

SOURCE:0 = 650
SOURCE:5 = 500
SOURCE:6 = 1000
SOURCE:18 = 500
SIF IS_MALE(TARGET) && (TALENT:쾌감을부정 == 0 || ABL:욕망 > 2) && !IS_LOOKSLIKE_MALE(PLAYER)
	SOURCE:18 /= 10
SOURCE:15 = 750
SOURCE:17 = 1500

;조교자に쾌Ａ
SOURCE:PLAYER:2 = 400

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 700

;-------------------------------------------------
;사정체크
;-------------------------------------------------

;사정先타입の설정
TCVAR:PLAYER:페니스사용대상 = TARGET
TCVAR:PLAYER:사정개소 = 14
;사정전선택の可・
TFLAG:704 = 0

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;사정に関係なく行われる処理はここから
;조교자のＡ⇔奴隷のＰの불결が移動
CALL MOVE_STAIN("애널", PLAYER, "페니스", TARGET)

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:10+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:10+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;조교자のＡ경험
CALL ADDS_EXP("Ａ경험", 1, PLAYER)

;PRINTFORML 봉사쾌락경험(조교자) + 1
;EXP:PLAYER:봉사쾌락경험 += 1
;PRINTFORML 성교경험(조교자) + 1
;EXP:PLAYER:성교경험 += 1
CALL ADDS_EXP("봉사쾌락경험", 1, PLAYER)
CALL ADDS_EXP("성교경험", 1, PLAYER)
;수간경험
IF (TALENT:TARGET:짐승 || TALENT:TARGET:새) && !(TALENT:PLAYER:짐승 || TALENT:PLAYER:새)
	CALL ADDS_EXP("수간경험", 1, PLAYER)
	
ENDIF
;레즈경험
A = 4
IF IS_LESBIAN(TARGET,PLAYER)
;	PRINTFORML %EXPNAME:40%+{A}
;	PRINTFORML %EXPNAME:40%(조교자)+{A}
;	EXP:40 += A
;	EXP:PLAYER:40 += A
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
;	PRINTFORML %EXPNAME:41%+{A}
;	PRINTFORML %EXPNAME:41%(조교자)+{A}
;	EXP:41 += A
;	EXP:PLAYER:41 += A
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
	
	
ENDIF

;의존도ベクトル
TFLAG:33 = 3

;의존도보정値(主人かつ연모、음란、복종で1。친애、창부、예속で2）
IF (TALENT:3 || TALENT:4 || TALENT:5) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:6 || TALENT:7 || TALENT:8) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

;봉사쾌락フラグ
TFLAG:100 = 1

RETURN 1

@MESSAGE_COM96
	;前回の調教がアナル後背位させるであり、入れっぱなしの場合
	IF TEQUIP:주도권 && TEQUIP:PLAYER:51 && (PREVCOM == 96 || PREVCOM == 520)
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:PLAYER%のアヌスの感触を味わいながら、再び腰を動かし始めた……
	;同様に、前回の調教がアナル正常位させるである場合
	ELSEIF TEQUIP:주도권 && TEQUIP:PLAYER:51 && PREVCOM == 95
		PRINTFORMW %CALLNAME:TARGET%は腸内に挿入したまま、%CALLNAME:PLAYER%を裏返すように四つん這いにさせ、再び腰を動かし始めた……
	;同様に前回の調教がアナル対面座位かアナル背面座位かアナル騎乗位の場合
	ELSEIF TEQUIP:주도권 && TEQUIP:PLAYER:51 && (PREVCOM == 97 || PREVCOM == 902 || PREVCOM == 903)
		PRINTFORMW %CALLNAME:TARGET%は挿入したまま%CALLNAME:PLAYER%を四つん這いにし、再び腰を動かし始めた……
	ELSEIF TEQUIP:주도권
		PRINTFORM %CALLNAME:TARGET%は\@TALENT:영맹 ? 乱暴に#\@%CALLNAME:PLAYER%を四つん這いにさせると、後ろから
		PRINTFORML \@ ABL:PLAYER:Ａ감각 > 2 ? 기대로 실룩거리는  #\@ 애널에 페니스를 대고
		;「TARGETが真性の새드か」「새드끼持ちか」で分岐
		IF (TALENT:새드 || ABL:새드끼 >= 5) && RAND:3 == 0
			PRINT 몸으로 두드리듯이
		ELSEIF TALENT:새드 || (ABL:새드끼 >= 4 && RAND:2 == 0)
			PRINT 2、3회 항문을 문지르고 
		ELSEIF RAND:2 == 0
			PRINT 느긋하게
		ENDIF
		;「TARGETが真性の새드か」「새드끼持ちか」で分岐
		IF (TALENT:새드 || ABL:새드끼 >= 5) && RAND:3 == 0
			PRINT 안쪽까지 찔러넣어、
		ELSEIF ABL:새드끼 >= 3 && RAND:2 == 0
			PRINT 삽입해、
		ELSE
			PRINT 넣어、
		ENDIF
		;연모で추가
		SIF (TALENT:연모 || TALENT:친애) && RAND:3 >= 1
			PRINTFORM 쾌감에 몸부림치는 %조사처리(CALLNAME:PLAYER,"를")% 사랑스러운 듯이 바라보면서
		;「TARGETが真性の새드か」「새드끼持ちか」で分岐
		IF (TALENT:새드 || ABL:새드끼 == 5) && RAND:3 == 0
			PRINTFORMW 격렬하게 허리를 피스톤운동해 %CALLNAME:PLAYER%의 직장 안을 깊게 도려냈다…
		ELSEIF TALENT:새드 || (ABL:새드끼 >= 3 && RAND:2 == 0)
			PRINTFORMW 마른소리를 내며 허리를 쳐올렸다…
		ELSE
			PRINTFORMW 허리를 앞뒤로 움직이기 시작했다…
		ENDIF
	;前回の調教がアナル後背位させるであり、抜かないフラグがある場合
	ELSEIF TEQUIP:PLAYER:51 && (PREVCOM == 96 || PREVCOM == 520)
			PRINTFORMW %CALLNAME:TARGET%をアナルに受け入れたまま、%CALLNAME:PLAYER%はもっと突くようにせがんだ……
	;同様に前回の調教がアナル正常位かアナル対面座位かアナル背面座位かアナル騎乗位の場合
	ELSEIF TEQUIP:PLAYER:51 && (PREVCOM == 95 || PREVCOM == 97 || PREVCOM == 902 || PREVCOM == 903)
			PRINTFORMW 繋がったまま%CALLNAME:PLAYER%は四つん這いになり、もっと突くようにと強請った……
	ELSE
		PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% %CALLNAME:TARGET%의 앞에 납죽 엎드려、
		SIF ABL:PLAYER:욕망 > 3
			PRINTFORM 음란하게 엉덩이를 내밀고 
		SIF EXP:PLAYER:Ａ경험 == 0
			PRINT 남자를 받아들인적 없는
		PRINTFORMW 자신의 애널에 삽입을 졸랐다…
		PRINTFORM %타겟은% 
		;음란
		SIF ABL:욕망 >= 3
			PRINTFORM 항문에 페니스를 2、3회 문지르고서、
		;연모で추가
		SIF TALENT:연모 && RAND:3 >= 1
			PRINT 몸을 딱 밀착시켜、
		PRINTFORMW %CALLNAME:PLAYER%의 애널에 삽입했다…
	ENDIF
;-------------------------------------------------
;実行판정処理
;애널정상위시킨다と同じ
;-------------------------------------------------
@COM_ORDER_96, ARG, ARG:1
CALL COM_ORDER_95, ARG, ARG:1
;-------------------------------------------------
;実行値
;-------------------------------------------------
