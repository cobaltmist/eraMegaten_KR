@ACT_COM91
;파이즈리한다


PRINTL 파이즈리한다
TSTR:0 = 파이즈리한다


;CALL KOJO_MESSAGE_COM
;-------------------------------------------------
;사정ゲージ체크
;-------------------------------------------------
B = 1500

;ABL:조교자の기교をみる
IF ABL:PLAYER:2 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:2 == 1
	TIMES B , 1.20
ELSEIF ABL:PLAYER:2 == 2
	TIMES B , 1.40
ELSEIF ABL:PLAYER:2 == 3
	TIMES B , 1.60
ELSEIF ABL:PLAYER:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:욕망をみる
IF ABL:1 == 0
	TIMES B , 1.00
ELSEIF ABL:1 == 1
	TIMES B , 1.10
ELSEIF ABL:1 == 2
	TIMES B , 1.20
ELSEIF ABL:1 == 3
	TIMES B , 1.30
ELSEIF ABL:1 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;조수のABL:Ｂ감각をみる
IF ABL:PLAYER:Ｂ감각 == 0
	TIMES B , 0.50
ELSEIF ABL:PLAYER:Ｂ감각 == 1
	TIMES B , 0.80
ELSEIF ABL:PLAYER:Ｂ감각 == 2
	TIMES B , 1.00
ELSEIF ABL:PLAYER:Ｂ감각 == 3
	TIMES B , 1.20
ELSEIF ABL:PLAYER:Ｂ감각 == 4
	TIMES B , 1.50
ELSE
	TIMES B , 2.00
ENDIF

	LOCAL:99 = B

;再装填処理中は사정ゲージは20分の1に
IF CFLAG:PLAYER:9 > 0
	B /= 20
	LOCAL:99 /= 20
ENDIF

	BASE:PLAYER:99 += LOCAL:99
;조교자→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
LOSEBASE:0 += 25
LOSEBASE:1 += 100

SOURCE:0 = 150
SOURCE:13 = 15
SOURCE:17 = 15
SOURCE:18 = 20

;조교자に쾌Ｂ
SOURCE:PLAYER:3 = 150
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 20

;-------------------------------------------------
;사정체크
;-------------------------------------------------

;사정先타입の설정
TCVAR:PLAYER:페니스사용대상 = TARGET
TCVAR:PLAYER:사정개소 = 14
;사정전선택の可・不可
TFLAG:704 = 0

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;奴隷のＰ⇔조교자のＢの불결が移動
CALL MOVE_STAIN("페니스", TARGET, "가슴", PLAYER)

;PRINTFORML 봉사쾌락경험(조교자) + 1
;EXP:PLAYER:봉사쾌락경험 += 1
CALL ADDS_EXP("봉사쾌락경험", 1, PLAYER)

;의존도ベクトル
IF TALENT:PLAYER:140 == 1
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 2
ENDIF

RETURN 1


