;-------------------------------------------------
;바기나전극
;道具사용コマンド、레벨2
;-------------------------------------------------
@CHECK_COM_ABLE253

;바이브実行판정
;촉수조교中はダメ
SIF TEQUIP:25
	RETURN 0

;삼각목마騎乗中はダメ
SIF TEQUIP:41
	RETURN 0

;해제はいつでも가능
SIF TEQUIP:바기나전극
	RETURN 1

;처녀면 불가
SIF TALENT:0 == 1
    RETURN 0
	
;아이템を持っているか、아이템なし설정でないとダメ
SIF !EXIST_ITEM(29)
	RETURN 0

SIF !USE_VAGINA(TARGET,,1)
	RETURN 0

;조교대상が소인체형の場合は主人が금단의지식持ちじゃないとダメ
;SIF TALENT:142 && (TALENT:MASTER:144 == 0 && EXP:Ｖ경험 > 10)
SIF TALENT:소인체형 && (TALENT:MASTER:144 == 0 && TALENT:PLAYER:142 == 0)
	RETURN 0

;처녀で、순종4以下・레즈끼4以下のASSIの場合바이브はできない（새드ならＯＫ）
IF TALENT:0 && ASSIPLAY
	SIF (ABL:ASSI:0 <= 4 || ABL:ASSI:11 <= 4) && TALENT:ASSI:83 == 0
		RETURN 0
ENDIF
;윤활부족で、순종3以下・레즈끼3以下のASSIの場合바이브はできない（새드ならＯＫ）
IF PALAM:4 < PALAMLV:2 && ASSIPLAY
	SIF (ABL:ASSI:0 <= 3 || ABL:ASSI:11 <= 3) && TALENT:ASSI:83 == 0
		RETURN 0
ENDIF

;목욕탕플레이中はダメ
SIF TEQUIP:31
	RETURN 0

RETURN 1

@COMNAME253
IF TEQUIP:바기나전극
	RESULTS = 바기나전극해제
ELSE
	RESULTS = 바기나전극삽입
ENDIF

@ACT_COM253
#LOCALSSIZE 1
CALL COMNAME253
LOCALS = %RESULTS%
PRINTFORML %LOCALS%

;着脱の処理
IF TEQUIP:바기나전극 == 0
	TEQUIP:바기나전극 = 1
ELSE
	TEQUIP:바기나전극 = 0
ENDIF


;조교者→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 90
DOWNBASE:TARGET:기력 += 140
SOURCE:일탈 = 1100

;ABL:V감각을 본다
IF ABL:Ｖ감각 == 0
    SOURCE:쾌Ｖ = 200
    SOURCE:아픔 = 800
    SOURCE:굴종 = 400
ELSEIF ABL:Ｖ감각 == 1
    SOURCE:쾌Ｖ = 500
    SOURCE:아픔 = 800
    SOURCE:굴종 = 700
ELSEIF ABL:Ｖ감각 == 2
    SOURCE:쾌Ｖ = 900
    SOURCE:아픔 = 800
    SOURCE:굴종 = 1150
ELSEIF ABL:Ｖ감각 == 3
    SOURCE:쾌Ｖ = 1700
    SOURCE:아픔 = 1000
    SOURCE:굴종 = 1750
ELSEIF ABL:Ｖ감각 == 4
    SOURCE:쾌Ｖ = 2200
    SOURCE:아픔 = 1200
    SOURCE:굴종 = 3000
ELSE
    SOURCE:쾌Ｖ = 3400
    SOURCE:아픔 = 1400
    SOURCE:굴종 = 4500
ENDIF

;EXP:V경험을 본다
IF EXP:Ｖ경험 < EXPLV:1
    TIMES SOURCE:쾌Ｖ , 0.20
    SOURCE:아픔 += 5500
ELSEIF EXP:Ｖ경험 < EXPLV:2
    TIMES SOURCE:쾌Ｖ , 0.60
    SOURCE:아픔 += 300
ELSEIF EXP:Ｖ경험 < EXPLV:3
    TIMES SOURCE:쾌Ｖ , 1.00
    SOURCE:아픔 += 50
ELSEIF EXP:Ｖ경험 < EXPLV:4
    TIMES SOURCE:쾌Ｖ , 1.20
    SOURCE:아픔 += 10
ELSEIF EXP:Ｖ경험 < EXPLV:5
    TIMES SOURCE:쾌Ｖ , 1.40
    SOURCE:아픔 += 0
ELSE
    TIMES SOURCE:쾌Ｖ , 1.60
    SOURCE:아픔 += 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:윤활 < PALAMLV:1
    TIMES SOURCE:쾌Ｖ , 0.10
    SOURCE:아픔 += 1000
    TIMES SOURCE:아픔 , 3.00
ELSEIF PALAM:윤활 < PALAMLV:2
    TIMES SOURCE:쾌Ｖ , 0.40
    SOURCE:아픔 += 400
    TIMES SOURCE:아픔 , 1.00
ELSEIF PALAM:윤활 < PALAMLV:3
    TIMES SOURCE:쾌Ｖ , 1.00
    TIMES SOURCE:아픔 , 0.50
ELSEIF PALAM:윤활 < PALAMLV:4
    TIMES SOURCE:쾌Ｖ , 1.40
    TIMES SOURCE:아픔 , 0.20
ELSE
    TIMES SOURCE:쾌Ｖ , 1.80
    TIMES SOURCE:아픔 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:욕정 < PALAMLV:1
    TIMES SOURCE:쾌Ｖ , 0.80
ELSEIF PALAM:욕정 < PALAMLV:2
    TIMES SOURCE:쾌Ｖ , 0.90
ELSEIF PALAM:욕정 < PALAMLV:3
    TIMES SOURCE:쾌Ｖ , 1.00
ELSEIF PALAM:욕정 < PALAMLV:4
    TIMES SOURCE:쾌Ｖ , 1.10
ELSE
    TIMES SOURCE:쾌Ｖ , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES SOURCE:쾌Ｖ , 0.80
ELSEIF ABL:순종 == 1
    TIMES SOURCE:쾌Ｖ , 0.90
ELSEIF ABL:순종 == 2
    TIMES SOURCE:쾌Ｖ , 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:쾌Ｖ , 1.10
ELSEIF ABL:순종 == 4
    TIMES SOURCE:쾌Ｖ , 1.20
ELSE
    TIMES SOURCE:쾌Ｖ , 1.30
ENDIF

;작은 몸집 체형
SIF TALENT:유아체형
    TIMES SOURCE:아픔 , 2.00

;V민감, 둔감을 본다
;쾌V자체의 체크는 다음에 정리해 준다
IF TALENT:Ｖ둔감
    TIMES SOURCE:아픔 , 1.50
    TIMES SOURCE:굴종 , 1.50
    TIMES SOURCE:일탈 , 1.50
ELSEIF TALENT:Ｖ민감
    TIMES SOURCE:아픔 , 0.60
    TIMES SOURCE:굴종 , 0.60
    TIMES SOURCE:일탈 , 0.60
ENDIF



;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;V경험
CALL ADDS_EXP("Ｖ경험", 1, TARGET)

;레즈경험
A = 1
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;의존도ベクトル
TFLAG:33 = 5


RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM253
;바이브挿入中

PRINTL ＜바기나에 전극 삽입중＞

DOWNBASE:TARGET:체력 += 70
DOWNBASE:TARGET:기력 += 110

;ABL:V감각을 본다
IF ABL:Ｖ감각 == 0
    LOCAL = 250
    LOCAL:1 = 400
ELSEIF ABL:Ｖ감각 == 1
    LOCAL = 600
    LOCAL:1 = 700
ELSEIF ABL:Ｖ감각 == 2
    LOCAL = 1200
    LOCAL:1 = 1150
ELSEIF ABL:Ｖ감각 == 3
    LOCAL = 1900
    LOCAL:1 = 1750
ELSEIF ABL:Ｖ감각 == 4
    LOCAL = 2500
    LOCAL:1 = 3000
ELSE
    LOCAL = 3900
    LOCAL:1 = 4500
ENDIF

;EXP:V경험을 본다
;처녀는 있을 수 없다
IF EXP:Ｖ경험 < EXPLV:2
    TIMES LOCAL , 0.60
    LOCAL:2 = 150
ELSEIF EXP:Ｖ경험 < EXPLV:3
    TIMES LOCAL , 1.00
    LOCAL:2 = 20
ELSEIF EXP:Ｖ경험 < EXPLV:4
    TIMES LOCAL , 1.20
    LOCAL:2 = 0
ELSEIF EXP:Ｖ경험 < EXPLV:5
    TIMES LOCAL , 1.40
    LOCAL:2 = 0
ELSE
    TIMES LOCAL , 1.60
    LOCAL:2 = 0
ENDIF

;PALAM:윤활을 본다
IF PALAM:윤활 < PALAMLV:1
    TIMES LOCAL , 0.10
    LOCAL:2 += 400
ELSEIF PALAM:윤활 < PALAMLV:2
    TIMES LOCAL , 0.40
    LOCAL:2 += 150
ELSEIF PALAM:윤활 < PALAMLV:3
    TIMES LOCAL , 1.00
    TIMES LOCAL:2 , 0.50
ELSEIF PALAM:윤활 < PALAMLV:4
    TIMES LOCAL , 1.40
    TIMES LOCAL:2 , 0.20
ELSE
    TIMES LOCAL , 1.80
    TIMES LOCAL:2 , 0.10
ENDIF

;PALAM:욕정을 본다
IF PALAM:욕정 < PALAMLV:1
    TIMES LOCAL , 0.80
ELSEIF PALAM:욕정 < PALAMLV:2
    TIMES LOCAL , 0.90
ELSEIF PALAM:욕정 < PALAMLV:3
    TIMES LOCAL , 1.00
ELSEIF PALAM:욕정 < PALAMLV:4
    TIMES LOCAL , 1.10
ELSE
    TIMES LOCAL , 1.20
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES LOCAL , 0.80
ELSEIF ABL:순종 == 1
    TIMES LOCAL , 0.90
ELSEIF ABL:순종 == 2
    TIMES LOCAL , 1.00
ELSEIF ABL:순종 == 3
    TIMES LOCAL , 1.10
ELSEIF ABL:순종 == 4
    TIMES LOCAL , 1.20
ELSE
    TIMES LOCAL , 1.30
ENDIF

;작은 몸집 체형
SIF TALENT:유아체형
    TIMES LOCAL:2 , 2.00

;V민감, 둔감을 본다
;쾌V자체의 체크는 다음에 정리해 준다
IF TALENT:Ｖ둔감
    TIMES LOCAL , 1.50
    TIMES LOCAL:1 , 1.50
    TIMES LOCAL:2 , 1.50
ELSEIF TALENT:Ｖ민감
    TIMES LOCAL , 0.60
    TIMES LOCAL:1 , 0.60
    TIMES LOCAL:2 , 0.60
ENDIF

SOURCE:쾌Ｖ += LOCAL
SOURCE:굴종 += LOCAL:1
SOURCE:아픔 += LOCAL:2
SOURCE:일탈 += 1100

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;자위実行時は경험＋１
IF SELECTCOM == 11
	CALL ADDS_EXP("Ｖ경험", 2, TARGET)
ELSE
	CALL ADDS_EXP("Ｖ경험", 1, TARGET)
ENDIF

RETURN 1

;===================================================
;조교타입
;===================================================
@COMTYPE_253
RETURN COMTYPE("하드계")


