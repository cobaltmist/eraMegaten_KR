@CHECK_COM_ABLE154
;요정오나홀実行판정

;조교자が소인체형なら主人が금단의지식を持っていないとダメ
SIF TALENT:PLAYER:소인체형 && TALENT:MASTER:금단의지식 == 0
	RETURN 0

;조교대상が처녀で、순종4以下・레즈끼4以下のASSIの場合はできない（새드ならＯＫ）
IF TALENT:0 && PLAYER == ASSI && !IS_MALE(PLAYER)
	SIF (ABL:PLAYER:0 <= 4 || ABL:PLAYER:11 <= 4) && TALENT:PLAYER:83 == 0
		RETURN 0
ENDIF
;윤활부족で、순종3以下・레즈끼3以下のASSIの場合はできない（새드ならＯＫ）
IF PALAM:4 < PALAMLV:2 && PLAYER == ASSI && !IS_MALE(PLAYER)
	SIF (ABL:PLAYER:0 <= 3 || ABL:PLAYER:11 <= 3) && TALENT:PLAYER:83 == 0
		RETURN 0
ENDIF

;조교대상の바기나が使えないとダメ
SIF !USE_VAGINA(TARGET)
	RETURN 0
;조교자の페니스が使えないとダメ、또는페니스밴드がないとダメ
SIF !USE_PENIS(PLAYER) && !USE_PBAND(PLAYER)
	RETURN 0

;촉수조교中はダメ
SIF TEQUIP:25
	RETURN 0

;삼각목마騎乗中はダメ
SIF TEQUIP:41
	RETURN 0

;목욕탕플레이中は플레이매트が無いとダメ
IF TEQUIP:31
	SIF !EXIST_ITEM(20)
		RETURN 0
ENDIF

;기본的に조교자が큰체형体型이상で体型-1までサイズOKとする
;조교대상が요정さんサイズなら조교자は誰でもOK
SIF TALENT:소인체형
	RETURN 1
;조교자が큰체형なら유아체형～普通までOK
SIF TALENT:PLAYER:큰체형 && (TALENT:유아체형 || (TALENT:유아체형 == 0 && TALENT:유아체형 == 0 && TALENT:큰체형 == 0 && TALENT:거체 == 0) )
	RETURN 1
;조교자が거체なら유아체형～큰체형体型までOK
SIF TALENT:PLAYER:거체 && TALENT:거체 == 0 
	RETURN 1

RETURN 0

;-------------------------------------------------
;요정오나홀
;섹스系コマンド、레벨1
;-------------------------------------------------
@COMNAME154
IF TALENT:소인체형
	RESULTS = 요정오나홀
ELSE
	RESULTS = 오나홀취급
ENDIF

@ACT_COM154
IF TALENT:소인체형
	RESULTS = 요정오나홀
ELSE
	RESULTS = 오나홀취급
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------

TSTR:0 = 요정오나홀
;CALL KOJO_MESSAGE_COM

;V경험을수반하는커맨드のフラグ(처녀膜재생対応)
TFLAG:19 = 1
SIF (TALENT:3 || TALENT:6) && ASSIPLAY == 0 && EXP:0 == 0
	TFLAG:20 = 1

;처녀상실時の텍스트用処理
SIF TFLAG:60 && (PREVCOM == 60 || PREVCOM == 61 || PREVCOM == 66 || PREVCOM == 68 || PREVCOM == 69) && ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)) && TFLAG:31
	TFLAG:31 = 2
;-------------------------------------------------
;사정ゲージ체크
;-------------------------------------------------
B = 2500

;ABL:기교をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.10
ELSEIF ABL:2 == 2
	TIMES B , 1.20
ELSEIF ABL:2 == 3
	TIMES B , 1.30
ELSEIF ABL:2 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;ABL:조교자の기교をみる
IF ABL:PLAYER:2 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:2 == 1
	TIMES B , 1.20
ELSEIF ABL:PLAYER:2 == 2
	TIMES B , 1.40
ELSEIF ABL:PLAYER:2 == 3
	TIMES B , 1.60
ELSEIF ABL:PLAYER:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:욕망をみる
IF ABL:1 == 0
	TIMES B , 1.00
ELSEIF ABL:1 == 1
	TIMES B , 1.10
ELSEIF ABL:1 == 2
	TIMES B , 1.20
ELSEIF ABL:1 == 3
	TIMES B , 1.30
ELSEIF ABL:1 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES B , 0.60
ELSEIF PALAM:4 < PALAMLV:2
	TIMES B , 0.80
ELSEIF PALAM:4 < PALAMLV:3
	TIMES B , 1.00
ELSEIF PALAM:4 < PALAMLV:4
	TIMES B , 1.20
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES B , 1.40
ENDIF

;ABL:봉사정신をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;플레이어のABL:Ｃ감각をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;EXP:Ｖ경험をみる
;처녀だと増える
SIF EXP:0 < EXPLV:1
	TIMES B , 2.50

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20


SIF HAVE_PENIS(PLAYER)
	BASE:PLAYER:2 += B

;조교자←→조교대상
TFLAG:28 = 3
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
LOSEBASE:0 += 250
LOSEBASE:1 += 150

SOURCE:1 = 650
SOURCE:4 = 100
SOURCE:5 = 250
SOURCE:7 = 1100
SOURCE:8 = 1600
SOURCE:9 = 200
SOURCE:13 = 100
SOURCE:14 = 1800
SOURCE:17 = 900
SOURCE:18 = 900

;上のほうで計算した불결데이터
SOURCE:15 = Y*10 + 60

;조교자に쾌Ｃ
SOURCE:PLAYER:0 = 200
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 300

;정조관념持ちで연모、친애持ち以外には常に반발のソースを추가する（섹스系のみ）
SIF TALENT:30 && !IS_LOVER(TARGET)
	SOURCE:18 += 1000

;처녀だった場合は、고통のソースと반발のソースを추가する
IF TALENT:0 == 1
	SOURCE:7 += 2000
	SOURCE:18 += 3000
ENDIF

;재생처녀だった場合は、고통のソースと반발のソースを추가する
IF TALENT:1 == 1
	SOURCE:7 += 500
	SOURCE:18 += 1000
ENDIF

;フィストなどのVへの大きな고통は先に処理しておく
;EXP:Ｖ경험をみる
IF EXP:0 < EXPLV:1
	TIMES SOURCE:7 , 2.50
	TIMES SOURCE:18 , 4.00
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:18 , 3.50
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:7 , 1.80
	TIMES SOURCE:18 , 3.00
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:7 , 1.60
	TIMES SOURCE:18 , 2.50
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:7 , 1.40
	TIMES SOURCE:18 , 2.00
ELSEIF EXP:0 >= EXPLV:5
	TIMES SOURCE:7 , 1.20
	TIMES SOURCE:18 , 2.00
ENDIF


;PALAM:윤활をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:7 , 1.20
	TIMES SOURCE:18 , 1.50
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:7 , 0.70
	TIMES SOURCE:18 , 1.20
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:7 , 0.40
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:7 , 0.10
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:7 , 0.10
ENDIF

;중독による중독충족のソースは先に処理しておく
;섹스중독を見る
IF ABL:18 == 0
	TIMES SOURCE:9 , 0.00
ELSEIF ABL:18 == 1
	TIMES SOURCE:9 , 0.30
ELSEIF ABL:18 == 2
	TIMES SOURCE:9 , 0.60
ELSEIF ABL:18 == 3
	TIMES SOURCE:9 , 0.90
ELSEIF ABL:18 == 4
	TIMES SOURCE:9 , 1.20
ELSEIF ABL:18 == 5
	TIMES SOURCE:9 , 1.50
ELSEIF ABL:18 == 6
	TIMES SOURCE:9 , 1.80
ELSEIF ABL:18 == 7
	TIMES SOURCE:9 , 2.10
ELSEIF ABL:18 == 8
	TIMES SOURCE:9 , 2.40
ELSEIF ABL:18 == 9
	TIMES SOURCE:9 , 3.00
ELSEIF ABL:18 >= 10
	TIMES SOURCE:9 , 5.00
ENDIF

;-------------------------------------------------
;사정체크
;-------------------------------------------------
;사정先타입の설정
TCVAR:PLAYER:페니스사용대상 = TARGET
TCVAR:PLAYER:사정개소 = 1
;사정전선택の可・
TFLAG:704 = 1

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;사정に関係なく行われる処理はここから
;질내に파과혈の불결を付加
SIF TALENT:0 == 1 || TALENT:1 == 1
	CALL SET_STAIN("질내", "파과혈", TARGET)
;奴隷の질내⇔조교자のＰの불결が移動
CALL MOVE_STAIN("질내", TARGET, "페니스", PLAYER)

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;Ｖ경험
CALL ADDS_EXP("Ｖ경험", 6, TARGET)

;Ｖ확장경험
;IF EXP:52 == 0 && EXP:53 == 0 
;	EXP:50 += 1
;	PRINTL 이상경험＋１
;	CALL ADDS_ABNORMAL_EXP("확장첫경험", TARGET)
;ENDIF

IF TALENT:0 == 1
	CALL ADDS_ABNORMAL_EXP("처녀상실（요정오나홀）", TARGET)
	SIF !IS_MALE(PLAYER)
		CALL ADDS_ABNORMAL_EXP("처녀탈취（동성）", PLAYER)
	SIF !IS_MALE(PLAYER)
		CALL ADDS_ABNORMAL_EXP("처녀상실（동성）", TARGET)
ENDIF

;근친상간の場合이상경험부여
IF 근친체크(TARGET , PLAYER)
	CALL ADDS_ABNORMAL_EXP( @"근친상간（%RESULTS:0%）", TARGET)
	CALL ADDS_ABNORMAL_EXP( @"근친상간（%RESULTS:1%）", PLAYER)
ENDIF

CALL ADDS_EXP("Ｖ확장경험", 2, TARGET)

;성교경험
CALL ADDS_EXP("성교경험", 1, TARGET)
CALL ADDS_EXP("성교경험", 1, PLAYER)

;레즈경험
A = 4
;레즈경험
A = 4
IF IS_LESBIAN(TARGET,PLAYER)
;	PRINTFORML %EXPNAME:40%+{A}
;	PRINTFORML %EXPNAME:40%(조교자)+{A}
;	EXP:40 += A
;	EXP:PLAYER:40 += A
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
;	PRINTFORML %EXPNAME:41%+{A}
;	PRINTFORML %EXPNAME:41%(조교자)+{A}
;	EXP:41 += A
;	EXP:PLAYER:41 += A
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
	
	
ENDIF

;애정경험
IF TALENT:3 == 1 && ASSIPLAY == 0
	CALL ADDS_EXP("애정경험", 1, PLAYER)
	CALL ADDS_EXP("애정경험", 2, TARGET)
ELSEIF TALENT:6 == 1 && ASSIPLAY == 0
	CALL ADDS_EXP("애정경험", 1, PLAYER)
	CALL ADDS_EXP("애정경험", 4, TARGET)
ENDIF

;의존도ベクトル
TFLAG:33 = 1

;의존도보정値(연모で1、친애で2）
IF TALENT:3 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:6 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

RETURN 1


@MESSAGE_COM154
;전회と今回の조교자が同じで、전회の조교が정상위であり、抜かないフラグがある場合
IF TFLAG:60 && PREVCOM == 154 && ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0))
	;조교자が남자か반인반요か후타나리である
	IF HAVE_PENIS(PLAYER)
		PRINTFORMW %CALLNAME:TARGET%의 질내의 감촉을 맛보면서、다시 %조사처리(CALLNAME:TARGET,"로")% 페니스를 훑기 시작했다……
	;페니스밴드を사용している場合
	ELSE
		PRINTFORMW %CALLNAME:TARGET%의 질내에서 뽑지 않고、다시 %조사처리(CALLNAME:TARGET,"로")% 페니스를 훑기 시작했다……
	ENDIF
ELSE
	PRINTFORM %타겟을% 움켜쥐고、그 
	;처녀
	IF TALENT:0
		PRINT 남자를 모르는 
	;윤활LV4이상
	ELSEIF PALAM:4 >= PALAMLV:4
		PRINT 흠뻑 젖은 
	ENDIF
	;유아체형か소인체형で
	IF TALENT:유아체형 || TALENT:소인체형
		PRINTL 작은 균열에 페니스를 대고 단숨에 꿰뚫어、
	ELSE
		PRINTL 비순에 페니스를 대곤 단숨에 관통해、
	ENDIF
	IF ABL:마조끼 > 4 && ABL:Ｖ감각 > 4
		PRINT 황홀한 표정으로 
	ELSEIF ABL:마조끼 > 2 && ABL:Ｖ감각 > 2
		PRINT 입을 뻐끔뻐끔거리면서도 얼굴을 상기시키는 
	ELSE
		PRINT 고통스러운 나머지 거품을 문 
	ENDIF
	
	PRINTFORML %CALLNAME:TARGET%의 몸을、오나홀처럼 사용해 페니스를 훑었다。
	PRINTFORMW 움직일 때마다 %CALLNAME:TARGET%의 배가 %CALLNAME:PLAYER%의 페니스의 모양으로 부풀어 올랐다……
ENDIF

;===================================================
;조교타입
;===================================================
@COMTYPE_154
RETURN COMTYPE("하드계")


