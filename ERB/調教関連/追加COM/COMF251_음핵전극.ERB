;-------------------------------------------------
;음핵전극
;道具사용コマンド、레벨2
;-------------------------------------------------
@CHECK_COM_ABLE251

;해제はいつでも가능
SIF TEQUIP:음핵전극
	RETURN 1

;奴隷が복종か예속でないとダメ
SIF TALENT:5 == 0 && TALENT:8 == 0 && TALENT:("함락내역(예속)") == 0
	RETURN 0

;촉수조교中はダメ
SIF TEQUIP:25
	RETURN 0

;아이템を持っているか、아이템なし설정でないとダメ
SIF !EXIST_ITEM(24)
	RETURN 0

SIF !USE_CLI(TARGET)
	RETURN 0

;목욕탕플레이中はダメ
SIF TEQUIP:31
	RETURN 0

;ASSIの場合、기교3이상じゃないと使えない。
IF ASSIPLAY
	SIF ABL:ASSI:2 < 3
		RETURN 0
ENDIF

;소인체형持ちは、双方が조교자と조교대상ならOK
;それ以外の場合は主人が금단의지식持ちじゃないとダメ
SIF TALENT:142 && (TALENT:MASTER:144 == 0 && TALENT:PLAYER:142 == 0)
	RETURN 0

RETURN 1

@COMNAME251
IF TEQUIP:음핵전극 == 1
	RESULTS = 음핵전극해제
ELSE
	RESULTS = 음핵전극부착
ENDIF

@ACT_COM251
#LOCALSSIZE 1
CALL COMNAME251
LOCALS = %RESULTS%
PRINTFORML %LOCALS%



;着脱の処理
IF TEQUIP:음핵전극 == 0
	TEQUIP:음핵전극 = 1
ELSE
	TEQUIP:음핵전극 = 0
ENDIF

;조교자→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 80
DOWNBASE:TARGET:기력 += 140

SOURCE:TARGET:일탈 = 650

SOURCE:일탈 = 650

;ABL:C감각을 본다
IF ABL:Ｃ감각 == 0
    SOURCE:쾌Ｃ = 150
    SOURCE:아픔 = 600
    SOURCE:굴종 = 800
ELSEIF ABL:Ｃ감각 == 1
    SOURCE:쾌Ｃ = 350
    SOURCE:아픔 = 600
    SOURCE:굴종 = 1400
ELSEIF ABL:Ｃ감각 == 2
    SOURCE:쾌Ｃ = 600
    SOURCE:아픔 = 600
    SOURCE:굴종 = 2300
ELSEIF ABL:Ｃ감각 == 3
    SOURCE:쾌Ｃ = 1500
    SOURCE:아픔 = 800
    SOURCE:굴종 = 3500
ELSEIF ABL:Ｃ감각 == 4
    SOURCE:쾌Ｃ = 2100
    SOURCE:아픔 = 1000
    SOURCE:굴종 = 6000
ELSE
    SOURCE:쾌Ｃ = 3000
    SOURCE:아픔 = 1200
    SOURCE:굴종 = 9000
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES SOURCE:쾌Ｃ , 0.80
ELSEIF ABL:순종 == 1
    TIMES SOURCE:쾌Ｃ , 0.90
ELSEIF ABL:순종 == 2
    TIMES SOURCE:쾌Ｃ , 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:쾌Ｃ , 1.10
ELSEIF ABL:순종 == 4
    TIMES SOURCE:쾌Ｃ , 1.20
ELSE
    TIMES SOURCE:쾌Ｃ , 1.30
ENDIF

;PALAM:욕정을 본다
IF PALAM:욕정 < PALAMLV:1
    TIMES SOURCE:쾌Ｃ , 0.80
ELSEIF PALAM:욕정 < PALAMLV:2
    TIMES SOURCE:쾌Ｃ , 0.90
ELSEIF PALAM:욕정 < PALAMLV:3
    TIMES SOURCE:쾌Ｃ , 1.00
ELSEIF PALAM:욕정 < PALAMLV:4
    TIMES SOURCE:쾌Ｃ , 1.10
ELSE
    TIMES SOURCE:쾌Ｃ , 1.20
ENDIF

;C민감, 둔감을 본다
;쾌C자체의 체크는 다음에 정리해 준다
IF TALENT:101
    TIMES SOURCE:아픔 , 1.50
    TIMES SOURCE:굴종 , 1.50
    TIMES SOURCE:일탈 , 1.50
ELSEIF TALENT:100
    TIMES SOURCE:아픔 , 0.60
    TIMES SOURCE:굴종 , 0.60
    TIMES SOURCE:일탈 , 0.60
ENDIF


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 30

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
A = 1
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;의존도ベクトル
TFLAG:33 = 5


RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM251
IF TEQUIP:25
	PRINTL ＜음핵 전극 장착중＞
ENDIF


;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
DOWNBASE:0 += 50
DOWNBASE:1 += 100

LOCAL:2 = 1000

;ABL:C감각을 본다
IF ABL:Ｃ감각 == 0
    LOCAL = 250
    LOCAL:1 = 1000
ELSEIF ABL:Ｃ감각 == 1
    LOCAL = 600
    LOCAL:1 = 2000
ELSEIF ABL:Ｃ감각 == 2
    LOCAL = 1200
    LOCAL:1 = 3000
ELSEIF ABL:Ｃ감각 == 3
    LOCAL = 1900
    LOCAL:1 = 5000
ELSEIF ABL:Ｃ감각 == 4
    LOCAL = 2500
    LOCAL:1 = 8000
ELSE
    LOCAL = 3900
    LOCAL:1 = 12000
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES LOCAL , 0.80
ELSEIF ABL:순종 == 1
    TIMES LOCAL , 0.90
ELSEIF ABL:순종 == 2
    TIMES LOCAL , 1.00
ELSEIF ABL:순종 == 3
    TIMES LOCAL , 1.10
ELSEIF ABL:순종 == 4
    TIMES LOCAL , 1.20
ELSE
    TIMES LOCAL , 1.30
ENDIF

;PALAM:욕정을 본다
IF PALAM:욕정 < PALAMLV:1
    TIMES LOCAL , 0.80
ELSEIF PALAM:욕정 < PALAMLV:2
    TIMES LOCAL , 0.90
ELSEIF PALAM:욕정 < PALAMLV:3
    TIMES LOCAL , 1.00
ELSEIF PALAM:욕정 < PALAMLV:4
    TIMES LOCAL , 1.10
ELSE
    TIMES LOCAL , 1.20
ENDIF

;C민감, 둔감을 본다
;쾌C자체의 체크는 다음에 정리해 준다
IF TALENT:Ｃ둔감
    TIMES LOCAL , 1.50
    TIMES LOCAL:1 , 1.50
    TIMES LOCAL:2 , 1.50
ELSEIF TALENT:Ｃ민감
    TIMES LOCAL , 0.60
    TIMES LOCAL:1 , 0.60
    TIMES LOCAL:2 , 0.60
ENDIF

SOURCE:쾌Ｃ += LOCAL
SOURCE:굴종 += LOCAL:1
SOURCE:아픔 += LOCAL:2


RETURN 1

;===================================================
;조교타입
;===================================================
@COMTYPE_251
RETURN COMTYPE("하드계")


@MESSAGE_COM251
PRINTL
IF TEQUIP:음핵전극
    IF MARK:반발각인 > 1
        PRINTFORMW %조사선택(CALLNAME:PLAYER, "가")% 미처 손을 쓰기도 전에, %조사선택(CALLNAME:TARGET, "는")% 전극을 벗어 던져버렸다….
	ELSEIF TALENT:클리피어스 || TEQUIP:속옷（하） == 620
		PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 음핵에 연결된 전극을 떼어냈다….
    ELSE
        PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 음핵에서 전극을 벗겨 주었다….
    ENDIF
ELSE
    IF MARK:반발각인 > 1
        PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% 싫어서 버둥대는 %조사선택(CALLNAME:TARGET, "를")% 억눌렀다.
		IF TALENT:클리피어스 || TEQUIP:속옷（하） == 620
			PRINTFORMW 그리고, %CALLNAME:TARGET%의 음핵에 달린 피어스를 가볍게 당기며 전극을 연결하곤 스위치를 넣었다….
		ELSE
			PRINTFORMW 그리고, %CALLNAME:TARGET%의\@(TALENT:Ｃ민감) ? 민감한 # \@음핵에 전극을 붙이고 스위치를 넣었다….
		ENDIF
    ELSEIF TALENT:클리피어스 || TEQUIP:속옷（하） == 620
		PRINTFORMW 그리고, %CALLNAME:TARGET%의 음핵에 달린 피어스를 가볍게 당기며 전극을 연결하곤 스위치를 넣었다….
	ELSE	
        PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의\@(TALENT:Ｃ민감) ? 민감한 # \@음핵에 전극을 붙이고 스위치를 넣었다….
    ENDIF
ENDIF
