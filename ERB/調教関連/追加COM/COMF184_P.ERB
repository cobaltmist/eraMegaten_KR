;ビット説明
;TEUIP:29
;廃ビル=1
;森林=2
;公園=4
;水辺=8

@ACT_COM184_P
;야외플레이(公園)
;公園：무서움・노출・일탈があがりやすい。반감は高め。

PRINTL 야외플레이(공원)
TSTR:0 = 야외플레이(공원)

TEQUIP:29 |= 4

;구상→総括Ｍ→個別セルコム
;CALL KOJO_MESSAGE_COM

;---MOD-1	ADD
;---MOD-1	COMMENT		다리노출と배꼽노출は取り敢えずセーフ。一先ず雑に処理をスキップ。
IF TEQUIP:유방노출 && TEQUIP:유두노출 && TEQUIP:음순노출 && TEQUIP:둔부노출 && TEQUIP:음순보임 != -1 && TEQUIP:둔부보임 != -1 && (TALENT:연모 || TALENT:친애)
	RETURN 1
ENDIF
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
LOSEBASE:체력 += 0
LOSEBASE:기력 += 150
;8恐れ　13露出　17逸脱　18反感
SOURCE:TARGET:무서움 = 800
SOURCE:TARGET:노출 = 1300
SOURCE:TARGET:일탈 = 300
SOURCE:TARGET:반감 = 600

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:TARGET:울굴 += 600
;公園보정
SIF TALENT:억압 || TALENT:저항
	SOURCE:TARGET:울굴 += 300

;公園補正
SOURCE:TARGET:무서움 += 50
TIMES SOURCE:TARGET:노출 , 1.30
TIMES SOURCE:TARGET:일탈 , 1.30
TIMES SOURCE:TARGET:반감 , 1.50

;苦痛を伴わないマゾっ気及びマゾの処理は先にしておく
;マゾっ気を見る
;10液体　11欲情　13露出
IF ABL:마조끼 == 0
	TIMES SOURCE:TARGET:노출 , 0.80
	SOURCE:TARGET:10 += 0
	SOURCE:TARGET:11 += 0
ELSEIF ABL:마조끼 == 1
	TIMES SOURCE:TARGET:노출 , 1.00
	SOURCE:TARGET:10 += 20
	SOURCE:TARGET:11 += 30
ELSEIF ABL:마조끼 == 2
	TIMES SOURCE:TARGET:노출 , 1.20
	SOURCE:TARGET:10 += 40
	SOURCE:TARGET:11 += 70
ELSEIF ABL:마조끼 == 3
	TIMES SOURCE:TARGET:노출 , 1.40
	SOURCE:TARGET:10 += 70
	SOURCE:TARGET:11 += 120
ELSEIF ABL:마조끼 == 4
	TIMES SOURCE:TARGET:노출 , 1.50
	SOURCE:TARGET:10 += 110
	SOURCE:TARGET:11 += 180
ELSEIF ABL:마조끼 == 5
	TIMES SOURCE:TARGET:노출 , 1.70
	SOURCE:TARGET:10 += 150
	SOURCE:TARGET:11 += 250
ELSEIF ABL:마조끼 == 6
	TIMES SOURCE:TARGET:노출 , 1.90
	SOURCE:TARGET:10 += 200
	SOURCE:TARGET:11 += 300
ELSEIF ABL:마조끼 == 7
	TIMES SOURCE:TARGET:노출 , 2.10
	SOURCE:TARGET:10 += 250
	SOURCE:TARGET:11 += 350
ELSEIF ABL:마조끼 == 8
	TIMES SOURCE:TARGET:노출 , 2.30
	SOURCE:TARGET:10 += 300
	SOURCE:TARGET:11 += 400
ELSEIF ABL:마조끼 == 9
	TIMES SOURCE:TARGET:노출 , 2.50
	SOURCE:TARGET:10 += 350
	SOURCE:TARGET:11 += 450
ELSEIF ABL:마조끼 >= 10
	TIMES SOURCE:TARGET:노출 , 3.00
	SOURCE:TARGET:10 += 500
	SOURCE:TARGET:11 += 500
ENDIF

;의존도ベクトル
TFLAG:33 = 3


RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM184_P
;야외플레이(公園)

PRINTL ＜야외플레이중(공원)＞

;---MOD-1	ADD
;---MOD-1	COMMENT		다리노출と배꼽노출は取り敢えずセーフ。一先ず雑に処理をスキップ。
IF TEQUIP:유방노출 && TEQUIP:유두노출 && TEQUIP:음순노출 && TEQUIP:둔부노출 && TEQUIP:음순보임 != -1 && TEQUIP:둔부보임 != -1 && (TALENT:연모 || TALENT:친애)
	RETURN 1
ENDIF
;---MOD-1	END

LOSEBASE:체력 += 50
LOSEBASE:기력 += 100

;8恐れ　13露出　17逸脱　18反感
SOURCE:TARGET:무서움 += 1000
SOURCE:TARGET:일탈 += 1000
SOURCE:TARGET:반감 += 1000


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:억압 || TALENT:저항
	SOURCE:TARGET:울굴 += 700
;公園보정
SIF TALENT:억압 || TALENT:저항
	SOURCE:TARGET:울굴 += 300


;고통を伴わない마조끼及び마조の処理は先にしておく
;마조끼を見る
A = 2000

;公園보정
TIMES A , 1.20
SOURCE:TARGET:일탈 += 200
SOURCE:TARGET:반감 += 400

;10액체　11욕정　13노출
IF ABL:마조끼 == 0
	TIMES A , 0.80
	SOURCE:TARGET:10 += 0
	SOURCE:TARGET:11 += 0
ELSEIF ABL:마조끼 == 1
	TIMES A , 1.00
	SOURCE:TARGET:10 += 20
	SOURCE:TARGET:11 += 30
ELSEIF ABL:마조끼 == 2
	TIMES A , 1.20
	SOURCE:TARGET:10 += 40
	SOURCE:TARGET:11 += 70
ELSEIF ABL:마조끼 == 3
	TIMES A , 1.40
	SOURCE:TARGET:10 += 70
	SOURCE:TARGET:11 += 120
ELSEIF ABL:마조끼 == 4
	TIMES A , 1.50
	SOURCE:TARGET:10 += 110
	SOURCE:TARGET:11 += 180
ELSEIF ABL:마조끼 == 5
	TIMES A , 1.70
	SOURCE:TARGET:10 += 150
	SOURCE:TARGET:11 += 250
ELSEIF ABL:마조끼 == 6
	TIMES A , 1.90
	SOURCE:TARGET:10 += 200
	SOURCE:TARGET:11 += 300
ELSEIF ABL:마조끼 == 7
	TIMES A , 2.10
	SOURCE:TARGET:10 += 250
	SOURCE:TARGET:11 += 350
ELSEIF ABL:마조끼 == 8
	TIMES A , 2.30
	SOURCE:TARGET:10 += 300
	SOURCE:TARGET:11 += 400
ELSEIF ABL:마조끼 == 9
	TIMES A , 2.50
	SOURCE:TARGET:10 += 350
	SOURCE:TARGET:11 += 450
ELSEIF ABL:마조끼 >= 10
	TIMES A , 3.00
	SOURCE:TARGET:10 += 500
	SOURCE:TARGET:11 += 500
ENDIF
SOURCE:TARGET:노출 += A

;의존도ベクトルを強制的に변경
SIF TFLAG:33 == 1 || TFLAG:33 == 2
	TFLAG:33 = 3

;의존도보정値(복종で1、예속で2　強制변경なので리셋）
IF TALENT:복종 && ASSIPLAY == 0
	TFLAG:30 = 1
ELSEIF TALENT:예속 && ASSIPLAY == 0
	TFLAG:30 = 2
ENDIF


RETURN 1

