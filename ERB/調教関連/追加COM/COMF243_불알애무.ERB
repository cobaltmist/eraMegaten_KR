;-------------------------------------------------
;강제 불알 핥기
;봉사계コマンド、레벨1
;-------------------------------------------------
;………………………………………………
;실행 판정
;………………………………………………
@CHECK_COM_ABLE243
;수음実行판정
SIF !USE_PENIS(PLAYER)
	RETURN 0
SIF !USE_MOUTH(TARGET)
	RETURN 0
SIF !USE_HAND(TARGET)
	RETURN 0
;촉수조교中はダメ
SIF TEQUIP:25
	RETURN 0
;삼각목마騎乗中はダメ
SIF TEQUIP:41
	RETURN 0
RETURN 1

@ACT_COM243
PRINTL 강제 불알 핥기

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 강제 불알 핥기
;CALL KOJO_MESSAGE_COM

;조교대상→조교者
TFLAG:28 = 1

;-------------------------------------------------
;사정ゲージ체크
;-------------------------------------------------
B = 700

;ABL:조교者の기교をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;TALENT:혀놀림、고양이혀をみる
SIF TALENT:52 || TALENT:54
	TIMES B , 2.00

;플레이어のABL:Ｃ감각をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20


SIF HAVE_PENIS(PLAYER)
	BASE:PLAYER:2 += B


;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 10
DOWNBASE:TARGET:기력 += 100

SOURCE:TARGET:정애 = 100
SOURCE:TARGET:성행위 = 300
SOURCE:TARGET:달성 = 150
SOURCE:TARGET:굴종 = 700
SOURCE:TARGET:일탈 = 200
SOURCE:TARGET:반감 = 200

;上のほうで計算した불결데이터
SOURCE:TARGET:불결 = Y*10 + 60

;조교者に쾌Ｃ
SOURCE:PLAYER:0 = 120
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 400

;-------------------------------------------------
;사정체크
;-------------------------------------------------
;사정先타입の설정
TCVAR:PLAYER:페니스사용대상 = TARGET
TCVAR:PLAYER:사정개소 = 6
;사정先選択の可・不可
TFLAG:704 = 1

;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;사정に関係なく行われる処理はここから
;奴隷の입⇔조교者のＰの불결が移動
CALL MOVE_STAIN("입", TARGET, "페니스", PLAYER)
CALL MOVE_STAIN("손", TARGET, "페니스", PLAYER)
;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:8+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:8+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;펠라경험
;PRINTFORML %EXPNAME:22%＋１
CALL ADDS_EXP("펠라경험", 1, TARGET)

;레즈경험
A = 7
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;애정경험
IF (TALENT:3 == 1 || TALENT:6 == 1) && ASSIPLAY == 0
;	PRINTS EXPNAME:23
;	PRINTL +1
	CALL ADDS_EXP("애정경험", 1, TARGET)
;	PRINTFORML 애정경험(주인)＋１
	CALL ADDS_EXP("애정경험", 1, MASTER)
ENDIF

;奴隷が고양이혀持ちの場合は、조교者に가학쾌락경험を추가
IF TALENT:TARGET:54
	CALL ADDS_EXP("가학쾌락경험", 1, TARGET)
;	PRINTFORML %EXPNAME:32%＋１
ENDIF

;의존도ベクトル
TFLAG:33 = 1

;의존도보정値(主人かつ연모、음란、복종で1。친애、창부、예속で2）
IF (TALENT:3 || TALENT:4 || TALENT:5) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:6 || TALENT:7 || TALENT:8) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

TFLAG:100 = 1

RETURN 1
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------
@COM_ORDER_243, ARG, ARG:1
IF TEQUIP:ARG:음문발동
	CALL ADD_COM_ORDER, "판정無視", "음문 발동중"
	RETURN 0
ENDIF
CALL COM_ORDER_81, ARG, ARG:1
;-------------------------------------------------
;実行値
;-------------------------------------------------
@COM_ORDER_NUM_243
;그도그럴게 입에 넣는거 만큼 위험한걸 시키자나;;;;
RETURN 24

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@MESSAGE_COM243
PRINTL
;연모か망신か친애で主人조교のとき
IF (TALENT:연모 || TALENT:친애 || TALENT:망신) && ASSIPLAY == 0
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 불알을 핥게 하기 위해 페니스를 가까이 가져가자
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 조심스럽게 %CALLNAME:PLAYER%의 불알을 핥기 시작했다.
;헌신적인 경우
ELSEIF TALENT:헌신적
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 불알을 핥게 하기 위해 페니스를 가까이 가져가자
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 열심히 불알을 핥아 %조사선택(CALLNAME:PLAYER, "를")% 즐겁게 했다….
;도착적인 경우
ELSEIF TALENT:도착적
    PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 불알을 핥게 하기 위해 페니스를 가까이 가져가자
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 더러운 곳을 빠는 자신에게 도취된 것 같다.
;반발각인持ちで연모がない場合
ELSEIF MARK:반발각인 && TALENT:연모 == 0
    PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% 싫어서 버둥대는 %CALLNAME:TARGET%에게 불알을 핥게 했다.
;그 외의 경우
ELSE
    PRINTFORMW %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%에게 불알을 핥게 했다.
ENDIF

;===================================================
;조교타입
;===================================================
@COMTYPE_243
RETURN COMTYPE("봉사계")
