;-------------------------------------------------
;유두깨물기
;SM系コマンド、레벨4
;-------------------------------------------------
@CHECK_COM_ABLE111
SIF ITEM_NIPLE(TARGET) > 0
	RETURN 0
SIF !USE_HAND(PLAYER)
	RETURN 0
SIF !USE_MOUTH(PLAYER)
	RETURN 0

RETURN 1

@ACT_COM111
PRINTL 유두깨물기
TSTR:0 = 유두깨물기
;CALL KOJO_MESSAGE_COM

;조교者→조교대상
TFLAG:28 = 2
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 15
DOWNBASE:TARGET:기력 += 150

SOURCE:노출 = 280
SOURCE:성행위 = 90
SOURCE:불결 = 80

;ABL:B감각을 본다
IF ABL:Ｂ감각 == 0
    SOURCE:쾌Ｂ = 100
    SOURCE:정애 = 60
    SOURCE:아픔 = 150
ELSEIF ABL:Ｂ감각 == 1
    SOURCE:쾌Ｂ = 400
    SOURCE:정애 = 120
    SOURCE:아픔 = 300
ELSEIF ABL:Ｂ감각 == 2
    SOURCE:쾌Ｂ = 900
    SOURCE:정애 = 180
    SOURCE:아픔 = 450
ELSEIF ABL:Ｂ감각 == 3
    SOURCE:쾌Ｂ = 1600
    SOURCE:정애 = 230
    SOURCE:아픔 = 600
ELSEIF ABL:Ｂ감각 == 4
    SOURCE:쾌Ｂ = 2000
    SOURCE:정애 = 250
    SOURCE:아픔 = 800
ELSE
    SOURCE:쾌Ｂ = 2800
    SOURCE:정애 = 300
    SOURCE:아픔 = 1000
ENDIF

IF TALENT:모유체질
    ;ABL:분유중독을 본다
    IF ABL:34 == 0
        TIMES SOURCE:쾌Ｂ , 1.00
        TIMES SOURCE:정애 , 1.00
    ELSEIF ABL:34 == 1
        TIMES SOURCE:쾌Ｂ , 1.20
        TIMES SOURCE:정애 , 1.00
    ELSEIF ABL:34 == 2
        TIMES SOURCE:쾌Ｂ , 1.20
        TIMES SOURCE:정애 , 1.20
    ELSEIF ABL:34 == 3
        TIMES SOURCE:쾌Ｂ , 1.40
        TIMES SOURCE:정애 , 1.20
    ELSEIF ABL:34 == 4
        TIMES SOURCE:쾌Ｂ , 1.40
        TIMES SOURCE:정애 , 1.40
    ELSE
        TIMES SOURCE:쾌Ｂ , 1.60
        TIMES SOURCE:정애 , 1.60
    ENDIF
ENDIF

;ABL:순종을 본다
IF ABL:순종 == 0
    TIMES SOURCE:쾌Ｂ , 0.50
    TIMES SOURCE:정애 , 0.60
    TIMES SOURCE:성행위 , 0.60
    TIMES SOURCE:불결 , 1.40
ELSEIF ABL:순종 == 1
    TIMES SOURCE:쾌Ｂ , 0.70
    TIMES SOURCE:정애 , 0.80
    TIMES SOURCE:성행위 , 0.80
    TIMES SOURCE:불결 , 1.20
ELSEIF ABL:순종 == 2
    TIMES SOURCE:쾌Ｂ , 0.80
    TIMES SOURCE:정애 , 1.00
    TIMES SOURCE:성행위 , 1.00
    TIMES SOURCE:불결 , 1.00
ELSEIF ABL:순종 == 3
    TIMES SOURCE:쾌Ｂ , 1.00
    TIMES SOURCE:정애 , 1.20
    TIMES SOURCE:성행위 , 1.20
    TIMES SOURCE:불결 , 0.80
ELSEIF ABL:순종 == 4
    TIMES SOURCE:쾌Ｂ , 1.20
    TIMES SOURCE:정애 , 1.60
    TIMES SOURCE:성행위 , 1.60
    TIMES SOURCE:불결 , 0.40
ELSE
    TIMES SOURCE:쾌Ｂ , 1.50
    TIMES SOURCE:정애 , 2.00
    TIMES SOURCE:성행위 , 2.00
    TIMES SOURCE:불결 , 0.00
ENDIF


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;조교자의 TALENT:고양이혀을 본다
SIF TALENT:PLAYER:고양이혀
    TIMES SOURCE:쾌Ｂ , 0.70
;조교자가［유아 퇴행］
IF TALENT:PLAYER:유아／유아퇴행
    TIMES SOURCE:정애 , 1.20
;조교자가［유아］,［유치］
ELSEIF TALENT:PLAYER:유치
    TIMES SOURCE:쾌Ｂ , 1.20
    TIMES SOURCE:정애 , 1.20
ENDIF
;음유
IF TALENT:음유
    TIMES SOURCE:쾌Ｂ , 1.40
    TIMES SOURCE:정애 , 1.40
    TIMES SOURCE:불결 , 0.80
ENDIF
;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;奴隷のＢ⇔조교者の指の불결が移動
CALL MOVE_STAIN("가슴", TARGET, "손", PLAYER)

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:1+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ非被흡수 == 0
	LOCAL = (RAND:1+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
A = 2
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;의존도ベクトル
TFLAG:33 = 5

;의존도보정値(복종で1、예속で2）
IF TALENT:5 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:8 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1

;===================================================
;조교타입
;===================================================
@COMTYPE_111
RETURN COMTYPE("ＳＭ계")


