;-------------------------------------------------
;겨드랑이잡
;봉사계コマンド、레벨1
;-------------------------------------------------
;………………………………………………
;실행 판정
;………………………………………………
@CHECK_COM_ABLE242
;수음実行판정
SIF !USE_PENIS(PLAYER,3) && !ITEM_PENIS(PLAYER)
	RETURN 0
SIF !USE_HAND(TARGET) && !IS_BEAST(TARGET)
	RETURN 0
;촉수조교中はダメ
SIF TEQUIP:25
	RETURN 0
;삼각목마騎乗中はダメ
SIF TEQUIP:41
	RETURN 0
RETURN 1

@ACT_COM242
PRINTL 겨드랑이잡

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 겨드랑이잡
;CALL KOJO_MESSAGE_COM

;조교대상→조교者
TFLAG:28 = 1

;-------------------------------------------------
;사정ゲージ체크
;-------------------------------------------------
B = 500

;ABL:조교者の기교をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:순종をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:봉사정신をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;플레이어のABL:Ｃ감각をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は사정ゲージは20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20


SIF HAVE_PENIS(PLAYER)
	BASE:PLAYER:2 += B

;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
DOWNBASE:TARGET:체력 += 10
DOWNBASE:TARGET:기력 += 100

SOURCE:TARGET:정애 = 50
SOURCE:TARGET:성행위 = 250
SOURCE:TARGET:달성 = 50
SOURCE:TARGET:굴종 = 350
SOURCE:TARGET:일탈 = 100
SOURCE:TARGET:반감 = 100

;上のほうで計算した불결데이터
SOURCE:TARGET:불결 = Y*10 + 60

;조교者に쾌Ｃ
SOURCE:PLAYER:0 = 80
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;억압、저항持ちには、억울ソース추가
SIF TALENT:32 || TALENT:34
	SOURCE:TARGET:울굴 += 200

;-------------------------------------------------
;사정체크
;-------------------------------------------------
;사정先타입の설정
TCVAR:PLAYER:페니스사용대상 = TARGET
TCVAR:PLAYER:사정개소 = 9
;사정전선택の可・
TFLAG:704 = 1
;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;사정に関係なく行われる処理はここから
;奴隷の指⇔조교者のＰの불결が移動
CALL MOVE_STAIN("머리카락", TARGET, "페니스", PLAYER)

;-------------------------------------------------
;吸精処理
;-------------------------------------------------
IF CFLAG:PLAYER:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:7+1) * BASE:LV
	CALL DRAIN_MAG,PLAYER,LOCAL,TARGET
ELSEIF CFLAG:TARGET:ＭＡＧ비피흡수 == 0
	LOCAL = (RAND:7+1) * BASE:LV
	CALL DRAIN_MAG,TARGET,LOCAL,PLAYER
ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;레즈경험
A = 4
IF IS_LESBIAN(TARGET,PLAYER)
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
ENDIF

;의존도ベクトル
TFLAG:33 = 1

;의존도보정値(主人かつ연모、음란、복종で1。친애、창부、예속で2）
IF (TALENT:3 || TALENT:4 || TALENT:5) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:6 || TALENT:7 || TALENT:8) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

;봉사쾌락フラグ
TFLAG:100 = 1


RETURN 1
;-------------------------------------------------
;実行판정処理
;수음と同じ
;-------------------------------------------------
@COM_ORDER_242, ARG, ARG:1
IF TEQUIP:ARG:음문발동
	CALL ADD_COM_ORDER, "판정無視", "음문 발동중"
	RETURN 0
ENDIF
CALL COM_ORDER_80, ARG, ARG:1

;-------------------------------------------------
;実行値
;-------------------------------------------------
@COM_ORDER_NUM_242
RETURN 16

;--------------------------------------------------
;조교시 메시지
;--------------------------------------------------
@MESSAGE_COM242
PRINTL
PRINTFORML %조사선택(CALLNAME:PLAYER, "는")% %CALLNAME:TARGET%의 겨드랑이를 조금 벌린 후, 그 사이에 자지를 끼우고 비비기 시작했다.
IF TALENT:연모 == 0 && TALENT:복종 == 0 && (TALENT:음란 || TALENT:창부) == 0 && MARK:반발각인 > 1
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 자신의 겨드랑이를 왕복하는 자지를 더럽다는 눈초리로 보고 있다.
ELSEIF ABL:13 < 3 && ABL:노출증 < 3
    PRINTFORMW %조사선택(CALLNAME:TARGET, "는")% 완전 질렸다는 듯한 표정으로 겨드랑이로 계속 마찰하고 있다….
ELSE
    PRINTFORML %조사선택(CALLNAME:TARGET, "는")% 간지럽다는 듯이 약간 몸을 꼬면서, 
    PRINTW 자신의 겨드랑이에 앞뒤로 왕복하는 그것을 홍조 띤 시선으로 바라보고 있다….
ENDIF

;===================================================
;조교타입
;===================================================
@COMTYPE_242
RETURN COMTYPE("봉사계")

