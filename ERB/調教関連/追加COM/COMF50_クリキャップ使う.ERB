;-------------------------------------------------
;클리캡쓴다
;道具사용コマンド、레벨2
;촉수클리자극시킨다
;-------------------------------------------------
@CHECK_COM_ABLE50

;촉수조교中はダメ
SIF TEQUIP:25
	RETURN 0

;아이템を持っているか、아이템なし설정でないとダメ
SIF !EXIST_ITEM(2)
	RETURN 0

;해제はいつでも가능
SIF TEQUIP:PLAYER:11
	RETURN 1


;소인체형持ちは、双方が조교자と조교대상ならOK
;それ以外の場合は主人が금단의지식持ちじゃないとダメ
SIF TALENT:PLAYER:142 && PLAYER != MASTER && TALENT:MASTER:144 == 0
	RETURN 0

SIF !USE_CLI(PLAYER)
	RETURN 0

;샤워中はダメ
SIF TEQUIP:32
	RETURN 0
RETURN 1

@CHECK_COM_ABLE172

;촉수클리자극
SIF TEQUIP:25 == 0
	RETURN 0

;남자だとダメ
SIF !HAVE_CLITORIS(PLAYER)
	RETURN 0
RETURN 1

@ACT_COM50
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
IF TEQUIP:25
	PRINTL 촉수클리자극시킨다
	TSTR:0 = 촉수클리자극시킨다
	SELECTCOM = 172
ELSE
	PRINTL 전동클리캡쓴다
	TSTR:0 = 전동클리캡쓴다
ENDIF
;CALL KOJO_MESSAGE_COM

;着脱の処理
IF TEQUIP:PLAYER:11 == 0
	TEQUIP:PLAYER:11 = 1
ELSE
	TEQUIP:PLAYER:11 = 0
ENDIF

;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;소비체력、기력
LOSEBASE:0 += 10
LOSEBASE:1 += 10

SOURCE:4 = 10
SOURCE:13 = 10
SOURCE:17 = 10

SOURCE:PLAYER:도구쾌Ｃ = 200


;-------------------------------------------------
;불결の処理
;-------------------------------------------------
;촉수클리자극なら、촉수体液が바기나に付く
SIF TEQUIP:25
	STAIN:PLAYER:3 |= 32

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
A = 1
IF IS_LESBIAN(TARGET,PLAYER)
;	PRINTFORML %EXPNAME:40%+{A}
;	PRINTFORML %EXPNAME:40%(조교자)+{A}
;	EXP:40 += A
;	EXP:PLAYER:40 += A
	CALL ADDS_EXP("레즈경험", A, TARGET)
	CALL ADDS_EXP("레즈경험", A, PLAYER)
ELSEIF IS_GAY(TARGET,PLAYER)
;	PRINTFORML %EXPNAME:41%+{A}
;	PRINTFORML %EXPNAME:41%(조교자)+{A}
;	EXP:41 += A
;	EXP:PLAYER:41 += A
	CALL ADDS_EXP("ＢＬ경험", A, TARGET)
	CALL ADDS_EXP("ＢＬ경험", A, PLAYER)
	
	
ENDIF

;촉수소환時、촉수경험の리셋をする
;SIF TEQUIP:25 == 1
;	T = 0

;의존도ベクトル
TFLAG:33 = 1


RETURN 1


;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM50
IF TEQUIP:25
	PRINTL ＜촉수클리자극중(조교자)＞
ELSE
	PRINTL ＜클리캡장착중(조교자)＞
ENDIF


;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------

SOURCE:PLAYER:도구쾌Ｃ += 100


;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;촉수경험加算
SIF TEQUIP:25 == 1
	CALL ADDS_EXP("촉수경험", 1, PLAYER)
;	T += 1


RETURN 1

;-------------------------------------------------
;조교時テキスト
;-------------------------------------------------
;50,클리캡쓴다
;-------------------------------------------------
@MESSAGE_COM50
;装着時
IF TEQUIP:PLAYER:11
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 스스로 전동클리캡을 달았다
;해제時
ELSE
	PRINTFORMW %조사처리(CALLNAME:PLAYER,"는")% 전동클리캡을 떼었다
ENDIF

;-------------------------------------------------
;172,촉수클리자극시킨다
;-------------------------------------------------
@MESSAGE_COM172
;装着時
IF TEQUIP:PLAYER:11
	PRINTFORM 미세하게 갈라진 촉수가、%CALLNAME:PLAYER%의 
	PRINTFORMW 클리토리스를 괴롭히기 시작했다……
;해제時
ELSE
	PRINTFORMW 촉수는 %CALLNAME:PLAYER%의 클리토리스를 괴롭히는 것을 그만두었다
ENDIF

;===================================================
;조교타입
;===================================================
@COMTYPE_50
RETURN COMTYPE("대조교자도구계")

