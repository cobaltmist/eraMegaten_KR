;-------------------------------------------------
;EVENTEND関係（X1をキャラ番号に置換）
;調教終了時のセリフ
;-------------------------------------------------
@EVENTEND_KX1
;--------------------------------------------------
;特定キャラが助手、主人時に特殊キャラ口上を呼び出す(Y1を特定キャラ、X1を奴隷のキャラ番号に置換）
;--------------------------------------------------
;IF NO:MASTER == Y1
;	CALL EVENTEND_PY1_KX1
;	RETURN 0
;ENDIF
;IF ASSI > 0 && ASSIPLAY
;	SIF NO:ASSI == Y1
;	CALL EVENTEND_PY1_KX1
;	RETURN 0
;ENDIF
;--------------------------------------------------
;調教終了時のセリフ
;--------------------------------------------------
DRAWLINE
;反発刻印Lv3
IF MARK:3 == 3 && (CFLAG:217 < 1 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:217 = 1
;屈服刻印Lv1以下
ELSEIF MARK:2 <= 1 && (CFLAG:219 < 1 || FLAG:7 == 2)
	PRINTFORMW 
	CFLAG:219 = 1
;屈服刻印Lv2
ELSEIF MARK:2 == 2
	PRINTFORMW 
	CFLAG:219 = 2
;屈服刻印Lv3＋陥落素質無し
ELSEIF MARK:2 == 3 && (TALENT:3 == 0 && TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0)
	PRINTFORMW 
	CFLAG:219 = 3
;陥落済み＋体力500未満
ELSEIF BASE:0 < 500 && (CFLAG:218 < 1 || FLAG:7 == 2)
	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		PRINTFORMW 
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:218 = 1
;陥落済み
ELSEIF CFLAG:219 < 4 || FLAG:7 == 2
	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		PRINTFORMW 
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:219 = 4
ENDIF
RETURN 0

