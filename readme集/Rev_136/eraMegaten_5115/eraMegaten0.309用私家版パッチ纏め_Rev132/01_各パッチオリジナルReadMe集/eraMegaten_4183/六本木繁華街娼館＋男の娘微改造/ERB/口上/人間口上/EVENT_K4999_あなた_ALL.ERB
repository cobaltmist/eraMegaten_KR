	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		PRINTFORMW 
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:218 = 1
;陥落済み
ELSEIF CFLAG:219 < 4 || FLAG:7 == 2
	;恋慕、親愛
	IF TALENT:3 || TALENT:6
		PRINTFORMW 
	;淫乱、娼婦
	ELSEIF TALENT:4 || TALENT:7
		PRINTFORMW 
	;服従、隷属
	ELSEIF TALENT:5 || TALENT:8
		PRINTFORMW 
	ENDIF
	CFLAG:219 = 4
ENDIF
RETURN 0

