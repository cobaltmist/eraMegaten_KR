@EVENT_DUNGEON_END_31
SIF ダンジョンフラグ:31:2 == 1
	ダンジョンフラグ:31:2 = 2
	;ベリンダ登場１
SIF ダンジョンフラグ:31:2 == 3
	ダンジョンフラグ:31:2 = 4
	;ベリンダ登場２
SIF ダンジョンフラグ:31:6 >= 1 && ダンジョンフラグ:31:6 < 3
	ダンジョンフラグ:31:6 += 10
	;早坂がハヤちゃん化
SIF ダンジョンフラグ:31:22 == 2
	ダンジョンフラグ:31:22 = 3
	
SIF ダンジョンフラグ:31:25 == 1
	ダンジョンフラグ:31:25 = 0
;=======================================================================
;指定したマスに進入可能か
;=======================================================================
@CAN_ENTER_31,ARG,ARG:1
IF FLAG:現M == 1 && ARG >= 12 && ARG <= 18 && ARG:1 <= 3 && (DA:(ARG):(ARG:1) % 10 == 2)
	CALL MESSAGE_WINDOW_D, "", @"＞扉には鍵がかかっている/＞個室へ勝手に入れないようにしているのだろう…"
	RETURN 0
ELSEIF FLAG:現M == 1 && (ARG == 13 && ARG:1 == 11)
	CALL FOUNTAIN_OF_TRISH
	RETURN 0
ELSEIF FLAG:現M == 2 && (DA:(ARG):(ARG:1) / 10 == 1)
	RETURN 1
ELSE
	CALL CAN_ENTER,ARG,ARG:1
	RETURN RESULT
ENDIF


